<script type="text/javascript">
	function setCountry(country)
	{
		if (country == "--") return;

		var expireDate = new Date();
		expireDate.setDate(expireDate.getDate() + (365*99));

		var value = "AS_country=" + country + "; expires=" + expireDate.toGMTString() + "; path=/";
		document.cookie = value;
		
		location.reload();
	}
</script>
<style>
	#countrycurrencyselector
	{
/*		position: absolute;
		left: 600px;
		top: 20px;
*/
		float: right;
		color:white;
		font-size:10pt;
	}
</style>
