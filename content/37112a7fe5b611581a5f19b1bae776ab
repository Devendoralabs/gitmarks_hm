<html><head><title>nandnerd.info | Sculpt Texture Maker | Revolve</title><link rel="stylesheet" href="../ps/style.css?08060921" type="text/css"><link rel="stylesheet" href="sculpt.css?1" type="text/css"><script type="text/javascript" src="wz_jsgraphics.js"></script><script type="text/javascript" src="prototype.js"></script><script language="JavaScript" type="text/javascript">Event.observe(window, 'load', load_white_board);// Global variablesvar arr_pos;function valueChange(pointToObject, change, max_value) { // Increment value in _pointToObject_ by _change_, test that it is within range _min_value_ to _max_value_ and round it accordingly.	var value = parseFloat(pointToObject.value);	//var max_value = 127;	var min_value = 0.0;	var rounding = 1.0;	if ( !(value.NaN) && (pointToObject.value != "NaN") && (value != NaN) )	{		value = value + change;		if ( value >= max_value )		{			value = max_value;		}		else if ( value <= min_value )		{			value = min_value;		}		if( value == NaN )		{			value = 0;		}		pointToObject.value = Math.round(value /rounding ) * rounding; //rounding to 3 dec places		if (pointToObject.value == "NaN")		{			pointToObject.value = "0";		}	}	else	{		pointToObject.value = "0";	}	redraw();}function load_white_board() // Tim-Jarrett.com
{
	var elem_wb = $('whiteboard');
	arr_pos = Position.cumulativeOffset(elem_wb);
	Element.setStyle($('surface'), $H({left: arr_pos[0], top: arr_pos[1]}));	Event.observe($('surface'), 'mousedown', hndlWbMouseDown);		/*Event.observe($('surface'), 'mousemove', hndlWbMouseMove);
	Event.observe($('surface'), 'mouseover', hndlWbMouseOver);
	Event.observe($('surface'), 'mouseout', hndlWbMouseOut);
	Event.observe($('surface'), 'mousedown', hndlWbMouseDown);
	Event.observe($('surface'), 'mouseup', hndlWbMouseUp);*/}function hndlWbMouseDown(event)
{	// so we've received an event to tell us the mouse was clicked while over the chart.	// determine the position of the click and set the value of a point such that it lines up with the click.//	draw($('whiteboard'), Event.pointerX(event), Event.pointerY(event)); // only temporary while we assess that this works.		var canvas_x = Event.pointerX(event) - arr_pos[0]; // number of pixels from the left	var canvas_y = Event.pointerY(event) - arr_pos[1]; // number of pixels from the top(?)	var xborder = 50;	var yborder = 50;		var r_value = canvas_x - xborder - 127;	var z_value = canvas_y - yborder;		if ((r_value < 0) && (r_value >= -127))		r_value = -r_value;	else if ((r_value > 127) || (r_value < -127)) // if it's out of bounds clamp it to 127.		r_value = 127;	if ((z_value > -5) && (z_value < 0))		z_value = 0;	else if ((z_value > 255) && (z_value < 260))		z_value = 255;		if ((z_value >= 0) && (z_value <= 255)) // likewise we're clamping the z_value but if it's beyond a certain point we're ignoring it completely.	{		// choose which point we're editing by looking at the closest z-coord match.		// when doing zrrev or cross this would be controlled via a checkbox/radio button next to each point's z,r / x,y entry		var res = 32;		var n = Math.round(z_value / 255.0 * res);		if (n <= 9)			cur = '0'+n;		else			cur = n;		// dont mod 'r00' or 'r'+res if we have sphere checked.		var not_a_sphere = !(document.getElementById('mapsphere').checked);		if (not_a_sphere || ((n != 0) && (n != res))) // if (it's not a sphere) OR (it's not a pole) then we modify the point and redraw the preview		{			document.getElementById('r'+cur).value = r_value;			redraw();		}	}}function draw(wb_elem, x, y)
{
	//var arr_pos = Position.cumulativeOffset(wb_elem);
	
	//var offsetX = x - arr_pos[0];
	//var offsetY = y - arr_pos[1];
	
	//$('relx').value = offsetX;
	//$('rely').value = offsetY;
	
	var tmp_div = document.createElement('DIV');
	wb_elem.appendChild(tmp_div);
	Element.setStyle(tmp_div, $H({zIndex: "99", position: "absolute", width: "5px", height: "5px", backgroundColor: "black", top: y + "px", left: x + "px"}));
//	$('logger').innerHTML = "top: " + Element.getStyle(tmp_div, "top") + " - left: " + Element.getStyle(tmp_div, "left") + "<br />";
}function even(num) { // returns true for even numbers.	return (Math.round(num/2.0)*2 == num);}function ldraw(x1, y1, x2, y2, text) { // draw a line between x1,y1 and x2,y2 optionally include a label.// lines are mirrored down the vertical centre.	var xborder = 50;	var yborder = 50;	//	jg.drawLine(x1 + xborder, y1 + yborder, x2 + xborder, y2 + yborder);	jg.drawLine(x1 + 127 + xborder, 255 - y1 + yborder, x2 + 127 + xborder, 255 - y2 + yborder); // right-hand side, not mirrored	jg.drawLine(127 - x1 + xborder, 255 - y1 + yborder, 127 - x2 + xborder, 255 - y2 + yborder); // left-hand side, mirrored	if (text != "") {		jg.drawString(text, x1 + 127 + xborder, 255 - y1 + yborder);	}}function arrowdraw(x, y, up, canvas) {	if (up) {		canvas.drawLine(x-5, y+10, x, y);		canvas.drawLine(x+5, y+10, x, y);	}	else {		canvas.drawLine(x-10, y+5, x, y);		canvas.drawLine(x-10, y-5, x, y)	}}function redraw() {	var xborder = 50;	var yborder = 50;	var x1;	var y1;	var x2;	var y2;	var cur;	var res = 32;	jg.clear();	jg.setColor("#FF0000"); // blue pen	jg.drawLine(127 + xborder, 0 + yborder, 127 + xborder, 255 + yborder); // centre line	//arrowdraw(127 + xborder, 255 + yborder + 10, true, jg); // y-axis arrow head.	jg.drawLine(127 + xborder, 255 + yborder, 255 + xborder + 30, 255 + yborder); // r-axis line	arrowdraw(255 + xborder + 30, 255 + yborder, false, jg); // r-axis arrow head.	jg.drawString("r", 255 + xborder + 30, 255 + yborder);	jg.drawString("0", 127 + xborder - 10, 255 + yborder + 10); // origin	jg.setColor("#0000FF"); // blue pen	var pinch = false;	if( document.getElementById('mapsphere').checked ) {		pinch = true;	}	/*	else if( document.getElementById('mapcylinder').selected ) {		pinch = False;	}	else if( document.getElementById('maptorus').selected ) {		pinch = False;	}	else if( document.getElementById('mapplane').selected ) {		pinch = False;	} */		var LOD_low = document.getElementById('LOD_low').checked;	var show_num = document.getElementById('show_num').checked;		for (i=0; i<=res; i++) {		if (i <= 9)			cur = '0'+i;		else			cur = i;		if (i == 0) {			if (pinch) {				x2 = 0;				y2 = parseInt(document.getElementById('r00').value);			}			else {				x2 = parseInt(document.getElementById('r00').value);				y2 = 255;			}		}		else if (i == res) {			if (pinch) {				x1 = 0;				y1 = parseInt(document.getElementById('r'+cur).value);			}			else {				x1 = parseInt(document.getElementById('r'+cur).value);				y1 = 0;			}			ldraw(x1,y1,x2,y2, "");		}		else if (even(i) || (!LOD_low)) { // at LOD_low we only show the even numbers.			x1 = parseInt(document.getElementById('r'+cur).value);			y1 = 255 - (255 * (i / (res-1)));											//$pointz = 255 - round(255 * ($v / ($res-1))); // points atregular intervals			if (show_num)				ldraw(x1,y1,x2,y2,cur+" ");			else				ldraw(x1,y1,x2,y2,"");			x2 = x1;			y2 = y1;		}	}	jg.paint();	}</script></head><body onLoad="redraw();"><div id='main'>
