<div id="tab2d" class="tabv" width="210" height="800">
<b>2-D plotting</b><br/>
Here you can plot functions, functions in polar coordinates, and parametric equations.<br/><br/>

<script type="text/javascript">
// <![CDATA[
function formeq_seltype() {
  var i,j;
  for(i=0;i<5;i++) for(j=0;j<3;j++)
  document.getElementById('eq'+i+'_'+j).className=document.forms['formeq'].elements['type'+i].value==j?'tabv':'tabh';
}


function formeq_submit() {
  var i,j;
  try {
  update2doptions();
  c0=document.getElementById('ccdisplay0').style.backgroundColor;
  c1=document.getElementById('ccdisplay1').style.backgroundColor;
  c2=document.getElementById('ccdisplay2').style.backgroundColor;
  c3=document.getElementById('ccdisplay3').style.backgroundColor;
  c4=document.getElementById('ccdisplay4').style.backgroundColor;
  type0=document.forms['formeq'].elements['type0'].value;
  type1=document.forms['formeq'].elements['type1'].value;
  type2=document.forms['formeq'].elements['type2'].value;
  type3=document.forms['formeq'].elements['type3'].value;
  type4=document.forms['formeq'].elements['type4'].value;
  if(type0=="0") {ey0=jeq(document.forms['formeq'].elements['y0'].value,"x");er0="";}
  if(type0=="1") {ey0="";er0=jeq(document.forms['formeq'].elements['r0'].value,"theta");thetamin0=eval(jeq(document.forms['formeq'].elements['thetamin0'].value,"-"));thetamax0=eval(jeq(document.forms['formeq'].elements['thetamax0'].value,"-"));}
  if(type0=="2") {epx0=jeq(document.forms['formeq'].elements['px0'].value,"s");epy0=jeq(document.forms['formeq'].elements['py0'].value,"s");smin0=eval(jeq(document.forms['formeq'].elements['smin0'].value,"-"));smax0=eval(jeq(document.forms['formeq'].elements['smax0'].value,"-"));}
  if(type1=="0") {ey1=jeq(document.forms['formeq'].elements['y1'].value,"x");er1="";}
  if(type1=="1") {ey1="";er1=jeq(document.forms['formeq'].elements['r1'].value,"theta");thetamin1=eval(jeq(document.forms['formeq'].elements['thetamin1'].value,"-"));thetamax1=eval(jeq(document.forms['formeq'].elements['thetamax1'].value,"-"));}
  if(type1=="2") {epx1=jeq(document.forms['formeq'].elements['px1'].value,"s");epy1=jeq(document.forms['formeq'].elements['py1'].value,"s");smin1=eval(jeq(document.forms['formeq'].elements['smin1'].value,"-"));smax1=eval(jeq(document.forms['formeq'].elements['smax1'].value,"-"));}
  if(type2=="0") {ey2=jeq(document.forms['formeq'].elements['y2'].value,"x");er2="";}
  if(type2=="1") {ey2="";er2=jeq(document.forms['formeq'].elements['r2'].value,"theta");thetamin2=eval(jeq(document.forms['formeq'].elements['thetamin2'].value));thetamax2=eval(jeq(document.forms['formeq'].elements['thetamax2'].value));}
  if(type2=="2") {epx2=jeq(document.forms['formeq'].elements['px2'].value,"s");epy2=jeq(document.forms['formeq'].elements['py2'].value,"s");smin2=eval(jeq(document.forms['formeq'].elements['smin2'].value,"-"));smax2=eval(jeq(document.forms['formeq'].elements['smax2'].value,"-"));}
  if(type3=="0") {ey3=jeq(document.forms['formeq'].elements['y3'].value,"x");er3="";}
  if(type3=="1") {ey3="";er3=jeq(document.forms['formeq'].elements['r3'].value,"theta");thetamin3=eval(jeq(document.forms['formeq'].elements['thetamin3'].value,"-"));thetamax3=eval(jeq(document.forms['formeq'].elements['thetamax3'].value,"-"));}
  if(type3=="2") {epx3=jeq(document.forms['formeq'].elements['px3'].value,"s");epy3=jeq(document.forms['formeq'].elements['py3'].value,"s");smin3=eval(jeq(document.forms['formeq'].elements['smin3'].value,"-"));smax3=eval(jeq(document.forms['formeq'].elements['smax3'].value,"-"));}
  if(type4=="0") {ey4=jeq(document.forms['formeq'].elements['y4'].value,"x");er4="";}
  if(type4=="1") {ey4="";er4=jeq(document.forms['formeq'].elements['r4'].value,"theta");thetamin4=eval(jeq(document.forms['formeq'].elements['thetamin4'].value,"-"));thetamax4=eval(jeq(document.forms['formeq'].elements['thetamax4'].value,"-"));}
  if(type4=="2") {epx4=jeq(document.forms['formeq'].elements['px4'].value,"s");epy4=jeq(document.forms['formeq'].elements['py4'].value,"s");smin4=eval(jeq(document.forms['formeq'].elements['smin4'].value,"-"));smax4=eval(jeq(document.forms['formeq'].elements['smax4'].value,"-"));}
  g.clear();
  do2d();
  } catch(e) {alert(e);}
  setpermalink();
  return false;
}
// ]]> 
</script>

