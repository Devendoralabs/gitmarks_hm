if(window.top.location.href.match(/plot\.php/)) { window.top.location.replace('http://fooplot.com/'); }

var jxmin=0;jxmax=3,jymin=0,jymax=3,jzmin=0,jzmax=3,jxsteps=12,jysteps=12;
function update3doptions() {
  eval('jxmin='+jeq(document.forms['form3doptions'].elements['jxmin'].value,"-")+';');
  eval('jxmax='+jeq(document.forms['form3doptions'].elements['jxmax'].value,"-")+';');
  eval('jymin='+jeq(document.forms['form3doptions'].elements['jymin'].value,"-")+';');
  eval('jymax='+jeq(document.forms['form3doptions'].elements['jymax'].value,"-")+';');
  eval('jzmin='+jeq(document.forms['form3doptions'].elements['jzmin'].value,"-")+';');
  eval('jzmax='+jeq(document.forms['form3doptions'].elements['jzmax'].value,"-")+';');
  jxsteps=parseFloat(document.forms['form3doptions'].elements['jxsteps'].value);
  jysteps=jxsteps;
}

var ipw=0,ixmin=-5,ixmax=5,iymin=-3,iymax=3,igl=1,igs=0,igx=1,igy=1,iax=1,ila=1;function update2doptions() {
  var tmp,pi=3.14159265358979,e=2.718281828459045;
  document.getElementById('igxwarn').className='tabh';
  document.getElementById('igywarn').className='tabh';
  ipw=document.forms['form2doptions'].elements['ipw'][0].checked ? 0:1;
  eval('ixmin='+jeq(document.forms['form2doptions'].elements['ixmin'].value,"-")+';');
  eval('ixmax='+jeq(document.forms['form2doptions'].elements['ixmax'].value,"-")+';');
  eval('iymin='+jeq(document.forms['form2doptions'].elements['iymin'].value,"-")+';');
  eval('iymax='+jeq(document.forms['form2doptions'].elements['iymax'].value,"-")+';');
  eval('igx='+jeq(document.forms['form2doptions'].elements['igx'].value,"-")+';');
  eval('igy='+jeq(document.forms['form2doptions'].elements['igy'].value,"-")+';');
  if(ixmin>ixmax) { tmp=ixmin;ixmin=ixmax;ixmax=tmp; }
  if(iymin>iymax) { tmp=iymin;iymin=iymax;iymax=tmp; }
  igl=document.forms['form2doptions'].elements['igl'].checked ? 1:0;
  igs=document.forms['form2doptions'].elements['igs'][0].checked ? 0:1;
  iax=document.forms['form2doptions'].elements['iax'].checked ? 1:0;
  ila=document.forms['form2doptions'].elements['ila'].checked ? 1:0;
  setpermalink();
}
function setpermalink() {
  var permalink=document.getElementById('permalink');
  var url='http://fooplot.com/index.php?';
  url+='&type0='+type0+'&type1='+type1+'&type2='+type2+'&type3='+type3+'&type4='+type4;
  url+='&y0='+escape(document.forms['formeq'].elements['y0'].value)+'&y1='+escape(document.forms['formeq'].elements['y1'].value)+'&y2='+escape(document.forms['formeq'].elements['y2'].value)+'&y3='+escape(document.forms['formeq'].elements['y3'].value)+'&y4='+escape(document.forms['formeq'].elements['y4'].value);
  url+='&r0='+escape(document.forms['formeq'].elements['r0'].value)+'&r1='+escape(document.forms['formeq'].elements['r1'].value)+'&r2='+escape(document.forms['formeq'].elements['r2'].value)+'&r3='+escape(document.forms['formeq'].elements['r3'].value)+'&r4='+escape(document.forms['formeq'].elements['r4'].value);
  url+='&px0='+escape(document.forms['formeq'].elements['px0'].value)+'&px1='+escape(document.forms['formeq'].elements['px1'].value)+'&px2='+escape(document.forms['formeq'].elements['px2'].value)+'&px3='+escape(document.forms['formeq'].elements['px3'].value)+'&px4='+escape(document.forms['formeq'].elements['px4'].value);
  url+='&py0='+escape(document.forms['formeq'].elements['py0'].value)+'&py1='+escape(document.forms['formeq'].elements['py1'].value)+'&py2='+escape(document.forms['formeq'].elements['py2'].value)+'&py3='+escape(document.forms['formeq'].elements['py3'].value)+'&py4='+escape(document.forms['formeq'].elements['py4'].value);
  url+='&smin0='+escape(document.forms['formeq'].elements['smin0'].value)+'&smin1='+escape(document.forms['formeq'].elements['smin1'].value)+'&smin2='+escape(document.forms['formeq'].elements['smin2'].value)+'&smin3='+escape(document.forms['formeq'].elements['smin3'].value)+'&smin4='+escape(document.forms['formeq'].elements['smin4'].value);
  url+='&smax0='+escape(document.forms['formeq'].elements['smax0'].value)+'&smax1='+escape(document.forms['formeq'].elements['smax1'].value)+'&smax2='+escape(document.forms['formeq'].elements['smax2'].value)+'&smax3='+escape(document.forms['formeq'].elements['smax3'].value)+'&smax4='+escape(document.forms['formeq'].elements['smax4'].value);
  url+='&thetamin0='+escape(document.forms['formeq'].elements['thetamin0'].value)+'&thetamin1='+escape(document.forms['formeq'].elements['thetamin1'].value)+'&thetamin2='+escape(document.forms['formeq'].elements['thetamin2'].value)+'&thetamin3='+escape(document.forms['formeq'].elements['thetamin3'].value)+'&thetamin4='+escape(document.forms['formeq'].elements['thetamin4'].value);
  url+='&thetamax0='+escape(document.forms['formeq'].elements['thetamax0'].value)+'&thetamax1='+escape(document.forms['formeq'].elements['thetamax1'].value)+'&thetamax2='+escape(document.forms['formeq'].elements['thetamax2'].value)+'&thetamax3='+escape(document.forms['formeq'].elements['thetamax3'].value)+'&thetamax4='+escape(document.forms['formeq'].elements['thetamax4'].value);
  url+='&ipw='+ipw+'&ixmin='+ixmin+'&ixmax='+ixmax+'&iymin='+iymin+'&iymax='+iymax+'&igx='+igx+'&igy='+igy+'&igl='+igl+'&igs='+igs+'&iax='+iax+'&ila='+ila;
  url+='&xmin='+xmin+'&xmax='+xmax+'&ymin='+ymin+'&ymax='+ymax;
  url=url.replace(/\+/g,'%2B');
  if(browser=='msie') {
    permalink.href=url;
  } else {
    permalink.setAttribute('href',url);
  }
}
setpermalink();
var xmin=-5;
var xmax=5;
var ymin=-3;
var ymax=3;
var dragging=false;
var dragx,dragy;
var dobj;
var oxmin,oxmax,oymin,oymax;
var currenttool=0;
var resizerect;
var oloc,oori,oorio,loc=new Array(4);

var browser='';
if(navigator.userAgent.toLowerCase().indexOf('opera')!=-1) browser='opera';
else if(navigator.userAgent.toLowerCase().indexOf('gecko')!=-1) browser='firefox';
else if(navigator.userAgent.toLowerCase().indexOf('msie')!=-1) browser='msie';
else if(navigator.userAgent.toLowerCase().indexOf('safari')!=-1) browser='safari';

var emethod = (browser=='firefox'||browser=='safari'||browser=='opera')?1:0;

function settool(t){currenttool=t;
for(i=0;i<=5;i++) {
document.getElementById('tool'+i).className='toolu';
}
document.getElementById('tool'+t).className='tools';
if(t==0) { document.getElementById('canvasi').style.cursor='move'; }
else if(t==2) { document.getElementById('canvasi').style.cursor='crosshair'; }
else if(t==3) { document.getElementById('canvasi').style.cursor='crosshair'; }
else { document.getElementById('canvasi').style.cursor='default'; }
}

settool(0);

function setwindow(nxmin,nxmax,nymin,nymax){xmin=parseFloat(nxmin);ymin=parseFloat(nymin);xmax=parseFloat(nxmax);ymax=parseFloat(nymax);}

function movehandler(e) {
  var offx=document.getElementById('canvas').offsetLeft;
  var offy=document.getElementById('canvas').offsetTop;
  var mx=(emethod?e.pageX:(document.body.scrollLeft+event.clientX))-offx;
  var my=(emethod?e.pageY:(document.body.scrollTop+event.clientY))-offy;
  var dx=mx-dragx;
  var dy=my-dragy;
  if(dragging) {
    if(currenttool==0) {
      xmin=oxmin-((oxmax-oxmin)/swidth)*dx;
      xmax=oxmax-((oxmax-oxmin)/swidth)*dx;
      ymin=oymin+((oymax-oymin)/sheight)*dy;
      ymax=oymax+((oymax-oymin)/sheight)*dy;
      document.getElementById("canvasi").style.left=dx+"px";
      document.getElementById("canvasi").style.top=dy+"px";
    } else if(currenttool==1) {
      if(dragx>mx) { xlow=mx;xhigh=dragx; }
      else { xlow=dragx-5;xhigh=mx-5; }
      if(dragy>my) { ylow=my;yhigh=dragy; }
      else { ylow=dragy-5;yhigh=my-5; }
      g.setselrect(resizerect,xlow,ylow,xhigh-xlow,yhigh-ylow);
      xmin=oxmin+xlow/swidth*(oxmax-oxmin);
      xmax=oxmax-(1-xhigh/swidth)*(oxmax-oxmin);
      ymin=oymin+(1-yhigh/sheight)*(oymax-oymin);
      ymax=oymax-ylow/sheight*(oymax-oymin);
    }
    return false;
  }
}
function draghandler(e) {
  var fobj=emethod?e.target:event.srcElement;
  while(fobj!=document.body&&fobj!=document) {
    fobj=emethod?fobj.parentNode:fobj.parentElement;
    if(fobj==document.getElementById('canvas')) {
      dragging=true;
    }
  }
  if(dragging) {
    dobj=fobj;
    var offx=document.getElementById('canvas').offsetLeft;
    var offy=document.getElementById('canvas').offsetTop;
    dragx=(emethod?e.pageX:(document.body.scrollLeft+event.clientX))-offx;
    dragy=(emethod?e.pageY:(document.body.scrollTop+event.clientY))-offy;
    oxmin=xmin;oxmax=xmax;oymin=ymin;oymax=ymax;
    document.onmousemove=movehandler;
    if(currenttool==1) {
      resizerect=g.createselrect(dragx,dragy,1,1);
    }
    return false;
  }
}
function uphandler(e) {
  var offx=document.getElementById('canvas').offsetLeft;
  var offy=document.getElementById('canvas').offsetTop;
  var ex=(emethod?e.pageX:(document.body.scrollLeft+event.clientX))-offx;
  var ey=(emethod?e.pageY:(document.body.scrollTop+event.clientY))-offy;
  if(dragging) {
  dragging=false;
  if(currenttool==0) {
    g.clear();
    document.getElementById("canvasi").style.left=0;
    document.getElementById("canvasi").style.top=0;
    do2d();
  }
  if(currenttool==1) {g.clear();do2d();}
  if(currenttool==4) { // zoom in
    xmin=((ex/swidth)*(xmax-xmin)+xmin)-(xmax-xmin)/4;
    xmax=((ex/swidth)*(xmax-xmin)+xmin)+(xmax-xmin)/4;
    ymin=((1-ey/sheight)*(ymax-ymin)+ymin)-(ymax-ymin)/4;
    ymax=((1-ey/sheight)*(ymax-ymin)+ymin)+(ymax-ymin)/4;
    g.clear();do2d();
  }
  if(currenttool==5) { // zoom out
    xmin=((ex/swidth)*(xmax-xmin)+xmin)-(xmax-xmin);
    xmax=((ex/swidth)*(xmax-xmin)+xmin)+(xmax-xmin);
    ymin=((1-ey/sheight)*(ymax-ymin)+ymin)-(ymax-ymin);
    ymax=((1-ey/sheight)*(ymax-ymin)+ymin)+(ymax-ymin);
    g.clear();do2d();
  }
  if(currenttool==3) {
    var rx,ry1,ry2,x,y;
    var y0=null,y1=null,y2=null,y3=null,y4=null,useey1,useey2,ym1=1e20,ym2=1e20;
    x=ex/swidth*(xmax-xmin)+xmin;
    y=(1-ey/sheight)*(ymax-ymin)+ymin;
    if(ey0!="") y0=eval(ey0);
    if(ey1!="") y1=eval(ey1);
    if(ey2!="") y2=eval(ey2);
    if(ey3!="") y3=eval(ey3);
    if(ey4!="") y4=eval(ey4);

    if(y0!=null && !isNaN(y0)) { useey1=ey0;ym1=Math.abs(y-y0); }

    if(y1!=null && !isNaN(y1) && Math.abs(y-y1)<ym1) { useey2=useey1;ym2=ym1;useey1=ey1;ym1=Math.abs(y-y1); }
    else if(y1!=null && !isNaN(y1) && Math.abs(y-y1)<ym2) { useey2=ey1;ym2=Math.abs(y-y1); }

    if(y2!=null && !isNaN(y2) && Math.abs(y-y2)<ym1) { useey2=useey1;ym2=ym1;useey1=ey2;ym1=Math.abs(y-y2); }
    else if(y2!=null && !isNaN(y2) && Math.abs(y-y2)<ym2) { useey2=ey2;ym2=Math.abs(y-y2); }

    if(y3!=null && !isNaN(y3) && Math.abs(y-y3)<ym1) { useey2=useey1;ym2=ym1;useey1=ey3;ym1=Math.abs(y-y3); }
    else if(y3!=null && !isNaN(y3) && Math.abs(y-y3)<ym2) { useey2=ey3;ym2=Math.abs(y-y3); }

    if(y4!=null && !isNaN(y4) && Math.abs(y-y4)<ym1) { useey2=useey1;ym2=ym1;useey1=ey4;ym1=Math.abs(y-y4); }
    else if(y4!=null && !isNaN(y4) && Math.abs(y-y4)<ym2) { useey2=ey4;ym2=Math.abs(y-y4); }
    
    rx=findroot('('+useey1+')-('+useey2+')',ex/swidth*(xmax-xmin)+xmin);
    x=rx;
    eval('ry1='+useey1);
    eval('ry2='+useey2);
    rx=Math.round(rx*1000000)/1000000;
    ry1=Math.round(ry1*1000000)/1000000;
    ry2=Math.round(ry2*1000000)/1000000;
    g.clear();
    do2d();
    if(ry1!=null && ry2!=null && !isNaN(ry1) && !isNaN(ry2) && Math.abs(ry1-ry2)<1e-5 && rx>=xmin && rx<=xmax) {
      g.setcolor('#000000');
      g.rect((rx-xmin)/(xmax-xmin)*swidth-2,sheight-(ry1-ymin)/(ymax-ymin)*sheight-2,4,4);
      g.text(15,280,'Intersection: ('+rx+','+ry1+')',"Verdana",10);
    } else {
      g.setcolor('#000000');
      g.text(15,280,'Cannot find intersection',"Verdana",10);
    }
  }
  if(currenttool==2) {
    var rx,ry,x;
    var y0=null,y1=null,y2=null,y3=null,y4=null,useey,ym=1e20;
    x=ex/swidth*(xmax-xmin)+xmin;
    if(ey0!="") eval('y0=Math.abs('+ey0+')');
    if(ey1!="") eval('y1=Math.abs('+ey1+')');
    if(ey2!="") eval('y2=Math.abs('+ey2+')');
    if(ey3!="") eval('y3=Math.abs('+ey3+')');
    if(ey4!="") eval('y4=Math.abs('+ey4+')');
    if(y0!=null && !isNaN(y0)) { useey=ey0; ym=y0; }
    if(y1!=null && !isNaN(y1) && y1<ym) { useey=ey1;ym=y1; }
    if(y2!=null && !isNaN(y2) && y2<ym) { useey=ey2;ym=y2; }
    if(y3!=null && !isNaN(y3) && y3<ym) { useey=ey3;ym=y3; }
    if(y4!=null && !isNaN(y4) && y4<ym) { useey=ey4;ym=y4; }
    rx=findroot(useey,ex/swidth*(xmax-xmin)+xmin);
    x=rx;
    eval('ry='+useey);
    rx=Math.round(rx*1000000)/1000000;
    g.clear();
    do2d();
    if(ry!=null && Math.abs(ry)<=1e-6 && rx>=xmin && rx<=xmax) {
      g.setcolor('#000000');
      g.rect((rx-xmin)/(xmax-xmin)*swidth-2,sheight-(ry-ymin)/(ymax-ymin)*sheight-2,4,4);
      g.text(15,280,'Root: ('+rx+',0)',"Verdana",10);
    } else {
      g.setcolor('#000000');
      g.text(15,280,'Cannot find root',"Verdana",10);
    }
  }
  }
  setpermalink();
}
document.onmousedown=draghandler;
document.onmouseup=uphandler;
var q;

var swidth=500;
var sheight=300;

var g=new ezvg('canvasi',swidth,sheight);
var camera = new ezvgcamera();

g.init();


function findroot(f,initx) {
  var x=initx,xpd,y,ypd,step=.0000000001;
  var i=0;
  if(f==null) { return null; }
  eval('y='+f);
  if(y==null) { return null; }
  while(i<100) {
    i++;
    eval('y='+f);
    x+=step;
    eval('ypd='+f);
    if(y-ypd!=0) {
      x+=y*step/(y-ypd);
    }
  }
  return x;
}

var eyz;
var type0=0;
var type1=0;
var type2=0;
var type3=0;
var type4=0;
var ey0=jeq("sin(x)","x");
var ey1=jeq("","x");
var ey2=jeq("","x");
var ey3=jeq("","x");
var ey4=jeq("","x");
var er0=jeq("","theta");
var er1=jeq("","theta");
var er2=jeq("","theta");
var er3=jeq("","theta");
var er4=jeq("","theta");
var epx0=jeq("","s");
var epx1=jeq("","s");
var epx2=jeq("","s");
var epx3=jeq("","s");
var epx4=jeq("","s");
var epy0=jeq("","s");
var epy1=jeq("","s");
var epy2=jeq("","s");
var epy3=jeq("","s");
var epy4=jeq("","s");
var thetamin0=eval(jeq("0"),"-");
var thetamax0=eval(jeq("2pi"),"-");
var thetamin1=eval(jeq("0"),"-");
var thetamax1=eval(jeq("2pi"),"-");
var thetamin2=eval(jeq("0"),"-");
var thetamax2=eval(jeq("2pi"),"-");
var thetamin3=eval(jeq("0"),"-");
var thetamax3=eval(jeq("2pi"),"-");
var thetamin4=eval(jeq("0"),"-");
var thetamax4=eval(jeq("2pi"),"-");
var smin0=eval(jeq("0"),"-");
var smax0=eval(jeq("2pi"),"-");
var smin1=eval(jeq("0"),"-");
var smax1=eval(jeq("2pi"),"-");
var smin2=eval(jeq("0"),"-");
var smax2=eval(jeq("2pi"),"-");
var smin3=eval(jeq("0"),"-");
var smax3=eval(jeq("2pi"),"-");
var smin4=eval(jeq("0"),"-");
var smax4=eval(jeq("2pi"),"-");
var c0="#000000";
var c1="#ff0000";
var c2="#0000ff";
var c3="#008000";
var c4="#ff8000";

var txmin;
var txmax;
var tymin;
var tymax;
var txsteps;
var tysteps;
var gridlines=1;
var s,x,y,sx,sy,lsx,lsy,asx,asy;
var sxpoints = new Array();
var sypoints = new Array();

function resetzoom() {
if(ipw) {
  xmin=ixmin;
  ymin=iymin;
  xmax=ixmax;
  ymax=iymax;
} else {
  xmin=-5;
  xmax=5;
  ymin=-3;
  ymax=3;
}
txmin=jxmin;txmax=jxmax;
tymin=jymin;tymax=jymax;
txsteps=jxsteps;
tysteps=jysteps;
camera.setcameralocation(new Array(1,2,1,-10));
camera.setcameraorientation(new Array(0,0,1));
g.clear();
do2d();
}

function do2d() {
  draw2dgridlines();
  draw2dplot();
}

function draw2dgridlines() {
 var axvis=false,ayvis=false,lx,ly,i,rnxp,rnx,rnyp,rny;
 var igxp,igxwarn=0,igywarn=0;
 if(igs && ((xmax-xmin)/igx > 30 || igx<=0)) { igxwarn=1; document.getElementById('igxwarn').className='tabv'; }
 if(igs && ((ymax-ymin)/igy > 30 || igy<=0)) { igywarn=1; document.getElementById('igywarn').className='tabv'; }

 if(!igs || igxwarn) {
   igxp=Math.floor(Math.log((xmax-xmin)/2.5)/Math.log(10));
   igx=1;if(igxp>=1) for(i=0;i<igxp;i++) igx*=10;if(igxp<=-1) for(i=0;i<-igxp;i++) igx/=10;
 }
 if(!igs || igywarn) {
   igyp=Math.floor(Math.log((ymax-ymin)/2.5)/Math.log(10));
   igy=1;if(igyp>=1) for(i=0;i<igyp;i++) igy*=10;if(igyp<=-1) for(i=0;i<-igyp;i++) igy/=10;
 }
 rnxp=(igx>=1 ? 1 : Math.floor(-Math.log(igx)))+2;
 rnx=1;if(rnxp>=1) for(i=0;i<rnxp;i++) rnx*=10;if(rnxp<=-1) for(i=0;i<-rnxp;i++) rnx/=10;
 rnyp=(igy>=1 ? 1 : Math.floor(-Math.log(igy)))+2;
 rny=1;if(rnyp>=1) for(i=0;i<rnyp;i++) rny*=10;if(rnyp<=-1) for(i=0;i<-rnyp;i++) rny/=10;
 g.setcolor("#505050");
 asy=(1+ymin/(ymax-ymin))*sheight;
 asx=(-xmin/(xmax-xmin))*swidth;
 if(ymin<0 && 0<ymax) { // x axis is visible
  axvis=true;
  if(iax) { g.line(0,asy,swidth,asy); }
 }
 if(xmin<0 && 0<xmax) { // y axis is visible
  ayvis=true;
  if(iax) { g.line(asx,0,asx,sheight); }
 }
 g.setcolor("#BBBBBB");
 xpoints=new Array();
 ypoints=new Array();
 for(x=igx*Math.ceil(xmin/igx);x<=igx*Math.floor(xmax/igx);x+=igx) {
   sx=Math.floor((x-xmin)/(xmax-xmin)*swidth);
   if(x!=0||!iax) {
   xpoints.push(sx);
   ypoints.push(-1);
   xpoints.push(sx);
   ypoints.push(sheight);
   xpoints.push(sx);
   ypoints.push(-1);
   }
   if(asy>sheight) asy=sheight;
   else if(asy<10) asy=10;
   if(ila) { g.text(sx,asy,Math.round(x*rnx)/rnx,"Verdana",10); }
 }
 if(igl) { g.polyline(xpoints,ypoints); }
 xpoints=new Array();
 ypoints=new Array();
 for(y=igy*Math.ceil(ymin/igy);y<=igy*Math.floor(ymax/igy);y+=igy) {
   sy=Math.floor((1-(y-ymin)/(ymax-ymin))*sheight);
   if(y!=0||!iax) {
   xpoints.push(-1);
   ypoints.push(sy);
   xpoints.push(swidth);
   ypoints.push(sy);
   xpoints.push(-1);
   ypoints.push(sy);
   }
//   if(y!=0) g.line(0,sy,swidth,sy);
   if(asx>swidth-15) asx=swidth-15;
   else if(asx<0) asx=0;
   if(ila) { g.text(asx,sy,Math.round(y*rny)/rny,"Verdana",10); }
 }
 if(igl) { g.polyline(xpoints,ypoints); }
}


function draw2dplot() {
  if(type0==0 && ey0!="") draw2dfunction(ey0,c0);
  if(type0==1 && er0!="") draw2dpolar(er0,c0,thetamin0,thetamax0,.02);
  if(type0==2 && epx0!="" && epy0!="") draw2dparametric(epx0,epy0,c0,smin0,smax0,.02);
  if(type1==0 && ey1!="") draw2dfunction(ey1,c1);
  if(type1==1 && er1!="") draw2dpolar(er1,c1,thetamin1,thetamax1,.02);
  if(type1==2 && epx1!="" && epy1!="") draw2dparametric(epx1,epy1,c1,smin1,smax1,.02);
  if(type2==0 && ey2!="") draw2dfunction(ey2,c2);
  if(type2==1 && er2!="") draw2dpolar(er2,c2,thetamin2,thetamax2,.02);
  if(type2==2 && epx2!="" && epy2!="") draw2dparametric(epx2,epy2,c2,smin2,smax2,.02);
  if(type3==0 && ey3!="") draw2dfunction(ey3,c3);
  if(type3==1 && er3!="") draw2dpolar(er3,c3,thetamin3,thetamax3,.02);
  if(type3==2 && epx3!="" && epy3!="") draw2dparametric(epx3,epy3,c3,smin3,smax3,.02);
  if(type4==0 && ey4!="") draw2dfunction(ey4,c4);
  if(type4==1 && er4!="") draw2dpolar(er4,c4,thetamin4,thetamax4,.02);
  if(type4==2 && epx4!="" && epy4!="") draw2dparametric(epx4,epy4,c4,smin4,smax4,.02);
}

function draw2dfunction(ey,plotcolor) {
  var incr=0;
  sxpoints=new Array();
  sypoints=new Array();
  g.setcolor(plotcolor);
  var xstep=(xmax-xmin)/swidth*2;
  for(x=xmin;x<xmax;x+=xstep) {
    if(incr>0) { incr--; }
    eval("y="+ey);
    if(!isNaN(y) && isFinite(y)) {
      sx=Math.floor((x-xmin)/(xmax-xmin)*swidth);
      sy=Math.floor((1-(y-ymin)/(ymax-ymin))*sheight*4)/4;
      if(Math.abs(lsy-sy)>20 && incr==0) {
        x-=(xmax-xmin)/swidth;
	incr++;
      }
      if(sy<0) { sy=-10; }
      if(sy>sheight) { sy=sheight+10; }
      sxpoints.push(sx);
      sypoints.push(sy);
      lsx=sx;
      lsy=sy;
    } else {
      if(sxpoints.length>0) {
        g.polyline(sxpoints,sypoints);
        sxpoints=new Array();
        sypoints=new Array();
      }
    }
  }
  g.polyline(sxpoints,sypoints);
}

function draw2dpolar(ey,plotcolor,start,end,step) {
  var incr=0;
  var r=0;
  sxpoints=new Array();
  sypoints=new Array();
  g.setcolor(plotcolor);
  for(theta=start;theta<end;theta+=step) {
    eval("r="+ey);
    if(!isNaN(r) && isFinite(r)) {
      x=r*Math.cos(theta);
      y=r*Math.sin(theta);
      sx=Math.floor((x-xmin)/(xmax-xmin)*swidth);
      sy=Math.floor((1-(y-ymin)/(ymax-ymin))*sheight);
      if(sx<0 || sx>swidth || sy<0 || sy>sheight) {
        if(sxpoints.length>0) g.polyline(sxpoints,sypoints);
        sxpoints=new Array();
        sypoints=new Array();
      } else {
      if(sy<0) sy=-1;
      if(sy>sheight) sy=sheight+1;
      if(sx<0) sx=-1;
      if(sx>swidth) sx=swidth+1;
      sxpoints.push(sx);
      sypoints.push(sy);
      }
    }
  }
  g.polyline(sxpoints,sypoints);
}

function draw2dparametric(epx,epy,plotcolor,start,end,step) {
  var incr=0;
  var r=0;
  sxpoints=new Array();
  sypoints=new Array();
  g.setcolor(plotcolor);
  for(s=start;s<end;s+=step) {
    eval("x="+epx);
    eval("y="+epy);
    if(!isNaN(x) && isFinite(x) && !isNaN(y) && isFinite(y)) {
      sx=Math.floor((x-xmin)/(xmax-xmin)*swidth);
      sy=Math.floor((1-(y-ymin)/(ymax-ymin))*sheight);
      if(sx<0 || sx>swidth || sy<0 || sy>sheight) {
        if(sxpoints.length>0) g.polyline(sxpoints,sypoints);
        sxpoints=new Array();
        sypoints=new Array();
      } else {
      if(sy<0) sy=-1;
      if(sy>sheight) sy=sheight+1;
      if(sx<0) sx=-1;
      if(sx>swidth) sx=swidth+1;
      sxpoints.push(sx);
      sypoints.push(sy);
      }
    }
  }
  g.polyline(sxpoints,sypoints);
}

function getscreencoords(p) {
  sx=Math.floor((p[0]-xmin)/(xmax-xmin)*swidth);
  sy=Math.floor((1-(p[1]-ymin)/(ymax-ymin))*sheight);
  return new Array(sx,sy);
}
function draw3drender() {
  var s,ls,xi,yi;
  var xp,yp;
  g.setcolor('#000000');
  for(var xi=0;xi<jxsteps;xi++) {
  ls=null;
  xp=new Array();
  yp=new Array();
  for(var yi=0;yi<jysteps;yi++) {
    if(!isNaN(zpoints[xi+jxsteps*yi]) && isFinite(zpoints[xi+jxsteps*yi])) {
      s=getscreencoords(camera.getprojection(new Array(xpoints[xi+jxsteps*yi],ypoints[xi+jxsteps*yi],zpoints[xi+jxsteps*yi])));
    } else {
      s=null;
    }
    if(s) { /* g.line(ls[0],ls[1],s[0],s[1]); */ xp.push(s[0]);yp.push(s[1]); }
    ls=s;
  }
  g.polyline(xp,yp);
  }
  for(var xi=0;xi<jxsteps;xi++) {
  ls=null;
  xp=new Array();
  yp=new Array();
  for(var yi=0;yi<jysteps;yi++) {
    if(!isNaN(zpoints[yi+jysteps*xi]) && isFinite(zpoints[yi+jysteps*xi])) {
      s=getscreencoords(camera.getprojection(new Array(xpoints[yi+jysteps*xi],ypoints[yi+jysteps*xi],zpoints[yi+jysteps*xi])));
    } else {
      s=null;
    }
    if(s) { /* g.line(ls[0],ls[1],s[0],s[1]); */ xp.push(s[0]);yp.push(s[1]); }
    ls=s;
  }
  g.polyline(xp,yp);
  }
}

var xpoints=new Array();
var ypoints=new Array();
var zpoints=new Array();

function build3d() {
  var incr=0,xi,yi;
  xpoints=new Array();
  ypoints=new Array();
  zpoints=new Array();
  g.setcolor('#000000');
  for(xi=0;xi<jxsteps;xi++) {
  for(yi=0;yi<jysteps;yi++) {
    x=jxmin+xi*(jxmax-jxmin)/(jxsteps-1);
    y=jymin+yi*(jymax-jymin)/(jysteps-1);
    eval("z="+eyz);
    xpoints.push(3*(x-jxmin)/(jxmax-jxmin));
    ypoints.push(3*(y-jymin)/(jymax-jymin));
    zpoints.push(3*(z-jzmin)/(jzmax-jzmin));
  }
  }
}

