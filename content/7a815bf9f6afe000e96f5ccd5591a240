<script type="text/javascript">
<!--
if (document.getElementById && !document.all)
	document.writeln('<p><a href="/ref/css/filters/">turn off interactive highlight</a></p>');
else document.writeln('<p><em>See individual test pages for notes.</em></p>');
/**********************************
highlightTableColumn v0.1b
	- modified to accomodate existing classes
	- affects color, instead of backgroundColor

Latest version:
	<http://centricle.com/lab/dom/col/>

**********************************/

function assignClasses() {
	var tables = document.getElementsByTagName("table");
	for (var t = 0; t < tables.length; t++) {
		var tbods = tables[t].getElementsByTagName("tbody");
		for (var b = 0; b < tbods.length; b++) {
			for (var r = 0; r < tbods[b].rows.length; r++)
				for (var c = 0; c < tbods[b].rows[r].cells.length; c++) {
					currentClass = tbods[b].rows[r].cells[c].className;
					tbods[b].rows[r].cells[c].className = currentClass + " " + c;
				}
		}
	}
}
function highlightColumn (evt) {
	elem = (evt.target.nodeType == 3) ? evt.target.parentNode : evt.target;
	theColumn = elem.className.substr(2);
	if (document.colsHighlighted) {
		colColor = '';
		cellColor = ''
		document.colsHighlighted = false;
	}
	else {
		colColor = 'rgb(0,0,0)';
		cellColor = 'rgb(255,255,255)';
		document.colsHighlighted = true;
	}
	var tables = document.getElementsByTagName("table");
	for (var t = 0; t < tables.length; t++) {
		var tbods = tables[t].getElementsByTagName("tbody");
		for (var b = 0; b < tbods.length; b++) {
			for (var r = 0; r < tbods[b].rows.length; r++)
				tbods[b].rows[r].cells[theColumn].style.color = colColor;
				elem.style.color = cellColor;
		}
	}
}
function init() {
	if (document.addEventListener) {
		cells = document.getElementsByTagName("td");
		for (var c = 0; c < cells.length; c++) {
			cells[c].addEventListener("mouseover", highlightColumn, true);
			cells[c].addEventListener("mouseout", highlightColumn, true);
		}
	}
	assignClasses();
}
//-->
</script>

