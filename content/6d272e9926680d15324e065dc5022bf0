<script type="text/javascript">
    function previewComment() {
        addComment("comment");
    }
    function postComment() {
        addComment("addcomment");
    }
    function addComment(l) {
        comment = $("#txtcomment").val().replace(/^\s+|\s+$/g, '');
        $("#txtcomment").val(comment);
        if (comment.length > 0) {
            $("#phxCommentMessage").html("HTML Prohibited");
            omodal("Add Comment","/tools/login/?login=" + l);
        } else {
            $("#phxCommentMessage").html("<strong>Comment missing!</strong>");
        }
    }
    function clp() {
        loadCommentPage('Right-click to learn', $("#commentPages").val(), true);
    }
    function cpg(page) {
        loadCommentPage('Right-click to learn', page, false);
    }
    function crpt(commentid) {
        omodal("Report this","/tools/report/?commentid=" + commentid);
    }
    function loadCommentPage(key, page, nocache) {
        $("#commentPage").val(page);
        $("#commentList").html("<p><img src=\"http://cache.thephoenix.com/j/jquery/loadinganimation.gif\"></br>Loading Comments...</p>");
        var url = "/tools/comments/list.aspx?s=" + escape(key) + "&p=" + page;
        if (nocache){
            url = url + "&c=1";
        }
        $.get(url, function(data) {
            $("#commentList").html(data);
        });
    }
</script>
            







