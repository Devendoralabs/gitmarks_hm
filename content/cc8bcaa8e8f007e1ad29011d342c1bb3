    e.onerror = function (e) {
      if (!e) var e = window.event;
      e.cancelBubble = true;
      if (e.stopPropagation) e.stopPropagation();
      return true;
    };
    document.getElementById('fb-root').appendChild(e);
  }());

//]]>
</script>

</body>

</html>
