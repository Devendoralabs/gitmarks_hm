<script language=Javascript>


	function newsletter_subscribe ()
	{
		var req = null; 
		var start = new Date();
		var randomnumber=Math.floor(Math.random()*30000);
		
		xEmail = document.getElementById('Email').value; 
		
		xtime = start.getTime();
		dummyvar = xtime + randomnumber;
		
		x = 0;

		//document.ajax.dyn.value="Started...";
 
		if (window.XMLHttpRequest)
		{
 			req = new XMLHttpRequest();

		} 
		else if (window.ActiveXObject) 
		{
			try {
				req = new ActiveXObject("Msxml2.XMLHTTP");
			} catch (e)
			{
				try {
					req = new ActiveXObject("Microsoft.XMLHTTP");
				} catch (e) {}
			}
			
       	}


		req.onreadystatechange = function()
		{ 
			
			//document.ajax.dyn.value="Wait server...";
			if(req.readyState == 4)
			{
				if(req.status == 200)
				{
					//document.ajax.dyn.value="Received:" + req.responseText;	
					
				}	
				else	
				{
					//document.ajax.dyn.value="Error: returned status code " + req.status + " " + req.statusText;
				}	
			} 
		}; 
		
		x = 1;
		xUrl = "newsletter_subscribe.php?Email=" + xEmail + "&dummy=" + dummyvar;
		req.open("GET", xUrl, false); 
		req.send(null); 
		
		
		d = document.getElementById("newsletter_result"); 
		d.innerHTML = req.responseText;
		
		
		return true;
	} 





</script>
