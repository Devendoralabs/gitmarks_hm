    (function() {
        var x = function() {
            var links = document.getElementsByTagName
                ? document.getElementsByTagName( "a" )
                : document.links;
            for( var i = 0; i < links.length; i++ ) {
                var link = links[ i ];
                if( link.host && link.host != window.location.host )
                    link.target = "_blank";
            }
        }
        
        var o = window.onload;
        window.onload = function( e ) {
            x( e );
            if( o )
                return o( e );
        }
    })();
