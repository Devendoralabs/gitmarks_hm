        var pageTracker = _gat._getTracker("UA-13115868-1");
        var path = window.location.pathname;
          if (path.search("/intl/")!=-1)
            {
              var get_cookie_path = path.split("/doubleclick/")[0] + "/doubleclick/";
              var cookie_path = "/intl/" + get_cookie_path.split("/intl/")[1];
            }else{
              var cookie_path = "/doubleclick/";
