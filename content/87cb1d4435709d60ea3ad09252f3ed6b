  <script type="text/javascript" src="http://static.tumblr.com/dbcxhwx/HWI9SN2SQ/mootools-beta_eval.js"></script>
    <script type="text/javascript">
      var videosPerPage = 8;
      var jokesPerPage = 1;
      
      var videoOffset = 0;
      var jokeOffset = 0;
      
      var totalVideos = 0;
      var totalJokes = 0;
    
    function videostream(data) {
        var videostreamContent = '';
        totalVideos = data['posts-total'];
        
      data.posts.forEach(function(post) {
        caption = post['video-caption'];
        if(caption.indexOf("<h2>") != -1) caption = caption.substr(4, caption.indexOf("<\/h2>") - 5);
                
        videostreamContent += '<a href="' + post['url-with-slug'] + '#offset:' + videoOffset + ";" + jokeOffset + '" class="video">\
          <div class="inner">\
            <div class="glass"><img src="http://static.tumblr.com/thpaaos/fHpkkf1t1/glass.png"/></div>\
            <div class="embed">' + post['video-source'] + '</div>\
          </div>\
          <div class="caption">' + caption + '</div>\
        </a>';
      });
      
      $("videostream_wrapper").set('html', videostreamContent);
      
      videostream_pagination();
      adjust_wmodes();
    }
    
    function videostream_pagination() {
      $("videostream_pagination_next").setStyle("display", "none");
      if(totalVideos > (videoOffset + videosPerPage)) $("videostream_pagination_next").setStyle("display", "block");
      
      $("videostream_pagination_prev").setStyle("display", "none");
      if(videoOffset != 0) $("videostream_pagination_prev").setStyle("display", "block");
    }
      
      function jokestream(data) {
        var jokestreamContent = '';
        totalJokes = data['posts-total'];
        
      data.posts.forEach(function(post) {
        var title = "";
        if($chk(post['regular-title'])) title = "<a href='" + post['url'] + "'><strong>" + post['regular-title'] + "</strong></a> ";
        
        jokestreamContent += '<li class="joke">' + title + post['regular-body'] + "</li>";
      });
      
      $("jokestream").set('html', jokestreamContent);
      
      jokestream_pagination();
    }
    
    function jokestream_pagination() {
      $("jokestream_pagination_next").setStyle("display", "none");
      if(totalJokes > (jokeOffset + jokesPerPage)) $("jokestream_pagination_next").setStyle("display", "block");
      
      $("jokestream_pagination_prev").setStyle("display", "none");
      if(jokeOffset != 0) $("jokestream_pagination_prev").setStyle("display", "block");
    }
      
      function topvideo(data) {
        if($chk($("topvideo_wrapper"))) {
          var videostreamContent = '';
        
        data.posts.forEach(function(post) {
          caption = post['video-caption'];
        
          topVideoContent = '<div class="player">' + post['video-source'] + '</div>\
                  <div id="controls">\
                    <div class="side left">\
                      <a href="mailto:espiegelman@gstreet.com?subject=OJTJ%20Newsletter%20Request" class="button subscribe"></a>\
                      <a href="mailto:?subject=Old%20Jews%20Telling%20Jokes&body=' + post['url-with-slug'] + '" class="button send"></a>\
                      <a href="' + post['url-with-slug'] + '#disqus_thread" class="button comment viewAll"></a>\
                    </div>\
                    <div id="info">\
                      <div class="top">' +
                        post['video-caption'] + '<br/><br/>' +
                        '<a href="' + post['url-with-slug'] + '" class="permalink">Posted on ' + post.date.substr(0, post.date.lastIndexOf(" ")) + '</a>\
                      </div>\
                    </div>\
                    <div class="side right">\
                      <a href="http://itunes.apple.com/WebObjects/MZStore.woa/wa/viewPodcast?id=303783970" class="button podcast"></a>\
                      <a href="http://feeds.feedburner.com/OldJewsTellingJokes" class="button rss"></a>\
                      <a href="mailto:espiegelman@gstreet.com?subject=OJTJ%20Video%20Submission" class="button submit"></a>\
                    </div>\
                  </div>';
        });
      
        $("topvideo_wrapper").set('html', topVideoContent);
      }
    }
    
      function adjust_wmodes() {
        $$("embed").forEach(function(vid) {
          vid.setAttribute("wmode", "opaque");
        });
        
        $$("object").forEach(function(vid) {
          vid.innerHTML += '<param name="wmode" value="opaque" />';
        });
      }
      
      window.addEvent("domready", function() {
        function loadVideos(start) {
          var videoRequest = new Request({method: 'get', url: '/api/read/json?callback=videostream&type=video&num=' + videosPerPage + '&start=' + start, evalResponse: true}).send();
        }
        
        loadVideos(videoOffset);
        
        $("videostream_pagination_next").addEvent("click", function() {
          videoOffset += videosPerPage;
          loadVideos(videoOffset);
          
          document.location.hash = "offset:" + videoOffset + ";" + jokeOffset;
        });
        
        $("videostream_pagination_prev").addEvent("click", function() {
          videoOffset -= videosPerPage;
          loadVideos(videoOffset);
          
          document.location.hash = "offset:" + videoOffset + ";" + jokeOffset;
        });
        
        function loadJokes(start) {
          var videoRequest = new Request({method: 'get', url: '/api/read/json?callback=jokestream&type=regular&num=' + jokesPerPage + '&start=' + start, evalResponse: true}).send();
        }
        
        loadJokes(jokeOffset);
        
        $("jokestream_pagination_next").addEvent("click", function() {
          jokeOffset += jokesPerPage;
          loadJokes(jokeOffset);
          
          document.location.hash = "offset:" + videoOffset + ";" + jokeOffset;
        });
        
        $("jokestream_pagination_prev").addEvent("click", function() {
          jokeOffset -= jokesPerPage;
          loadJokes(jokeOffset);
          
          document.location.hash = "offset:" + videoOffset + ";" + jokeOffset;
        });
        
        function loadTopVideo(start) {
          var videoRequest = new Request({method: 'get', url: '/api/read/json?callback=topvideo&type=video&num=1&start=0', evalResponse: true}).send();
        }
        
        loadTopVideo();
        
        if($chk(document.location.hash) && document.location.hash != "disqus_thread") {
          var hash = document.location.hash.substr(8).split(";");
        
          videoOffset = hash[0];
          jokeOffset = hash[1];
          
          loadVideos(videoOffset);
          loadJokes(jokeOffset);
        }
      });
    </script>


