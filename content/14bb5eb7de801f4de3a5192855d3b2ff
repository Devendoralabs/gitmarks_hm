<script type="text/javascript">

 this.tooltip = function(){	
	
		xOffset = 5;
		yOffset = 5;		
	
	$(".titletooltip").live('mouseover', function(e){											  
		this.t = this.title;
		this.title = "";									  
		$("body").append("<p id='titletooltip'>"+ this.t +"</p>");
		$("#titletooltip")
			.css("top",(e.pageY - xOffset) + "px")
			.css("left",(e.pageX + yOffset) + "px")
			.show();		
    });	
	$(".titletooltip").live('mouseout', function(e){	
		this.title = this.t;		
		$("#titletooltip").remove();
    });	
	$(".titletooltip").live('mousemove', function(e){
		$("#titletooltip")
			.css("top",(e.pageY - xOffset) + "px")
			.css("left",(e.pageX + yOffset) + "px");
	});			
};



// starting the script on page load
$(document).ready(function(){
   $('#main-gallery').load('/_ajax/carousel/') ;
	tooltip();
    $('#feed').load('/_ajax/community_feed_homepage/0/20/members/all/', function(){
        $('#activity-feed').vTicker({ 
    		speed: 500,
    		pause: 4500,
    		animation: '',
    		mousePause: true,
    		showItems: 5,
    		direction: 'down'
    	});
     });
});

</script>




