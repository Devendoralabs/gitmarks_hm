<script type="text/javascript">
$(document).ready(function() {
   $('#header').load("/_postload/header/", {path: "//////"}, function(){
initTopPanel(); 
initMenu(); 
initSearch(); 
initGlobalPopup('div.open-global-popup'); 
	$('div.yellow-bar a.btn-close').click(function(){
		$(this).parents('div.yellow-bar:eq(0)').fadeOut(400);
		return false;
	});
});
});
</script>   

