      <td id="singleFHbCell1"><script type="text/javascript">
      var cnt_curFi = 0;
      var cnt_filmTimer;
      var cnt_fiCount;  
      
      function cnt_init_filmstrip() {
      cnt_fiCount = new Array()
      var cnt_allFi = document.getElementById('cnt_filmstrip_row').getElementsByTagName('td');
      for (var x = 0; x < cnt_allFi.length; x++) {
      var test = cnt_allFi[x];
      if (test.className == 'cnt_filmstrip_item_hide' || test.className == 'cnt_filmstrip_item_show') {
      cnt_fiCount[cnt_fiCount.length] = cnt_allFi[x];
      }
      }
      }


      function cnt_scroll_filmstrip(sender) {
      var newFi = sender.id.substring(sender.id.length - 1);
      if (newFi != cnt_curFi) {
      cnt_curFi = newFi;
      cnt_change_filmstrip();
      clearInterval(cnt_filmTimer);
      }
      }



      function cnt_change_filmstrip() {
      for (var x = 0; x < cnt_fiCount.length; x++) {
      cnt_fiCount[x].className = 'cnt_filmstrip_item_hide';
      }
      document.getElementById('cnt_fi_' + cnt_curFi).className = 'cnt_filmstrip_item_show';
      var cnt_navTds = document.getElementById('cnt_filmstrip_nav').getElementsByTagName('td');
      for (var x = 0; x < cnt_navTds.length; x++) {
      cnt_navTds[x].className = 'cnt_filmstrip_nav_number';
      }
      document.getElementById('cnt_filmstrip_nav_' + cnt_curFi.toString()).className = 'cnt_filmstrip_nav_number_active';
      var cnt_navTitles = document.getElementById('cnt_filmstrip_nav_head').getElementsByTagName('div');
      for (var y = 0; y < cnt_navTitles.length; y++) {
      cnt_navTitles[y].className = 'cnt_filmstrip_nav_title_hidden';
      }
      document.getElementById('cnt_filmstrip_title_' + cnt_curFi.toString()).className = 'cnt_filmstrip_nav_title_visible';
      }



      function cnt_filmstrip_timer_tick() {
      if (cnt_curFi == cnt_fiCount.length - 1) cnt_curFi = 0;
      else cnt_curFi++;
      cnt_change_filmstrip();
      }


      function cnt_filmstrip_hover(sender, newClass) {
      if (sender.id != 'cnt_filmstrip_nav_' + cnt_curFi.toString()) {
      sender.className = newClass;
      }
      }      
    </script><style>#cnt_filmstrip_nav_head{width:428px;}</style>
