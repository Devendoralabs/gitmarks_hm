      #cnt_image_module_table { }
      </style><script language="javascript" type="text/javascript">var visited = new Array();
function cntChangeImage(showItem, sender) {
            var shouldbeVisible;
            var shouldNotbeVisible;                   
            var parentTable = document.getElementById('cnt_image_module_table');
            var allTds = parentTable.getElementsByTagName('div');
            var allLowerTds = sender.getElementsByTagName('div');             
             for (var x = 0; x < allTds.length; x++) {                
                 for (var y = 0; y < visited.length; y++) {

                     if (allTds[x].getAttribute("id") == visited[y]) {
                         if (allTds[x].className == 'cnt_image_module_lower_hidden') { allTds[x].className = 'cnt_image_module_lower_visible'; visited[y]=0; }
                         else allTds[x].className = 'cnt_image_module_lower_hidden'; visited[y]=0;
                     }

                 }
                 if (allTds[x].className == 'cnt_image_module_top_visible') { allTds[x].className = 'cnt_image_module_top_hidden'; } 
             }                  
                           
            document.getElementById(showItem).className = 'cnt_image_module_top_visible';          
           
            
            for (var n = 0; n < allLowerTds.length; n++) {
                if (allLowerTds[n].className = 'cnt_image_module_lower_hidden'){shouldbeVisible = allLowerTds[n];}
                else if (allLowerTds[n].className == 'cnt_image_module_lower_visible') { shouldNotbeVisible = allLowerTds[n]; }
                visited.push(allLowerTds[n].getAttribute("id"));
            }
            
            if(shouldbeVisible)shouldbeVisible.className = 'cnt_image_module_lower_visible';
            if(shouldNotbeVisible)shouldNotbeVisible.className = 'cnt_image_module_lower_hidden';            

        }     

        function cntExpandCollapse(showItem, showHead) {   
            var parentTableId = $('#'+ showHead).closest("table").attr('id'); 
            var parentTable = document.getElementById(parentTableId);
            var allTds = parentTable.getElementsByTagName('td');
            for (var x = 0; x < allTds.length; x++) { 
                if (allTds[x].className == 'cntFHa_expand_show') { allTds[x].className = 'cntFHa_expand_hide'; }
                if (allTds[x].className == 'cntFHa_expand_table_head_selected') { allTds[x].className = 'cntFHa_expand_table_head'; }
                if(allTds[x].className == 'cntFHa_expand_table_head_selected' || allTds[x].className == 'cntFHa_expand_table_head') {$(allTds[x]).attr('name',' '); }
            }
            document.getElementById(showItem).className = 'cntFHa_expand_show';
            document.getElementById(showHead).className = 'cntFHa_expand_table_head_selected';           
            $('#'+ showHead).attr('name',showHead);  
        }

       function cntHover(hoverItem,hoverClass){ if(hoverItem.id!=$(hoverItem).attr('name')) hoverItem.className=hoverClass; }
       
    function cnt_gTable_change_app(sender) {
        var cnt_newItem = sender.id.substring(20);
        var cnt_allGcontent = document.getElementById('cnt_gTable_content_container_cell').getElementsByTagName('div');
        for (var x = 0; x < cnt_allGcontent.length; x++) {
            if (cnt_allGcontent[x].className == 'cnt_gTable_content' || cnt_allGcontent[x].className == 'cnt_gTable_content_hidden') {
                cnt_allGcontent[x].className = 'cnt_gTable_content_hidden';
            }
        }
        document.getElementById('cnt_gTable_content_cell_' + cnt_newItem.toString()).className = 'cnt_gTable_content';
             var cnt_allGTabs = document.getElementById('cnt_gTable_left_nav').getElementsByTagName('table');
            for (var y = 0; y < cnt_allGTabs.length; y++) {
                if (cnt_allGTabs[y].className == 'cnt_nav_cell_table_selected' || cnt_allGTabs[y].className == 'cnt_nav_cell_table') {
                    cnt_allGTabs[y].className = 'cnt_nav_cell_table';
                }
            }
            document.getElementById('cnt_gTable_left_nav_' + cnt_newItem).childNodes[0].className = 'cnt_nav_cell_table_selected';
   }
    function cnt_gTable_change_tab(sender) {
        var cnt_newRow = sender.id.substr(sender.id.length - 2, 1);
        var cnt_newItem = sender.id.substring(sender.id.length - 2);
        var cnt_allGcontent = document.getElementById('cnt_gTable_top_content_row_'+cnt_newRow).getElementsByTagName('td');
        for (var x = 0; x < cnt_allGcontent.length; x++) {
            if (cnt_allGcontent[x].className == 'cnt_gTable_content_tab' || cnt_allGcontent[x].className == 'cnt_gTable_content_tab_hidden') {
                cnt_allGcontent[x].className = 'cnt_gTable_content_tab_hidden';
            }
        }
        document.getElementById('cnt_gTable_top_content_' + cnt_newItem.toString()).className = 'cnt_gTable_content_tab';
            var cnt_allGTopTabs = sender.parentNode.getElementsByTagName('td');
            for (var y = 0; y < cnt_allGTopTabs.length; y++) {
                if (cnt_allGTopTabs[y].className == 'cnt_gTable_top_tab_selected' || cnt_allGTopTabs[y].className == 'cnt_gTable_top_tab') {
                    cnt_allGTopTabs[y].className = 'cnt_gTable_top_tab';
                }
            }
            document.getElementById('cnt_gTable_top_nav_' + cnt_newItem.toString()).className = 'cnt_gTable_top_tab_selected';
    }
       function EmailForm() {
               var ChosenContentType;
               var contentTypes = document.getElementsByName('ContentType');
               for (var i = 0; i < contentTypes.length; i++) {
                   if (contentTypes[i].checked)
                       ChosenContentType = contentTypes[i].value;
               }
               var body_message = "Company Name: " + document.getElementById('cpName').value + "\nCompany Website: " + document.getElementById('cpWebsite').value
                                   + "\nBusiness Description: " + document.getElementById('BusinessDescription').value + "\nContact Name: " + document.getElementById('contactName').value + "\nContact Email: " + document.getElementById('contactEmail').value +
                                  "\nAddress: " + document.getElementById('Address').value + "\nPhone: " + document.getElementById('phoneNumber').value + "\ncontent Type: " + ChosenContentType + "\nLink To Your Content: " + document.getElementById('LinkTo').value;
               var email = "ocs@microsoft.com";
               var subject = "Office.com Content and Services partnership inquiry";

               window.location = 'mailto:' + encodeURIComponent(email) + '?subject=' + encodeURIComponent(subject) + '&body=' + encodeURIComponent(body_message);             

           }

           function ClearForm() {
               var fields = document.getElementsByTagName('input');            
               for (i = 0; i < fields.length; i++) {

                   var field_type = fields[i].type.toLowerCase();
                    switch (field_type) {
                        case "text":                                       
                         fields[i].value = "";
                           break;
                       case "radio":
                           if (fields[i].checked) {
                               fields[i].checked = false;
                           }
                           break;                
                       default:
                           break;
                   }

               }
            document.getElementById('BusinessDescription').value="";
            document.getElementById('Address').value="";
           }         
                 
