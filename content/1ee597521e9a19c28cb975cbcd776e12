<script>
  function showMovie() {
  var flashvars = {};
  var params = {};
  var attributes = {id: "docsVideo"};
  swfobject.embedSWF('http://www.youtube.com/v/A7y7NafWXeM&rel=0&autoplay=1',
  'movieDiv', '350', '260', '9.0.0', null, flashvars, params, attributes);
  document.getElementById('movieImg').style.display = 'none';
  document.getElementById('clickIfMovieTrouble').style.display = '';
  }
/**
 * Define variables based on divs and assign links to them.
 */
function init() {
  var docsDocuments = document.getElementById('docsDocuments');
  var docsSpreadsheets = document.getElementById('docsSpreadsheets');
  var docsPresentations = document.getElementById('docsPresentations');
  var docsForms = document.getElementById('docsForms');
  var docsDrawings = document.getElementById('docsDrawings');
  docsDocuments.onclick = function() {
  show('http://www.google.com/google-d-s/documents/');
  }
  docsSpreadsheets.onclick = function() {
  show('http://www.google.com/google-d-s/spreadsheets/');
  }
  docsPresentations.onclick = function() {
  show('http://www.google.com/google-d-s/presentations/');
  }
  docsForms.onclick = function() {
  show('http://www.google.com/google-d-s/forms/');
  }
  docsDrawings.onclick = function() {
  show('http://www.google.com/google-d-s/drawings/');
  }
}
/**
 * Point page location to link depending on product link clicked.
 * @param {string} url Url to which we redirect.
 */
function show(url) {
  window.location = url;
}
/**
 * Change the background to darker grey when hovering over div.
 * @param {HTMLElement} iconHover Area changes its display properties.
 */
function setBackgroundDark(iconHover) {
  iconHover.style.backgroundColor = '#DFDEDE';
  iconHover.style.border = '1px solid #d1d1d1';
}
/**
 * Change the background to darker grey when hovering over div.
 * @param {HTMLElement} iconHover Area changes its display properties.
 */
function setBackgroundLight(iconHover) {
  iconHover.style.backgroundColor = '#f0f0f0';
  iconHover.style.border = '1px solid #f0f0f0';
}
</script>
