				<script>
				
				function nav_tab_select(in_item){
					
											
						document.getElementById("nav_tab_home").src = "/images/nav-home-off.gif";
						document.getElementById("nav_context_home").style.display = "none";
						
											
						document.getElementById("nav_tab_newstopics").src = "/images/nav-newstopics-off.gif";
						document.getElementById("nav_context_newstopics").style.display = "none";
						
											
						document.getElementById("nav_tab_opinion").src = "/images/nav-opinion-off.gif";
						document.getElementById("nav_context_opinion").style.display = "none";
						
											
						document.getElementById("nav_tab_toplists").src = "/images/nav-toplists-off.gif";
						document.getElementById("nav_context_toplists").style.display = "none";
						
											
						document.getElementById("nav_tab_events").src = "/images/nav-events-off.gif";
						document.getElementById("nav_context_events").style.display = "none";
						
											
						document.getElementById("nav_tab_photos").src = "/images/nav-photos-off.gif";
						document.getElementById("nav_context_photos").style.display = "none";
						
											
						document.getElementById("nav_tab_specialsections").src = "/images/nav-specialsections-off.gif";
						document.getElementById("nav_context_specialsections").style.display = "none";
						
											
						document.getElementById("nav_tab_newsletters").src = "/images/nav-newsletters-off.gif";
						document.getElementById("nav_context_newsletters").style.display = "none";
						
										
					document.getElementById("nav_tab_"+in_item).src = "/images/nav-"+in_item+"-on.gif";
					document.getElementById("nav_context_"+in_item).style.display = "";
					
				}
				
				nav_tab_select('home');
				
				</script>
			
