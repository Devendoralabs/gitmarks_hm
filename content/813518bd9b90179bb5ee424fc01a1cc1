function hResumeDeleteEntry(name, i)
{
    var d = document.getElementById(name + i);

    if (document.getElementById(name + "delete_" + i).checked)
    {
        d.style.background = "#ff0000";
    }
    else
    {
        d.style.background = "";
    }
}

function hResumeGenerateDateBlocks(container, prefix, i, date_type)
{
    var j;
    var k;

    var d1 = document.createElement("div");
    d1.setAttribute("class", "hresume_form_block");
    d1.style.width = "175px";

    var dateTitle = document.createElement("span");
    if (date_type == 1)
    {
        dateTitle.innerHTML = "Start Date:";
    }
    else if (date_type == 2)
    {
        dateTitle.innerHTML = "End Date:";
    }
    else /* (date_type == 0) */
    {
        dateTitle.innerHTML = "Date:";
    }
    d1.appendChild(dateTitle);

    var br = document.createElement("br");
    d1.appendChild(br);

    var monthInput = document.createElement("select");
    if (date_type == 1)
    {
        monthInput.setAttribute("id", prefix + "start_date_m_" + i);
        monthInput.setAttribute("name", prefix + "start_date_m_" + i);
    }
    else if (date_type == 2)
    {
        monthInput.setAttribute("id", prefix + "end_date_m_" + i);
        monthInput.setAttribute("name", prefix + "end_date_m_" + i);
    }
    else /* (date_type == 0) */
    {
        monthInput.setAttribute("id", prefix + "date_m_" + i);
        monthInput.setAttribute("name", prefix + "date_m_" + i);
    }
    var months = ['January', 'February', 'March', 'April',
                  'May', 'June', 'July', 'August',
                  'September', 'October', 'November', 'December'];
    for (j = 0; j < months.length; j++)
    {
        monthInput.options[j] = new Option(months[j], months[j], false, false);
    }
    monthInput.options[12] = new Option("None", "None", false, true);
    d1.appendChild(monthInput);

    var yearInput = document.createElement("select");
    if (date_type == 1)
    {
        yearInput.setAttribute("id", prefix + "start_date_y_" + i);
        yearInput.setAttribute("name", prefix + "start_date_y_" + i);
    }
    else if (date_type == 2)
    {
        yearInput.setAttribute("id", prefix + "end_date_y_" + i);
        yearInput.setAttribute("name", prefix + "end_date_y_" + i);
    }
    else /* (date_type == 0) */
    {
        yearInput.setAttribute("id", prefix + "date_y_" + i);
        yearInput.setAttribute("name", prefix + "date_y_" + i);
    }
    var year_start = 1930;
    var curDate = new Date();
    var year_end = curDate.getFullYear();
    for (j = year_start, k = 0; j <= year_end; j++, k++)
    {
        yearInput.options[k] = new Option(j, j, false, false);
    }
    yearInput.options[k] = new Option("None", "None", false, true);
    d1.appendChild(yearInput);

    container.appendChild(d1);
}

function hResumeAddAnotherEducation()
{
    var i = 0;

    while (1)
    {
        if (!document.getElementById("hresume_education_" + i))
        {
            break;
        }

        i++;
    }

    if (i == 0)
    {
        // something is really messed up, 0 must exist
        alert("ERROR: Failed to parse the current entries!")
        return false;
    }

    if ((document.getElementById("hresume_education_school_" +
                                 (i - 1)).value == "") &&
        (document.getElementById("hresume_education_degree_" +
                                 (i - 1)).value == "") &&
        (document.getElementById("hresume_education_start_date_m_" +
                                 (i - 1)).value == "None") &&
        (document.getElementById("hresume_education_start_date_y_" +
                                 (i - 1)).value == "None") &&
        (document.getElementById("hresume_education_end_date_m_" +
                                 (i - 1)).value == "None") &&
        (document.getElementById("hresume_education_end_date_y_" +
                                 (i - 1)).value == "None"))
    {
        alert("Sorry, an empty entry already exists!")
        return false;
    }

    var d1 = document.createElement("div");
    d1.setAttribute("id", "hresume_education_" + i);
    d1.setAttribute("class", "hresume_entry_block");

    var d2 = document.createElement("div");
    d1.appendChild(d2);

    var d3 = document.createElement("div");
    d3.setAttribute("class", "hresume_form_block");
    d3.style.width = "395px";
    var title = document.createElement("span");
    title.innerHTML = "University/College:";
    d3.appendChild(title);
    var br = document.createElement("br");
    d3.appendChild(br);
    var input = document.createElement("input");
    input.setAttribute("class", "hresume_stretch");
    input.setAttribute("id", "hresume_education_school_" + i);
    input.setAttribute("name", "hresume_education_school_" + i);
    input.setAttribute("type", "text");
    d3.appendChild(input);
    d2.appendChild(d3);

    hResumeGenerateDateBlocks(d2, "hresume_education_", i, 1);

    var d4 = document.createElement("div");
    d4.style.clear = "both";
    d2.appendChild(d4);

    var d3 = document.createElement("div");
    d3.setAttribute("class", "hresume_form_block");
    d3.style.width = "190px";
    var title = document.createElement("span");
    title.innerHTML = "Major:";
    d3.appendChild(title);
    var br = document.createElement("br");
    d3.appendChild(br);
    var input = document.createElement("input");
    input.setAttribute("class", "hresume_stretch");
    input.setAttribute("id", "hresume_education_major_" + i);
    input.setAttribute("name", "hresume_education_major_" + i);
    input.setAttribute("type", "text");
    d3.appendChild(input);
    d2.appendChild(d3);

    var d3 = document.createElement("div");
    d3.setAttribute("class", "hresume_form_block");
    d3.style.width = "190px";
    var title = document.createElement("span");
    title.innerHTML = "Degree:";
    d3.appendChild(title);
    var br = document.createElement("br");
    d3.appendChild(br);
    var input = document.createElement("input");
    input.setAttribute("class", "hresume_stretch");
    input.setAttribute("id", "hresume_education_degree_" + i);
    input.setAttribute("name", "hresume_education_degree_" + i);
    input.setAttribute("type", "text");
    d3.appendChild(input);
    d2.appendChild(d3);

    hResumeGenerateDateBlocks(d2, "hresume_education_", i, 2);

    var d4 = document.createElement("div");
    d4.style.clear = "both";
    d2.appendChild(d4);

    document.getElementById("hresume_education").appendChild(d1);

    return false;
}

function hResumeAddAnotherAccreditation()
{
    var i = 0;

    while (1)
    {
        if (!document.getElementById("hresume_accreditations_" + i))
        {
            break;
        }

        i++;
    }

    if (i == 0)
    {
        // something is really messed up, 0 must exist
        alert("ERROR: Failed to parse the current entries!")
        return false;
    }

    if ((document.getElementById("hresume_accreditations_name_" +
                                 (i - 1)).value == "") &&
        (document.getElementById("hresume_accreditations_date_m_" +
                                 (i - 1)).value == "None") &&
        (document.getElementById("hresume_accreditations_date_y_" +
                                 (i - 1)).value == "None"))
    {
        alert("Sorry, an empty entry already exists!")
        return false;
    }

    var d1 = document.createElement("div");
    d1.setAttribute("id", "hresume_accreditations_" + i);
    d1.setAttribute("class", "hresume_entry_block");

    var d2 = document.createElement("div");
    d1.appendChild(d2);

    var d3 = document.createElement("div");
    d3.setAttribute("class", "hresume_form_block");
    d3.style.width = "395px";
    var nameTitle = document.createElement("span");
    nameTitle.innerHTML = "Accreditation (PE, CPA, JD, etc):";
    d3.appendChild(nameTitle);
    var br = document.createElement("br");
    d3.appendChild(br);
    var nameInput = document.createElement("input");
    nameInput.setAttribute("class", "hresume_stretch");
    nameInput.setAttribute("id", "hresume_accreditations_name_" + i);
    nameInput.setAttribute("name", "hresume_accreditations_name_" + i);
    nameInput.setAttribute("type", "text");
    d3.appendChild(nameInput);
    d2.appendChild(d3);

    hResumeGenerateDateBlocks(d2, "hresume_accreditations_", i, 0);

    var d4 = document.createElement("div");
    d4.style.clear = "both";
    d2.appendChild(d4);

    document.getElementById("hresume_accreditations").appendChild(d1);

    return false;
}

function hResumeAddAnotherWork()
{
    var i = 0;

    while (1)
    {
        if (!document.getElementById("hresume_work_" + i))
        {
            break;
        }

        i++;
    }

    if (i == 0)
    {
        // something is really messed up, 0 must exist
        alert("ERROR: Failed to parse the current entries!")
        return false;
    }

    if ((document.getElementById("hresume_work_employer_" +
                                 (i - 1)).value == "") &&
        (document.getElementById("hresume_work_location_" +
                                 (i - 1)).value == "") &&
        (document.getElementById("hresume_work_title_" +
                                 (i - 1)).value == "") &&
        (document.getElementById("hresume_work_start_date_m_" +
                                 (i - 1)).value == "None") &&
        (document.getElementById("hresume_work_start_date_y_" +
                                 (i - 1)).value == "None") &&
        (document.getElementById("hresume_work_end_date_m_" +
                                 (i - 1)).value == "None") &&
        (document.getElementById("hresume_work_end_date_y_" +
                                 (i - 1)).value == "None") &&
        (document.getElementById("hresume_work_description_" +
                                 (i - 1)).value == ""))
    {
        alert("Sorry, an empty entry already exists!")
        return false;
    }

    var d1 = document.createElement("div");
    d1.setAttribute("id", "hresume_work_" + i);
    d1.setAttribute("class", "hresume_entry_block");

    var d2 = document.createElement("div");
    d1.appendChild(d2);

    var d3 = document.createElement("div");
    d3.setAttribute("class", "hresume_form_block");
    d3.style.width = "285px";
    var title = document.createElement("span");
    title.innerHTML = "Employer:";
    d3.appendChild(title);
    var br = document.createElement("br");
    d3.appendChild(br);
    var input = document.createElement("input");
    input.setAttribute("class", "hresume_stretch");
    input.setAttribute("id", "hresume_work_employer_" + i);
    input.setAttribute("name", "hresume_work_employer_" + i);
    input.setAttribute("type", "text");
    d3.appendChild(input);
    d2.appendChild(d3);

    var d3 = document.createElement("div");
    d3.setAttribute("class", "hresume_form_block");
    d3.style.width = "285px";
    var title = document.createElement("span");
    title.innerHTML = "Title:";
    d3.appendChild(title);
    var br = document.createElement("br");
    d3.appendChild(br);
    var input = document.createElement("input");
    input.setAttribute("class", "hresume_stretch");
    input.setAttribute("id", "hresume_work_title_" + i);
    input.setAttribute("name", "hresume_work_title_" + i);
    input.setAttribute("type", "text");
    d3.appendChild(input);
    d2.appendChild(d3);

    var d4 = document.createElement("div");
    d4.style.clear = "both";
    d2.appendChild(d4);

    var d3 = document.createElement("div");
    d3.setAttribute("class", "hresume_form_block");
    d3.style.width = "205px";
    var title = document.createElement("span");
    title.innerHTML = "Location:";
    d3.appendChild(title);
    var br = document.createElement("br");
    d3.appendChild(br);
    var input = document.createElement("input");
    input.setAttribute("class", "hresume_stretch");
    input.setAttribute("id", "hresume_work_location_" + i);
    input.setAttribute("name", "hresume_work_location_" + i);
    input.setAttribute("type", "text");
    d3.appendChild(input);
    d2.appendChild(d3);

    hResumeGenerateDateBlocks(d2, "hresume_work_", i, 1);

    hResumeGenerateDateBlocks(d2, "hresume_work_", i, 2);

    var d4 = document.createElement("div");
    d4.style.clear = "both";
    d2.appendChild(d4);

    var d3 = document.createElement("div");
    d3.setAttribute("class", "hresume_form_block");
    d3.style.width = "585px";
    var title = document.createElement("span");
    title.innerHTML = "Description of Position:";
    d3.appendChild(title);
    var br = document.createElement("br");
    d3.appendChild(br);
    var input = document.createElement("textarea");
    input.setAttribute("class", "hresume_stretch");
    input.setAttribute("id", "hresume_work_description_" + i);
    input.setAttribute("name", "hresume_work_description_" + i);
    input.setAttribute("rows", "5");
    input.setAttribute("cols", "70");
    d3.appendChild(input);
    d2.appendChild(d3);

    var d4 = document.createElement("div");
    d4.style.clear = "both";
    d2.appendChild(d4);

    document.getElementById("hresume_work").appendChild(d1);

    return false;
}

