<script>
(function() {
    var t = setTimeout(function() {
        var wtwImages = [];
        var allImages = document.getElementsByTagName('img');
        var timeoutBeacon = null;
        var image = null;
        for (var i = 0, len = allImages.length; i < len; i++) {
            image = allImages[i];
            if (image.src.indexOf('worthathousandwords.com') != '-1' || image.src.indexOf('yellowpages.com') != '-1') {
                if (!image.complete) {
                    //log timeout beacon
                    timeoutBeacon = new Image();
                    timeoutBeacon.src = 'http://local.yahoo.com/img/b.gif';

                    //remove external beacon if beacon has not loaded in X milliseconds
                    image.src = '';
                }
            }
        }
    }, 2000);
})();
</script>

