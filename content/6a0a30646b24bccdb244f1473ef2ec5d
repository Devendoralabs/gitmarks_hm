<br>                 if(settings.method == "POST")
<br>                   {
<br>                      $.post(settings.url, {"q": query}, function (results) {
<br>                         cache.add(query, results);
<br>                         populate_dropdown(query, results);
<br>                       }, "json");
<br>                   }
<br>                  else
<br>                    {
<br>                       $.get(settings.url, {"q": query }, function (results) {
<br>                         cache.add(query, results);
<br>                         populate_dropdown(query, results);
<br>                       }, "json");
<br>                    }</div>
