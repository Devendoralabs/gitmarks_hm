					<div id="dsq-comment-message-52296092" class="dsq-comment-message">Create  a handler:<br>------<br>using System;<br>using System.Web;<br>using System.Data.SqlClient;<br>using System.Configuration;<br>using System.Text;<br><br>namespace VideoManager<br>{<br>	public class JsonTags : IHttpHandler<br>	{<br><br>		public void ProcessRequest(HttpContext context)<br>		{<br>			string tag = context.Request.QueryString["q"];<br>			StringBuilder json = new StringBuilder();<br>			string sql = "Select Distinct Top 10 Tag_Value from Element_Tags Where Tag_Value like &#39;" + tag + "%&#39; ORDER BY Tag_Value";<br>			using (SqlConnection connection = new SqlConnection(Database.TagsConnection))<br>			using (SqlCommand command = new SqlCommand(sql, connection))<br>			{<br>				connection.Open();<br>				using (SqlDataReader reader = command.ExecuteReader())<br>				{<br>					while (reader.Read())<br>					{<br>						json.AppendFormat("{{\"name\":\"{0}\"}},", reader["Tag_Value"]);<br>					}<br>					context.Response.Write("[" + json.ToString() + "]");<br>				}<br>			}<br>		}<br><br>		public bool IsReusable<br>		{<br>			get<br>			{<br>				return false;<br>			}<br>		}<br>	}<br>}<br>-----<br><br></div>
