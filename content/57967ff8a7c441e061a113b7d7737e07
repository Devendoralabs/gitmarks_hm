<script language="javascript" type="text/javascript">

    function cleankeywords_Home( objTextField ) {
	    with (objTextField) {
		    if (objTextField.value == "Keyword") {
			    objTextField.value = "";
			    return true;
		    }
    	
		    var sValue = getCleanKeywords(objTextField.value);
            jobsearch.kwrd.value = sValue;
            return true;
	    }
    }

	function clearDefaultText( objTextField, strDefaultText ){
        if( objTextField.value == strDefaultText ){
            objTextField.value = "";
        } 
	}		
	function setDefaultText( objTextField, strDefaultText ){
        if( objTextField.value == "" ){
            objTextField.value = strDefaultText;
        }
	}	

</script>

