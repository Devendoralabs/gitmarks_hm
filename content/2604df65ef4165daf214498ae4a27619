$j(document).ready(function(){
createLink('div.valeur_login');
});
function connexion() {
		WaitingEffect();
		$j('#return').val($j('#requestURI').val());
		$j("#connexion").ajaxSubmit({ 
			url: '/login',
			success:  function(responseText, responseStatus) { 
				$j(".login").html(responseText);
				
			}
		});
}


function getMyPassword() {
	$j('html, body').animate({scrollTop:0}, 'slow');
	WaitingContact();
	recalculatePosition("#dialog2",425,119);
	$j.get('/register/callpassword', null, function(data) {
		$j("#dialog2").html(data);
		document.onkeydown = function(e){
			  if (e == null) { // ie
			    keycode = event.keyCode;
			  } else { // mozilla
			    keycode = e.which;
			  }
			  if(keycode == 27){ // escape, close box
				  closeEffect();
			  } 
		}	
	});
	return false;
}
var checked = true;
function changeCheck() {
	
	var input = $j('#LoginRememberMe');
	
	if (checked == true){
		checked = false;
		input.val("0");
		$j('.jNiceCheckbox').removeClass('jNiceChecked');
	}
	else {
		checked = true;
		input.val("1");
		$j('.jNiceCheckbox').addClass('jNiceChecked');
	}
	return false;
}
</script>
