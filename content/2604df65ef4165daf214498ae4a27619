function featured(id, type) {
	$j(".text_featured").css({color: "black"});
	if(type == 'lect'){
		$j('#masque_fe').animate({left: "0px"}); 
	}
	if(type == 'comment'){
		$j('#masque_fe').animate({left: "102px"}); 
	}
	if(type == 'sent'){
		$j('#masque_fe').animate({left: "202px"}); 
	}
	$j.get('/home/featured', {id: id, type: type}, function (data) {
		$j("#feat").html(data);
	});
}
</script> 
