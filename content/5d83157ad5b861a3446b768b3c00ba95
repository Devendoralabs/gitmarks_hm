function GetOrg4(){

var pathObj1=(typeof pathObj.dir[1] === "undefined") ? "landing" : pathObj.dir[1].toLowerCase();

	if (pathObj1 === "corporateresponsibility") {
		pathObj1 = "corpres";
	}
	else if (pathObj1 === "companyinfo") {
		pathObj1 = "compinfo";
	}

	p_org4 = wa_org3 + ":" + pathObj1;
	
	return unescape(p_org4);
}

function GetDir(numDir){
	var pathObjn=(typeof pathObj.dir[numDir] === "undefined") ? "" : pathObj.dir[numDir].toLowerCase();
	return unescape(pathObjn);
}		

function GetPageName(pagename){
	if ((pagename.indexOf(".") > -1) && (pagename.charAt(pagename.length-1) === "/")) {
		pagename = pagename.substring(0,pagename.length-1);
	}
	else {
		pagename = pagename.replace(/(\/)$/, "/index.htm");
	}
	return unescape(pagename);
}

