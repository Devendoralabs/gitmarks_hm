    <form action="http://www.daylife.com/search" method="get" class="DL-autocomplete-flag">
	<input type="text" id="query" name="q" value="Search news & the largest editorial photo archive on the web" class="DL-input-text" />
		<input type="submit" value="Search" class="DL-submit-button"/>
</form>
<script type="text/javascript">
$DL(document).ready(function(){
	// the following clears the search box
	$DL("#query").bind("focus", function(){
		if( this.value == "Search news & the largest editorial photo archive on the web" ) {
			this.value = "";
			$DL(this).parent().addClass("active-form");
		}
	});
	$DL("#query").bind("blur", function(){
		if( !this.value.length ) {
			this.value = "Search news & the largest editorial photo archive on the web";
			$DL(this).parent().removeClass("active-form");
		}
	});
});

</script>
