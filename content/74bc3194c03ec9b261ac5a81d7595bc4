<script language="javascript" type="text/javascript">

     function findWidth(stringLabel) {
         if (stringLabel) {
             // Hack to get button widths close to proper in IE 6 & 7.
             var IEVersion = getInternetExplorerVersion();
             if (IEVersion > 0 && IEVersion < 8) {
                 var buttonwidth = 47 + 5.5 * stringLabel.length;
                 return buttonwidth.toString();
             }
         }
         return "auto";
     }

     function lightboxFormat(title, currentArray, currentIndex, currentOpts, downloadHighResLabel, prevLabel, nextLabel, closeLabel, textImage, textOf) {

    var element = currentArray[currentIndex];
    var info = $j(element).find('#info');
    var icon = $j(element).find('#icon');

    return '' +
    '<div id="lightbox-container-image-data-box">' + 
    '    <div id="lightbox-container-image-data">' + 
    '        <div id="lightbox-image-details">' + 
    '            <div id="lightbox-image-details-caption">' + 
    '                ' + (info.attr('imgtitle') && info.attr('imgtitle').length ? '<h2>' + info.attr('imgtitle') + '</h2>' : '') + '' + 
    '                ' + info.html() + '' + 
    '            </div>' + getHighResLink(info, downloadHighResLabel, getFileTypeIcon(icon)) +
    '        </div>' + 
    '        <div id="lightbox-footer-nav" class="clearfix" style="display: block;">' + 
    '            <div id="lightbox-nav" style="">' + 
    '                <div class="fl_left">' + 
    '                    <div id="previous-button" class="rc_box-rc_whiteBtn ' + getArrowStyle(true, currentIndex, currentArray.length) + '">' + 
    '                        <div class="rc_whiteBtn narrow">' + 
    '                            <div class="content">' + 
    '                                <div class="contentPadding clearfix">' + 
    '                                    <div class="rc_top" />' + 
    '                                    <a id="lightbox-nav-btnPrev" href="javascript:;" onclick="$j.fancybox.prev();" style="background: transparent url() no-repeat scroll left 0pt; -moz-background-clip: border; -moz-background-origin: padding; -moz-background-inline-policy: continuous;">' + 
    '                                        <span class="btnspan">' + prevLabel + '</span>' + 
    '                                    </a>' + 
    '                                </div>' + 
    '                            </div>' +
    '                            <div class="rc_bottom" style="width:' + findWidth(prevLabel) + '">' + 
    '                                <div/>' + 
    '                            </div>' + 
    '                        </div>' + 
    '                    </div>' + 
    '                </div>' + 
    '                <span id="lightbox-image-details-currentNumber" style="display: block;">' + textImage + ' ' + (currentIndex + 1) + ' ' + textOf + ' ' + currentArray.length + '</span>' + 
    '                <div class="fl_left">' + 
    '                    <div id="next-button" class="rc_box-rc_whiteBtn ' + getArrowStyle(false, currentIndex, currentArray.length) + '">' + 
    '                        <div class="rc_whiteBtn narrow">' + 
    '                            <div class="content">' + 
    '                                <div class="contentPadding clearfix">' + 
    '                                    <div class="rc_top" />' + 
    '                                    <a id="lightbox-nav-btnNext" href="javascript:;" onclick="$j.fancybox.next();" style="background: transparent url() no-repeat scroll left 0pt; -moz-background-clip: border; -moz-background-origin: padding; -moz-background-inline-policy: continuous;">' + 
    '                                        <span class="btnspan">' + nextLabel + '</span>' + 
    '                                    </a>' + 
    '                                </div>' + 
    '                            </div>' +
    '                            <div class="rc_bottom" style="width:' + findWidth(nextLabel) + '">' + 
    '                                <div/>' + 
    '                            </div>' + 
    '                        </div>' + 
    '                    </div>' + 
    '                </div>' + 
    '            </div>' + 
    '            <div id="lightbox-secNav">' + 
    '                <a id="lightbox-secNav-btnClose" href="javascript:;" onclick="$j.fancybox.close();">' + 
    '                    <img alt="Close" src="/images/lightbox/lightbox-btn-close.gif"/>' + 
    '                    <span class="closetext">' + closeLabel + '</span>' + 
    '                </a>' + 
    '            </div>' + 
    '        </div>' + 
    '    </div>' + 
    '</div>'; 
 }

 function getFileTypeIcon(icon) {
    var fileTypeIconHTML = '<dt/>';
    if (icon.attr('src')) {
        fileTypeIconHTML = '' +
        '                          <dt>' +
        '                            <img width="32" height="32" src="' + icon.attr('src') + '" />' +
        '                          </dt>';
    }
    return fileTypeIconHTML;
 }
 
 function getHighResLink(info, downloadHighResLabel, fileTypeIconHTML) {
    var highResolutionImageLinkHTML = '';
    
    var sizeText = '';
    
    if (info.attr('size')) {
        sizeText = '  [' + info.attr('size') + ']';
    }
    
    if (info.attr('download') && info.attr('download').length > 0) {
        highResolutionImageLinkHTML = '' +
        '            <div id="lightbox-download">' +
        '                <dl class="iconDL clearfix">' +
        fileTypeIconHTML +
        '                    <dd>' +
        '                        <a href="' + info.attr('download') + '">' + downloadHighResLabel + '</a>' +
        '                    </dd>' +
        '                    <dd>' + info.attr('ext')  + sizeText + ' </dd>' +
        '                </dl>' +
        '            </div>';
    }
    return highResolutionImageLinkHTML;
 }
  
 function getArrowStyle(isLeft, currentIndex, arrayLength)
 {
    if (isLeft)
    {
        if (currentIndex == 0)
        {
            return 'LeftDisabled';
        }
        else
        {
            return 'LeftArrow';
        }
    }
    else
    {
        if (currentIndex == (arrayLength - 1))
        {
            return 'RightDisabled';
        }
        else
        {
            return 'RightArrow';
        }
    }    
 }
</script>


