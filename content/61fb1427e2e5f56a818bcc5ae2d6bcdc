<!-- script for setting cookies for HH Test-->

<script type="text/javascript">


var cookies = document.cookie;
var name = "hammer";
if (cookies.indexOf(name) != -1) {

    var startpos = cookies.indexOf(name)+name.length+1;
    var endpos = cookies.indexOf(";",startpos)-1;

    if (endpos == -2) endpos = cookies.length;

    var nscCookie = unescape(cookies.substring(startpos,endpos+1));

    // control.
    if (nscCookie == "control") {
        document.write("<iframe src=\"http://www.download.com/html/hammer/control.html\" width=\"0\" height=\"0\" scrolling=\"no\" marginwidth=\"0\" marginheight=\"0\" frameborder=\"0\"></iframe>");
        document.write("<iframe src=\"http://www.cnettv.com/html/hammer/cnettv_control.html\" width=\"0\" height=\"0\" scrolling=\"no\" marginwidth=\"0\" marginheight=\"0\" frameborder=\"0\"></iframe>");
    }

}


</script>


