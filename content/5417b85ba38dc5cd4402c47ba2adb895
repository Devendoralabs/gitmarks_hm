						 id="newsContainer">
						 <a href="about-us/press.html" style="color:black;" id="newsHolderStart">
							 <span>
							 	RECENT NEWS:
							 </span>
						 </a>
						 <div id="newsHolder" 
						 		style="position:absolute;
						 				left:100px;
						 				top:0px;
						 				line-height:34px;
						 				text-align:center;
						 				width:694px;"></div>
						<script type="text/javascript">
							var newsIndex = 0;
							var rotateNewsID = 0;

							function fixNewsIndex()
							{
								if (newsIndex >= newsJson.results.length)
								{
									newsIndex = 0;
								}
								if (newsIndex < 0)
								{
									newsIndex = newsJson.results.length - 1;
								}
							}

							function rotateNewsTicker(amt)
							{
								var trueAmt = amt;
								if (amt == null || amt == 0)
								{
									amt = 1;
								}
								clearTimeout(rotateNewsID);
								$("#newsHolder div").eq(newsIndex).hide();

								newsIndex += amt;
								fixNewsIndex();
								
								$("#newsHolder div").eq(newsIndex).fadeIn(1000);

								rotateNewsID = setTimeout("rotateNewsTicker()", 6000);
							}

							if (newsJson != null && newsJson.results != null && newsJson.results.length > 0)
							{

								var newsHolder = document.getElementById('newsHolder');
								for (var i = 0; i < newsJson.results.length; i++)
								{
									var newsObj = newsJson.results[i];

									var spanNode = document.createElement("span");
									//spanNode.style.fontStyle = "italic";
									spanNode.style.fontSize = "12px";
									spanNode.innerHTML = newsObj.source + ",";

									var spanHeadline = document.createElement("span");
									spanHeadline.style.color = "#ed5207";
									spanHeadline.innerHTML = newsObj.headline;

									var linkNode = document.createElement("a");
									linkNode.style.color = "#ed5207";
									linkNode.href = newsObj.url;
									linkNode.target = "_blank";

									var divNode = document.createElement("div");
									divNode.style.display = "none";
									
									linkNode.appendChild(spanHeadline);

									divNode.appendChild(spanNode);
									divNode.innerHTML += " " + newsObj.date + " &nbsp;&mdash;&nbsp; ";
									divNode.appendChild(linkNode);
									
									newsHolder.appendChild(divNode);
								}
								
								newsIndex = newsJson.results.length - 1;
								document.getElementById('newsContainer').style.display = 'block';
								rotateNewsTicker();
							}
							
						</script>
