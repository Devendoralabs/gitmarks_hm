<!-- custom Google sitesearch -->
<script type="text/javascript">
function	submitSearch() {
	setSearchOptions();
}
function	setSearchOptions() {
	if ( !document.SiteSearchGoogle ) { return; }
	clearSearchOptions();
	if ( !document.SiteSearchGoogle.mousepad.checked ) {
		document.SiteSearchGoogle.q.value += ' -inurl:mousepad ';
	}
	if ( !document.SiteSearchGoogle.triprpt.checked ) {
		document.SiteSearchGoogle.q.value += ' -inurl:trip.rpt ';
	}
	document.getElementById('SiteSearchGoogleQID').focus();
}
function	clearSearchOptions() {
	if ( !document.SiteSearchGoogle ) { return; }
	document.SiteSearchGoogle.q.value = document.SiteSearchGoogle.q.value.replace ( / -inurl:mousepad /g, '' );
	document.SiteSearchGoogle.q.value = document.SiteSearchGoogle.q.value.replace ( / -inurl:trip.rpt /g, '' );
}
function	showSearchOptions() {
	if ( document.SiteSearchGoogle.sitesearch[0].checked ) {
		document.getElementById('searchOptions').style.visibility='inherit';
	} else {
		document.getElementById('searchOptions').style.visibility='hidden';
	}
}
</script>
<!-- end custom Google sitesearch -->

<script type="text/javascript">
function getStyle (oElm, strCssRule){
	var strValue = "";
	if(document.defaultView && document.defaultView.getComputedStyle){
		strValue = document.defaultView.getComputedStyle(oElm, "").getPropertyValue(strCssRule);
	}
	else if(oElm.currentStyle){
		strCssRule = strCssRule.replace(/\-(\w)/g, function (strMatch, p1){
			return p1.toUpperCase();
		});
		strValue = oElm.currentStyle[strCssRule];
	}
	return strValue;
}	// getStyle
</script>

