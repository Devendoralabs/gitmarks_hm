<!-- iFrame sizing -->
<script type="text/javascript">
/* http://tips.kaali.co.uk/2010/06/19/cross-bowser-iframe-auto-resize-script/ */
function autoIframe(frameId){

try{
frame = document.getElementById(frameId);
innerDoc = (frame.contentDocument) ? frame.contentDocument : frame.contentWindow.document;

if (innerDoc == null){
// Google Chrome
frame.height = document.all[frameId].clientHeight + document.all[frameId].offsetHeight + document.all[frameId].offsetTop;
}
else{
objToResize = (frame.style) ? frame.style : frame;
var extraHeight = 0;
objToResize.height = innerDoc.body.scrollHeight + extraHeight;
}
}

catch(err){
var e = err.message;
//alert('Err: ' + err.message);
//window.status = err.message;
}
}
</script>
<!-- end iFrame sizing -->

<!-- dump -->
<script type="text/javascript">
/**
 * Function : dump()
 * Arguments: The data - array,hash(associative array),object
 *    The level - OPTIONAL
 * Returns  : The textual representation of the array.
 * This function was inspired by the print_r function of PHP.
 * This will accept some data as the argument and return a
 * text that will be a more readable version of the
 * array/hash/object that is given.
 * Docs: http://www.openjs.com/scripts/others/dump_function_php_print_r.php
 */
function dump(arr,level) {
	var dumped_text = "";
	if(!level) level = 0;
	
	//The padding given at the beginning of the line.
	var level_padding = "";
	for(var j=0;j<level+1;j++) level_padding += "    ";
	
	if(typeof(arr) == 'object') { //Array/Hashes/Objects 
		for(var item in arr) {
			var value = arr[item];
			
			if(typeof(value) == 'object') { //If it is an array,
				dumped_text += level_padding + "'" + item + "' ...\n";
				dumped_text += dump(value,level+1);
			} else {
				dumped_text += level_padding + "'" + item + "' => \"" + value + "\"\n";
			}
		}
	} else { //Stings/Chars/Numbers etc.
		dumped_text = "===>"+arr+"<===("+typeof(arr)+")";
	}
	return dumped_text;
}	// dump
</script>
<!-- end dump -->

<!-- hide HelloBar button -->
<script type="text/javascript">
function hideHelloBarButton () {
	var closeButton = document.getElementById ( 'hellobar-close' );
	if ( closeButton ) {
		closeButton.style.display = 'none';
	} else {
		setTimeout ( hideHelloBarButton, 1000 );
	}
}	// hideHelloBarButton
</script>
<!-- end hide HelloBar button -->

<!-- onLoad handler -->
<script type="text/javascript">
function	onLoadHandler() {
	clearSearchOptions();
	setFontSizes();
	hideHelloBarButton();
}
if ( window.addEventListener ) {
	window.addEventListener('load', onLoadHandler, false );
} else {
	window.onload = onLoadHandler;
}	// if
</script>
<!-- end onLoad handler -->


	

