var useAnywhere = false;
var useAnywhereLite = false;
if ( document.all || document.getElementsByClassName ) {
	//	nothing that doesn't support one or the other of these will ever work
	useAnywhereLite = true;
	if ( BrowserDetect.browser.toLowerCase() == 'chrome' ||
		BrowserDetect.browser.toLowerCase() == 'safari' ||
		BrowserDetect.browser.toLowerCase() == 'opera' ||
		BrowserDetect.browser.toLowerCase() == 'msie' ||
		( BrowserDetect.browser.toLowerCase() == 'firefox' && BrowserDetect.version >= '3.6' ) ) {
		useAnywhere = true;
	}
}
</script>
