  yt.pubsub.subscribe('init', yt.www.comments.watch5.addCommentMouseListener);




    yt.pubsub.subscribe('init', yt.www.watch.sow.resizeForSearch);
    yt.events.listen(window, 'resize', yt.www.watch.sow.resizeForSearch);

      function tn_load(index) {
    if (window.yt.timing && yt.timing.handleThumbnailLoad){
      yt.timing.handleThumbnailLoad(index);
    }
  }




    yt.pubsub.subscribe('init', yt.www.watch.player.enableVideoQualityDisplay);
    yt.pubsub.subscribe('init', yt.www.watch.player.handleHashArgumentsOnWatchLoad);
    yt.pubsub.subscribe('init', yt.www.watch.flagging.maybeShowFlaggingPannel);


    yt.pubsub.subscribe('init', function() {
      var description = _gel('watch-description');
      if (!_hasclass(description, 'yt-uix-expander-collapsed')) {
        yt.www.watch.watch5.handleToggleDescription(description);
      }
    });




      yt.pubsub.subscribe('init', function() {
        yt.flash.showDivIfVersionNotSupported('flash10-promo-div',[9,0,115]);
        var showCount = yt.UserPrefs.get('ftuc');
        if (!showCount) {
          showCount = 0;
        }
        yt.UserPrefs.set('ftuc', parseInt(showCount, 10) + 1);
        yt.UserPrefs.save();
      });
  </script>

  
