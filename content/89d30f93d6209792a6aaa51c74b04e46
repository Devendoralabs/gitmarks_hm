//<![CDATA[
  
function afficher(info)
{
        var tooltip = document.getElementById(info);
        tooltip.style.visibility = "visible";
        run= true ;
}
function masquer(info)
{
        var tooltip = document.getElementById(info) ;
        tooltip.style.visibility = "hidden";
        run= false ;
}
