        // Hero Graphics
        $(function(){
            $('.hero .promo:gt(0)').hide();
            setInterval(function () { $('.hero .promo:first-child').fadeOut('slow').next('.promo').fadeIn('slow').end().appendTo('.hero'); }, 10000);            
        });
        
        // Promo Graphics
        $(function(){
            $('.promos .promo:gt(0)').hide();
            setInterval(function () { $('.promos .promo:first-child').fadeOut('slow').next('.promo').fadeIn('slow').end().appendTo('.promos'); }, 10000);            
        });

        // Newsletter Complete
        function onSignForNewsletterComplete(event) {
            $(function () {
                $("#newsletterContentForm").hide();
                $("#newsletterComplete").show();
            });
