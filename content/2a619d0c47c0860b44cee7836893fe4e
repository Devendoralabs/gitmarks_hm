    <script type="text/javascript">

    window.fbAsyncInit = function() {
    FB.init({ appId: '163444852604', status: true, cookie: true,
            xfbml: true
        });
    };
    (function() {
        var e = document.createElement('script'); e.async = true;
        e.src = document.location.protocol +
      '//connect.facebook.net/en_US/all.js';
        document.getElementById('fb-root').appendChild(e);
    } ());
    
//    FB.Event.subscribe('auth.sessionChange', function(response) {
//        if (response.session && showFBAvatar) {
//            //SetFBAvatar();
//        }
//    });

    var handleFacebookSession = true;

    var logoutFromFB = false;

    
        
    FB.Event.subscribe('auth.sessionChange', function(response) {
        if(logoutFromFB)
            FB.logout(function(response) {});    
    });

    if(!logoutFromFB)
    {
        FB.Event.subscribe('auth.sessionChange', function(response) {
            if (response.session && handleFacebookSession) {
                handleFbConnected(false);
            } else {
                // The user has logged out, and the cookie has been cleared
            }
        });
    }
    function handleFbConnected(isNew) {
        window.location = (isNew) ? '/signup/fbinvite.aspx' : '/signup/signup.aspx?fromFacebookLogin=1';
    }


    </script>
    
