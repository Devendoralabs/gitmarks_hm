        var pT = new sb.Analytics('sbcom');
        pT._trackPageview();
        function trackEvent(tag) {
            pT._trackEvent('Homepage', tag);
            return true;
        }
        function redirect(tag, loc) {
            pT._trackEvent('Clicks', tag);
            window.location = loc;
        }
    </script>
    
