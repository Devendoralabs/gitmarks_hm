function show()
{
	document.getElementById('JobBoxScript').style.display = 'block';
	document.getElementById('JobBoxLink').innerHTML = "<a href='javascript:;' onClick='hide();'>Put this Job box on your website</a>";
}
function hide()
{
	document.getElementById('JobBoxScript').style.display = 'none';
	document.getElementById('JobBoxLink').innerHTML = "<a href='javascript:;' onClick='show();'>Put this Job box on your website</a>";
}
</script>
