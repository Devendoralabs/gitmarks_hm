<script type="text/javascript">
function shownav(f,pos,size)
{
if(f){visi="visible";}
else{visi="hidden";}
for(var i = 0; i < size; i++)
{
if(i == pos)
{if(document.getElementById){document.getElementById("navbar"+pos).style.visibility=visi;}}
else
{if(document.getElementById){document.getElementById("navbar"+i).style.visibility="hidden";}}
}
}
</script>
