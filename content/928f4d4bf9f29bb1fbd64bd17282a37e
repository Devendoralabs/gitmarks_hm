		<script type="text/javascript">
			function getUserFromCookie() {
				var vals = document.cookie.split(/;\s?/)
				for (var i = 0; i < vals.length; i++) {
					if (vals[i].split("=")[0] == "user_id") {
						return vals[i].split("=")[1];
					}
				}
				return null;
			}
		</script>
