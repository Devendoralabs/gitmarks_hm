function browse(aid, name, pass){
	uploader.document.forms.upload.aid.value = aid;
	uploader.document.forms.upload.pass.value = pass;
	uploader.document.forms.upload.name.value = name;
	uploader.document.forms.upload.image.click();
	//alert(aid + pass + name);
	while(uploader.document.forms.upload.image == ""){
		setTimeout(100);
	}
 	 if(uploader.document.forms.upload.image.value != ""){
		// var obj=main.reboot;
  		// if (obj){
  		//  	obj.TGotoFrame('/upload', 2);
  		// }
  		setTimeout("uploader.document.upload.submit()", 200);
	}
}

document.write('<FRAMESET COLS="100%,0">');
document.write('<FRAME NAME="main" SCROLLING="AUTO" NORESIZE SRC="main.html?'+window.location.search.substring(1)+'">');
document.write('<FRAME NAME="uploader" SCROLLING="NO" NORESIZE SRC="uploader.php">');
document.write('</FRAMESET>');

