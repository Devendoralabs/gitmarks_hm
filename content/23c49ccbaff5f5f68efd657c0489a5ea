<SCRIPT LANGUAGE="JavaScript">
function confirmDeleteComment()
{
var agree=confirm('Are you sure you wish to REMOVE THIS COMMENT?');
if (agree)
	return true ;
else
	return false ;
}
</SCRIPT>

<SCRIPT LANGUAGE="JavaScript">
function confirmDeletePost()
{
var agree=confirm('Are you sure you wish to DELETE THIS POST?');
if (agree)
	return true ;
else
	return false ;
}
</SCRIPT>

