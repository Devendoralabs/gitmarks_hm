<script type='text/javascript'>
    // <!--

function load_recent_photos()
{

    var feed_url = 'http://api.flickr.com/services/feeds/photos_public.gne?id=73509078@N00&lang=en-us&format=json&jsoncallback=?';
    $.getJSON(feed_url, function(data) {
        $('#photos_spinner').hide('slow');

        $.each(data.items, function(i,item){
            var div = $("<div class='photo' />")
                .appendTo('#recent_photos')
            ;

            $('<h3>').text(item.title).appendTo(div);

            var a = $("<a/>")
                .attr("href", item.link)
                .attr("title", item.title)
                .appendTo(div)
            ;

            $("<img/>")
                .attr("src", item.media.m)
                .attr("alt", item.title)
                .appendTo(a)
            ;

            // Build tags
            /*
            var tags = $("<div class='tags' />");
            var tag_base_url = "http://www.flickr.com/photos/sybrenstuvel/tags/";
            $.each(item.tags.split(' '), function(tag_nr, tagname) {
                var tag = $("<a/>")
                    .attr("href", tag_base_url + tagname + "/")
                    .text(tagname)
                ;

                tag.appendTo(tags);
                tags.append(' ');
            });
            tags.appendTo(div);
            */

            // Max. 8 recent photos.
            if ( i == 7 ) return false;
        });

    });

}

$().ready(load_recent_photos);

    // -->

</script>

