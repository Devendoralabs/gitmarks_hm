<div id='h2tocholder'>
    <h2>On this page</h2>
</div>

<script type='text/javascript'>
    // <![CDATA[
    function add_link(idx, elt)
    {
        var target = $(elt).attr('id');

        // Build an ID if it doesn't exist yet.
        if(!target) {
            target = 'header' + idx;
            $(elt).attr('id', target);
        }

        var html = '<li><a href="#' + target + '"></a></li>'
        $('#h2toc').append(html);

        // Set the link text.
        var text = $(elt).text();
        $("#h2toc a[href='#" + target + "']").text(text);

        // Make the header link back to the top
        $(elt).append('<a class="toclink" href="#h2tocholder">top</a>');
    }

    function build_toc()
    {
        // Append it here instead of just hard-coding it in the HTML to prevent
        // the HTML validator from seeing an empty OL tag.
        $('#h2tocholder').append("<ol id='h2toc'></ol>");
        $('#contentarea h2').each(add_link);
        $('#h2tocholder').show();
    }

    $(document).ready(build_toc);
    $('#h2tocholder').hide();
    
    // ]]>
</script>


