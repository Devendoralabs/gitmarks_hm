col1[1] = "/img/recruiters0328/BLI_logo.jpg";
col1[2] = "/img/recruiters0328/IDT_logo.gif";
col1[3] = "/img/recruiters0328/HSY_logo.jpg";
col1[4] = "/img/recruiters0328/SC1_logo.gif";
col1[5] = "/img/recruiters0328/NUA_logo.jpg";
getOthers["logo"] = col1;
col1 = null;
col2 = new Array();
col2[0] = "999";
col2[1] = "999";
col2[2] = "999";
col2[3] = "999";
col2[4] = "999";
col2[5] = "999";
getOthers["priority"] = col2;
col2 = null;

		//document.write(getOthers.dump())
		
		//initialise some vars
		var noSponsorAds=1
		var noOtherAds=5-1;
		
		var randomAdsArray=new Array('VEN','BLI','IDT','HSY','SC1','NUA');
		//for (i = 0; i < randomAds.length; i++) alert(randomAdsArray[i])
		
		//simply make an array containing random rows numbers
		var randRows=new Array()
		
		count=0
		var usedList='0'
		// loop for 20 as this should be enough
		// makes sure that we do not go into an infinite loop also
		for (i = 0; row < 20; row++) {
			ranNo=Math.floor(Math.random()*getOthers.getRowCount());
			//assign random number
			if (usedList.indexOf(',' + ranNo) == -1)  { 
				randRows[randRows.length]=ranNo;
				usedList=usedList + ',' + ranNo;
			}
			//once we get the amount of random rows break
			if (randRows.length == noOtherAds) break;
		}
		
		// get the random rows
		for (var i = 0; i < randRows.length; ++i) {
			//get the DataType for this daTypeName
			var client_ID=getOthers.getField(randRows[i], 'client_id')
			
			//get the DataType for this dataTypeName
			var logo=getOthers.getField(randRows[i], 'logo')
			
			randomAds=randomAds + '<a href="' + obsLink + '/recruitment?client_ID=' + client_ID + '"><img src="' + logo + '" border="0"></a>\n';
			randomAds=randomAds + '<hr />\n';
		}
	
	</script>
	

