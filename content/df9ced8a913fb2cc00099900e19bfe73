<script type="text/javascript" language="javascript">
			currentlySelected = 'r';  //read or commented,  r or c
			
			function show(thisPanel) {
				
				
				if (thisPanel == 'r') {
					document.getElementById('r').style.display = 'block';
					document.getElementById('rtab').src = '/gfx/most_read_tabOn.gif';
					document.getElementById('c').style.display = 'none';
					document.getElementById('ctab').src = '/gfx/most_commented_tab.gif';
					currentlySelected = 'r'
				} else {
					document.getElementById('r').style.display = 'none';
					document.getElementById('rtab').src = '/gfx/most_read_tab.gif';
					document.getElementById('c').style.display = 'block';
					document.getElementById('ctab').src = '/gfx/most_commented_tabOn.gif';
					currentlySelected = 'c'
				}
			}
			
			function showAmount(howMany) {
				
				var obj1 = document.getElementById(currentlySelected + '1');
				var obj7 = document.getElementById(currentlySelected + '7');
				var obj30 = document.getElementById(currentlySelected + '30');
				
				if (howMany == '1') {
					obj1.style.display = 'block';
					obj7.style.display = 'none';
					obj30.style.display = 'none';
				}
				
				if (howMany == '7') {
					obj1.style.display = 'none';
					obj7.style.display = 'block';
					obj30.style.display = 'none';
				}
				
				if (howMany == '30') {
					obj1.style.display = 'none';
					obj7.style.display = 'none';
					obj30.style.display = 'block';
				}
				
			}
		</script>
		
