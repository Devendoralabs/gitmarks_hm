<script language='javascript' id='log_script' src='http://eclog005.cafe24.com/weblog.js?uid=ysc16261626&uname=ysc16261626&r_ref='></script>
<script type="text/javascript" charset="utf-8" src="https://login2.cafe24ssl.com/crypt/AuthSSLManager.js"></script> 
               <script type="text/javascript" charset="utf-8" src="https://login2.cafe24ssl.com/crypt/AuthSSLManager.plugin.js"></script>
                <script type='text/javascript'>
                try {
                AuthSSLManager.getValue = function(id) {
                	if( /::/.test(id) == true ) {
                		var divide, o, type;
                		divide = id.split('::');
                		var frm = divide[0], id = divide[1];
                		o = document.forms[frm][id];
                        try {
                		    val = o.value;
                		} catch(e) {
                            val = '';
                        }
                	} else {
                		var o, type;
                		o = document.getElementById(id);
                 
                		if( o != null ) {
                			type = o.getAttribute('type');
                		} else {
                			o = document.getElementsByName(id);
                			if( o.length == 0 ) return -1;
                			type = o[0].getAttribute('type');
                		}
                 
                		var val;
                		switch(type) {
                			default:
                			case 'text':
                				o = document.getElementById(id);
                				if( o == null ) {
                					o = document.getElementsByName(id);
                                    try {
                            		    val = o[0].value;
                            		} catch(e) {
                            		    val = '';
                            		}
                				} else {
                                    try {
                					    val = o.value;
                            		} catch(e) {
                            		    val = '';
                            		}
                				}
                				break;
                			case 'radio':
                			case 'checkbox':
                				val = this.checkbox({'name': id, 'mode': 'val'});
                				break;
                		}
                	}
                 
                	return val;
                };
                } catch(e) {}
                </script>
                
            <script language='javascript'>
            <!--
            try {
                document.login_f.onsubmit = function(){
                encryptSubmit_LoginF();
                return false;
                }
            } catch(e){}
            //-->
            </script>
            <script language='javascript'>
            function encryptSubmit_LoginF()
            {
                if (document.login_f.is_ssl.checked === false) {
                    document.login_f.submit();
        		    return false;
                }
            	AuthSSLManager.weave({
            		// encrypt / decrypt
            		'auth_mode': 'encrypt'
            		// 엘리먼트 아이디 혹은 라디오박스는 name

            		, 'aEleId': ['login_f::id', 'login_f::passwd']

            		// callback function-name
            		, 'auth_callbackName': 'encryptSubmit_Complete_LoginF'
            	});
            }
            function encryptSubmit_Complete_LoginF(output) {
                var oForm = document.createElement("form");
                document.body.appendChild(oForm);
                var oBjencode_str = document.createElement("input");
                oBjencode_str.name = "encode_str";
                oBjencode_str.id = "encode_str";
                oBjencode_str.type = "hidden";
                oForm.appendChild(oBjencode_str);

            	if( AuthSSLManager.isError(output) == true ) {
            		alert("개인정보 암호화 실패. 관리자에게 문의해 주세요.");
            		return false;
            	}
            	document.getElementById('encode_str').value = output;
              oForm.method = "post";
              oForm.action = "/front/php/login/login_a.php?is_ssl=T";
              oForm.submit();
            }
            </script>