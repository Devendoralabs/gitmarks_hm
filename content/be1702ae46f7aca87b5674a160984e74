<script type="text/javascript">
    /* <![CDATA[ */
        

    var mdp = mdp || {app:{},util:{}};
    mdp.util = mdp.util || {};

    
            /*
                name: siteInfo
                description: returns site information
            */
            mdp.site = {
                brand : 'bhg'
            };
        
            /*
                name: getFBUtil
                description: returns a Facebook API key based on either the domain or application key passed
            */
            
                    mdp.util.fb = {"apiKey":"687edde5392fd21e26472e56cb552494","profileId":"72571226018","applicationId":"85037231102","pageId":"72571226018"};    
                
            mdp.util.cleanDomain = function(domain){
                return domain.replace("www.","").replace(".com","").replace(".tv","").replace("http://","");
            };
            mdp.util.fb.getConfig = function(key){
                key = mdp.util.cleanDomain(key);
                return mdp.util.fb[key];
            };
        
            /*
                name: Include
                description: Lazy loader for scripts
            */
            /* define service states */
            mdp.util.state = {
                notLoaded : "not_loaded",
                isLoading : "is_loading",
                isLoaded  : "is_loaded"
            };

            /*
            * Example feature to pass to the loader
            *
             var addthis ={
                script:"http://s7.addthis.com/js/250/addthis_widget.js?domready=1",
                beforeLoad:function(){

                },
                callback:function(){

                },
                identifier:{
                    name:"addthis",
                    type:"object"
                }
            };
            mdp.util.include(addthis);
            * */
            mdp.util.include = function(feature){

                /* state model object */
                var State = {
                    notLoaded : "not_loaded",
                    isLoading : "is_loading",
                    isLoaded  : "is_loaded"
                };

                /* predefined features */
                var features = {};

                feature.state = feature.state || State.notLoaded;

                if(typeof(feature.script) != "undefined"){
                    /* make sure feature is not already loaded */

                    if(typeof(feature.identifier) != "undefined" && typeof(feature.identifier.name) !== feature.identifier.type){
                        /* change state */
                        feature.state = State.isLoading;
                        if(feature.beforeLoad){
                            feature.beforeLoad();
                        }
                        var interval = setInterval(function(){
                            /* poll the DOM until resource is available */
                            if(typeof(window[feature.identifier.name]) === feature.identifier.type){
                                /* call the callback */
                                feature.callback();
                                /* change state */
                                feature.state = State.isLoaded;
                                clearInterval(interval);
                            }
                        },100);

                    }

                    injectScript(feature.script);
                    if(feature.onInject){
                        feature.onInject();
                    }

                }

                if(typeof(feature.style) != "undefined"){
                    injectStyle(feature.style);
                    if(feature.onInject){
                        feature.onInject();
                    }
                }

                function injectScript(url){
                    var script = document.createElement("script");
                    script.type="text/javascript";
                    script.src=url;
                    document.body.appendChild(script);
                }

                function injectStyle(url){
                    var style = document.createElement("link");
                    style.rel = "stylesheet";
                    style.type = "text/css";
                    style.href=url;
                    document.body.appendChild(style);
                }
            };

        <!-- Call Callback -->
    
    /* ]]> */
</script>
