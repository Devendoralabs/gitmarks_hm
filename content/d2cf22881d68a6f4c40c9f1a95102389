/*<![CDATA[*/
function BlogLeftNavTypeChange()
{
    var v = document.getElementById("sortby").value;
    var url = document.URL;
    
    // Remove the hash and stuff that follows
    if (url.indexOf("#") >= 0)
    {   url = url.substr(0, url.indexOf("#"));
    }

    url = url.replace(/[&/?]fl=[^&]*/g, "");
    if (url.indexOf("?") == -1)
        url += "?fl=" + v;
    else
        url += "&fl=" + v;
        
    try
    {   document.location = url;
    } catch (ex) {}
}
/* ]]> */
</script>

