  return "/static/4.5.2.5" + rest;
}

function nlw_make_plugin_path(rest) {
  return "/static/4.5.2.5".replace(/static/, 'nlw/plugin') + rest;
}





Socialtext.attachments = [{"date":"2008-08-31 18:54:38 GMT","page-id":"speakers","uploader_id":"29927","name":"Inagural.jpg","content-length":"294720","local_date":"Aug 31, 2008 11:54am","size":"287.81K","uri":"/data/workspaces/speakers/attachments/speakers:20080831185438-0-443/original/Inagural.jpg","uploader":"ccs_140@hotmail.com","content-type":"image/jpeg","id":"20080831185438-0-443","uploader_name":"Guest User"},{"date":"2010-09-24 20:23:01 GMT","page-id":"speakers","uploader_id":"29927","name":"Carol Dekkers 250 words Sept 2010.docx","content-length":"12175","local_date":"Sep 24, 2010 1:23pm","size":"11.89K","uri":"/data/workspaces/speakers/attachments/speakers:20100924202301-0-28744/original/Carol%2520Dekkers%2520250%2520words%2520Sept%25202010.docx","uploader":"dekkers@qualityplustech.com","content-type":"application/vnd.openxmlformats-officedocument.wordprocessingml.document","id":"20100924202301-0-28744","uploader_name":"Guest User"}];
