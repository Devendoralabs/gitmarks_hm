				<script> 
				  function search(){
				    var loc = window.location;
						var hashtag = document.getElementById('hashtag').value.replace(/#/g,'');
						window.location = loc.protocol + '//' + loc.hostname + '/' + hashtag;
						return false;
					}
				</script>
