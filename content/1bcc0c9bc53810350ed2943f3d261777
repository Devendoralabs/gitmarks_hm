<SCRIPT language="JavaScript">
selected = false;
group = 'inf';
numberOfItems = 1;
alwaysTrue = -1;
alwaysTrueSubItem = -1;
pn  = new Array;
pu = new Array;
expandedItems = new Array;
su  = new Array;
sn = new Array;
pn[0] = 'Find Programs';
pu[0]  = '/programs/iegpsugisf/index.html?exp=0';
expandedItems[0]  = false;
su[0]  = new Array;
sn[0] = new Array;
