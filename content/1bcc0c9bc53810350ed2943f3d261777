function navChange( item ) {
    serve = false;
    if( myDOM( 'leftNav' ) ) serve = true;
    if( browser.indexOf( 'ie' ) >=0 && navigator.userAgent.indexOf( 'Mac' ) >=0 ) serve = false;
    if( !serve ) { document.location = pu[item]; return; }
    if( item != null ) expandedItems[ item ] = ! expandedItems[ item ];
    text = '<TABLE summary="This table is for layout only" cellpadding="0" cellspacing="0" border="0" width="170" align="left">';
    for( y = 0 ; y < numberOfItems ; y++ ) {
        if( alwaysTrue == y ) text += '<TR><TD class="leftNavTextHighlight"><IMG src="/images/spacer.gif" alt="&#39;'+pn[y]+'&#39; subtopics menu is expanded" width="27" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftNavTextHighlight" colspan="3" valign="top" align="left" width="135">'+pn[y]+'</TD><TD class="leftNavTextHighlight"><IMG src="/images/spacer.gif" alt=" " width="8" height="9" hspace="0" vspace="0" border="0" ></TD></TR>';
        else if( expandedItems[ y ] ) text += '<TR><TD class="leftNavText"><IMG src="/images/spacer.gif" alt=" " width="27" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftNavText" colspan="3" valign="top" align="left" width="135"><A href="'+pu[y]+'" onclick="navChange('+y+'); return false;" title="Collapse &#39;'+pn[y]+'&#39; subtopics menu">'+pn[y]+'</A></TD><TD class="leftNavText"><IMG src="/images/spacer.gif" alt=" " width="8" height="9" hspace="0" vspace="0" border="0" ></TD></TR>';
        else text += '<TR><TD class="leftNavText"><IMG src="/images/spacer.gif" alt=" " width="27" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftNavText" colspan="3" valign="top" align="left" width="135"><A href="'+pu[y]+'" onclick="navChange('+y+'); return false;" title="Expand &#39;'+pn[y]+'&#39; subtopics menu">'+pn[y]+'</A></TD><TD class="leftNavText"><IMG src="/images/spacer.gif" alt=" " width="8" height="9" hspace="0" vspace="0" border="0" ></TD></TR>';
        if( alwaysTrue == y || expandedItems[ y ] ) {
            for( z = 0 ; z < sn[ y ].length ; z++ ) {
                if( alwaysTrue == y && alwaysTrueSubItem == z && selected ) text += '<TR><TD class="leftSubNavTextHighlight"><IMG src="/images/spacer.gif" alt=" " width="15" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavTextHighlight" valign="top" ><IMG src="/images/left_nav/ed_'+group+'_lj_bullet.gif" alt="&#39;'+sn[y][z]+'&#39; page is open" width="9" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavTextHighlight"><IMG src="/images/spacer.gif" alt=" " width="2" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavTextHighlight" valign="top" align="left" width="135">'+sn[y][z]+'</TD><TD class="leftSubNavTextHighlight"><IMG src="/images/spacer.gif" alt=" " width="9" height="9" hspace="0" vspace="0" border="0" ></TD></TR><TR><TD colspan="5"><IMG src="/images/spacer.gif" alt=" " width="1" height="1" hspace="0" vspace="0" border="0" ></TD></TR>';
                else if( alwaysTrue == y && alwaysTrueSubItem == z ) text += '<TR><TD class="leftSubNavTextCurrent"><IMG src="/images/spacer.gif" alt=" " width="15" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavTextCurrent" valign="top" ><IMG src="/images/left_nav/ed_'+group+'_lj_bullet.gif" alt=" " width="9" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavTextCurrent"><IMG src="/images/spacer.gif" alt=" " width="2" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavTextCurrent" valign="top" align="left" width="135"><A href="'+su[y][z]+'" title="Return to &#39;'+sn[y][z]+'&#39; Editor&#39s Picks">'+sn[y][z]+'</A></TD><TD class="leftSubNavTextCurrent"><IMG src="/images/spacer.gif" alt=" " width="9" height="9" hspace="0" vspace="0" border="0" ></TD></TR><TR><TD colspan="5"><IMG src="/images/spacer.gif" alt=" " width="1" height="1" hspace="0" vspace="0" border="0" ></TD></TR>';
                else text += '<TR><TD class="leftSubNavText"><IMG src="/images/spacer.gif" alt=" " width="15" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavText" valign="top" ><IMG src="/images/left_nav/ed_'+group+'_lj_hyphen.gif" alt=" " width="9" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavText"><IMG src="/images/spacer.gif" alt=" " width="2" height="9" hspace="0" vspace="0" border="0" ></TD><TD class="leftSubNavText" valign="top" align="left" width="135"><A href="'+su[y][z]+'" title="Go to &#39;'+sn[y][z]+'&#39; page">'+sn[y][z]+'</A></TD><TD class="leftSubNavText"><IMG src="/images/spacer.gif" alt=" " width="9" height="9" hspace="0" vspace="0" border="0" ></TD></TR><TR><TD colspan="5"><IMG src="/images/spacer.gif" alt=" " width="1" height="1" hspace="0" vspace="0" border="0" ></TD></TR>';
            }
        }
        text += '<TR><TD colspan="5"><IMG src="/images/spacer.gif" alt=" " width="1" height="7" hspace="0" vspace="0" border="0" ></TD></TR>';  
    }
    text += '</table>';
    myDOM('leftNav').innerHTML=text;
} 
