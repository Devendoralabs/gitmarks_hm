jQuery(window).load(function () {
	var height = 0;
	jQuery('#fidgetr_container_3 .fidgetr_item_crossfade').each(function() {
		if(jQuery(this).height() > height) {
			height = jQuery(this).height();
		}
	});
	jQuery('#fidgetr_container_3').animate( { height: height+"px" },500);

	//this block unhides and rehides the commentcontainers very rapidly to obtain the offset.
	//with this info we know if the sidebar is on the left or right
	var firstComment = jQuery("#fidgetr_container_3 .fidgetr_commentcontainer_crossfade:first");
	jQuery("#fidgetr_container_3 .fidgetr_item_crossfade").css("display","block");
	firstComment.css("display","block");
	var lrCheck = firstComment.offset();
	jQuery("#fidgetr_container_3 .fidgetr_item_crossfade").css("display","none");
	jQuery("#fidgetr_container_3 .fidgetr_item_crossfade:first").css("display","block");
	firstComment.css("display","none");
	if(lrCheck && lrCheck.left < 100) { // must be a left sidebar, flip the comment location
		var width = jQuery("#fidgetr_container_3 .fidgetr_item_crossfade img:first").width()+1;
		jQuery("#fidgetr_container_3 .fidgetr_commentcontainer_crossfade").css("left",width+"px");
	}
	//end sidebar check logic
	
	var containerWidth = jQuery('#fidgetr_container_3').width()
	jQuery('#fidgetr_container_3 .fidgetr_item_crossfade').each(function() {
		var diff = containerWidth - jQuery(this).width();
		if(diff > 0) {
			var left = Math.round(diff/2);
			jQuery(this).css('left',left);
		}
	})
})

jQuery(document).ready(function () {
	imageRotator_3(0);
	jQuery("#fidgetr_container_3 .fidgetr_item_crossfade").bind("mouseenter", function() {
		var comments = jQuery(this).find(".fidgetr_commentcontainer_crossfade");
		comments.toggle("fast");
		clearTimeout(timeOutId_3);
	});
	jQuery("#fidgetr_container_3 .fidgetr_item_crossfade").bind("mouseleave", function() {
		var comments = jQuery(this).find(".fidgetr_commentcontainer_crossfade");
		comments.toggle("fast");
		imageRotator_3(currentIndex_3);
	});
});

var timeOutId_3 = 0;
var fade_3 = jQuery('#fidgetr_container_3 .fidgetr_item_crossfade');
var currentIndex_3 = 0;
function imageRotator_3(index) {
	currentIndex_3 = index;
	if(!index || index > (parseInt(fade_3.length)-1)) {
		index = 0;
		var prevIndex = parseInt(fade_3.length)-1;
	} else {
		var prevIndex = parseInt(index)-1;
	}
	jQuery(fade_3[index]).fadeIn(2500);
	jQuery(fade_3[prevIndex]).fadeOut(2500);
	clearTimeout(timeOutId_3);
	timeOutId_3 = setTimeout('imageRotator_3('+(index+1)+')', 5000);
}

