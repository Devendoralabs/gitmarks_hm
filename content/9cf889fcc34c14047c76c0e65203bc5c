	if (document.all && xUA.indexOf('msie')!=-1) {
		var cl = xHeight('leftcontent');
		var cr = xHeight('rightcontent');
		var ca = xHeight('contentarea');
		if((ca<cl)||(ca<cr)){
			//alert('we are fixing the vertical length');
			var mism = xHeight('contentarea') - xHeight('content');
