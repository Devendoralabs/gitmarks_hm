<div id="fb-root"></div>
<script>
  window.fbAsyncInit = function() {
    FB.init({appId: '114486945280436', status: true, cookie: true, xfbml: true});
    FB.Event.subscribe('auth.sessionChange', function(response) {
      if (response.session) {
        fb_connect_setup_if_new_user();
        } else {
        fbc_top_button_logout();
      }
    });
    check_fbc_login();
    FB.Event.subscribe('auth.logout', function(response) {
      //we have been logged out, swap upper button if present
      //with connect button
      check_fbc_login();
    });
    //universal catcha-all for any el's named fbc_name
    if($('fbc_name')) {
      FB.api('/me', function(response) {
        $('fbc_name').update(response.name);
      });
    }
  };
  (function() {
    var e = document.createElement('script'); 
    e.async = true;
    e.src = document.location.protocol +
    '//connect.facebook.net/en_US/all.js';
    document.getElementById('fb-root').appendChild(e);
  }());
  var fb_connect_setup = function() {
      new Ajax.Request("/account/fbc_initial_setup", {onSuccess: function(t) { 
          Event.stopObserving('login_overlay', 'mousedown');
          }, onComplete: function(t) {
          if($('fbc_name')) {
            FB.api('/me', function(response) {
              $('fbc_name').update(response.name);
              $('hidden_fbc_name').setValue(response.name);
              $('user_firstname').focus();
            });
          }
      }});
      fbcl = new iLike.LoginDialog('fb_connect_initial'); 
      fbcl.onClickLogin(false);
  }
  var fb_connect_setup_if_new_user = function() {
    new Ajax.Request("/account/fbc_needs_setup", 
    {onSuccess: function(t) {
        if(parseInt(t.responseText) == 1) {
          fb_connect_setup();
          } else {
          check_fbc_login();
        }
    }});
  }
  var fbc_top_button_login = function(response) {
     if($('fbc_top_button').hasClassName("fbcloggedout")) { 
          $('fbc_top_button').update('<img src="http://graph.facebook.com/'+response.session.uid+'/picture?type=square" style="max-height: 25px; vertical-align: middle;" /> '+
          '<a href="#" onclick="new Ajax.Request(\'/account/fbc_login\', {asynchronous:true, evalScripts:true}); return false;">Login with Facebook</a> or '+
          ' <a href="/account/login?emaillogin=1">Login with Email</a>');
          $('signup_and_login_links').style.display = 'none';
          if($('boxed_fbc_login').style.display == '') {
            $('boxed_fbc_login').style.display = 'none';
          }
        }
    }
    var fbc_top_button_logout = function() {
      if(!$('fbc_top_button').hasClassName("fbcloggedout")) {
        $('fbc_top_button').update("<fb:login-button perms='publish_stream,offline_access,user_likes' size='small' >Connect</fb:login-button>");
        if($('signup_and_login_links')) {
          $('signup_and_login_links').style.display = '';
        }
        $('fbc_top_button').addClassName("fbcloggedout");
        FB.init({appId: '114486945280436', status: true, cookie: true, xfbml: true});
      }
  }
  var check_fbc_login = function() {
    if($('fbc_top_button')) {
      FB.getLoginStatus(function(response) {
        if (response.session) {
          //ok we are on the home page and logged in, so lets
          //hide the connect box and put their image there
          fbc_top_button_login(response);
          } else {
          fbc_top_button_logout();
        }
      });
    }
  }
</script>


