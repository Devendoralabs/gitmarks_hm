    <a style="cursor: pointer;" id="fb_connect_initial_close_link" onclick="FB.api({method: 'auth.revokeAuthorization'}, function(response) {}); return false;"><img src="http://s2.ilike.com/images/purchase/close.gif" alt="Close" /></a>
