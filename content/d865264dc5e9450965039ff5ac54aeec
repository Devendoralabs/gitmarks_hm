<script>
	$('one-tab').observe('click', function() {
		direction = "forward"
		scrollSection('one-pane', 'scroller', 'one-pane');
		stopScroller();
	});
	
	$('two-tab').observe('click', function() {
		direction = "forward"
		scrollSection('two-pane', 'scroller', 'one-pane');
		stopScroller();
	});
	
	$('three-tab').observe('click', function() {
		direction = "backward"
		scrollSection('three-pane', 'scroller', 'one-pane');
		stopScroller();
	});
	
	function scrollNext() {
		
		if($('one-tab').className == "active") {
			scrollSection('two-pane', 'scroller', 'one-pane');		
		}
		
		else if($('two-tab').className == "active") {
			if(direction == "forward") {
				scrollSection('three-pane', 'scroller', 'one-pane');
			} else {
				scrollSection('one-pane', 'scroller', 'one-pane');
				direction = "forward"
			}
		}
		
		else if($('three-tab').className == "active") {
			scrollSection('two-pane', 'scroller', 'one-pane');
			direction = "backward"
		}
	}
	
	//starts the auto scrolling
	direction = "forward"
	startScroller();
	
	function startScroller() {
		eventId = setInterval(scrollNext, 8000);
	}
	
	function stopScroller() {
		clearTimeout(eventId);
	}
	
	$('scroll_area').observe('mouseover', function() {
		stopScroller();
	});
	
	$('scroll_area').observe('mouseout', function() {
		startScroller();
	});

	
</script>


