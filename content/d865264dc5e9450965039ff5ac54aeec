<script>
	$('chat_button').observe('click', function() {
			window.open('https://www.websitealive8.com/508/rRouter.asp?groupid=508&websiteid=0&departmentid=0&dl='+escape(document.location.href),'','width=450,height=400');
	});
	
	$('signin_button').observe('click', function() {
		if($('sign_box').visible()) {
			$('sign_box').hide();
		}
		else {
			$('sign_box').show();
		}
	});
	
	// handles any click event on the page.
	document.observe('click', function(e) {
		var element = $(Event.element(e));
		// handle sign in box hiding
		if(!(element == $('signin_button'))) {
			if (!(element.childOf($('sign_box')))) {
				if(!(element == $('sign_box'))) {
					$('sign_box').hide();
				}
			}
		}	
	});
	
	document.observe("dom:loaded", function() {
	  $('sign_box').hide();
	});
	
	
</script>
