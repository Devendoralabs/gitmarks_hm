<script type="text/javascript">
	var knownWidgets=$w('producersPicks activeSeries selectedTopics mostPopularRecent mostPopularArchived channelList blob');
	$$('div.cnWidget').each(function(widgetDiv) {
		knownWidgets.each(function(widgetName) {
			if (widgetDiv.hasClassName(widgetName)) {
				//alert(widgetName);
				var params=widgetDiv.firstChild ?  widgetDiv.firstChild.data.strip() : '';
				new Ajax.Updater(widgetDiv,'/core/ajax/' + widgetName + '.php', {
					parameters: {params:params},
					evalScripts: true,
					onComplete: function() {
						widgetDiv.show();
					}
				});
			}
		});
	});
</script>

