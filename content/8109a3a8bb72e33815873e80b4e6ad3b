	var re = new RegExp("^.+\\@(\\[?)[a-zA-Z0-9\\-\\.]+\\.([a-zA-Z]{2,3}|[0-9]{1,3})(\\]?)$");
	var alpha = new RegExp("[a-zA-Z]");
	var digit = new RegExp("[0-9]");
	var truecount=0;

	function valid(){
		truecount=0;
		if(document.mlist.Name.value==""){
			alert("Please enter your name!");
			truecount=0;
			document.mlist.Name.focus();
			return false;
		}
		else {
			truecount++;
		}

  	mail=document.mlist.Email.value;
	  if(!re.test(mail)){
  		alert("Please enter your email");
  		truecount=0;
	  	document.mlist.Email.focus();
  		return false;
	  }
  	else {
  		truecount++;
	  }
		if(truecount==2){
			return true;
		}
		else {
			truecount=0;
		}
	}
</script> 

