<script type="text/javascript">
  function sendMessage(form) {
    var data = {
      to_id: '96929',
      subject: $j(form).find('input[name=subject]').val(),
      message: $j(form).find('textarea[name=message]').val(),
      authenticity_token: 'ssetdpbSdrqubg06aVQOmBBne12ssw4LSnSF07cyfAE='
    };
    
    $j.post('/account/send_message', data, function() {
      hideLightbox();
    })
  }
</script>

