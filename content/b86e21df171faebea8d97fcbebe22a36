<script type="text/javascript" charset="utf-8">
  onReady(function() {

    // Setup load more
    $j('.load-more a').click(function(e) {
      e.preventDefault();

      var $trigger = $j(this).parent('div');
      $trigger.addClass("active").find('img').show();
      $j('a', $trigger).hide();

      $j.get("/user/get_videos?collection_id=99978&amp;tag_id=&amp;user_id=96929",
             {page: $trigger.attr('data-next-page')},
             function(data) {
               $j('#user-feed').append(data);
               $j('img', $trigger).hide();
               $j('a', $trigger).show();
               $trigger.attr('data-next-page', parseInt($trigger.attr('data-next-page')) + 1);
             });
    });
  });
</script>



