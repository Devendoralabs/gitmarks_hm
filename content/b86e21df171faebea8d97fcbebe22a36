<script>
// ------------------------ NEW UNOBTRUSIVE EVENT TRACKING SYSTEM -----------

// Setup jQuery live handlers on forms, buttons and links that have .event-track class.

if (typeof(jQuery) != 'undefined') {
  jQuery(function() {
    // Live handlers for new event tracking
    jQuery('form.event-track-live').live('submit', function() {
      fire_event_tracking(this);
    });

    jQuery('button.event-track').live('click', function() {
      fire_event_tracking(this);
    });

    jQuery('a.event-track').live('click', function() {
      fire_event_tracking(this);
    });

    // Static handler for most forms
    jQuery('form.event-track').bind('submit', function() {
      fire_event_tracking(this);
    });

    // Fire events for on-display event-track-onload
    jQuery('.event-track-onload').each(function() {
      fire_event_tracking(this, true);
    });
  });
}

function fire_event_tracking(element, during_onload) {
  var action = element.getAttribute('data-et');
  var label = element.getAttribute("data-et-label");
  var category = element.getAttribute("data-et-category") ||
    et_category_from_path(window.location.pathname); 
  
  if (action) {
    if (during_onload) {
      action = action + '_display';
    }

    et_track(action, label, category);
  } else {
    if (typeof(console) == 'object') {
      console.log("Error: event tracking call on element with no data-et attribute: ", element);
    }
  }
}

function et_category_from_path(path, dont_reenter) {
  var category = 'Profile';
  if (path.match(/^\/watch\//)) {
    category = 'Video';
  } else if (path.match(/^\/search\//)) {
    category = 'Search';
  } else if (path.match(/^\/tag\//)) {
    category = 'Tag';
  } else if (path.match(/^\/account/)) {
    category = 'Account';
  } else if (path.match(/^\/topic\//) || path.match(/^\/group\//)) {
    category = 'Group';
  } else if (path.match(/^\/site\//)) {
    category = 'Site';
  } else if (path.match(/^\/savevideo\//)) {
    category = 'AddVideo';
    if (window.opener) {
      category = 'Bookmarklet';
    } else if (!dont_reenter) {
      // Lightbox mode, so use window.parents path
      category = et_category_from_path(window.parent.location.pathname, true);
    }
  } else if (path.match(/^\/(popular|music|funny|politics|sports|anime|tech)/)) {
    category = 'Category';
  }
  return category;
}

function et_default_label() {
  if (document.getElementById('tray')) {
    return 'member';
  } else {
    return 'guest';
  }
}

function et_track(action, label, category) {
  if (!category) {
    category = et_category_from_path(window.location.pathname);
  }
  if (typeof(console) == 'object') {
    console.log("Tracking " + category + "/" + action + " (" + label + ")");
  }
  if (typeof(pageTracker) == 'object') {
    label = label || et_default_label();
    pageTracker._trackEvent(category, action, label);
  }
}

</script>

  </body>
</html>

