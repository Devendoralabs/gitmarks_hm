                        googVar = googCookie.replace(/^[0-9]*\./, ""); // Cookie is a bunch of numbers and a period at the beginning, get rid of those
                    }
                    var pairList = googVar.split("_");

                    // Remove non-active vars
                    for (var i=0; i<pairList.length; i++) {
                        var pair = pairList[i];
                        if (pair.length < 1) {
                            continue;
                        }
                        var nameValPair = pair.split("-");
                        if (activeVars.indexOf("_" + nameValPair[0] + "_") < 0) {
                            changed = true;
                        } else {
                            // In cookie multiple times (ignore it)
                            if (newGoogVar.indexOf("_" + nameValPair[0] + "-") >= 0) { 
                                changed = true;

                            // In cookie, but not in user vals
                            } else if (userVars.indexOf("_" + nameValPair[0] + "-") < 0) {
                                newGoogVar += "_" + pair;

                            } else {
                                // Value Changed
                                if (userVars.indexOf("_" + pair + "_") < 0) {
                                    changed = true;
                                    var pattern = new RegExp("_"+ nameValPair[0] + "\\-.*_");
                                    var newPair = userVars.match(pattern)[0].replace("_", "");
                                    userVars = userVars.replace(pattern, "_");
                                    newGoogVar += "_" + newPair;

                                // Same in cookie and user vals
                                } else {
                                    newGoogVar += "_" + pair;
                                    var pattern = new RegExp("_"+pair.replace(/\-/,"\\-")+"_");
                                    userVars = userVars.replace(pattern, "_");
                                }
                            }
                        }
                    }
                    if (!newGoogVar.match(/^_/)) {
                        newGoogVar = "_"+newGoogVar;
                    } 
                    if (!newGoogVar.match(/_$/)) {
                        newGoogVar = newGoogVar+"_";
                    } 
                    if (userVars.length > 1) {
                        changed = true;
                        if (newGoogVar.length < 1) {
                            newGoogVar = userVars;
                        } else {
                            newGoogVar = newGoogVar.replace(/^_/, userVars);
                        }
                    }
                    if (changed) {
                        pageTracker._setVar(newGoogVar);
                    }
                }
            </script>
        <script type="text/javascript">
        document.write(unescape("%3Cscript src='" + (document.location.protocol == "https:" ? "https://sb" : "http://b")
        + ".scorecardresearch.com/beacon.js' %3E%3C/script%3E"));
        </script>
