pg" onblur="try {parent.deselectBloggerImageGracefully();} catch(e) {}" onmouseout="return nd();" onmouseover="return overlib('&lt;img src=http://www.studiolum.com/wang/cards/dachau/06k.jpg&gt;', BGCOLOR, '#7777aa', ABOVE, MOUSEOFF, WIDTH, 520, HEIGHT, 390)" shape="rect" target="new"></area><area coords="0, 92, 90, 182" href="http://www.studiolum.com/wang/cards/dachau/07.jpg" onblur="try {parent.deselectBloggerImageGracefully();} catch(e) {}" onmouseout="return nd();" onmouseover="return overlib('&lt;img src=http://www.studiolum.com/wang/cards/dachau/07k.jpg&gt;', BGCOLOR, '#7777aa', ABOVE, RIGHT, MOUSEOFF, WIDTH, 520, HEIGHT, 390)" shape="rect" target="new"></area><area coords="92, 92, 182, 182" href="http://www.studiolum.com/wang/cards/dachau/08.jpg" onblur="try {parent.deselectBloggerImageGracefully();} catch(e) {}" onmouseout="return nd();" onmouseover="return overlib('&lt;img src=http://www.studiolum.com/wang/cards/dachau/08k.jpg&gt;', BGCOLOR, '#7777aa', ABOVE, RIGHT, MOUSEOFF, WIDTH, 520, HEIGHT, 390)" shape="rect" target="new"></area><area coords="184, 92, 273, 182" href="http://www.studiolum.com/wang/cards/dachau/09.jpg" onblur="try {parent.deselectBloggerImageGracefully();} catch(e) {}" onmouseout="return nd();" onmouseover="return overlib('&lt;img src=http://www.studiolum.com/wang/cards/dachau/09k.jpg&gt;', BGCOLOR, '#7777aa', ABOVE, RIGHT, MOUSEOFF, WIDTH, 520, HEIGHT, 390)" shape="rect" target="new"></area><area coords="276, 92, 365, 182" href="http://www.studiolum.com/wang/cards/dachau/10.jpg" onblur="try {parent.deselectBloggerImageGracefully();} catch(e) {}" onmouseout="return nd();" onmouseover="return overlib('&lt;img src=http://www.studiolum.com/wang/cards/dachau/10k.jpg&gt;', BGCOLOR, '#7777aa', ABOVE, MOUSEOFF, WIDTH, 520, HEIGHT, 390)" shape="rect" target="new"></area><area coords="367, 92, 457, 182" href="http://www.studiolum.com/wang/cards/dachau/11.jpg" onblur="try {parent.deselectBloggerImageGracefully();} catch(e) {}" onmouseout="return nd