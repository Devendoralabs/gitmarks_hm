<script type="text/javascript">
	function callLowPriorityCallbacks() {
    	if (lowPriorityCallbacks !== undefined && lowPriorityCallbacks != null) {
    		var index = 0;
    		for(index = 0; index < lowPriorityCallbacks.length; index = index + 1) {
    			try {
    				lowPriorityCallbacks[index]();
    			} catch(err) {
    			}
    		}
    	}
    }	

	dojo.addOnLoad(function loadPageElements() {

		var callback = null;
		if (highPriorityCallbacks !== undefined && highPriorityCallbacks != null) {
			var index = 0;
			for(index = 0; index < highPriorityCallbacks.length; index = index + 1) {
				try {
					highPriorityCallbacks[index]();
				} catch(err) {
				}
			}
		}
		timeoutID = window.setTimeout(callLowPriorityCallbacks, 200);
	});
	
</script>

