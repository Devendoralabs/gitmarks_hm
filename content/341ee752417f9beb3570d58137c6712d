          + '<tr style="margin-top:5px;"><td>Username </td><td><input id="username_hb" type="text" style="width:100px;" name="u"/></td></tr>'
          + '<tr><td>Password </td><td><input id="password_hb" type="password" style="width:100px;" name="p"/></td></tr>'
          + '<tr><td colspan="2">Remember? <input id="RememberME_hb" type="checkbox" checked="checked" name="RememberME" style="border:none;" /><input class="button" type="submit" value="go" name="Submit"/></td></tr>'
          + '<tr><td colspan="2"><a style="margin-left:1px;font-size:11px;font-weight:normal;font-style:normal;margin-right:2px;" href="/account/forgot?' + loginContinueParams + '">Forgot username or password?</a></td></tr>'
          + '</table></form>'
}

