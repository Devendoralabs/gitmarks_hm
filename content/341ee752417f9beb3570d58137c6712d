    InstructJS.load(["prototype"], function () {
      var id = "EBEXSILF6B7SY7Z";
      var keywords = ["s60","pys60","python","nokia","osx","mac","bluetooth"];
      var categories = ["technology electronics"];   

      //

      // Set up View All link
      // We do this in script because we need to do the output of keywords
      // as <c:out escapeXml="false"/> calls, which cannot be placed inside
      // another element's attribute (<a href>.)
      var viewall =  $("viewallLink_id");
      var viewAllParams = {
        q: (keywords.length > 0 || categories.length > 0 ?
            keywords.join(" ") + " " + categories.join(" ") : "*:*"),
        handler: "standard",
        relatedToId: id,
        sort: "none"
      };
      viewall.down("a").href = ""
        + "/tag/type-id/?"
        + Object.toQueryString(viewAllParams);
      viewall.show();

      //
    });
