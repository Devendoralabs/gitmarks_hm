<script>
function PageSwitch(t){if (t){
document.getElementById("FirstPage").style.display='none';
document.getElementById("SecondPage").style.display='block';document.getElementById("num").innerHTML='2';}
else {document.getElementById("FirstPage").style.display='block';document.getElementById("num").innerHTML='1';
document.getElementById("SecondPage").style.display='none';}}
</script>
