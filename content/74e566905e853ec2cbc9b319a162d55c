			window.onload = onLoad;

			function onLoad()
			{
				if (window.onLoadPage) {
					onLoadPage();
				}
			}

			var ready_list = new Array();

			function addReady(func)
			{
				ready_list.push(func);
			}

			$(document).ready(function(){
				for (var i=0; i < ready_list.length; i++) {
					ready_list[i]();
				}
			});
		
