<form id="search-form" action="search" method="get">
	
	












<script type="text/javascript">

	addReady(function(){
		$("form#search-form").submit(function(){
			if ($("form#search-form input[name=search_string]").val() == 'optional') {
				$("form#search-form input[name=search_string]").val('');
			}

			if ($("form#search-form input[name=location_string]").val() == 'Enter location name') {
				$("form#search-form input[name=location_string]").val('');
			}
		});

		$("a#search-block-more-options-link").toggle(function(){
			$("tr#search-block-more-options").show();
			$(this).html('Less options...');
			return false;
		}, function(){
			$("tr#search-block-more-options").hide();
			$(this).html('More options...');
			return false;
		});

		$("a#search-block-show-job-functions-link").toggle(function(){
			$("div.search-block-job-function-extended").show();
			$(this).html('Show less...');
			return false;
		}, function(){
			$("div.search-block-job-function-extended").hide();
			$(this).html('Show more...');
			return false;
		});

		$("a#search-block-show-job-groups-link").toggle(function(){
			$("div.search-block-job-group-extended").show();
			$(this).html('Show less...');
			return false;
		}, function(){
			$("div.search-block-job-group-extended").hide();
			$(this).html('Show more...');
			return false;
		});

		$("a#search-block-show-locations-link").toggle(function(){
			$("div.search-block-location-extended").show();
			$(this).html('Show less...');
			return false;
		}, function(){
			$("div.search-block-location-extended").hide();
			$(this).html('Show more...');
			return false;
		});

		$("a#search-block-show-company-names-link").toggle(function(){
			$("div.search-block-company-name-extended").show();
			$(this).html('Show less...');
			return false;
		}, function(){
			$("div.search-block-company-name-extended").hide();
			$(this).html('Show more...');
			return false;
		});

		$("input[name=search_string]").focus(function(){
			updateSearchString('focus');
		});

		$("input[name=search_string]").blur(function(){
			updateSearchString('blur');
		});

		$("input[name=location_string]").focus(function(){
			updateLocationString('focus');
		});

		$("input[name=location_string]").blur(function(){
			updateLocationString('blur');
		});

		
				

		updateSearchString('init');
		updateLocationString('init');
	});

	function updateSearchString(action)
	{
		if (action == 'init' || action == 'blur') {
			if ($("form#search-form input[name=search_string]").val() == '') {
				$("form#search-form input[name=search_string]").addClass('input-info');
				$("form#search-form input[name=search_string]").val('optional');
			}
		} else if (action == 'focus') {
			if ($("form#search-form input[name=search_string]").hasClass('input-info')) {
				$("form#search-form input[name=search_string]").val('');
				$("form#search-form input[name=search_string]").removeClass('input-info');
			}
		}
	}

	function updateLocationString(action)
	{
		if (action == 'init' || action == 'blur') {
			if ($("form#search-form input[name=location_string]").val() == '') {
				$("form#search-form input[name=location_string]").addClass('input-info');
				$("form#search-form input[name=location_string]").val('Enter location name');
			}
		} else if (action == 'focus') {
			if ($("form#search-form input[name=location_string]").hasClass('input-info')) {
				$("form#search-form input[name=location_string]").val('');
				$("form#search-form input[name=location_string]").removeClass('input-info');
			}
		}
	}

</script>





	

