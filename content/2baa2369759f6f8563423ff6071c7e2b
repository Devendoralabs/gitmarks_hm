    <script type="text/javascript" language="javascript">
    __vid_initialized=false;
    __vid_shown=false;
    function showVideo() {
        if( __vid_initialized && !__vid_shown ) { 
            $("div#player0").show( "slow" ).css("margin","10px");
            __vid_shown = true;
        } else if( __vid_initialized ) { 
            $("div#player0").hide( "slow" ).css("margin","0px");
            __vid_shown = false;
        } else {
	        document.getElementById( "player0" ).innerHTML = "";
	        // video
	        var s1 = new SWFObject( "/static/swf/mediaplayer.swf","mpl",740,520,"9.0.115" );
	        s1.addVariable( "file", 'http://poseidon.uuorld.com/UUorld_Overview_large_nf.mp4');
	        s1.addVariable( "image", 'http://poseidon.uuorld.com/uu_overview_vid_thumb.jpg' );
	        s1.addVariable( "autostart", "true" );
	        s1.addParam('allowscriptaccess','always');
	        s1.addParam('allowfullscreen','true');
	        s1.addVariable( "displayheight", 520 );
	        s1.addVariable( "searchbar", "false" );
	        s1.addVariable( "callback", "analytics" );
	        var flashOK = s1.write( "player0" );
	        // fallback to FLV
	        if( !flashOK ) {
	            s1 = new SWFObject( "/static/swf/mediaplayer.swf","mpl",740,520,"6" );
	            s1.addVariable( "file", 'http://poseidon.uuorld.com/UUorld_Overview_large_n.flv' );
	            s1.addVariable( "image",  'http://poseidon.uuorld.com/uu_overview_vid_thumb.jpg');
	            s1.addVariable( "autostart", "true" );
	            s1.addVariable( "displayheight", 520);
	            s1.addVariable( "searchbar", "false" );
	            s1.addVariable( "callback", "analytics" );
	            flashOK = s1.write( "player0" );
	        }
	        $("div#player0").show( "slow" ).css("margin","10px");
	        __vid_shown = true;
	        __vid_initialized = true;
	    }
    }
    </script>

