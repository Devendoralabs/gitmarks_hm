    <script type="text/javascript" charset="utf-8">
    // <![CDATA[
      var search_options = {
        minChars: 2,
        cacheLength: 50,
        autoFill: false,
        scroll: false,
        cacheLength: 0,
        selectFirst: false,
        formatItem: function(data, i, total) {
          if( data[1] > 10 && i == total ) {
            return "<span>and " + data[1] + " more...</span>";
          } else {
            return data[4] + "<span>" + data[0].substring(0, 45) + "</span>";
          }
        }
      }

      var search_url = "/product/autocomplete_suggestion";

      var result_callback = function(event, item, formatted) {
        var searchUrl = item[2];
        var resultIndex = item[3];
        var productUrl = item[5];
        if( resultIndex == 10 ) {
          $j(this).val("");
          window.location.href = searchUrl;
        } else {
          window.location.href = productUrl;
        }
      }

      $j(document).ready(function () {
        $j(".offpage_link").click(function(event) {
          event.preventDefault();
          window.open($j(this).attr("href"));
        })
        
        $j("#main_search_form").autocomplete( search_url, search_options ).result( result_callback );
        
        var currentEmail = readCookie('current_user_email');
        var masqueradeEmail = readCookie('masquerade_user_email');
        if (currentEmail && masqueradeEmail) {
          html = "<p>You are masquerading as user " + currentEmail + "</p><p>You are actually " + masqueradeEmail + "</p>"
          html += '<a href=\"http://www.gazelle.com/masquerades/1\" onclick=\"var f = document.createElement(\'form\'); f.style.display = \'none\'; this.parentNode.appendChild(f); f.method = \'POST\'; f.action = this.href;var m = document.createElement(\'input\'); m.setAttribute(\'type\', \'hidden\'); m.setAttribute(\'name\', \'_method\'); m.setAttribute(\'value\', \'delete\'); f.appendChild(m);var s = document.createElement(\'input\'); s.setAttribute(\'type\', \'hidden\'); s.setAttribute(\'name\', \'authenticity_token\'); s.setAttribute(\'value\', \'wh50yjp1quXI4jsaDm1yELkeTKMbA8IiJocnsUwZqLQ=\'); f.appendChild(s);f.submit();return false;\">Click Here to Stop Masquerading<\/a>'
          $j("#masquerade_notice").html(html).show();
        }
        
      });
    // ]]>
    </script>
    
    
      <script type="text/javascript" charset="utf-8">
      // <![CDATA[
        // ping the server with the referrer hash, since the cached page won't trigger it
        // REFERRAL_REMOVAL
        // if (window.location.search.indexOf('?ref=') != -1) {
        //   $j.get('/main/set_referrer' + window.location.search)
        // }
                function cart_status(amount) {
          var item_count_pluralized = readCookie('box_phrase').replace(/\+/," ");
          var box_total = readCookie('box_total');
          var packing_slip_number = readCookie('box_number');
          var cart_status = '<a href="/boxes/';
          cart_status += packing_slip_number;
          cart_status += '"><img alt="Gazelle_box" src="http://classic2-www.gazelle.com/images/gazelle/shared/gazelle_box.png?1291293179" /></a>\n';
          cart_status += "<p>My box has ";
          cart_status += item_count_pluralized;

          if( parseInt(box_total) > 0 ) {
            cart_status += " worth $";
            cart_status += amount;
          } else {
            cart_status += " for recycling";
          }

          cart_status += "</p>";
          return cart_status;
        }

        $j(document).ready(function () {
          var currentEmail = readCookie('current_user_email');
          var account_tab = "";
          if(in_gazelle_for_good_funnel != true) {
            if(currentEmail) {
              account_tab += '<a href="/account/transactions">My Account</a> |';
            } else {
              account_tab += '<a href="/account/login" rel="nofollow">Sign In</a> |';
              account_tab += '<a href="/account/new" id="new_account_link" rel="nofollow">Create Account</a> | ';
            }
            account_tab += '<a href="/help">Help</a>';
            if(currentEmail) {
              account_tab += '| <a href="/account/logout">Log Out</a>';
            }
          }

          $j("#account_tab_content").html(account_tab);

          if (currentEmail) {
            $j("#login_status").html("Logged in as " + currentEmail);
          }

            var box_count = readCookie('box_size');
            var amount = readCookie('box_total');
            if ((box_count!=null && box_count!=0) && amount!=null) {
              $j("#cart_status").html(cart_status(amount));
            }
        });
        // ]]>

      </script>
    

  
