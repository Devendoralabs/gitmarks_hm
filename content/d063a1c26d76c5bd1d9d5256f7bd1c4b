        <div>
            
            


<h3>Sign In</h3>
<form name="openid_signin" action="/accounts/signin/" method="post"><div style='display:none'><input type='hidden' name='csrfmiddlewaretoken' value='1c2053aa52b6316dc137ca4d763bc540' /></div>
<input type="text" value="http://" name="openid_url" id="login-input" />
<input type="submit" value="Continue" name="submit" id="login-submit" />
<ul id="login-providers"></ul>
</form>

<script type="text/javascript">
var providers = new Array();
providers['openid'] = 'http://'
providers['google'] = 'https://www.google.com/accounts/o8/id'
providers['yahoo'] = 'http://yahoo.com'
providers['launchpad'] = 'http://login.launchpad.net'

$(document).ready(function(){
    for(p in providers){
        btn = $('<li id="oidb-'+p+'">'+p+'</li>');
        btn.appendTo('#login-providers');
        btn.css('background-image', 'url(http://files.fosswire.com/site/images/openid/'+p+'.png)');
    }
    $('#oidb-openid').css('background-position', '0px 0px');
    // events
    $('#login-providers li').live('click', function(){
        $('#login-providers li').css('background-position', '16px 0px');
        $(this).css('background-position', '0px 0px');
        $('#login-input').val(providers[$(this).text()]);
        if($(this).text() == 'openid'){
            $('#login-input').css('color', '#000').focus()
        }else{
            $('#login-input').css('color', '#444');
            $('#login-submit').focus()
        }
    });
    $('#login-input').focus(function(){
        $('#login-providers li').css('background-position', '16px 0px');
        $('#oidb-openid').css('background-position', '0px 0px');
        $(this).css('color', '#000');
    });
});
</script>



            
