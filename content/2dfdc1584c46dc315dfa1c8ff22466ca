	<script language="JavaScript" type="text/javascript">
		/* emuliert max-width fuer MSIE bei zu grossen Bildern */
		function ImageMaxWidth()
		{
			var imgmaxwidth = 0.68; // maximaler Anteil #content-Breite 

			// Breite von #content in Pixel
			var contentwidth = document.getElementById("content").offsetWidth;

			// alle Bilder
			var images = document.getElementsByTagName("img");

			// teste Bildbreite aller Bilder und verkleinere, falls noetig
			for (var i=0; i < images.length; i++)
			{
				// muss longDesc als Speicher fuer eigentliche Breite verwenden
				if (images[i].longDesc < images[i].width) images[i].longDesc = images[i].width;

				if (images[i].longDesc > contentwidth*imgmaxwidth) images[i].width = contentwidth*imgmaxwidth;
			}
		}

		// Initialisierung muss direkt unter <body> erfolgen, da MSIE sonst nicht auf Objekte/Elemente zugreifen kann
		var ThisIsAFuckingMSIE = true;

	</script>

  <![endif]-->
