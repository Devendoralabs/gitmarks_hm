          + '<tr><td colspan="2"><a style="margin-left:1px;font-size:11px;font-weight:normal;font-style:normal;margin-right:2px;" href="/account/forgot?' + loginContinueParams + '">Forgot username or password?</a></td></tr>'
          + '</table></form>'
}

