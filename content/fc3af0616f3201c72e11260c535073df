var showfailed = 0;


function Trim(sInString) {
  sInString = sInString.replace( /^\s+/g, "" );// strip leading
  return sInString.replace( /\s+$/g, "" );// strip trailing
}

function e(s) {
	r1 = new RegExp("^[A-Za-z0-9]([A-Za-z0-9_-]|((\\.|\\+)[A-Za-z0-9]))+@[A-Za-z0-9](([A-Za-z0-9]|(-[A-Za-z0-9]))+)(\\.([A-Za-z]{2,20}))?(\\.([A-Za-z]{2,6}))(\\.([A-Za-z]{2,6}))?$") 
	if ((!r1.test(s))) {
		return false;
	}
	return true;
	
}



function checkSignUpForm () {
	
	//Email validation
	if (!e(document.SignUp.email.value) || !e(document.SignUp.r_email.value))
	{
		alert("Please enter and confirm your email address. It needs a user@yahoo.com format.");
		document.SignUp.email.focus();
		return false;
	}
	
	//Email validation
	if (document.SignUp.r_email.value!=document.SignUp.email.value)
	{
		alert("Your email address didn't match.");
		document.SignUp.email.focus();
		return false;
	}
	
	//first_name
	if (document.SignUp.first_name.value == null || document.SignUp.first_name.value.length == 0) 
	{
		alert("Please enter your first name.");
		document.SignUp.first_name.focus();
		return false;
	}
	
	//Passcode
	var strng = document.SignUp.passcode.value;
	if (strng == "") {
   alert("You didn't enter a password.");
   return false;
}

    var illegalChars = /[\W_]/; // allow only letters and numbers
    
    if ((strng.length < 4) || (strng.length > 12)) {
       alert("The password is the wrong length. Min 4 letter or number. Max 12,");
	    return false;
    }
    else if (illegalChars.test(strng)) {
      alert("The password contains illegal characters. Only letters and numbers are supported.");
	  return false;
    } 
	
	//passcode
	if (document.SignUp.r_passcode.value!=document.SignUp.passcode.value) 
	{
		alert("The passwords didn't match.");
		document.SignUp.passcode.focus();
		return false;
	}
	
}

function checkSignInForm () {
	
	//Email validation
	if (!e(document.SignIn.email.value))
	{
		alert("Please enter your email address. It needs a user@yahoo.com format.");
		document.SignIn.email.focus();
		return false;
	}
	
	
	//Passcode
	var strng = document.SignIn.passcode.value;
	if (strng == "") {
   alert("You didn't enter a password.");
   return false;
}

    var illegalChars = /[\W_]/; // allow only letters and numbers
    
    if ((strng.length < 4) || (strng.length > 12)) {
       alert("The password is the wrong length. Min 4 letter or number. Max 12,");
	    return false;
    }
    else if (illegalChars.test(strng)) {
      alert("The password contains illegal characters. Only letters and numbers are supported.");
	  return false;
    }
	
}

function checkPasswordRemindForm () {
	
	//Email validation
	if (!e(document.PasswordRemind.email.value))
	{
		alert("Please enter your email address.");
		document.PasswordRemind.email.focus();
		return false;
	}
	
}
			
