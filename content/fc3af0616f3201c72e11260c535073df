var form2show='none';
var domove='0';
var doclose='0';

YAHOO.namespace('example.dom');

YAHOO.example.dom.init = function() {

   var move = function(e) {	 
	 	
		if (doclose==1)
		{
			form2show='none';
			doclose=0;
		}
		
			
    	switch(form2show)
		{
			case 'editcell':				
				document.getElementById('editcell').style.display='block';
			break;
			
			case 'none':
				document.getElementById('editcell').style.display='none';
			
			break;
		}

		if (domove==1)
		{
			var xy = [YAHOO.util.Event.getPageX(e), YAHOO.util.Event.getPageY(e)];
			YAHOO.util.Dom.setXY(form2show, xy);
			domove=0;
			form2show='none';
		}
		else
		{
			form2show='none';
		}
   
   }
   
   YAHOO.util.Event.addListener(document, 'click', move);
}

YAHOO.util.Event.addListener(window, 'load', YAHOO.example.dom.init);
		
		function actEditCellForm() {
			
			var myDate=new Date();
			
			document.getElementById(document.editcell.cell_id.value + '_note').innerHTML = document.editcell.notes.value;
			document.getElementById(document.editcell.cell_id.value + '_preview').innerHTML = document.editcell.notes.value.substring(0,38);
			if (document.editcell.notes.value.length > 38)
			{
				document.getElementById(document.editcell.cell_id.value + '_preview').innerHTML = document.getElementById(document.editcell.cell_id.value + '_preview').innerHTML + '...';
			}
			cell=document.getElementById(document.editcell.cell_id.value);
			displaytext = document.getElementById(document.editcell.cell_id.value + '_note').innerHTML;
			
			if (document.getElementById(document.editcell.cell_id.value + '_note').innerHTML.length != 0 
			&& (cell.className == 'nonday_note' || cell.className == 'day_note')) {
			tmp=1;
			
			
			} else if (document.getElementById(document.editcell.cell_id.value + '_note').innerHTML.length != 0 && cell.className == 'nonday') {
			cell.className='nonday_note';
			//add flag
			
				
			}else if (document.getElementById(document.editcell.cell_id.value + '_note').innerHTML.length != 0 && cell.className == 'day') {
			cell.className='day_note';
			//add flag
		
			
						
			} else if (cell.className == 'nonday_note') {
			cell.className='nonday';
			
			
			
			//remove flag
			} else if (cell.className == 'day_note') {
			cell.className='day';
			
			
			
			//remove flag
			}
			
			if (document.getElementById(document.editcell.cell_id.value + '_note').innerHTML.length != 0 
			&& (cell.className == 'nonday_log_note' || cell.className == 'day_log_note')) {
			tmp=1;
			
			
			} else if (document.getElementById(document.editcell.cell_id.value + '_note').innerHTML.length != 0 && cell.className == 'nonday_log') {
			cell.className='nonday_log_note';
			//add flag
			
				
			}else if (document.getElementById(document.editcell.cell_id.value + '_note').innerHTML.length != 0 && cell.className == 'day_log') {
			cell.className='day_log_note';
			//add flag
			
			
						
			} else if (cell.className == 'nonday_log_note') {
			cell.className='nonday_log';
			
			
			//remove flag
			} else if (cell.className == 'day_log_note') {
			cell.className='day_log';
					
			
			//remove flag
			}
			
			
			
			doclose=1;
			form2show='none';
			document.getElementById('editcell').style.display='none';
			return false;
		}
		
		
		function setEditCellValues (cellid) {
				
				if (cellid.indexOf('preview') > -1)
				{
					end = cellid.length - 8;
					cellid = cellid.substring(0,end);
				}
				
				mycell = document.getElementById(cellid);
				
				document.editcell.cell_id.value = cellid;
				
				for( var x = 0; x < mycell.attributes.length; x++ ) {
				if( mycell.attributes[x].nodeName.toLowerCase() == 'goal_id' ) {
					document.editcell.goal_id.value = mycell.attributes[x].nodeValue;
				}
				if( mycell.attributes[x].nodeName.toLowerCase() == 'notedate' ) {
					document.editcell.celldate.value = mycell.attributes[x].nodeValue;
					document.getElementById("editcell_date").innerHTML = mycell.attributes[x].nodeValue;
				}
				}
				

				mygoal = document.getElementById(document.editcell.goal_id.value);
				document.editcell.notes.value = document.getElementById(cellid + '_note').innerHTML;

				for( var x = 0; x < mygoal.attributes.length; x++ ) {
					if( mygoal.attributes[x].nodeName.toLowerCase() == 'goal_name' ) {
						document.getElementById("editcell_goal").innerHTML = mygoal.attributes[x].nodeValue;
					}
				}
				
				
				
			}

		

	
	  // "move" event handler for the context menu

            function onMoveContextMenu() {

                var oNode = this.contextEventTarget;
                var bDisabled = (oNode.tagName == "UL");
                var i = this.getItemGroups()[0].length - 1;

                do {
                
                    this.getItem(i).cfg.setProperty("disabled", bDisabled);

                }
                while(i--);

            }
        

            // "click" event handler for each item in the context menu
            
            function onContextMenuItemClick(p_sType, p_aArguments) {
				
                var oNode = this.parent.contextEventTarget,
                    oLI;
				thisid = 0;
				for( var x = 0; x < oNode.attributes.length; x++ ) {
					if( oNode.attributes[x].nodeName.toLowerCase() == 'id' ) {
						thisid = oNode.attributes[x].nodeValue;
					}
				}

				this.parent.hide();
				
				if (thisid != 0) {
					setEditCellValues(thisid);
					document.getElementById('editcell').style.display='block';
					var xy = YAHOO.util.Dom.getXY(thisid);
					YAHOO.util.Dom.setXY('editcell', xy);
					document.editcell.notes.focus();
					
				}
				
				
                
            
            }


            // "load" event handler for the "window" object
			 
			 menuids = new Array("ccc1","ccc2","ccc3","ccc4","ccc5","ccc6","ccc7")  
            	
			
				
			    function initTool() {
			        for( var i=0; i<menuids.length; i++ ) {
						el = 'tt' + i;
						displaytext = document.getElementById(menuids[i] + '_note').innerHTML;
						var tp = new YAHOO.widget.Tooltip( el, { context:menuids[i], text: displaytext, showdelay: 500, autodismissdelay: 15000, preventoverlap: 0, hidedelay:350});
			        }
			    }
			    YAHOO.util.Event.addListener(window, "load", initTool);
				
			function onWindowLoad(p_oEvent) {

                // Create the context menu

                var oContextMenu = new YAHOO.widget.ContextMenu(
                                        "contextmenu", 
                                        { trigger: menuids } 
                                    );

                var aMainMenuItems = [
                        "Edit Log"
                    ];
                    
                var nMainMenuItems = aMainMenuItems.length;
                
                var oMenuItem;


                // Add items to the main menu

                for(var i=0; i<nMainMenuItems; i++) {

                    oMenuItem = 
                        new YAHOO.widget.ContextMenuItem(aMainMenuItems[i]);

                    /*
                        Add a "click" event handler to each 
                        ContextMenuItem instance
                    */

                    oMenuItem.clickEvent.subscribe(onContextMenuItemClick);

                    oContextMenu.addItem(oMenuItem);

                }


                //  Add a "move" event handler to the context menu 
                    
                oContextMenu.moveEvent.subscribe(
                    onMoveContextMenu, 
                    oContextMenu, 
                    this
                );
                

                // Render the context menu and color picker submenu

                oContextMenu.render(document.body);
                
            }


            // Assign a "load" event handler to the window

            YAHOO.util.Event.addListener(window, "load", onWindowLoad);
			
	
	function setRowTotal(id,flag,jsdate)
	{
		if (flag=='minus')
		{
			newtotal = parseInt(document.getElementById('total_' + id).innerHTML) - 1;
			var update_id='total_' + id;
			setTimeout("document.getElementById('"+update_id+"').innerHTML = "+newtotal,50);
		} else if (flag=='plus')
		{
			newtotal = parseInt(document.getElementById('total_' + id).innerHTML) + 1;
			var update_id='total_' + id;
			setTimeout("document.getElementById('"+update_id+"').innerHTML = "+newtotal,50);
		}
	}
	

