	var dropobject = '';
	var myDate=new Date();
	function doplus (cell,column,goal_id,updatedate,weight,jsdate) {
		
		if (doclick == 1)
		{
			cell.innerHTML = cell.innerHTML + '<img src="images/plus.gif" alt="" align="center" width="16" height="16" border="0" onclick="doclick=0;dropobject=this;">';
			document.getElementById(column + '_total').innerHTML = parseInt(document.getElementById(column + '_total').innerHTML) + weight;	
			
			var tmp=1;
			//tmp = setRowTotal(goal_id,'plus',jsdate);
		}
		if (doclick == 0)
		{
			document.getElementById(column + '_total').innerHTML = parseInt(document.getElementById(column + '_total').innerHTML) - weight;
			
			dropobject.parentNode.removeChild(dropobject);
			
			doclick = 1;
			//tmp = setRowTotal(goal_id,'minus',jsdate);
		}
	}
	function dominus (cell,column,goal_id,updatedate,weight,jsdate) {
		
		if (doclick == 1)
		{
			cell.innerHTML = cell.innerHTML + '<img src="images/minus.gif" alt="" align="center" width="16" height="16" border="0" onclick="doclick=0;dropobject=this;">';
			document.getElementById(column + '_total').innerHTML = parseInt(document.getElementById(column + '_total').innerHTML) - weight;
			
			

			var tmp=1;
			//tmp = setRowTotal(goal_id,'plus',jsdate);
		}
		if (doclick == 0)
		{
			document.getElementById(column + '_total').innerHTML = parseInt(document.getElementById(column + '_total').innerHTML) + weight;
			
			//tmp = setRowTotal(goal_id,'minus');
			dropobject.parentNode.removeChild(dropobject);
			doclick = 1;
			//tmp = setRowTotal(goal_id,'minus',jsdate);
			
		}
	}

