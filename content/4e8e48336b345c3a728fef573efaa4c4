    <script type="text/javascript">
      if ( ! get_cookie ( "CM-Ref" ) )
      {
       var ref = document.referrer;
       var landingPage = location.href;

       var current_date = new Date;
       var cookie_year = current_date.getFullYear ( ) + 3;
       var cookie_month = current_date.getMonth ( );
       var cookie_day = current_date.getDate ( );
       set_cookie ( "CM-Ref", ref + "|" + current_date.toGMTString() + "|"
    + landingPage , cookie_year, cookie_month, cookie_day,"/",".campaignmonitor.com");
      }


      function get_cookie ( cookie_name )
      {
       var results = document.cookie.match ( cookie_name + '=(.*?)(;|$)' );

       if ( results )
         return ( unescape ( results[1] ) );
       else
         return null;
      }

      function set_cookie ( name, value, exp_y, exp_m, exp_d, path, domain, secure )
      {
       var cookie_string = name + "=" + escape ( value );

       if ( exp_y )
       {
         var expires = new Date ( exp_y, exp_m, exp_d );
         cookie_string += "; expires=" + expires.toGMTString();
       }

       if ( path )
             cookie_string += "; path=" + escape ( path );

       if ( domain )
             cookie_string += "; domain=" + escape ( domain );

       if ( secure )
             cookie_string += "; secure";

       document.cookie = cookie_string;
      }
    </script>
    
