var dnow = new Date(); var t = Math.floor(((dnow.getTime()/1000)-time_t)/60); document.write('<span class="wp_elapsed">'); if (t<0) { document.write(''); } else if (t<60) { document.write(t + '&nbsp;minutes&nbsp;ago'); } else if (t<(60*24)) { t = Math.floor(t/60); document.write(t + '&nbsp;hours&nbsp;ago'); } else if (t<(60*24*2)) { document.write(1 + '&nbsp;day&nbsp;ago'); } else { t = Math.floor(t/(60*24)); document.write(t + '&nbsp;days&nbsp;ago'); } document.write('</span>'); }
