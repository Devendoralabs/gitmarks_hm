      // NOTE: name and value combined cannot exceed 64 bytes
      _setCustomVar: function(index, name, value, opt_scope) {
        //if(typeof(console) != 'undefined')try{console.log('pushing values on _gaq: %o %o %o', index, name, value, opt_scope);}catch(e){}
        _gaq.push(['_setCustomVar', index, name, value, opt_scope]); 
      },
      _trackPageview: function(tracking_code) {
        _gaq.push(['_trackPageview', tracking_code]);
      },
      _trackEvent: function(category, action, opt_label, value) {
        _gaq.push(['_trackEvent', category, action, opt_label, value]);
      },
      _recordTimeEvent: function(identifier, label, time) {
        if (time > 0 && time < MAX_EVENT_TIME) {
          this._trackEvent('LoadTime', identifier, label, time);
        }
      }
    };


//]]>
</script>
