<script type="text/javascript">
//<![CDATA[
                                      function checkTipAmount(ele) {
                                        setTimeout(function() {
                                          tip_button = $('tip_checkout_submit');
                                          if (ele.value >= 1 && tip_button.disabled) { tip_button.enable(); ele.focus(); }
                                          else if (ele.value < 1 && !tip_button.disabled) { tip_button.disable(); ele.focus(); }
                                        }, 10); // Use setTimeout since onKeyPress event triggers before element value has actually changed
                                      }
                                      checkTipAmount($('t_amount')); // Invoke check manually in case user navigates back and field is still populated

//]]>
</script>                                </div>
