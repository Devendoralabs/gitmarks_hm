<script>
function setLex(v) {
  if (v == 'Y' || v == 't') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik9275';
  }
  else if (v == 'a') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik1404';
 }
  else if (v == 'b') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik8464';
  }
  else if (v == 'c') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik8981';
  }
  else if (v == 'd') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik9377';
  }
  else if (v == 'e') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik9219';
  }
  else if (v == 'f') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik9726';
  }
  else if (v == 'g') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik1412';
  }
  else if (v == 'h') {
    setLangCookie(v);
    enableLA();
    document.search.laKey.value = 'Wik1846';
  }
  else
    disableLA();
}
<!-- /SURFWAX -->

  function SetCookie (name, value)
  {
    var argv = SetCookie.arguments;
    var argc = SetCookie.arguments.length;
    var expires = (argc > 2) ? argv[2] : null;
    var path = (argc > 3) ? argv[3] : null;
    var domain = (argc > 4) ? argv[4] : null;
    var secure = (argc > 5) ? argv[5] : false;
    var curCookie = name + "=" + value +
      ((expires == '') ? "" : "; expires=" + expires) +
      ((path == '') ? "" : ("; path=" + path)) +
      ((domain == '') ? "" : ("; domain=" + domain)) +
      ((secure == true) ? "; secure" : "");
    document.cookie = curCookie;
  }


  function setLangCookie() {
      value = document.search.language.options[document.search.language.selectedIndex].value;
      SetCookie("language", value, 'Saturday, 11-February-2023 21:15:00 GMT', '/', '.surfwax.com');
  }

  function getCookie(name) {
    var dc = document.cookie;
    var prefix = name + "=";
    var begin = dc.indexOf("; " + prefix);
    if (begin == -1) {
      begin = dc.indexOf(prefix);
      if (begin != 0)
      	return null;
    }
    else
      begin += 2;
    var end = dc.indexOf(";", begin);
    if (end == -1)
      end = dc.length;
    return dc.substring(begin + prefix.length, end);
  }
  SetCookie('accepts_cookie', 'Yes', '', '/', '.surfwax.com');
</script>

