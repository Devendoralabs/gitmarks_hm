(function(ele) {
var sessionid = null;
if (document.cookie) try {
sessionid = encodeURIComponent(document.cookie.match(/_cookie_id=([^;]*)/)[1]);
} catch (e) {
//
}
var flashversion=deconcept.SWFObjectUtil.getPlayerVersion().major;
