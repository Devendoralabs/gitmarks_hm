				var popwindow = false;
				var ajaxurl="http://redalt.com/special:ajax";
				function popupLogin(e) {
					Event.stop(e);
					
					if(!popwindow) {
						$A(document.getElementsByTagName("body"))[0].appendChild(Builder.node("div", {id: "popuplogin", style: "display:none;"}));
						$("popuplogin").innerHTML = "Loading...";
					}
						var myAjax = new Ajax.Updater(
						{success: "popuplogin"},
						ajaxurl, 
						{
							method: "post", 
							parameters: "handler=popuplogin&action=showform&forward=" + escape(Event.element(e).href),
							evalScripts: true
						});
					popwindow = true;
					Effect.Appear("popuplogin");
				}				
				function doLogin(e) {
					Event.stop(e);
					var user = escape($F("microsession_user"));
					var pass = escape($F("microsession_pass"));
					var forward = escape($F("popuploginforward"));
					var myAjax = new Ajax.Updater(
					{success: "popuplogin"},
					ajaxurl, 
					{
						method: "post", 
						parameters: "handler=popuplogin&action=login&microsession_user=" + user + "&microsession_pass=" + pass + "&forward=" + forward,
						evalScripts: true
					});
				}
				Event.observe(
					window, "load", function()  {
						$A(document.getElementsByTagName("a")).each(function(a){
							if(a.href.match(/\/edit$/)) {
								Event.observe(a, "click", popupLogin);
							}
						});
					}
				);
			</script>
		
