    var currentPage = "static";

    function hNav(e) {
        if (!e) var e = window.event;
        var relTarg = e.target || e.srcElement || e.relatedTarget || e.fromElement;

        if ( !relTarg.rollsrc && relTarg.getAttribute && relTarg.getAttribute("rollsrc"))
            relTarg.rollsrc = relTarg.getAttribute("rollsrc");

        if ( !relTarg.page && relTarg.getAttribute && relTarg.getAttribute("page"))
            relTarg.page = relTarg.getAttribute("page");

        if ( relTarg.page != currentPage )
        { 
            relTarg.style.backgroundImage = "url("+relTarg.rollsrc+")";
        }

        if ( e.stopPropagation )
            e.stopPropagation();
        else
            e.cancelBubble = true;
    }

    function cNav(e) {
        if (!e) var e = window.event;
        var relTarg = e.target || e.relatedTarget || e.fromElement;

        if ( !relTarg.page && relTarg.getAttribute && relTarg.getAttribute("page"))
            relTarg.page = relTarg.getAttribute("page");
        
        if ( relTarg.page != currentPage )
        { 
            if ( relTarg.style.backgroundImage && relTarg.rollsrc)
                relTarg.style.backgroundImage = "";
        }

        if ( e.stopPropagation )
            e.stopPropagation();
        else
            e.cancelBubble = true;
    }
