		if (('jQuery' in window) && !('bw' in window && bw.cfg && bw.cfg.activateHeaderFlyout === false)) { //Check if jQuery exists & Check for activateHeaderFlyout flag
		$(document).ready(function(){
			$('.nav-flyout-link').hover(
				function() {
					var el = $(this),
						lPos = el.position().left,
						flyout = $('.flyout', this),
						right = flyout.hasClass('flyout-right');
					$(this).addClass('flyout-hover');
					$('.flyout', this).css('left', right ? lPos - flyout.outerWidth() + el.outerWidth() : lPos);
				},
				function() {
					$(this).removeClass('flyout-hover');
				}
			);
		});
		}
		//]]></script>

