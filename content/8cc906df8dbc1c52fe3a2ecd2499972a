	var bizoLib = {
		bizographics_data : {},
		bizoSmart : '',
		bizoDFP : '',
		getbizodata : function(bizo_data){
			if(typeof bizo_data.bizographics != 'undefined'){
				this.bizographics_data = bizo_data;
				this.parseBizoData();
				//console.debug(this.bizoSmart);
			}
		},
		bizoLoad : function(){
			var bizoscript = document.createElement('script');
			bizoscript.src = 'http://api.bizographics.com/v1/profile.json?callback=bizoLib.getbizodata&api_key=1a93669f69254ccf9125e070689d2320';
			bizoscript.setAttribute('async', 'true');
			var s = document.getElementsByTagName("head")[0];
			s.appendChild(bizoscript);
		},
		parseBizoData : function(){
			if(typeof this.bizographics_data.bizographics != 'undefined'){
				var tmp = this.bizographics_data.bizographics;
				for(x in tmp){
					if(typeof tmp[x] == 'object'){
						if(typeof tmp[x].code != 'undefined'){
							this.bizoSmart += ';' + tmp[x].code;
							this.bizoDFP += ';retkw=' + tmp[x].code;
						}else{
							var tmp2 = tmp[x];
							for (y in tmp2)
							{
								if(typeof tmp2[y] == 'object' && typeof tmp2[y].code != 'undefined'){ 
									this.bizoSmart += ';' + tmp2[y].code;
									this.bizoDFP += ';retkw=' + tmp2[y].code;
								}
							}
						}
					}
				}
			}
		}
	};
	
	bizoLib.bizoLoad();  	
</script>

