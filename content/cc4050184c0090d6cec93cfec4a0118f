  <script src="http://beatthat.com/javascripts/prototype-1.6.0.2-packed.js" type="text/javascript"></script>
  <link href="/stylesheets/compiled/shared_blue.css?1275930948" media="screen" rel="stylesheet" type="text/css" />
  
  <link href="/stylesheets/compiled/daily_deals.css?1280266667" media="screen" rel="stylesheet" type="text/css" />

  <script type="text/javascript">
	//<![CDATA[
	document.observe("dom:loaded", function() {
    dd = new DailyDeals(false);
  });
  
    // find to events that could fire loading items at the bottom
    Event.observe(window, 'scroll', function(e){
      loadRemainingItems();
    });

    Event.observe(window, 'resize', function(e){
      loadRemainingItems();
    });
  
    var dates_to_do = ["12/19/2010", "12/18/2010", "12/17/2010", "12/16/2010", "12/15/2010"];
    var loadingPage=false;
    function loadRemainingItems(){
      var holder = $('loading').previous('.deals');
      // compute amount of page below the current scroll position
      var remaining = (holder.viewportOffset()[1] + holder.getHeight()) - Position.GetWindowSize().height;
      if (remaining < 200 && dates_to_do.length > 0 && !loadingPage) {
        date = dates_to_do[0];
        loadingPage=true;
        new Ajax.Request('/daily_deals/archive/'+date, {
          method: 'get',
          parameters: {
            order: "date"
          },
          onLoading: function(){
            $('loading').show();
          },
          onSuccess: function(req){
            loadingPage=false;
            dates_to_do.shift();
            $('loading').hide();
            if (!req.responseText.blank()) {
              $('loading').insert({before : req.responseText});
              var inserted = $('loading').previous('.deals');
              // TODO: DRY this up with daily_deals.js
              inserted.select('li').each(function(item) {
                dd.setUpListeners(item);
              }.bind(this));
              inserted.select('.winner').each(function(item) {
                item.observe('mouseover', dd.winnerMouseOver.bindAsEventListener(this));
              }.bind(this));
            }
          }
        });
      }
    }
  
	//]]>
	</script>

  
