			<script type="text/javascript">
				function googleSearch() {
					var q=$("#google_cse_q").val();
					if ($.trim(q).length==0) { return false; }
					//q=q.replace(/[^0-9a-zA-Z_]/g, ' ');
					//q=q.replace(/\s+/g, '-');
					q=q.replace(/\s/g, '+');
					self.location='/search/search.php?sa=search&q='+escape(q);
					return false;
				}
				$("#google_cse_q").blur(function() {
					$(this).css("background-image", "url(google_custom_search_watermark.gif)");
				});
				$("#google_cse_q").focus(function() {
					$(this).css("background-image", "none");
				});
			</script>

