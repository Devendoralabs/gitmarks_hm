<script type="text/javascript">    
    function confirmCommunityDelete() {    
                
        return confirm( 'Are you sure you want to delete this group? Deleting this group will remove all data including any content that has been added to the group. Click OK to delete this group or Cancel to return to the group. ');
    }   
    // Enable buttons only after the page is fully loaded (for IE)
    function enableButton(buttonId, buttonFunc) {        
        var btn = dojo.byId(buttonId);        
        if (btn) {                         
            dojo.connect(btn, "onclick", null, function( evt ) {buttonFunc(); dojo.stopEvent(evt); return false; });
        }
    } 

    // calculate the iframe height based on the iframe content
    function resizeIframe(newHeight) {
	var e=document.getElementById('blogIframe');
	if (e != undefined && e != null) {  
      	   e.style.height = parseInt(newHeight) + 'px'; 
	}
    }

    // determine whether to show iframe for description
    function onLoadIframe() { 
        var e=document.getElementById('blogIframe');  
	 if (e != undefined && e != null) {
	   var doc = e.contentDocument || e.contentWindow.document;
	   if (doc != undefined && doc != null) {
	 	if (doc.title == 'overview') {
		  e.style.visibility = 'visible';
    		  e.style.display = '';
		} else {
		  e.style.display = 'none';
		}
	   } else {
		e.style.display = 'none';
	   }
       }
    }
    
              
</script>





