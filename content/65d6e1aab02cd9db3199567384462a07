<script language="Javascript" type="text/javascript">
    dojo.addOnLoad(
        function(){
		var lotusContent = dojo.byId('lotusContent');
		var mainContent = dojo.byId('mainContent');

		if (lotusContent != undefined && lotusContent != null) {
			if (mainContent == undefined || mainContent != null) {
				mainContent = document.createElement("a");
				mainContent.setAttribute('id', 'mainContent'); 
				mainContent.setAttribute('name', 'mainContent'); 

				if ( lotusContent.firstChild ) {
					lotusContent.insertBefore(mainContent, lotusContent.firstChild);
				} else {
					lotusContent.appendChild(mainContent);
				} 
			}
		}

		var lotusColLeft = dojo.byId('lotusColLeft');
		var subNavigation = dojo.byId('subNavigation');

		if (lotusColLeft != undefined && lotusColLeft != null) {
			if (subNavigation == undefined || subNavigation != null) {
				subNavigation = document.createElement("a");
				subNavigation.setAttribute('id', 'subNavigation'); 
				subNavigation.setAttribute('name', 'subNavigation'); 

				if ( lotusColLeft.firstChild ) {
					lotusColLeft.insertBefore(subNavigation, lotusColLeft.firstChild);
				} else {
					lotusColLeft.appendChild(subNavigation);
				} 
			}
		}		
        }
    );
</script>
