<script type="text/javascript">

// Get the ibm.com footer from the ibm.com generator tool then use
// http://www.htmlescape.net/stringescape_tool.html to escape it.  It'll do HTML, JS, and UTF8 encoding

// Never manipulate the DOM during page load or you will be sorry in IE, always do it after page load (using dojo addonload here).
// See http://support.microsoft.com/default.aspx/kb/927917


// The following is an attempt to adjust the nav for ja and es.  It causes some issues and needs to be throughly for example On https://astra.raleigh.ibm.com/developerworks/mydeveloperworks/files/app?lang=ja
function updateNavCSSClass(){

  if (GetCookie('conxnsCookie') == "ja" || GetCookie('conxnsCookie') == "jp")
  {
    document.getElementById('appLinks').innerHTML = document.getElementById('appLinks').innerHTML.replace("communities-nav","communities-nav-575");
  }
   else if (GetCookie('conxnsCookie') == "es")
  {
    document.getElementById('appLinks').innerHTML = document.getElementById('appLinks').innerHTML.replace("communities-nav","communities-nav-575");
  }
};

 //dojo.addOnLoad(updateNavCSSClass);

</script>

<script type="text/javascript">
function addLoadEvent(func){
  var oldonload = window.onload;
  if (typeof window.onload != 'function') {
	 window.onload = function() {
		func();
	 }
  } else {
	 window.onload = function() {
		func();
	 }
  }
}
</script>

<script type="text/javascript">
	var loadLangCSS = function(){
		var target = "";
		  if (GetCookie('conxnsCookie') == "ja" || GetCookie('conxnsCookie') == "jp")
		  {
		  target = "/developerworks/lc/html/dw-ja.css";
		  }
		   else if (GetCookie('conxnsCookie') == "es")
		  {
		  target = "/developerworks/lc/html/dw-es.css";
		  }
		   else if (GetCookie('conxnsCookie') == "pt_br")
		  {
		  target = "/developerworks/lc/html/dw-pt_br.css";
		  }
		   else if (GetCookie('conxnsCookie') == "ko")
		  {
		  target = "/developerworks/lc/html/dw-ko.css";
		  }
		   else if (GetCookie('conxnsCookie') == "zh")
		  {
		  target = "/developerworks/lc/html/dw-zh.css";
		  }
		   else if (GetCookie('conxnsCookie') == "ru")
		  {
		  target = "/developerworks/lc/html/dw-ru.css";
		  }
		var fileref=document.createElement("link");
		fileref.setAttribute("rel", "stylesheet");
		fileref.setAttribute("type", "text/css");
		fileref.setAttribute("href", target);
		document.getElementsByTagName("head")[0].appendChild(fileref);
	};


	addLoadEvent(loadLangCSS);

</script>

