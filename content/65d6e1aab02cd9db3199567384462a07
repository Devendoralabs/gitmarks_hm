     <script language="JavaScript1.2">
        function setCommunitiesReqCookie() {
            document.cookie = "CommunitiesReqURL=" + location.href + "; expires=" +
                new Date(new Date().getTime() + 365*24*60*60*1000).toGMTString() + "; path=/developerworks/mydeveloperworks/groups";
        }
        var loginLink = dojo.byId("logoutLink");
        dojo.connect(loginLink, 'onclick', setCommunitiesReqCookie);
     </script>
   
        
