<script type="text/javascript">    
    // Enable buttons only after the page is fully loaded (for IE)
    function enableTopButton(buttonId, buttonFunc) {        
        var btn = dojo.byId(buttonId);          
        if (btn) {                  
            dojo.connect(btn, "onclick", null, function( evt ) {buttonFunc(evt); dojo.stopEvent(evt); return false; });
        }
    }   
    // Confirm deletion of Community
    function confirmCommunityDeletion() {            
                
        return confirm( 'Are you sure you want to delete this group? Deleting this group will remove all data including any content that has been added to the group. Click OK to delete this group or Cancel to return to the group. ');
    }  
</script>



    
    
