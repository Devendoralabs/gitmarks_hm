<script type="text/javascript">
/*
function window.document.getElementById('search').onkeydown(){
 if(window.event.keyCode==13){	
	key= $('#keywords').val();
	location = 'http://www.playscreen.com/ps/index.php/search/'+key;
 } 
} 
*/
function search_result(e)
{
	if(e.keyCode==13)
	{
		key= $('#keywords').val();
		if(key=='keywords')
			key = '';
		if(key=='')
		{
			alert('Please input your keywords');	
		}else{
			location = 'http://www.playscreen.com/ps/index.php/search/index/'+key;
		}
	}
}

</script>
