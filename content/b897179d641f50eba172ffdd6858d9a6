<script type="text/javascript">
function show_image()
{
jQuery(document).ready(function($){

				 
				$('.cover1', this).stop().animate({left:'-200px'},{queue:false,duration:300});
				
				$('.boxgrid1.slidedown').hover(function(){
					$(".cover1", this).stop().animate({top:'-200px'},{queue:false,duration:300});
				}, function() {
					$(".cover1", this).stop().animate({top:'0px'},{queue:false,duration:300});
				});
				//Horizontal Sliding
				$('.boxgrid1.slideright').hover(function(){
					$(".cover1", this).stop().animate({left:'0px'},{queue:false,duration:300});
				}, function() {
					$(".cover1", this).stop().animate({left:'-200px'},{queue:false,duration:300});
				});
				
				//Horizontal Sliding2
				$('.boxgrid1.slideright2').hover(function(){
					$(".cover1", this).stop().animate({left:'0px'},{queue:false,duration:300});
				}, function() {
					$(".cover1", this).stop().animate({left:'200px'},{queue:false,duration:300});
				});
				//Diagnal Sliding
				$('.boxgrid1.thecombo').hover(function(){
					$(".cover1", this).stop().animate({top:'200px', left:'200px'},{queue:false,duration:300});
				}, function() {
					$(".cover1", this).stop().animate({top:'0px', left:'0px'},{queue:false,duration:300});
				});
				
				//Horizontal Sliding
				$('.boxgrid1.sliderightimage').hover(function(){
					$(".cover1", this).stop().animate({left:'200px'},{queue:false,duration:300});
				}, function() {
					$(".cover1", this).stop().animate({left:'0px'},{queue:false,duration:300});
				});

				
				//Full Caption Sliding (Hidden to Visible)
				$('.boxgrid1.captionfull').hover(function(){
					$(".cover1", this).stop().animate({top:'0px'},{queue:false,duration:260});
				}, function() {
				
					$(".cover1", this).stop().animate({top:'200px'},{queue:false,duration:260});
				});
				//Full Caption Sliding (Hidden to Visible)
				$('.boxgrid1.captionfull2').hover(function(){
					$(".cover1", this).stop().animate({top:'0px'},{queue:false,duration:260});
				}, function() {
				
					$(".cover1", this).stop().animate({top:'-px'},{queue:false,duration:260});
				});
				//Caption Sliding (Partially Hidden to Visible)
				$('.boxgrid1.captionpartial').hover(function(){
					$(".cover1", this).stop().animate({top:'px'},{queue:false,duration:160});
				}, function() {
					$(".cover1", this).stop().animate({top:'200px'},{queue:false,duration:160});
				});
				
				$('.boxgrid1.titlepartialcover').hover(function(){
					$(".cover1", this).stop().animate({top:'px'},{queue:false,duration:160});
				}, function() {
					$(".cover1", this).stop().animate({top:'px'},{queue:false,duration:160});
				});
				
				$('.boxgrid1.verticalsipartial').hover(function(){
					$(".cover1", this).stop().animate({top:'px'},{queue:false,duration:160});
				}, function() {
					$(".cover1", this).stop().animate({top:'0px'},{queue:false,duration:160});
				});
				




 });

} 
show_image();

</script>
