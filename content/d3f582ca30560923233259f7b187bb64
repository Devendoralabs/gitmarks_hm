var popupIframe;

function showPopupbox(wrapId, url) {
    var wrap = document.getElementById(wrapId);
    //wrap.style.display = "block";
    window.location.href = "#pagetop";
    popupIframe = wrap.getElementsByTagName("IFRAME")[0];
