	<script type="text/javascript">
/* Menu JS */
function initMenuClass() {

  	$('#main_nav ul').hide();
  	$('#sub_nav p').hide();

  $('#main_nav li a').click(
    function() {

      var mainElement = $(this).next();

      if((mainElement.is('ul')) && (mainElement.is(':visible'))) {
      	$('#main_nav ul:visible').slideUp('normal');
        return false;
        }
      if((mainElement.is('ul')) && (!mainElement.is(':visible'))) {
        $('#main_nav ul:visible').slideUp('normal');
        mainElement.slideDown('normal');
        return false;
        }
	 }
  );

  $('#sub_nav li a').click(
    function() {

      var subElement = $(this).next();

      if((subElement.is('p')) && (subElement.is(':visible'))) {
      	$('#sub_nav p:visible').slideUp('normal');
        return false;
        }
      if((subElement.is('p')) && (!subElement.is(':visible'))) {
        $('#sub_nav p:visible').slideUp('normal');
        subElement.slideDown('normal');
        return false;
        }
	 }
  );

}
$(document).ready(function(){initMenuClass();});
</script>


