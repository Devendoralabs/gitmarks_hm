<td colspan=2 class=npl><div style="position:relative;z-index:2"><div id=acdiv class=acd style="display:none;"></div></div>








<script type="text/javascript">
function $(id) { return document.getElementById(id); }
function shac(b) {
  acdiv.style.display=b?'block':'none';
  var iframe_id = 'acbgif';
  var iframe =  $(iframe_id);
  if (!iframe) {
    iframe = document.createElement('iframe');
    iframe.id = iframe_id;
    iframe.style.position = 'absolute';
    iframe.frameBorder = 0;
    iframe.setAttribute('tabindex', 1000);
    acdiv.parentNode.insertBefore(iframe, acdiv.parentNode.firstChild);
  }
  if (b) {
    iframe.width = acdiv.offsetWidth;
    iframe.height = acdiv.offsetHeight;
    iframe.style.display = '';
    document.onclick = function() { shac(0); document.onclick = null; }
  } else {
    iframe.style.display = 'none';
  }
}
function enc(s) { return encodeURIComponent(s).replace(/%20/g,'+'); }
function acreq(val) {
  var headel = document.getElementsByTagName("head")[0];
  if (window.indeed_ac) headel.removeChild(window.indeed_ac);
  window.indeed_ac = document.createElement("script");
  window.indeed_ac.src = '/rpc/suggest?from=hp&q='+val+'&tk=15k29i0310k337mu';
  headel.appendChild(window.indeed_ac);
}
function dbg(s) {
  var el = $('dbg');
  if (el) el.innerHTML=s;
}
function okuh(e) {
  if (!e && window.event) e = window.event;
  var kc = e.keyCode;
  switch (kc) { // arrows: 38 = up, 40 = down
    case 9: case 12: case 16: case 17: case 18: case 19: case 20: case 27:
    case 33: case 34: case 35: case 36: case 37: case 38: case 39: case 40: case 45: break;
    case 13: whereEl.blur(); break;
    default: wkp(whereEl.value);
  }
  return 1;
}
function okdh(e) {
  if (!e && window.event) e = window.event;
  var kc = e.keyCode;
  kp = kc;
  switch (kc) {
    case 38: case 40: if (!e.shiftKey) arrows(kc); break;
    case 27: clrsugg(e); break;
    case 13: tabout(e); sgmethod='enter'; break;
    case 9: tabout(e); return; break;
  }
  return 1;
}
function clrsugg(e) {
  shac(0);
  sgsel=sgmethod='';
  setTimeout("whereEl.value=lastTyped", 10);
}
function tabout(e) {
  
  if (!e && window.event) e = window.event;
  shac(0);
  sgsel=(selidx>=0)?whereEl.value:'';
  sgmethod='tab';
}

var ITEM_CLASSNAME = 'aci';
var SELECTED_ITEM_CLASSNAME = 'aci acis';

function arrows(kc) {
  if (nosuggs) return;
  var hidn = acdiv.style.display=='none';
  if (hidn) shac(1);
  var spans = acdiv.getElementsByTagName('div');
  if (!hidn) {
    if (kc==38) {
      if (selidx<0) selidx=spans.length;
      selidx--;
    } else if (kc==40) {
      selidx++;
      if (selidx>=spans.length) selidx=-1;
    }
    sgsel='';
    if (selidx<0 || selidx >= spans.length) { whereEl.value = lastTyped; } else { sgsel = spans[selidx]; }
  }
  for (var i=0; i<spans.length; i++) {
    if (i==selidx) {
      spans[i].className = SELECTED_ITEM_CLASSNAME;
      whereEl.value=unbold(spans[i].innerHTML);
    } else {
      spans[i].className = ITEM_CLASSNAME;
    }
  }
}
function wkp(val) {
  if (window.indeed_ti) clearTimeout(window.indeed_ti);
  window.indeed_ti = setTimeout('acreq("'+enc(val)+'")', 100);
  lastTyped=val;
  sgsel='';
  return false;
}
function sugg_cb(val) {
  var suggs = val[1];
  while (acdiv.childNodes.length > 0) acdiv.removeChild(acdiv.childNodes[0]);
  if (suggs.length == 0) {
    nosuggs=1;
    suggcount=0;
    selidx=-1;
    shac(0);
    return;
  }
  nosuggs=0;
  suggcount=suggs.length;
  selidx=-1;

  for (var i=0; i<suggs.length; i++) {
    var u = document.createElement("DIV");
    u.className = ITEM_CLASSNAME;
    u.onmouseover=onmov;
    u.onclick=onclk;
    u.innerHTML=suggs[i];
    acdiv.appendChild(u);
  }
  shac(1);
}
function onmov() {
  var el = this;
  var txt = el.innerHTML;
  var ch = acdiv.childNodes;
  for (var i=0; i<ch.length; i++) {
    var value = ch[i].innerHTML;
    if (value==txt) {
      selidx=i;
    } else {
      ch[i].className = ITEM_CLASSNAME;
    }
  }
  el.className = SELECTED_ITEM_CLASSNAME;
}
function onclk() {
  shac(0);
  whereEl.value=sgsel=unbold(this.innerHTML);
  sgmethod='clk';
  formptk('form');
  $('fj').focus();
}
function unbold(s) { return s.replace(/\<\/?b\>/gi,''); }

var nosuggs=1;
var suggcount=0;
var selidx=-1;
var kp=-1;
var whereEl=$('where');
var acdiv=$('acdiv');
var lastTyped=whereEl.value;
var sgmethod='';
var sgsel='';
whereEl.setAttribute('autocomplete', "off");
whereEl.onkeyup=okuh;
whereEl.onkeydown=okdh;
</script>

