$.get("http://www.webshotspro.com/pagerank.php", { pagerankurl: value }, function(data){
	var ranks = data.split("|");
	var googleRank = ranks[0];
	var alexaRank = ranks[1];
	$("#googlePageRank").replaceWith(googleRank); 
	$("#alexaRank").replaceWith(alexaRank); 

});


});
