// todo: onclick handler for items that displays video

function selectfeed(ndx) {
  t1 = document.getElementById("showcasefeedtab1");
  t2 = document.getElementById("showcasefeedtab2");
  o1 = document.getElementById("showcasefeed1");
  o2 = document.getElementById("showcasefeed2");
  if (ndx == 2) {
    if (o1) o1.style.display = "none";
    if (o2) o2.style.display = "block";
    if (t1) t1.className = "";
    if (t2) t2.className = "selected";
  } else {
    if (o2) o2.style.display = "none";
    if (o1) o1.style.display = "block";
    if (t2) t2.className = "";
    if (t1) t1.className = "selected";
  }
}

function swapmedia(url, enc, com) {
  //alert('enc is: '+enc);
  var flashvars = {};
  var params = {};
  var attributes = {};

  // todo: check for url in description (embed or object or iframe)

  // cheap trick for now - hope for transcoding
  enc2 = enc.replace(/\.wmv/, ".flv");

  //if (!com.length)
  //  com = "javascript: alert('No comment url available for this media item.');";

  // ac = document.getElementById("addcomment");
  // ac.href = com;

  if (enc.indexOf("mediamobz") > 0) {
    swfobject.embedSWF(enc, "spotvideocontent", "461", "308", "8", "swfobject/expressInstall.swf");
    return;
  }

  if ( enc2.length && ((enc2.indexOf(".mp4") > 0) || (enc2.indexOf(".mov") > 0)  || (enc2.indexOf(".mpg") > 0)   || (enc2.indexOf(".m4v") > 0)   || (enc2.indexOf(".3gpp") > 0)   || (enc2.indexOf(".mpeg") > 0)) ) {
      var myQTObject = new QTObject(enc2, "spotvideocontentqt", "461", "308");
      myQTObject.addParam("controller", "true");
      myQTObject.addParam("autostart", "true");
      myQTObject.addParam("scale", "aspect");
      myQTObject.write("spotvideocontent");
      return;
  }

  if (enc2.length && (enc2.indexOf(".flv") > 0) ) {
    // todo: use local player
    url = "http://www.ourmedia.org/sites/all/themes/ourmedia3/players/flvplayer.swf?file=" + enc2 + "&autostart=true";
    swfobject.embedSWF(url, "spotvideocontent", "461", "308", "8", "swfobject/expressInstall.swf");
    return;
  }

  if (enc2.length && (enc2.indexOf(".mp3") > 0)) {
    // todo: use local player
    enc = encodeURIComponent(enc);
    swfurl = "http://www.ourmedia.org/sites/all/themes/ourmedia3/players/flvplayer.swf?file=" + enc2 + "&autostart=true";
    swfobject.embedSWF(swfurl, "spotvideocontent", "461", "308", "8", "swfobject/expressInstall.swf");
    return;
  }


  if (enc.indexOf("youtube") >0) {
    //alert('youtube');
    enc =enc.replace(/watch\?v=/, "v/");
    swfobject.embedSWF(enc, "spotvideocontent", "461", "308", "8", "swfobject/expressInstall.swf");
    return;
  }

  //alert(enc2);

  location.href = url;
}

</script>

