			function CheckQuickSearch(field){
				search = field.value;
				if (search == "" || search == "Enter Web ID, Agent's Name or Address") {
					return false;
				} else {
					return true;
				}
			}
			function QuickSearchFocus(field) {
				var myvalue = field.value;
				if (myvalue == "Enter Web ID, Agent's Name or Address") {
					field.value = "";
				}
			}
			function QuickSearchOutFocus(field) {
				var myvalue = field.value;
				if (myvalue == "") {
					field.value = "Enter Web ID, Agent's Name or Address";
				}
			}
