			function checkBottom() {
				var winW = 630, winH = 460;

				if (parseInt(navigator.appVersion)>3) {
				 if (navigator.appName=="Netscape") {
				  winW = window.innerWidth;
				  winH = window.innerHeight;
				 }
				 if (navigator.appName.indexOf("Microsoft")!=-1) {
				  winW = document.body.offsetWidth;
				  winH = document.body.offsetHeight;
				 }
				}
				var position = document.getElementById("pageFooter").offsetTop;
				if (winH > position) {
					var height = winH - position-4;
					document.getElementById("pageFooter").style.height = height + "px";
				}
			}
			if (window.addEventListener) {
				window.addEventListener("load", checkBottom, false);
				window.addEventListener("resize", checkBottom, false);
