<script>

$(window).load(function() {
	updateMenu();
});

function updateMenu() {
	$('.menuNode').each(function() {
		var img = new Image();
		var src = $(this).css('backgroundImage').replace("url(","");
		var tdObj = $(this).parent('td');
		var imgObj = $(this).find('img');
		src = src.replace("\"","");
		src = src.replace(")","");
		src = src.replace("\"","");
		img.onload = function() {
			imgObj.css('width',img.width +'px');
			tdObj.css('width',img.width +'px');
		}
		img.src = src;
		$(this).css('backgroundPosition','50% 0');
		$(this).mouseover(function() {
			$(this).css('backgroundPosition','50% -34px');
		});
		$(this).mouseout(function() {
			$(this).css('backgroundPosition','50% 0');
		});
		$(this).css('visibility','visible');
	});
	$('.menuNodeActive').each(function() {
		$(this).css('backgroundPosition','50% -34px');
		var img = new Image();
		var src = $(this).css('backgroundImage').replace("url(","");
		var tdObj = $(this).parent('td');
		var imgObj = $(this).find('img');
		src = src.replace("\"","");
		src = src.replace(")","");
		src = src.replace("\"","");
		$(this).css('visibility','visible');
		img.onload = function() {
			imgObj.css('width',img.width +'px');
			tdObj.css('width',img.width +'px');
		}
		img.src = src;
	});	
}


</script>

