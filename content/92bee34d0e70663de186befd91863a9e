	<script type="text/javascript">
		
		
		function formatSearch(row) {
			return row[2] + ' > ' + row[1];
		}

		function redirectSearch(event, data, formatted) {
			$('#search_query').val(data[1]);
			document.location.href = data[3];
		}
		
		$('document').ready( function() {

			/*if (document.location.protocol == "https:") {
				$("#headerSearch").attr("action","https://thecottonfactoryinc.com/search.php");
				$("#headerNewsletterForm").attr("action","https://thecottonfactoryinc.com/");
			} else {
				$("#headerSearch").attr("action","http://thecottonfactoryinc.com/search.php");
				$("#headerNewsletterForm").attr("action","http://thecottonfactoryinc.com/");
			} */
			$("#search_query").autocomplete(
				'search.php', {
				minChars: 3,
				max:10,
				width:400,
				scroll: false,
				formatItem:formatSearch,
				extraParams:{ajaxSearch:1,id_lang:1}
			}).result(redirectSearch)
		});
		
	</script>
