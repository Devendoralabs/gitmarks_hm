    function disableEnterKey(e)
	{
		var key;     
		if(window.event)
			key = window.event.keyCode; //IE
		else
			key = e.which; //firefox     

		return (key != 13);
	}
				
	function PopulateHidden(value) 
	{
		$ektron("#email").val(value.value);
	}
	</script>
