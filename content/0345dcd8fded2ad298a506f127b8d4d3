<script>
$(document).ready(function(){
	$("#codeinput").change(function(){
		var val = this.value;
		if(val.length) {
			$.post("/code/code-check.php",{code: val},
				function(data) {
					if(data.length) alert(data);
				}
			);
		}
	});
});
</script>
