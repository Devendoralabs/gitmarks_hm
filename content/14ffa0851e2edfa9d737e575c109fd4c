<script type="text/javascript">
var ControlPanel_ToLoad_expressions=[];
var ControlPanel_Html=[];
function ControlPanel_ToLoad(expression){
	ControlPanel_ToLoad_expressions.push(expression);
};
function ControlPanel_LoadMenu(){
	for(var idx=0;idx<ControlPanel_ToLoad_expressions.length;idx++){
		eval(ControlPanel_ToLoad_expressions[idx]);
	}
};
function ControlPanel_CreateMenu(name, tooltipId, tooltipId_hide, a_img, i_img, img_size){
	var html=ControlPanel_Html;
	var image_dir = window.NDImagesUrl || "images/";
	html.push('<a href="javascript: PDViewer.ControlPanel.showPanel(\''+name+'\')">');
	html.push('<img tooltipId="'+tooltipId+'" ID="controlpanel_menu_'+name+'_a" src="'+a_img+'" Width="22" Height="'+img_size+'" border="0" style="position:absolute; top: 0px; display: none;" />');
	html.push('<img tooltipId="'+tooltipId+'" ID="controlpanel_menu_'+name+'_i" src="'+i_img+'" Width="22" Height="'+img_size+'" border="0" style="position:absolute; top: 0px; display: none;" />');
	html.push('<img ID="controlpanel_menu_'+name+'_hide" src="'+image_dir+'pan_tab_hide.gif" Width="6" Height="7" border="0" style="position:absolute; top: 0px; left: 8px; display: none;" />');
	html.push('<img ID="controlpanel_menu_'+name+'_show" src="'+image_dir+'pan_tab_show.gif" Width="6" Height="7" border="0" style="position:absolute; top: 0px; left: 8px; display: none;" />');
	html.push('</a>');
	ControlPanel_ToLoad('PDViewer.ControlPanel.addMenu("'+name+'","controlpanel_menu_'+name+'_a","controlpanel_menu_'+name+'_i","controlpanel_menu_'+name+'_hide","controlpanel_menu_'+name+'_show","'+tooltipId+'","'+tooltipId_hide+'",'+img_size+')');
};
function ControlPanel_WriteMenu(){
	document.write( ControlPanel_Html.join("") );
};


ControlPanel_CreateMenu('navigator','cpNavigator','cpNavigatorHide','http://cache-styles.pressdisplay.com/res/v490/images/pan_tab2.gif','http://cache-styles.pressdisplay.com/res/v490/images/pan_tab2_g.gif',117);

document.write( ControlPanel_Html.join("") );
</script>

</div>
