<script type="text/javascript">
toLoad(function(){
	var c=window.PDConfig||{};
	var cm=c.Menu={};
	
	// menu config
	cm.EnableShowMenuOnMouseOver=false;
	
	// FindNewspapers config
	var cmfn=c.Menu.FindNewspapers={};	
	cmfn.EnableMyNewspapers=true;
	cmfn.EnableReadIssues=true;
	cmfn.EnableReadNewspapers=true;
	cmfn.EnableReadNewspapersServerLoading=true;
	cmfn.MenuType=0;

	// MyServices config
	var cmms=c.Menu.MyServices={};
	cmms.MyBookmars={
		enable:true,
		frame:null,
		url:"javascript:myBookmarks_onclick()"
	};
	cmms.MyNewspapers={
		enable:true,
		frame:"content_frame",
		url:'https://www.pressdisplay.com/pressdisplay/accountingmyaccount.aspx?subpage=mynewspapers'
	};
	cmms.MyMonitors={
		enable:true,
		frame:"content_frame",
		url:'AdvancedSearch.aspx?monitor=true'
	};
	cmms.MyPrefs={
		enable:true,
		frame:"content_frame",
		url:'/pressdisplay/userpreferences.aspx'
	};
	cmms.MyReferrals={
		enable:true,
		frame:"content_frame",
		url:'https://www.pressdisplay.com/pressdisplay/accountingmyaccount.aspx?subpage=referrals'
	};
	cmms.MyProfile={
		enable:true,
		frame:"content_frame",
		url:'https://www.pressdisplay.com/pressdisplay/accountingmyaccount.aspx?subpage=userprofile'
	};
	cmms.MyAccount={
		enable:true,
		frame:"content_frame",
		url:'https://www.pressdisplay.com/pressdisplay/accountingmyaccount.aspx'
	};
	

	c.Menu.Search={
		enableToday:true,
		enableLast3days:true,
		enableThisWeek:true,
		enableThisMonth:true,
		enableAnytime:true,
		enableCurrentIssue:true,
		enableCurrentNewspaper:true,
		enableCurrentCategory:true,
		enableLastSearchResult:true
	};



	c.Menu.Locales={
		ids:["ar","fr","de","it","ja","ru","es","zt"],
		names:["Arabic","French","German","Italian","Japanese","Russian","Spanish","Traditional Chinese"],
		namesNative:["العربية","Français","Deutsch","Italiano","日本語","Русский","Español","繁體中文"],
		changeLanguageUrl: "ChangeLanguage.aspx",
		changeLanguageParameterName: "lng"
	};



	var t=CustomToolbar;

	t.PlaceToShowCategory='/pressdisplay/homepage_v2.aspx?operation=selectnewspapers';

	t.minWidth = 840;
	
	
	window.PDConfig=c;
	
	t.init();
});

function ftsearch_query_onmouseout(event){
	if(window.ViewerMenus)
		return ViewerMenus.searchMenuQuery_onmouseout(event);
}

function ftsearch_query_onclick(event){
	if(window.ViewerMenus)
		return ViewerMenus.searchMenuQuery_onclick(event);
}

function ftsearch_query_onkeypress(event){
	if(window.ViewerMenus)
		return ViewerMenus.searchMenuQuery_onkeypress(event);
}
function ftsearch_onclick(){
	if(window.ViewerMenus)ViewerMenus.ftsearchSearch();
}
function changeClass(id, className){
	pdcore.changeClass(id, className);
}
function selectNewspapers(){

	if(window.content_frame.HomePageManager) window.content_frame.HomePageManager.showSelectNewspapersPanel();
	else if(typeof(pdcore)!="undefined") pdcore.setFrmLocation('content_frame','/pressdisplay/homepage_v2.aspx?operation=selectnewspapers');

}
function myServices_onclick(){

	if(window.content_frame.HomePageManager) window.content_frame.HomePageManager.showMyServicesPanel();
	else if(typeof(pdcore)!="undefined") pdcore.setFrmLocation('content_frame','/pressdisplay/homepage_v2.aspx?operation=myservices');

}

function myBookmarks_onclick(){
	if(window.content_frame.HomePageManager) window.content_frame.HomePageManager.showMyBookmarks();
	else if(typeof(pdcore)!="undefined") pdcore.setFrmLocation('content_frame','/pressdisplay/homepage_v2.aspx?operation=mybookmarks');
}

</script>
