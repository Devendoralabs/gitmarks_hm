	if(document.location.hash.substr(1)) {
		var paragraphs = document.getElementById('articlecontent').getElementsByTagName("p");
		paragraphs[document.location.hash.substr(1)].style.backgroundColor='#fff4aa';
		scrollvalue = paragraphs[document.location.hash.substr(1)].offsetTop + document.getElementById('articlecontent').offsetTop;
		window.scrollTo( 0, scrollvalue);
	}
</script>

