<script>
jQuery.event.add(window, "resize", resizeFrame);
jQuery.event.add(window, "scroll", reposition);

var startpos;
var startpos2;



function posinitiator() {
	var offset = $("#incsharebox").offset();
	startpos = offset.top;
}

function resizeFrame()
{
    var w = $(window).width();
    $("#incsharebox").css('float',(w < 1150) ? 'right' : 'left');
    $("#incsharebox").css('margin-left',(w < 1150) ? '8px' : '-98px');
    $("#incsharebox").css('border-color',(w < 1150) ? '#EEEEEE' : '#CCCCCC');
    reposition();
}

function reposition()
{
	var s = $(window).scrollTop();
	var w = $(window).width();

	if(w>1149) {
		$("#incsharebox").css('position',(s>startpos) ? 'fixed' : 'relative');
		$("#incsharebox").css('left',(s>startpos) ? '50%' : '0px');
		$("#incsharebox").css('top',(s>startpos) ? '0px' : '0px');
		$("#incsharebox").css('margin-left',(s>startpos) ? '-575px' : '-98px');
	} else {
		$("#incsharebox").css('position','relative');
		$("#incsharebox").css('left','0px');
	}

}


</script>


			
	

	
	
