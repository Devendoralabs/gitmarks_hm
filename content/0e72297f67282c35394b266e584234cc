		<h1 id="printHeader" style="display:none;">How-To Geek</h1>
		<!-- abovepost -->
					<div class="thepost" id="post-312">
				
				<div style="width:650px;margin-bottom:10px;margin-top:-30px;min-height:30px;">
												<script>TmnAdsense.display(options = {'adsbyposition':'topleft','adsbyColor':'000000','width':630,'ad_width':170,'max_ads':3,'col_margin':55,'titleFontSize':17,'titleFontColor':'003371','adFontFamily':'arial','titleMarginBottom':0,'descFontSize':12,'descFontColor':'000000','linkFontSize':10,'linkFontColor':'000000','channel_id':'2315583640'});</script>										
					<div style="clear:both;"></div>
				</div>
				<style>
					.thepost h2 a {text-decoration:none;}
					.thepost h2 a:hover {text-decoration:underline;}
					.thecontent p:first-child {margin-top:0px;}
					.sharinglinks li {display:inline;}
					.sharinglinks {width:650px;}
					.fbShare {margin-left:0px;}
