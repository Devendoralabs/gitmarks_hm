<script type="text/javascript">


function doPost(url,target,theId){

	var theTitle = escape(document.getElementById('postTitle').value);
	var theBody = escape(document.getElementById('postBody').value);
	var agentName = escape(document.getElementById('agentName').value);
	var postId = escape(document.getElementById('postId').value);

	if(theTitle==''){
		document.getElementById('postError').innerHTML='<font color=red size=3><b>Error: Please enter a title for this post</b></font>';
		return false;
	}

	//alert(theTitle);
	var postParams = 'thisAgent='+agentName+'&thisId='+postId+'&thisTitle='+theTitle+'&thisBody='+theBody+'&ok=OK';

	if (window.XMLHttpRequest) {
			req = new XMLHttpRequest();
			req.onreadystatechange = function() {postDone(target,theId);};
			req.open("POST", url, true);
			req.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
			req.send(postParams);

			// IE/Windows ActiveX version
		} else if (window.ActiveXObject) {
			req = new ActiveXObject("Microsoft.XMLHTTP");
			if (req) {
				req.onreadystatechange = function() {postDone(target,theId);};
				req.open("POST", url, true);
				req.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');

				req.send(postParams);


		}
	}


}

function postDone(target,id) {
	var results = "";

	//document.getElementById(target).innerHTML = "";
	// only if req is "loaded"
	if(req.readyState == 1){

		if(target=='postmessage'){
			document.getElementById(target).innerHTML='';
			document.getElementById(target).innerHTML = 'Loading...';
		}
	}
	if(req.readyState == 4)	{

		if (req.status == 200) {

			results = req.responseText;

			slotIn(id,'postmessage');


		}
	}
}



function slotIn(theId,target) {

//urchinTracker("/id/" + theId );

	if(target=='postmessage'){

		var injectorScript = '/bloghud.php';

	}

	// replace ALL ampersands with %26
	theId = theId.replace(/&/g,"%26");
	//url = escape(url);

	// add injector php script and append feed GET parameter
	var fullUrl = injectorScript + '?id=' + theId;

	  // native XMLHttpRequest object
	req = null;


    document.getElementById(target).innerHTML = 'loading post...';
    if (window.XMLHttpRequest) {
        req = new XMLHttpRequest();
        req.onreadystatechange = function() {slotDone(target,theId);};
        req.open("GET", fullUrl, true);
        req.send(null);
    // IE/Windows ActiveX version
    } else if (window.ActiveXObject) {
        req = new ActiveXObject("Microsoft.XMLHTTP");
        if (req) {
            req.onreadystatechange = function() {slotDone(target,theId);};
            req.open("GET", fullUrl, true);
            req.send();
        }
    }
}





function slotDone(target,theId) {
	var results = "";
	//document.getElementById(target).innerHTML = "";
    // only if req is "loaded"
    if (req.readyState == 4) {
        // only if "OK"
        if (req.status == 200) {
            results = req.responseText;

           // urchinTracker("/id/" + theId );
            document.getElementById(target).innerHTML = results;



        } else {

			document.getElementById(target).innerHTML="there was an error collecting info from "+ theId;

        }
    }
}


function inserter(target,id,type) {


	if(type=='edit'){

		var injectorScript = '/_service/posts.php';

	} else {
		// do nothing
		return;

	}


	// replace ALL ampersands with %26
	id = id.replace(/&/g,"%26");
	//url = escape(url);

	// add injector php script and append feed GET parameter
	var fullUrl = injectorScript + '?id=' + id;

	  // native XMLHttpRequest object
	req = null;
    document.getElementById(target).innerHTML = 'loading post...';
    if (window.XMLHttpRequest) {
        req = new XMLHttpRequest();
        req.onreadystatechange = function() {inserterDone(target,id,type);};
        req.open("GET", fullUrl, true);
        req.send(null);
    // IE/Windows ActiveX version
    } else if (window.ActiveXObject) {
        req = new ActiveXObject("Microsoft.XMLHTTP");
        if (req) {
            req.onreadystatechange = function() {inserterDone(target,id,type);};
            req.open("GET", fullUrl, true);
            req.send();
        }
    }
}

function inserterDone(target,id,type) {
	var results = "";
	//document.getElementById(target).innerHTML = "";
    // only if req is "loaded"
    if (req.readyState == 4) {
        // only if "OK"
        if (req.status == 200) {
            results = req.responseText;
            //alert(results);
            document.getElementById(target).innerHTML = results;

        } else {

		document.getElementById(target).innerHTML="there was an error";
        }
    }
}




function toUpper(strng) {
    var pattern = /(\w)(\w*)/; // a letter, and then one, none or more letters
    //var a = document.form1.text1.value.split(/\s+/g); // split the sentence into an array of words
	var a= strng.split(/\s+/g);
    for (i = 0 ; i < a.length ; i ++ ) {
        var parts = a[i].match(pattern); // just a temp variable to store the fragments in.
        var firstLetter = parts[1].toUpperCase();
        var restOfWord = parts[2].toLowerCase();
        a[i] = firstLetter + restOfWord; // re-assign it back to the array and move on
    }
    return a.join(' '); // join it back together
}



