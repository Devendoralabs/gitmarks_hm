<script type="text/javascript">
$(document).ready(function() {
	$("#nav1 li.menuparent").hoverIntent({
		interval: 75, // milliseconds delay before onMouseOver
		over: submenu_show, 
		timeout: 150, // milliseconds delay before onMouseOut
		out: submenu_hide
	});
	$("#nav1 li.menuparent").addClass('js');
function submenu_show(){ $(this).addClass('show'); $(this).removeClass('js'); }
function submenu_hide(){ $(this).removeClass('show'); $(this).addClass('js'); }
});
</script>

