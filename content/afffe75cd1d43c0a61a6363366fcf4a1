		<script type="text/javascript">
		
		function clipboard(){
			holdtext.innerText = copytext.innerText;
			copied = holdtext.createTextRange();
			copied.execCommand("Copy");
		}
		
		$(function(){
			//Assign initial values
			var short_url = "http://bit.ly/191aSP";
			var safetweet = ""
			if (safetweet == ""){
				var post_title = "Fonts Used In Logos of Popular Websites";
			}else{ 
				var post_title = safetweet;
			}
			var user_feedback;
			var tweet;

			function getFeedback(){
				//Assign selected item to feedback
				user_feedback = $("#feedback").val();
			}

			function composeTweet(){
				//Compose the tweet
				tweet = user_feedback + ': ' + post_title + ' ' + short_url;

				//Update destination before sending tweet
				$('#tweetit').attr("href", "http://twitter.com/home?status=" + tweet);
			}

			$("#tweetit").click(function(){
				composeTweet();
			});

			//Update feedback when option is changed
			$("#feedback").change(getFeedback);

			//Get the initial feedback value from dropdown menu
			getFeedback();

			//Compose a default tweet
			composeTweet();
		
		});
		
		</script>
			
