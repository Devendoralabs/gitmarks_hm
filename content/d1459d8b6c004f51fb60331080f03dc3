	<script type="text/javascript"> 
                var flyoutTimeout=undefined; 
                $(document).ready(function(){ 
                        $("#miniReviews,#flyoutWrapper").bind("mouseenter",function(){ 
                                if(undefined!=flyoutTimeout) 
                                        clearTimeout(flyoutTimeout); 
                                flyoutTimeout=setTimeout("showReviewsFlyout()",200); 
                        }); 
                
                
                        $("#flyoutWrapper,#miniReviews").bind("mouseleave",function(){ 
                                if(undefined!=flyoutTimeout) 
                                        clearTimeout(flyoutTimeout); 
                                flyoutTimeout=setTimeout("hideReviewsFlyout()",400); 
                        }); 
                }); 
                
                function hideReviewsFlyout(){ 
                        $("#miniReviews").removeClass("selected"); 
                        $('#flyoutWrapper').slideUp("fast"); 
                } 
                
                function showReviewsFlyout(){ 
                              $("#miniReviews").addClass("selected"); 
                                $('#flyoutWrapper').slideDown(200);                 
                } 
                
	</script> 

	
		
	

	
	
	
	
	

    
