    var i = 0,
        prevLink = $('example').down('.prev'),
        nextLink = $('example').down('.next');
    $('example')
      .observe('mouseover', function() {
        prevLink.appear(); nextLink.appear();
      })
      .observe('mouseleave', function() {
        prevLink.fade(); nextLink.fade();
      });

    var updateExample = function(dir, e) {
      i += dir;
      if (i == featured.length) i = 0
      else if (i < 0) i = featured.length - 1;

      var img = new Element('img', { 'class': 'screenshot' })
        .hide()
        .observe('load', function() {
          var old = $('example').down('.screenshot');
          var link = new Element('a', { href: featured[i].url });
          $('example').insert({top: this.wrap(link)});
          this.appear();
          if (old)
            old.fade({
              duration: 0.5,
              afterFinish: function() { old.up().remove(); }
            });
        })
        .src = featured[i].thumbnail;

      if (e && typeof e === 'object') Event.stop(e);
    };

    var interval = setInterval(updateExample.curry(1), 3000);
    [ nextLink, prevLink ].invoke('observe', 'click', function() {
      clearInterval(interval);
    });
    nextLink.observe('click', updateExample.curry(1));
    prevLink.observe('click', updateExample.curry(-1));

    updateExample(0);
  });

