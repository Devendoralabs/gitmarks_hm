                <script type="text/javascript">
//<![CDATA[
      document.observe('dom:loaded', function() {
        var fields = $$('#feedback_body'), text = "Send us feedback";

        fields.each(function(field){
          if ($F(field) === '') {
            field.value = text;
            field.addClassName('placeholder');
          }

          field.onfocus = function() {
            if (this.value === text)
              $(this).clear().removeClassName('placeholder');
          };

          field.onblur = function() {
            if (this.value === '') {
              this.value = text;
              $(this).addClassName('placeholder');
            }
          };
        });
      });

//]]>
</script>
                
