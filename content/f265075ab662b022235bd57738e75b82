<script language="javascript" type="text/javascript">
$$('.htab').each(function(t) {
    t.observe(
        'click',
        function(e) {
            elem = Event.element(e);
            
            location.href = '/' + (elem.id ? elem.id.split('_')[1] : "");
        }
    );    
        
    t.observe(
        'mouseover', 
        function() {
            t.style.background = 'url(/_/image/bg/bg_1px_grad_darkblue.png) repeat-x center center';
        }
    );

    t.observe(
        'mouseout', 
        function() {
            if (!t.hasClassName('selected'))
                t.style.background = 'url(/_/image/bg/bg_1px_grad_lightblue.png) repeat-x center center';
        }
    );
});

moduleManager.register(new Search("null"));
moduleManager.register(new SignIn(null,null));</script>
