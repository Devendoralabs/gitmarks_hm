				<script type="text/javascript">
					function getposOffset(overlay, offsettype){
						var totaloffset=(offsettype=="left")? overlay.offsetLeft : overlay.offsetTop;
						var parentEl=overlay.offsetParent;
							while (parentEl!=null){ totaloffset=(offsettype=="left")? totaloffset+parentEl.offsetLeft : totaloffset+parentEl.offsetTop; parentEl=parentEl.offsetParent; }
					return totaloffset;}
									//Call dropdowncontent.init("anchorID", "positionString", glideduration, "revealBehavior") at the end of the page:
					dropdowncontent.init("global1", "right-bottom", 10, "mouseover")
					dropdowncontent.init("global4", "left-bottom", 10, "mouseover")
					dropdowncontent.init("global5", "left-bottom", 10, "click")
					dropdowncontent.init("global6", "left-bottom", 10, "click")
					dropdowncontent.init("global7", "left-bottom", 10, "click")
					
				</script>
