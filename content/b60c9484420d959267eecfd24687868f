function sailthruWindow(url, wname, width, height) {
window.open(url, wname, "height=" + height + ",width=" + width + "location = 0, status = 1, resizable = 0, scrollbars=1, toolbar = 0");
setTimeout('clearSailthru()',3000);
return true;
}
function clearSailthru() {
    $('#sailthru-form input[type="text"]').val('');
}
