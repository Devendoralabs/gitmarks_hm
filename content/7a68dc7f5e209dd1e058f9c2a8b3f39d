<script language="JavaScript">
<!-- hide from old browsers
function printThisURL()
{
	if (window.print)
	{
		// Pop up the URL in a print friendly format.
		var height = 440;
		var width = 630;

		var printFlag = "pq-pf=1";
		var protoHostPath = location.protocol + "//" + location.host + location.pathname;

		var queryString = new String (unescape(window.location.search));
		(queryString.length == 0)?printFlag ="?" + printFlag:printFlag ="&" + printFlag;
		var urlToPopup = protoHostPath + queryString + printFlag;
		openPopup (urlToPopup, height, width);
	}
	else
	{
		alert ("Your browser does not support the print function");
	}
}

// hide from old browsers -->
</script>

<script language="JavaScript">
var infoSent = new String (unescape (window.location.search));

// is this the URL in printMode, or just a normal rendering?
// If normal, then output a link to the printFriendly version of
// the page.
if (infoSent.indexOf("pq-pf") == -1)
{
      document.write ("<table width='70' cellpadding='0' cellspacing='0'>");
	document.write ("<tr>");
	document.write ("<td width='70' height='25' valign='middle' align='center' class='bannerGrey'>"); 

	document.write ("<a href=javascript:location.replace(location.href) onClick='javascript:printThisURL()' alt='printer friendly page'>Print<img src='/eknec/documents/20/0900688a800a2e20/glo_0009_obj004.gif' border='0' align='center'/></a>");

	
	document.write ("</td></tr></table>"); 
}

// check for printMode flag, if true display the close and clickToPrint buttons;
// this is the popup window for printing
if (infoSent.indexOf("pq-pf") != -1)
{
	document.write ("<table width='70' cellpadding='0' cellspacing='0'>");
	document.write ("<tr>");
	document.write ("<td width='70' height='25' valign='middle' align='center' class='bannerGrey'>"); 

	document.write ("<a href=javascript:location.replace(location.href) onClick='javascript:window.print()'><font size='-3'>Print</font></a>");
	document.write(" | ");
	document.write ("<a href=# onClick='javascript:closePopup(window.window)'><font size='-3'>Close</font></a>");

	
	document.write ("</td></tr></table>");
}

</script>

      
    

									</td>
								
