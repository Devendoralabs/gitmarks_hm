<script language="JavaScript">
<!-- hide from old browsers -->

function emailURL()
{ 
  // Pop up the email a friend form.

  var width = 360;
  var height = 500;
  var x = (screen.availWidth - width) / 2;
  var y = (screen.availHeight - height) / 2;

  
  
  
      
          
          
              
            
        
    
  

  
  var urlToPopup = "/eknec/PageQuerier.jhtml?pq-path="+ "953"+
                    "&emailurl=" + escape(window.location.href) +
                    "&emailtitle=" +
                    escape ("Top 10 Tips for Great Pictures");

  openPopup (urlToPopup, height, width);
}

var urlString = window.location.href;
if (urlString.indexOf("pq-pf") == -1)
{
  document.write ("<table width='70' cellpadding='0' cellspacing='0'>");
  document.write ("<tr>");
  document.write ("<td width='70' height='25' valign='middle' align='center' class='bannerGrey'>"); 

  document.write ("<a href=javascript:location.replace(location.href) onClick='javascript:emailURL()'>E-mail<img src='/eknec/documents/21/0900688a800a2e21/glo_0010_obj004.gif' border='0' align='center'></a>");

  document.write ("</td></tr></table>"); 
}


// hide from old browsers 
</script>



      
    

