    function fareindex_search(depCity, retCity, depMonthDay, retMonthDay, sessionId) {
        search_url_base = "validate_airsearch.php?";
        search_url_base += "Type=roundtrip";
        search_url_base += "&depCity="+depCity;
	search_url_base += "&retCity="+retCity;
	search_url_base += "&depMonth="+depMonthDay.substring(0,2);
	search_url_base += "&depDay="+depMonthDay.substring(2, 4);
	search_url_base += "&retMonth="+retMonthDay.substring(0,2);
	search_url_base += "&retDay="+retMonthDay.substring(2,4);
	search_url_base += "&NumTix=1";
	search_url_base += "&Class=economy";
	search_url_base += "&t=fareindex_modfront";
	search_url_base += "&sessionid="+sessionId;
	location.href=search_url_base;
