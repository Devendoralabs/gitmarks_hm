<script type="text/javascript">

var searchtype = '';
var widgettype = 's';

function create_sortable() {
    Sortable.create("searchcol1", {onUpdate:update_widgets_nowl, dropOnEmpty:true,containment:["searchcol1","searchcol2","searchcol3"],constraint:false,handle:'module-handle'});
    Sortable.create("searchcol2", {onUpdate:update_widgets_nowl, dropOnEmpty:true,containment:["searchcol1","searchcol2","searchcol3"],constraint:false,handle:'module-handle'});
    Sortable.create("searchcol3", {onUpdate:update_widgets_nowl, dropOnEmpty:true,containment:["searchcol1","searchcol2","searchcol3"],constraint:false,handle:'module-handle'});
}

function disable_sortable() {
    Sortable.destroy("searchcol1");
    Sortable.destroy("searchcol2");
    Sortable.destroy("searchcol3");
}

create_sortable();

</script>

    
