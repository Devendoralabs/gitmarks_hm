                    <script type="text/javascript">
                        //<![CDATA[
                        function submitSearch(){
                            if ($("cKeywords").value == "Search NYLS") {
                                $("cKeywords").value = "";
                            }
                        }

                        var checkSearch = function() {
                            $("cKeywords").value = ($F("cKeywords") == "") ? "Search NYLS" : $F("cKeywords");
                        }

                        var checkSearchFocus = function() {
                            $("cKeywords").value = ($F("cKeywords") != "Search NYLS") ? $F("cKeywords") : "";
                        }
                        //]]>
                    </script>

                    <div id="search">
                        <div id="search-box-left"></div>
                        <input class="search" type="text" onfocus="checkSearchFocus()" onblur="checkSearch()" name="q" id="cKeywords" value="Search NYLS" />
                        <div id="search-box-right"></div>
                        <input id="search-submit" type="submit" value="" />
                    </div>

