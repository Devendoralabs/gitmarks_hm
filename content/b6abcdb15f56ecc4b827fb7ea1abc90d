  <script type="text/javascript">
    //  Load accordions when window loads
    //Event.observe(window, 'load', accordion_init, false);
    
    var populateContent = function() {		
      accordionTimer = setTimeout("defineSelected()", 500);			
    };
  
    //	Set up all accordions
    function accordion_init() {
      // build an array of accordion objects
      $w('.vertical-container').each(function(name){
        createBaseAccordion(name);
      });    
      // Open the accordions passed in the nav
      accordions = pageAccordions;
      for(var i = 0; i < ids.length; i++) {
        
        for(var j = 0; j < accordions.length; j++) {
          if (accordions[j].accordionCode == ids[i]) {
            accordions[j].activate($(ids[i]));
            accordions[j].isLocked = true;
            accordions = accordions[j].childAccordions;
            break;
          }
        }
      }
      populateContent();
    }
  </script>
