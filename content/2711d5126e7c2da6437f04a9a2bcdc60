<script type="text/javascript">
var first_load = true;
var toggle_checks = true;
function toggleFollow()
{
	if ($('FollowSporcleFooter').hasClassName('minimized'))
	{
		moveFeeds(-50);
		$('FollowSporcleFooter').removeClassName('minimized');
	}
	else
	{
		moveFeeds(50);
		$('FollowSporcleFooter').addClassName('minimized');
	}
}
function moveFeeds(i)
{
	var currentBottom = parseInt($('FollowSporcleFooter').getStyle('bottom'));
	if (i == 0)
	{
		return;
	}
	else if (i > 0)
	{
		$('FollowSporcleFooter').style.bottom = (currentBottom - 2)+'px';
		i -= 2;
	}
	else if (i < 0)
	{
		$('FollowSporcleFooter').style.bottom = (currentBottom + 2)+'px';
		i += 2;
	}
	setTimeout(function() { moveFeeds(i); }, 1);
}
</script>
