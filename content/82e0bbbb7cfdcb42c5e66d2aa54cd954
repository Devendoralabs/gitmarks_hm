<script type="text/javascript"> 
// if a userGroup has not been set, set it, and set the google analytics var 
var signupGroup = readCookie("signupGroup");

//if this is safari, force to jsp signup mode
//var ua = navigator.userAgent.toLowerCase(); 
//if (ua.indexOf('safari/') != -1){ 
//	signupGroup = 'signup_jsp';
//}

if( signupGroup == null ) {
    //if( Math.random() > .5 ) {
    //    signupGroup = 'signup_lzx'; 
    //} else { 
    //    signupGroup = 'signup_jsp'; 
    //} 

    //We now force everyone to become an anonymous login user.... for now 
    signupGroup = 'signup_lzx'; 
    createCookie('signupGroup', signupGroup,730);
    pageTracker._setVar(signupGroup);
} 

if( signupGroup == 'signup_lzx' ) { 
	changeDivDisplay('signup_jsp_div', 'none' );	
	changeDivDisplay('signup_jsp_div_a', 'none' );	
	changeDivDisplay('signup_jsp_div_b', 'none' );	
	changeDivDisplay('signup_jsp_div_c', 'none' );	
	changeDivDisplay('signup_lzx_div', 'block' );
	changeDivDisplay('signup_lzx_div_a', 'block' );
	changeDivDisplay('signup_lzx_div_b', 'block' );
	changeDivDisplay('signup_lzx_div_c', 'block' );
} else { 
    changeDivDisplay('signup_jsp_div', 'block' );	
    changeDivDisplay('signup_jsp_div_a', 'block' );	
    changeDivDisplay('signup_jsp_div_b', 'block' );	
    changeDivDisplay('signup_jsp_div_c', 'block' );	
	changeDivDisplay('signup_lzx_div', 'none' );	
	changeDivDisplay('signup_lzx_div_a', 'none' );	
	changeDivDisplay('signup_lzx_div_b', 'none' );	
	changeDivDisplay('signup_lzx_div_c', 'none' );	
}

</script>

