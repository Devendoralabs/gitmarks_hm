	    function OLOnLoadFunctions()
	    {
	        fixImages();
	        setUpDiscussionPages();
	        setUpBlogPages();
	        setUpTwitterFeeds();

	        var version = getSilverlightVersion();
	        if (version) {
	            TrackPopup("SLVer=" + version);
	        }
	    }

	    function SignIn() 
	    {
	        // Blog SignIn
		    try {
		        wt_SignIn();
			    var sSignInHREF = document.getElementById('RpsLoginLink').href;
			    window.location = sSignInHREF;
		    }
		    catch(e)
		    {
			    //ignore error
		    }
		}

		//	Hidden Menu
		function change(id) {
		    ID = document.getElementById(id);

		    if (ID.style.display == "")
		        ID.style.display = "none";
		    else
		        ID.style.display = "";
		}

		function setCurrentNav() {
		    // server side hint for current category
		    //try
		    //{
		        var hint = '#ol-NavItem-concepttests';
		        if (hint != "") {
		            // this item is selected
		            $(hint).css("color", "#000000");
		            // show the parent
		            $(hint).parent().parent().show();
		        }
		        else {
		            // look at each element in the nav, find the one that has the current url
		            var pos = location.href.lastIndexOf("/") + 1;
		            // get the page name
		            var page = location.href.substr(pos, location.href.length - pos);
		            // remove the extension and everything after
		            if (page.indexOf('.') >= 0) {
		                page = page.substr(0, page.indexOf('.'));
		            }

                    // open up projects menu by default
		            var defaultPatt = /^default$/i;
		            if (defaultPatt.test(page)) {
		                $("#ol-Nav a:menuContainsNoCase('ConceptTests')").parent().parent().show();
		            } else {
		                // select the menu item that contains page
		                $('#ol-Nav a:menuContainsNoCase(' + page + ')').css("color", "#000000");
		                // we should expand this selection if possible
		                $('#ol-Nav a:menuContainsNoCase(' + page + ')').parent().parent().show();
		            }
		        }
		    //} catch {}
		}

		function setupHtmlNav() {
		    $('#ol-Nav a').click(function() {
		        var speed = 'fast';
		        // get current display mode
		        var current = $(this).parent().parent().children('ul').css('display');
		        
		        // close all, if this is a header click
		        if ($(this).parent().parent().attr('class') == 'ol-NavHeader')
		            $('#ol-Nav ul').slideUp(speed);
		            
		        if (current == "none")
		            $(this).parent().parent().children('ul').slideDown(speed);
		        else
		            $(this).parent().parent().children('ul').slideUp(speed);
		    });
		    // set curent nav state
		    setCurrentNav();
		}

		$(document).ready(function() {
		    // Custom jQuery filter for nav menu
		    $.expr[":"].menuContainsNoCase = function(el, i, m) {
		        var search = m[3];
		        if (!search) return false;
		        var txt = $(el).attr('href') + ',' + $(el).attr('altMenuPages')
		        return eval("/" + search + "/i").test(txt);
		    };
		    setupHtmlNav();

		    if (typeof (_spBodyOnLoadWrapper) != 'undefined')
		        _spBodyOnLoadWrapper();

		    OLOnLoadFunctions();

		    $('#RpsLoginLink').click(function() {
		        wt_SignIn();
		    });		    
		});        		
	</script>
