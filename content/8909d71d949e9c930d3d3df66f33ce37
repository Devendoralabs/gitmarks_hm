<script type="text/javascript">
    function getCookie(name) {
        if (document.cookie.length > 0) {
            start = document.cookie.indexOf(name + "=");
            if (start != -1) {
                start += name.length + 1;
                end = document.cookie.indexOf(";", start);
                if (end == -1) end = document.cookie.length;
                return unescape(document.cookie.substring(start, end));
            }
        }
        return "";
    }
    
    // attach the events for the blog and discussion links
    $(document).ready(function() {
        // attach events to capture rating metrics
        $("span[id*='ProjectRating_Star']").click(function(event) {
            // a rating was clicked
            var ratingid = $(this).attr('id');
            var rating = ratingid.charAt(ratingid.length - 1);
            wt_ProjectRating('pptPlex', rating);
        });

        SetupProjectDetailNavigation();

        // remove the vertical spacing from some webpart zones
        $('#ProjectLowerRightZone div.ms-PartSpacingVertical').remove();

        // force silverlight to recalc the size for the html scroller
        recalculateSize();
    });
    
    function onMediaError() {
    }   
</script>
