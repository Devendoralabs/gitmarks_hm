        if (linkUnits && linkUnits.length > 0) {
	        var google_link_units = linkUnits;
          
	        if (google_link_units.length > 0) {
	        
                if (feedbackUrl > '') {
                    relatedSearches += '<div class="heading"><a style="text-decoration:none;color:#FFF;" TARGET="_blank" href="' + feedbackUrl + '" >Related Searches</a></div>';
                }
                else {
		            relatedSearches += '<div class="heading">Related Searches</div>';
                }
		        
		        relatedSearches += '<div id="relatedsearches">';
		        relatedSearches += '<table border="0" cellspacing="0" cellpadding="0" width="100%">';
        		
		        var columnCount = parseInt(2);
		        var columnWidth = '50%';
        		
		        if (columnCount == 4)
		        {
			        columnWidth = '25%';
		        }
        		
		        for (var i=0; i<google_link_units.length; i++) {
        			
			        if (i % columnCount == 0)
			        {
				        relatedSearches += '<tr>';
			        }
        			
			        relatedSearches += '<td width="' + columnWidth +'" valign="top">';
			        relatedSearches += '<table border="0" cellpadding="0" cellspacing="0">';
			        relatedSearches += '<tr>';
