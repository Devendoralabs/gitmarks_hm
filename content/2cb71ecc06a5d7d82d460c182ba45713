function processClick(location) {
window.open(location);
}
var google_afd_request = {
  client: 'ca-dp-godaddy26_3ph_xml',  channel: '131219',
  domain_name: 'metrospark.com',
  hl: 'en',
  num_radlinks: '10',
  s: 'METROSPARK.COM'
}
var handler = '/ct.html?';
var handler2 = 'http://metrospark.com';
var ads = '';
var popularCategories = '';
var relatedSearches = '';
var kotValue = '';
var searchBox = '';
var bottomSearchBox = '';

var linkUnits;
var popSearches;
var google_ads;
var feedbackUrl;

var loc1 = handler2 + '?1c6da500662cb848c2de8c6a5a7eb38c=325467165&6fa970d50480e816942b6fab7c4e7a23=506683626&5f17a83ba43604eb3d1c06f73d82e119=634284335742428257';
var loc2 = handler2 + '?0f41fdd8c9af62a4027fda82d597062a=1623525913&1c6da500662cb848c2de8c6a5a7eb38c=2344573&f67ff3454420ebb255fe4f673cf8cf0b=AwZ0Zwt0ZmZ1AmDlAQV4ZwD0-1';

function google_afd_ad_request_done(google_afd_response) {
    if (google_ads == undefined || google_ads == null) {
        google_ads = google_afd_response.ads;    
    }
    if (linkUnits == undefined || linkUnits == null) {    
        linkUnits = google_afd_response.link_units;
    }
    if (popSearches == undefined || popSearches == null) {
        popSearches = google_afd_response.categories;
    }
    
    if (google_afd_response.faillisted) {
        window.location.href = loc1;
        return;
    }
   
    if (google_afd_response.needsreview) {
        window.location.href = loc2;
        return;
    }
   
    if (google_afd_response.search_token)
    {
        kotValue = '<input type="hidden" name="kot" value="' + google_afd_response.search_token +'"/>';
    }
   
    if (google_afd_response.adult && google_afd_response.adult == 'false' && kotValue != '') {	
        searchBox += '<div id="search1" class="border">';
        searchBox += '<span class="searchtext">Search:</span><br />';
        searchBox += '<form name="search" action="/default.html" method="post" style="padding:0; margin:0"><input type="text" name="q" style="width:240px" />' + kotValue + '&nbsp;<input type="submit" value="Search" /></form>';
        searchBox += '</div>';
        searchBox += '<div><img src="http://images-pw.secureserver.net/images/061703/spc_trans.gif" border="0" height="3"></div>';
        
        if (google_ads && google_ads.length > 7)
        {
            bottomSearchBox += '<div><img src="http://images-pw.secureserver.net/images/061703/spc_trans.gif" border="0" height="3"></div>';
            bottomSearchBox += '<div id="search2" class="border">';
            bottomSearchBox += '<span class="searchtext">Search:</span><br />';
            
                bottomSearchBox += '<form name="bsearch" action="/default.html" method="post" style="padding:0; margin:0"><input type="text" name="q" style="width:240px" />' + kotValue + '&nbsp;<input type="submit" value="Search" /></form>';
            
		    bottomSearchBox += '</div>';
		}
    }
          
    if (google_afd_response.feedback_url > '' && '1' == '1'){
        feedbackUrl = google_afd_response.feedback_url;
    }    
    
    if (!(linkUnits && linkUnits.length > 0))
    {
        if (document.getElementById('contentrs') != null) {       
            document.getElementById('contentrs').style.display = 'none';
        }
    }
    
    if (!(google_ads && google_ads.length > 0))
    {
        if (document.getElementById('contentbottom') != null) {
            document.getElementById('contentbottom').style.display = 'none';
        }
        
        if (document.getElementById('contentmiddle') != null) {
            document.getElementById('contentmiddle').style.display = 'none';
        }
        
        if (document.getElementById('contentbottom2') != null) {
            document.getElementById('contentbottom2').style.display = '';
        }
        
        bottomSearchBox = '';
    }
    else 
    {
        if (document.getElementById('contentbottom2') != null) {
            document.getElementById('contentbottom2').style.display = 'none';
        }
    }
}
