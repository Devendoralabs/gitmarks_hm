			        relatedSearches += '<td style="padding-left:2px;padding-right:2px;" valign="top"><a id="rs' + i.toString() + '" href="" title="Search For:' + google_link_units[i].term + '">' + google_link_units[i].term + '</a></td>';
			        relatedSearches += '</tr>';
			        relatedSearches += '</table>';
			        relatedSearches += '<script type="text/javascript" language="javascript">';
			        relatedSearches += 'var rs' + i.toString() + ' = document.getElementById(\'rs' + i.toString() + '\');';
			        relatedSearches += 'if(rs' + i.toString() + ') {rs' + i.toString() + '.href = \'/?q=' + encodeURIComponent(google_link_units[i].term) + '&srcht=r&kot=' + google_link_units[i].token + '\';}';
			        relatedSearches += '<\/script>';			
			        relatedSearches += '</td>';
        			
			        if (i % columnCount == (columnCount - 1)) {
				        relatedSearches += '</tr>';
				        relatedSearches += '<tr>';
				        relatedSearches += '<td colspan="' + columnCount.toString() + '"><div><img src="http://images-pw.secureserver.net/images/061703/spc_trans.gif" border="0" height="6"></div></td>';
				        relatedSearches += '</tr>';
			        }
		        }
        		
		        if (google_link_units.length % columnCount == 1) {
			        relatedSearches += '<td></td></tr>';
		        }
		        else if (google_link_units.length % columnCount == 2) {
			        relatedSearches += '<td></td><td></td></tr>';
		        }
		        else if (google_link_units.length % columnCount == 3) {
			        relatedSearches += '<td></td><td></td><td></td></tr>';
		        }
        		
		        relatedSearches += '</table>';
		        relatedSearches += '</div>';
	        }
        }
        
        document.write(relatedSearches);
