<script type="text/javascript">
    //<![CDATA[

    $(function(){
        setToggleBannerInterval()
    });

    function setToggleBannerInterval()
    {
        window.setInterval(function(){ toggleImages(); }, 10000);
    }

    function toggleImages()
    {
        var $match1 = $('#divBanners').children().eq(0);
        var $match2 = $('#divBanners').children().eq(1);

        if ($match1.css('display') == 'none' )
        {
            // swap matched elements
            var $tmp = $match2;
            $match2  = $match1;
            $match1 = $tmp;
        }

        $match2.fadeIn(400);
        $match1.fadeOut(400);
    }

    //]]>
</script>

