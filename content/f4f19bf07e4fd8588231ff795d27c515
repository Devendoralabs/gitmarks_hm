<!--Hide JavaScript from Java-Impaired Browsers

function confirmsubmit(text, formname, delcmd){
    var OK = confirm(text);

    if(OK == true){
        eval('document.'+formname+'.'+delcmd+'.value = 561;');
        eval('document.'+formname+'.submit();');
    } else {
        return false;
    }
}

function selectpop (name, fn) {
	eval('document.'+fn+'.pop'+name+'.value = 1;');
	eval('document.'+fn+'.submit();');
}

function chkboxtoggle (name) {
	if (document.getElementById(name).checked == true) {
		document.getElementById(name).checked = false;
	} else {
		document.getElementById(name).checked = true;
	}
}

function win (url, h, w) {
	popwin = window.open(url,"sjtwin",'toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=1,resizable=1,width='+w+',height='+h);
}

function opencal(field, form) {
    mywin = window.open("calendar.php?field="+field+"&form="+form,"cal",'toolbar=0,location=0,directories=0,status=0,menubar=0,scrollbars=0,resizable=1,width=212,height=192');
}

function passDate(field, form, date){
	eval("window.opener.document."+form+"."+field+".value = '"+date+"';");
	window.close();  
}

function serviceCompleteCheck (field, check, form, val) {
	eval("var hasVal = document."+form+"."+field+".value;");
	
	if (document.getElementById(check).checked == true) {
		if (!hasVal) {
			eval("document."+form+"."+field+".value = '"+val+"';");
		}
	} else if (document.getElementById(check).checked == false) {
		if (hasVal) {
			eval("document."+form+"."+field+".value = '';");
		}
	}
}

function show (id) {
    obj = document.getElementById(id);
    obj.style.display="";

}

function hide (id) {
    obj = document.getElementById(id);
    obj.style.display="none";
}

function vis_toggle (o, id) {
	obj = document.getElementById(id);

	if (obj.style.display == "none") {
		show(id);
	} else {
		hide(id);
	}
}

function show_mouse_pos (obj, vis_show) {
	
	if (vis_show == true) {
		show(obj);
		mouse_pos(obj);
	} else {
		hide(obj);
	}
	
}

function expand (o, obj, lf) {
	class_toggle(o, 'collapse', '');
	class_toggle(obj, 'details', 'hidden');
	$(obj).innerHTML = 'Please wait... Loading...';

	if (lf == true) {
		$(obj).style.left = '-239px';
	}

	var query = 'schedule.php';
	var pars = 'details=true&y_m_d=' + obj.replace("date-", "");

	var myAjax = new Ajax.Updater(
				{success: obj }, 
				query, 
				{
					method: 'get', 
					parameters: pars, 
					onFailure: reportError
				});	
	
}

function reportError(request)
{
	alert('Sorry. There was an error processing your request.  Please try again.');
}

function class_toggle (obj, class_name, reset_class_name) {
	if ($(obj).className == class_name) {
		reset_class(class_name, reset_class_name);
		$(obj).className = reset_class_name;
	} else {
		reset_class(class_name, reset_class_name);
		$(obj).className = class_name;
	}
}

function reset_class (class_name, reset_class_name) {
	var obj = document.getElementsByClassName(class_name, reset_class_name);
	for(i = 0; i < obj.length; i++) {
		obj[i].className = reset_class_name;
	}
}

function showOnlyClass (class_name) {
	hideClass('bar');
	showClass(class_name);
}

function chkBoxToggleClass (o, class_name) {
	if (o.checked) {
		showClass(class_name);
	} else {
		hideClass(class_name);
	}
}

function hideClass (class_name) {
	var obj = document.getElementsByClassName(class_name);
	for(i = 0; i < obj.length; i++) {
		obj[i].style.display = 'none';
	}
}

function showClass (class_name) {
	var obj = document.getElementsByClassName(class_name);
	for(i = 0; i < obj.length; i++) {
		obj[i].style.display = 'block';
	}
}

function resetBoardList () {
	obj = document.getElementsByClassName('board');
	for(i = 0; i < obj.length; i++) {
		obj[i].className = 'board';
	}	
}

// End Hiding -->
