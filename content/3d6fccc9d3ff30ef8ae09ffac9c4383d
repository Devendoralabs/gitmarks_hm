 <script>
		
		function setLightAndTrack(_slot,_on,_track)
		{
			var h = document.getElementById("s"+_slot);
			if (h)
			{
						var col = "#222222";
						if (_on==1)
								col ="#FF0000";
						h.style.backgroundColor=col;
			}
			var h = document.getElementById("track"+_slot);
			if (h)
				h.innerHTML=_track;
		}
		
		function updateStatus()
		{
			var  pStatus =activeGS.command('status');
			if (typeof pStatus.error != 'undefined') return ;
			
			setLightAndTrack("51",pStatus.lights["slot51"].on,pStatus.lights["slot51"].track);
			setLightAndTrack("52",pStatus.lights["slot52"].on,pStatus.lights["slot52"].track);
			setLightAndTrack("61",pStatus.lights["slot61"].on,pStatus.lights["slot61"].track);
			setLightAndTrack("62",pStatus.lights["slot62"].on,pStatus.lights["slot62"].track);
			var sp= pStatus.speed.estimated;
			var h = document.getElementById("speed");
			if (h)
				h.innerHTML=sp;
			var pa= pStatus.pause;
			h = document.getElementById("pause");
			if (h)
				h.innerHTML=pa;
		
			setTimeout(updateStatus,500);
		}
		
			function showSlots()
		{
			
			var h = document.getElementById("images");
			if (!h) return ;
			
			var v = activeGS.command('runtime');			
			if (typeof v.error != 'undefined')
			{
				alert(v.error);
				 return ;
			}
			str = "";
			for(i = 0; i< v.slots.length; i++)
			{
				var s = v.slots[i];
				str+=s.slot+":"+s.name+"("+s.status+")";
				str+="<br />";
			}
			
			h.innerHTML = str;
		}
		
		updateStatus();

</script>  


</td></tr>

<!--- End of Buttons -->

</center>
</tbody></table></div></div>
</div>


