	<script>
		
	
		function retrieveCommands()
		{
			
			var v = activeGS.command('help');			
			if (typeof v.error != 'undefined') 
			{
				alert(v.error);
				return ;
			}
			str = "";
			for(i = 0; i< v.commands.length; i++)
			{
				var c = v.commands[i];
				str+="<input type="button" onclick="document.getElementById('statuswindow').innerHTML=JSON.stringify(activeGS.command('"+c+"'))" value=""+c+"" />";
			}

			var h=document.getElementById("commands");
			if (h)
				h.innerHTML = str;
		}
	</script>


