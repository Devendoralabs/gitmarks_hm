<script type="text/javascript">
$(function() {
	jQuery.fn.toggleText = function(a, b) {
		return this.each(function() {
			jQuery(this).text(jQuery(this).text() == a ? b : a);
		});
	};
	
	$("#searchBox label.fromDate input").datepicker({maxDate: '-1'});
	$("#searchBox label.toDate input").datepicker({maxDate: new Date()});
	
	if (!$(".news.search").length) {
		$("#searchBox .advanced").hide();
		$("#searchBox .advanced-toggle").click(function() {
			$(this).toggleClass("toggled");
			$(this).toggleText("Simple Search", "Advanced Search")
			$("#searchBox .advanced").slideToggle();
		});		
	}
});
</script>

