            <script type="text/javascript">
//<![CDATA[
    
    google_skip_num = 0;

    function render_ad(ad){
        if (ad.type == 'text/narrow'){
            return  '<a class="ad-line1-link" onmouseover="javascript:window.status=\'' + ad.url + '\';return true;" ' +
                        'onmouseout="javascript:window.status=\'\';return true;" ' +
                        'href="' + ad.url + '">' +
                    '<span class="ad-line1">' + ad.line1 + '</span></a><br>' +
                    '<span class="ad-text">' + ad.line2 + '</span><br>' +
                    '<span class="ad-text">' + ad.line3 + '</span><br>' +
                    '<a style="text-decoration:none" onmouseover="javascript:window.status=\'' + ad.url + '\';return true;" ' +
                        'onmouseout="javascript:window.status=\'\';return true;" ' +
                        'href="' + ad.url + '">' +
                    '<span class="ad_url">' + ad.visible_url + '</span></a><br /><div style="height:10px">&nbsp;</div>';
        } else if (ad.type == 'text'){
            return '<div class="ad-div">'+
                '<a class="ad-line1-link" onmouseover="javascript:window.status=\'' + ad.url + '\';return true;" ' +
                    'onmouseout="javascript:window.status=\'\';return true;" ' + 'href="' + ad.url + '">' +
                '<span class="ad-line1">' + ad.line1 + '</span></a>' +
                '<a class="ad-url-link" onmouseover="javascript:window.status=\'' + ad.url + '\';return true;" ' +
                    'onmouseout="javascript:window.status=\'\';return true;" ' + 'href="' + ad.url + '">' +
                '<span class="ad-url" style="text-decoration:none;">' + ad.visible_url + '</span></a><br />'+
                '<span class="ad-text" style="text-decoration:none;">' + ad.line2 + '</span> '+
                (ad.line3 ?
                    '<span class="ad-text" style="text-decoration:none;">' + ad.line3 + '</span></div>' :
                    '</div>'
                );
        } else if (ad.type == 'image') {
            return "<a href=\'" + google_ads[0].url + "\' style=\'text-decoration: none\'><img src=\'" + google_ads[0].image_url + "\' border=\"0\" /></a>"
        }
    }
    
    function google_ad_request_done(google_ads){
        var google_num_ads = google_ads.length;
        var open = "";
        var close = "";
        var isImage = false;
        
        if (google_ads.length == 0){
            open = '';
            close = '';
        } else if (google_ads[0].type == "image") {
            isImage = true;
            open = "<div align='left'><div style=\"width:300px;margin:5px 0 8px 0;\"><h3 class=\"ad-heading-div\">" +
                    '<a class="google_headings" href="'+ google_info.feedback_url +'">Ads by Google</a></h3>';
            close = "</div></div>";
        } else {
            open = '<h3 class="ad-heading-div"><a class="google_headings" href="'+google_info.feedback_url+'">Ads by Google</a></h3>';
            close = '<div style="height:10px">&nbsp;</div>';
        }
        
        var index = 0;
        for (section in ads_counts){
            section_id = ads_counts[section][0];
            section_count = ads_counts[section][1];
             
            if (index >= google_num_ads){
                $("#ads-section-" + section_id).hide();
                continue;
            } 
            
            var ads = "";
            var end = Math.min(index + section_count, google_num_ads);
            for(;index < end; index++){
                ads += render_ad(google_ads[index]);
            }
            if (ads != ""){
                ads = open + ads + close;
                $("#adsense_" + section_id).html(ads);
                $("#ads-section-" + section_id).show();
            }
        }
        google_skip_num = google_skip_num + google_ads.length;
    }
//]]>
</script>


<script type="text/javascript">
//<![CDATA[
    
    ads_counts = [ ['185137', 3] ]
    
    google_ad_client = 'pub-1812638130783065';
    google_hints = "How to Write a Resume";
    google_ad_channel = '' + '0390727813';
    google_ad_output = 'js';
    google_max_num_ads = '3';
    google_ad_type = 'text';
    google_feedback = 'on';
    google_skip = 0;
    
//]]>
</script>
