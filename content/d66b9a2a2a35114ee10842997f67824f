	<!-- ## FLASH TRACKING : this allows adding of any kind of external tracking without having to mess with Flash -->
		<script type="text/javascript">
			function flashTrack(op1_a, op2_a, op3_a, op4_a) {
				var tStr = "/"+op1_a;
				if (op2_a && op2_a != "") tStr += "/" + op2_a;
				if (op3_a && op3_a != "") tStr += "/" + op3_a;
				if (op4_a && op4_a != "") tStr += "/" + op4_a;
				pageTracker._trackPageview(tStr);
				
				var tDiv = document.getElementById("trackOut");
				if (tDiv) tDiv.innerHTML = "<b>Flash Track</b>&nbsp;&nbsp;("+tStr+")";
			}
			function showTracking() {
				var tDiv = document.getElementById("trackOut");
				tDiv.style.display = "";
			}
			
		</script>

	
	<!-- ## OMNITURE TRACKING -->
