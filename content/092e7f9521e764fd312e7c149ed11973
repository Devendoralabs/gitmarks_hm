	            <div id="twitter_featured_accounts" class="featured_accounts">           	
	            	<p class="featuring">
	                    Featured Accounts:
	                    </p>
	                	<script language="JavaScript">
	                	//Acount Name / Image References
	                	var vAccountNames  = new Array("StephenAtHome","ColbertReport");
	                	var vAccountImages = new Array("http://a1.twimg.com/profile_images/568525297/Picture_normal.png","http://a1.twimg.com/profile_images/1111488021/Colbert_TwitterProfile_sq_normal.jpg");
	                	
							for(var i=0; i<vAccountNames.length;i++){
								 document.writeln("<a href=\"javascript:void(0);\" class=\"featured_account\" onclick=\"loadTweets('"+vAccountNames[i]+"', 'from:'); return false;\">");
								 if(vAccountImages[i] == ""){
								 	 document.writeln("<span class=\"featured_name\">@"+vAccountNames[i]+"</span></a>");
								 }else{
									 document.writeln("<img id=\"feature_img_"+ (i+1) +"\" width=\"28\" height=\"28\" src=\""+vAccountImages[i]+"\" />");
									 document.writeln("<span class=\"featured_name\">"+vAccountNames[i]+"</span></a>");
								 }
							}
	                	
 		               	</script>
	            </div>
	            <div class="trending_row2">
