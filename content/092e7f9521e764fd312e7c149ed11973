<!-- typeahead function -->
<script type="text/javascript">
	
	$(document).ready(function () {
		var url = "http://www.colbertnation.com/autocomplete/q";
		$(".typeahead").autocomplete(url, {
			minChars:1,
			width:204,
			autoFill:false,
			scrollHeight:250,
			scroll:true,
			max:10,
			selectFirst:false,
			cacheLength:0,
			matchCase:0,
			matchSubset:1,
			matchContains:0,
			mustMatch:0,
			lineSeparator:"\n",
			cellSeparator:"|",
			delay:100,
			extraParams:{siteName : "colbertnation" },
			formatItem:function (row, i, n, max) {
				return getValue(max, "Term:");
			},
			formatResult: function(row, max) {
				return getValue(max, "Term:");
			}
		}).result(function(event, row, max) {
			var url = getValue(max, "URL:");
			if (url == max) {
				return;
			}
			if (url.length != 0) {
				location.href = url;
			}
		});
	});
	
</script>

