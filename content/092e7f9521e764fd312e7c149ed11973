<script>
    function subscribeUserNewsletter(email, newsletter, month, day, year, zip) {
        // clear previous error/success messages
        $('.subscribe_error').text("");
        //set dob
        var dob = month + "/" + day + "/" + year
        // validate email address
        if (!email || email.toLowerCase() == "enter your email address" || !validateEmail(email)) {
            $('.subscribe_error').text("Please enter a valid e-mail address.");
        } else if(!checkDOB(dob)) {
			if (month == 'MONTH') {
				$('.subscribe_error').text("Please select a month.");
			} else if (day == "DAY") {
				$('.subscribe_error').text("Please select a day.");
			} else if (year == "YEAR") {
				$('.subscribe_error').text("Please select a year.");
			} else {
			    $('.subscribe_error').text("Please select a valid date of birth."); 	
			}
        } else if (checkAge(month, day, year) == false) {
            $('.subscribe_error').text("We're sorry! We can not process your request. Registration is limited to individuals 18 & over.");    
        } else if (!validateZIP(zip)) {
            $('.subscribe_error').text("Please enter a valid ZIP Code.");
        } else {
            $('.subscribe_error').text("submitting request...");
            var rand = Math.floor(Math.random()*1111111111);
            var url = "/subscribe_user?email="+email+"&newsletter="+newsletter+"&dob="+dob+"&zip="+zip+"&r="+rand;
            $.post(url,"",subscribe_status,"text");
        }
        return false;
    }
	
	function validateEmail(emaillAddress) {
	    var reg = /^([A-Za-z0-9_\-\.])+\@([A-Za-z0-9_\-\.])+\.([A-Za-z]{2,4})$/;
	    var address = emaillAddress;
	    return reg.test(address);
	}
	
	function validateZIP(zip) {
	    var reg = /^([0-9]{5})$/;
	    var zipcode = zip;
	    return reg.test(zipcode);
	}
    
    function checkDOB(dob) {
        var reg = /^([0-9]{2})+\/([0-9]{2})+\/([0-9]{4})$/;
        var dateofbirth = dob;
        return reg.test(dateofbirth);
    }
    
    function checkAge(month,day,year){
        year = parseInt(year);
        month = parseInt(month) - 1;
        day = parseInt(day);
        
        var min_age = 18;       /* the minumum age you want to allow in */
        var theirDate = new Date((year + min_age), month, day);
        var today = new Date;
    
        if ((today.getTime() - theirDate.getTime()) < 0) {
            return false;
        } else {
            return true;
        }
    }
    
    function subscribe_status(responseText, textStatus, XMLHttpRequest){
        if (responseText.indexOf("OK") > -1){
            $(".subscribe_error").text("Thanks for subscribing!");
			mtvn.btg.Controller.sendLinkEvent ({
                linkName:'colbertnation_newsletter', 
                linkType:'o',
                eVar19:'colbertnation_newsletter_signup_success', 
                events:'event8'
            });
        } else if (responseText.indexOf("AGE") > -1) {
            $(".subscribe_error").text("You are not eligible to sign up at this time")
        } else {
            $(".subscribe_error").text("There was a problem, please try again");
        }
    }
</script>

															
	
	
	
