<script language="javascript" type="text/javascript">
	var loggedIn = false;
    var refreshInterval = 60*1000;
	var letters_count = 140;
	var currentQuery = "StephenAtHome,ColbertReport";
	var currentQueryArray = currentQuery.split(",");
	
		
			var siteName = "col";
		
	

	var currentTypecode = "";
	$(document).ready(function() {
	    $('.trending a').each(function(){
	    	var query = $(this).text();
	    	var typecode = query.substr(0,1);
	    	query=query.substr(1);
	    	if (typecode == "#"){ 
	        	typecode="%23";
	    	}else if(typecode == "@"){
	    		typecode = "%10";
	        }else{ 
	        	typecode = "";
	    	}
	        $(this).click(function(){
	        	currentQuery = query;
	        	currentTypecode = typecode;
	            loadTweets(query, typecode);
	            return false;
	        });
	   	});
	   	
	   	loadTweets();
	   	
	   	if (getCookie('oauth_token')) twitterShowPostView();
	});
	
	function loadTweets(query, typecode) {
		if (query == undefined || typecode == undefined) {
            var tweetUrl = "http://search.twitter.com/search.json?callback=?&q=from%3A" + currentQueryArray[0];
		} else {
			var tweetUrl = "http://search.twitter.com/search.json?callback=?&q=" + typecode + query;
		}
	
        $(".twitter-feed").empty();
        $.ajaxSetup({scriptCharset: "utf-8"});
        $.getJSON(tweetUrl, function(data){
			$.each(data.results, function(i, tweet){
                var tweetPic = tweet.profile_image_url;
                var twitterUserName = tweet.from_user;
                var text = tweet.text.replace(/(http:\/\/\S+)/g, '<a target="_blank" href="$1">$1</a>').replace(/\B#([_a-z0-9]+)/ig, function(hashtag){
                    return '<a target="_blank" href="http://twitter.com/search?q=%23' + hashtag.substring(1) + '">' + hashtag + '</a>';
                }).replace(/\B@([_a-z0-9]+)/ig, function(reply){
                    return '<a target="_blank" href="http://twitter.com/' + reply.substring(1) + '">' + reply.charAt(0) + reply.substring(1) + '</a>';
                });

                $(".twitter-feed").append('<li class="tweet"><img class="tweetPic" src="' + tweetPic + '" width="28" height="28"/> <div class="tweetText"><span class="twitterUserName">' + twitterUserName + '</span> ' + text + '<br/>' + relative_time(tweet.created_at) + '</div></li>');
            });
        });
	}

	function relative_time (time_value) {
        var b = new Date();
        var c = new Date(time_value);
    
        var d = b - c;
        var e = 1000;
        var minute = e * 60;
        var hour = minute * 60;
        var day = hour * 24;
        var week = day * 7;
        if (isNaN(d) || d < 0) {
            return ""
        }
        if (d < e * 7) {
            return '<span class="tweetDate">right now</span>';
        }
        if (d < minute) {
            return '<span class="tweetDate">' + Math.floor(d / e) + " seconds ago</span>";
        }
        if (d < minute * 2) {
            return '<span class="tweetDate">about 1 minute ago</span>';
        }
        if (d < hour) {
            return '<span class="tweetDate">' + Math.floor(d / minute) + ' minutes ago</span>';
        }
        if (d < hour * 2) {
            return '<span class="tweetDate"> about 1 hour ago</span>';
        }
        if (d < day) {
            return '<span class="tweetDate">' + Math.floor(d / hour) + ' hours ago</span>';
        }
        if (d > day && d < day * 2) {
            return "yesterday"
        }
        if (d < day * 365) {
            return '<span class="tweetDate">' + Math.floor(d / day) + ' days ago</span>';
        } else {
            return '<span class="tweetDate">over a year ago</span>';
        }
    }

	function lettersCount() {
        var twitter_message = $(".follow textarea[name='twitter_message']")[0].value;
        letters_count = 140 - twitter_message.length;
        $(".follow #twitter_letters_left").text(letters_count);
    }
    
	function twitterPostTweet() {
        var msg = $(".follow textarea[name='twitter_message']")[0].value;
        var url = 'http://www.colbertnation.com/fragments/follow/twitter_post_tweet/341766';
        //alert(msg);
        $.post(url, {msg:msg}, tweetPostResult, "text");
	}
    
    function tweetPostResult(response){
        if(response) {
            $(".follow textarea[name='twitter_message']")[0].value = currentQueryArray[0];
            setTimeout(loadTweets,20000);
        }
    }
    
    function switchTabs(activeTab) {
        $(".tab_content").hide();
        $(".tabs > div").removeClass("selected");
        $(".tabs > div."+activeTab+"_tab").addClass("selected");
        $("#"+activeTab+"_tab_content").show();
    }
    
    function twitterShowPostView() {
        $.ajax({
          url: "http://www.colbertnation.com/fragments/follow/twitter_post/341766",
          type:"GET",
          cache: false,
          success: function(data){
            $(".twitterForm").html(data);
            loggedIn = true;
          }
        });
    }
    
    function loginClick() {
        var win = window.open(
            'http://www.colbertnation.com/fragments/follow/twitter_token/341766', 
            'OAuthTwitterRequest', 
            'menubar=no,location=no,resizable=no,scrollbars=yes,status=no,width=800,height=400'
        );
        return false;
    }
    
    function getCookie(c_name) {
        if (document.cookie.length>0) {
            c_start=document.cookie.indexOf(c_name + "=");
            if (c_start!=-1) {
                c_start=c_start + c_name.length+1;
                c_end=document.cookie.indexOf(";",c_start);
                if (c_end==-1) c_end=document.cookie.length;
                return unescape(document.cookie.substring(c_start,c_end));
            }
        }
        return false;
    } 
    
    loadTweets();
    window.setInterval("loadTweets();", refreshInterval);
    if (getCookie("oauth_token")) { twitterShowPostView(); }
	
	function sendFollowClick(link,event) {
		var lName = 'tfbm|' + pageName + '|' + event;
		mtvn.btg.Controller.sendLinkEvent({linkName:lName,linkType:'o',eVar9:pageName});
	}
	
	$(document).ready(function() {
		//Omniture Handlers
		$(".twitter-feed a").each(function() {
			$(this).click(function() {
				sendFollowClick(this,'tfbm_' + siteName + '_twitter_messageclick');
			});
		});	
		$(".login_button").click(function() {
			sendFollowClick(this,'tfbm_' + siteName + '_twitter_login');
		});
		$(".featured_account a").each(function() {
			$(this).click(function() {
				sendFollowClick(this,'tfbm_' + siteName + '_featured_account');
			});
		});
		$(".trending_topics a").each(function() {
			$(this).click(function() {
				sendFollowClick(this,'tfbm_' + siteName + '_twitter_trend');
			});
		});
		$(".trending_accounts a").each(function() {
			$(this).click(function() {
				sendFollowClick(this,'tfbm_' + siteName + '_twitter_accounts');
			});
		});
		$(".twitter_tab").click(function() {
			sendFollowClick(this,'tfbm_' + siteName + '_twitter_tab');
		});
		$(".facebook_tab").click(function() {
			sendFollowClick(this,'tfbm_' + siteName + '_fb_tab');
		});
		$(".tweetPostBtn").click(function() {
			sendFollowClick(this,'tfbm_' + siteName + '_twitter_post');
		});

		//Highlight Handlers
		$('.featured_accounts').mouseenter(function() {
			//Featured - Enabled
			$(this).css('background-color', '#e0eef1');
			$('.featuring,.featured_accounts .featured_name').css('color', '#000000');
			$('.featured_accounts a img').fadeTo(0,1);
			//Treading - Dimmed
			$('.trending_row2').css('background-color', '#F9F9F9');	
			$('.trending_topics p a,.trending_accounts p a').css('color','#8b8686');
			//Tweet - Dimmed
			$('.twitterForm').css('background-color', '#F9F9F9');
			$('.trending,.twitterLoginName,.twitterMsgArea,.twitterLetters').css('color','#8b8686');	
			$('.twitterPost .twitterProfileImg,.twitterPost .tweetPostBtn').fadeTo(0,0.5);
			return false;
		});
		$('.trending_row2').mouseenter(function() {
			//Featured - Dimmed
			$('.featured_accounts').css('background-color', '#F9F9F9');
			$('.featuring,.featured_accounts .featured_name').css('color','#8b8686');
			$('.featured_accounts a img').fadeTo(0,0.5);

			//Trending - Enabled
			$(this).css('background-color', '#e0eef1');
			$('.trending').css('color','#000000');
			$('.trending_topics p a,.trending_accounts p a').css('color','#013E9E');
			
			//Tweet - Dimmed
			$('.twitterForm').css('background-color', '#F9F9F9');	
			$('.twitterLoginName,.twitterMsgArea,.twitterLetters').css('color','#8b8686');	
			$('.twitterPost .twitterProfileImg,.twitterPost .tweetPostBtn').fadeTo(0,0.5);
			return false;
		});

		$('.twitterForm').mouseenter(function() {
			
			
			//Featured - Dimmed
			if(loggedIn==true){
				$('.featured_accounts').css('background-color', '#F9F9F9');
				$('.featuring,.featured_accounts .featured_name').css('color','#8b8686');
				$('.featured_accounts a img').fadeTo(0,0.5);
			}else{
				$('.featured_accounts').css('background-color', '#e0eef1');
				$('.featuring,.featured_accounts .featured_name').css('color', '#000000');
				$('.featured_accounts a img').fadeTo(0,1);
			}
			//Trending - Dimmed
			$('.trending_row2').css('background-color', '#F9F9F9');	
			$('.trending,.trending_topics p a,.trending_accounts p a').css('color','#8b8686');
			//Tweet - Enabled
			$('.twitterPost .twitterProfileImg,.twitterPost .tweetPostBtn').fadeTo(0,1);
			$('.twitterLoginName,.twitterMsgArea,.twitterLetters').css('color','#000000');	
			if(loggedIn==true){
				//only highlight twitter post if logged in
				$(this).css('background-color', '#e0eef1');
			}
			return false;
		});

		$('.sub_feed').mouseleave(function(){
			//Featured - Enabled
			$('.featured_accounts').css('background-color', '#e0eef1');
			$('.featuring,.featured_accounts .featured_name').css('color', '#000000');
			$('.featured_accounts a img').fadeTo(0,1);
			//Trending - Enabled
			$('.trending_row2').css('background-color', '#F9F9F9');
			$('.trending').css('color','#000000');
			$('.trending_topics p a,.trending_accounts p a').css('color','#013E9E');
			//Tweet - Enabled
			$('.twitterPost .twitterProfileImg,.twitterPost .tweetPostBtn').fadeTo(0,1);
			$('.twitterLoginName,.twitterMsgArea,.twitterLetters').css('color','#000000');	
			$('.twitterForm').css('background-color', '#F9F9F9');
			return false;
		});
		
	});
</script>

