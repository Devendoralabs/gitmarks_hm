						<script type="text/javascript">	
			// It's necessary to guard against multiple subscribes when more than one like button is called thru Flux
			// It seems Flux calls this function multiple times
/////@todo Need to find a way to create & destroy each event
			window.indexOfLikeSubscribe = null;
						
			window.fbAsyncInit = function() {
				if (window.indexOfLikeSubscribe == null) {
					//if (typeof FB.Event._subscribersMap["edge.create"] == "undefined") { // removed because other subscribes are in there from Flux
						window.FB.Event.subscribe('edge.create', function(href, widget) {
							mtvn.btg.Controller.sendLinkEvent({linkName:'share', linkType:'o', eVar9:pageName, eVar19:'share_fblike', eVar33:holdReportingTitle[currentReportingTitleIndex], events:'event9'});
						});
						window.indexOfLikeSubscribe = FB.Event._subscribersMap["edge.create"].length -1;
					//}
				}
			};
			
			</script>
