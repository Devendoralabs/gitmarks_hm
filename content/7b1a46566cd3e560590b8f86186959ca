	<script type="text/javascript">
		//<![CDATA[
		
 		$(document).ready(function()
		{
		   var keywords = $('#keywords');
			// setup search box
			keywords.bind('click', function() {
				if (this.value == 'search for a job')
				{
					keywords.clearFields();
				}
			});

			keywords.bind('blur', function() {  
				if (this.value == 'search for a job' || this.value == '')
				{
					this.value = 'search for a job';
				}
			});

			// setup live search
			keywords.keyup(function(key) {
				if (this.value == '')
				{
					$('#job-listings').load('http://jobs.jsninja.com/search/home|/');
				}
				//var len = document.getElementById('keywords').value.length;
				var len = this.value.length;
			  if (key.keyCode != 9 && len >= 3)
				{
					$("#indicator").show();	
					clearTimeout(window.search_timer);	
					window.search_timer = setTimeout("Jobber.PerformSearch('http://jobs.jsninja.com/search/home|')", 800);
			  }
			});
		});
		Jobber.jobber_url = "http://jobs.jsninja.com/";
		//]]>
	</script>
	
