        //<![CDATA[
        
      scriptWarning = document.getElementById("noScript");
      if (scriptWarning) scriptWarning.style.display = "none";
    
      homePage = document.getElementById("homeContent");
      flashWarning = document.getElementById("noFlash");
      hash = document.location.hash;
      SHOW_FLASH = true;
      if ((hash == "") || (hash == "#") || (hash == "#home")) {
        SHOW_FLASH = false;
        if (homePage) homePage.style.display = "block";
      }
      else {
        if (flashWarning) flashWarning.style.display = "block";
      }
    
        //]]>
