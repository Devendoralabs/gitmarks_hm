      <script type="text/javascript">
        //<![CDATA[
        
      var lastHit = "";
        function hit(hash) {
          var u = "/hit/";
          var c;
          for (var i = 0; i < hash.length; i++) {
            c = hash.charAt(i);
            if (c == ".") u += "/";
            else if (c == "#") { }
            else u += c;
          }
          if (u == lastHit) return;
          lastHit = u;
          var frame = document.getElementById("hitFrame");
          if (frame) frame.src = u;
        }
        if (document.location.hash.length > 1) hit(document.location.hash);
    
        //]]>
      </script>
    
