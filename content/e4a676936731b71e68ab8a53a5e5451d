				<h5 class="accord-head" id="accord-weather"><a href="javascript:void(0);">weather</a></h5>
				<div class="accordian-content" style="display:none;">
				<script type="text/javascript">
var jqN = jQuery.noConflict();
var currentZip = '';
jqN(function(){
	jqN('.change-location').live('click',function(){
		jqN('.zip-form').animate({height:'40px'},100);
	});

	if(readCookie('localWeather')!=null){
		ajaxWeather(readCookie('localWeather'));
	}
	
	jqN('.weather-cancel').live('click',function(){
		jqN('.zip-form').animate({height:'0px'},100);
		jqN('.zip-error').animate({height:'0px'},100);
	});
});

function getWeather(form){
	if(isNumeric(form.zipCode) && form.zipCode.value.length == 5){
		createCookie('localWeather',form.zipCode.value,360);
		ajaxWeather(form.zipCode.value);
	}else{
		jqN('.zip-error').animate({height:'70px'},100);
	}
	return false;
}

function ajaxWeather(zip){	
	jqN('.zip-error').animate({height:'0px'},100);
	jqN('.ajax-loader').css({height:'195px',width:'187px'}).fadeIn('fast');
	jqN.ajax({
		type: 		'GET',
		url:			'/app/weather/zipcode/' + zip + '/json',
		dataType:	'json',		
		success:	showWeather
	});	
}

function showWeather(data){
	if(data.zip_code_valid == 0){
		jqN('.zip-error').animate({height:'70px'},100);
		return false;
	}else{
		jqN('.weather-cancel').show();
	var output = '<p class="weather-conditions">Current Conditions in<br />';
  output += data.location+'</p>';
  output += '<div class="temperature-condition">';
  output += 	'<p class="current-temperature">'+data.temperature+'&deg; F</p>';
  output += 	'<p class="current-condition">'+data.weather_description+'</p>';
  output += '</div>';
 	output +=   '<img src="/assets/images/v14/weather/61x61/'+data.temperature_icon+'.jpg" class="weather-icon" alt="'+data.weather_description+'" />';
  output += '<p class="highs-lows">Tonight\'s Low: <strong>'+data.tonights_low+'&deg; F</strong><br />';
 	output += 'Tomorrow\'s High: <strong>'+data.tomorrows_high+'&deg; F</strong></p>';
  output += '<a href="http://www.weather.com/weather/tenday/'+data.zip_code+'" onclick="window.open(this.href); return false;">Extended Forecast &raquo;</a>';
  output += '<a href="javascript:void(0);" class="change-location">Change Location &raquo;</a>';
	
	jqN('.weather-module').html(output);
	jqN('.zip-form').animate({height:'0px'},100,function(){jqN('.zip-form input[name="zipCode"]').val('Please enter a 5-digit zip code');});
	jqN('.ajax-loader').fadeOut(function(){jqN(this).css({height:'0px',width:'0px'})});
	}
}
function isNumeric(e){
	return e.value.match(/^[0-9]+$/);
}
</script>
<style type="text/css">
