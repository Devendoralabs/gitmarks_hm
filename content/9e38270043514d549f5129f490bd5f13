	<script type="text/javascript" language="javascript">
	
		getIncludeVaguePricing = function(){
			var includeVaguePricing = ""; //blank means yes
			if(getListingTypes() == types['rent'] || getListingTypes() == types['sale'] || getListingTypes() == types['bvr']){
				includeVaguePricing = "&includeVaguePricing=false";
			}	
			return includeVaguePricing;
		};
	
		ready = function(){
			new AutoComplete('address', 'areaName', 3);
			//initAutoComplete('address', 'areaName');
			initFieldTip({'address':"location (ex. 'Boston, MA' or '03857')"});
			initToolTip();
			
			$("#popularCities > a:not(#moreCities)").click(function() { 
				$.get('/main.htm?action=eventPopularCityClick&seed=' + Math.random());
				window.location = $(this).attr("href") + '?listingTypes=' + getListingTypes() + getIncludeVaguePricing();
				return false;
			});
			
			$('#quickSearch').submit(function(){
				$("#listingTypes").val(getListingTypes());
				$("#includeVaguePricing").val(getIncludeVaguePricing() == "");
				
				if (!$("#submitAction").val().length){
					$("#submitAction").val("Map Search");
				}
				return true;
			});
			
			$("#mapSearch").click(function() {  
				$("#submitAction").val("Map Search");
				$("#quickSearch").submit(); 

				return false;
			});
			
			$("#textSearch").click(function() { 
				$("#submitAction").val("Text Search");
				$("#quickSearch").submit();
				return false;
			});			
			
			
			$("map > area").click(function() {
				$("map > area").unbind();
				window.location = '/redirect/mapClick?state=' + $(this).attr("id") + '&listingTypes=' + getListingTypes() + getIncludeVaguePricing();
				return false;
			});

			
				$("#address").focus().val("New York, NY").css("color", "#333").blur();
			
			

			

		};
		
		var currentType = "rent";
		
		var types = {'rent':'rental,sublet,room,corporate',
					 'sale':'sale,newHome,foreclosure,auction',
					 'foreclosure':'foreclosure',
					 'bvr':'sale,newHome,rental',
					 'vacation':'vacation',
					 'hotel':'hotel'};
					 
		getListingTypes = function(){
			return types[currentType];
		};
							 
		changeTab = function(newType){
			$('#' + currentType).removeClass("selected");
			$('#' + newType).addClass("selected");
			$('#' + newType).blur();
			
			currentType = newType;
			return false;
		};

		loadMoreNews = function(page) {
			$.ajax({
				async: false,
				url:	"/main.htm?action=newsFeedBox&fragment=true&page=" + page + "&seed=" + Math.random(),
				success: function(data) {				
							$("#moreNews").hide()
										  .html(data)
										  .attr("id", "moreNews" + page)
										  .fadeIn("slow");
						}	
			});
		};
	</script>
	
	






















