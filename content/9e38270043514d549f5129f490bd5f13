<div id="fb-root" style="display: none"></div>

<script type="text/javascript"> 
	$j = jQuery;
	if (typeof hotpads == "undefined") var hotpads = {};

	if (typeof hotpads.fb == "undefined") {
		hotpads.fb = {	
			loaded : false,
			
			fbUID : null,
			
			showWhyWindow : function() {
				window.open('/pages/whyFacebookConnect.htm',
						'_blank','menubar=0, toolbar=0, location=0, scrollbars=0,resize=0, width=500, height=400');
			},
	
			connected : function(response) {
				if (hotpads.fb.loaded != true) {
					hotpads.fb.loaded = true;
					hotpads.fb.fbUID = response.session.uid;
				    var user_box = $j("#loginBox"); 
				
					if (user_box[0] != null) {			
						if ($j("#hotpadsUserInfo").size() > 0) {
					  		//has hotpads account, allow editing of it
					  	  	$j("#facebookUserStuff").html("<a target='_blank' href='http://www.facebook.com/profile.php?id=" + hotpads.fb.fbUID 
			    	             + "'><div id='fb_login_image' class='sprite sprite-connect_white_small_short' alt='Connect'></div></a> |");
			    	      	$j("#logoutLink").click(function() { 
				    	      							if (hotpads.fb.logout) { 
					    	      							hotpads.fb.logout(); 
					    	      							return false; 
					    	      						} else { 
						    	      						return true; 
						    	      					} 
						    	      				});
						} else {
							FB.api( 
								'/me',
								function(result) {			
									//doesnt have real hotpads account
							  	  	$j(user_box).html( 
							  	  		"<span><a target='_blank' href='http://www.facebook.com/profile.php?id=" + hotpads.fb.fbUID
					  		  	  		  + "'>" + result.name + "</a> " 
					                    + "<a target='_blank' href='http://www.facebook.com/profile.php?id=" + hotpads.fb.fbUID
					    	              + "'><div id='fb_login_image' class='sprite sprite-connect_white_small_short' alt='Connect'></div></a> " 
					                    + "</span> | <a href='javascript:hotpads.fb.logout();'>sign out</a>"
					                    + "</span>");													
								}
						   );
						}
					}
				}
			},
			
			notConnected : function() {
				if ($j("#hotpadsUserInfo").size() > 0) {
					$j("#facebookUserStuff").html("");
				}
			},

			login : function() {
				FB.login(function(response) {
					if (response && response.session) {
						hotpads.fb.onlogin(response);

						if (typeof(fbSuccess) != 'undefined') {
							fbSuccess();
						}
						
						hotpads.fb.connected(response);
					}
				},  {perms:'email'});
			},
			
			onlogin : function(response) {
				if (response.session) {
					$j.ajax({
						  type: 'POST',
						  url: "/api/facebookConnect/",
						  data: { "a":"onLogin",
							  	  "accessToken": response.session.access_token },
						  async: false,
						  cache: false
						});	
				}			
			},
			
			onloginToHome : function() {
				$j.get("/api/facebookConnect/?a=onLogin"); //sets up session with facebook info and associates with user databean
				window.location="/"; //send to homepage		
			},
	
			logout : function() {
				FB.logout(function(response) {
					window.location = "/logout.jsp"
				});
			},
					
			shareFavorite : function(listingType, listingName, listingLink, imageUrl) {
				callback = function() {  };			
	
				favoriteAttachment = {
						'name': 'I favorited a ' + listingType + ' on HotPads.com',
						'href': url,
						'caption':'{*actor*} is interested in ' + listingName
				};
				
				if (typeof(imageUrl) != 'undefined' && imageUrl != null) {
					favoriteAttachment.media = [{ 
							'type': 'image',
							'src': imageUrl,
							'href': imageUrl
						}];
						
				};
	
				var url = listingLink + "?ref=FacebookFav";
				links = [{'text':'See this listing', 'href':url}]; 
	
				FB.ui(
					{
						method : "stream.publish",
						message: '',
						attachment: favoriteAttachment,
						action_links:links,
						user_prompt_message: "This place looks cool!"
					},
					function(response) {
						callback();
					}
				);
			},
			
			share : function(url) {
				FB.ui(
					{
						method: 'stream.share',
						display: 'popup',
						u: url
					},
					function(response) {
					}
				);
			},
	
			getProfilePic : function() {
				FB.api(
						'/me/picture',
						function(result) {			
							return result;
						}
				);
			},
			
			getEmail : function(successFunc, failFunc) {
				if (typeof(failFunc) == 'undefined') {
					failFunc = function() { 
						alert('Sorry, we need your email address to complete this operation');
					}
				}

				/**
				 * call FB.login with perms:email to get email permissions
				 */
				FB.login(function(response) {
					if (response.session) {
						if (response.perms && response.perms.match('email')) {				
							// user is logged in and granted some permissions.
							// perms is a comma separated list of granted permissions			
							hotpads.fb.onlogin(response);
							successFunc();
						} else {
							failFunc();
							// user is logged in, but did not grant any permissions
						}
					} else {
						// user is not logged in
						failFunc();
					}
				}, {perms:'email'});
			}
			
		};
	}
	
  window.fbAsyncInit = function() {
	    FB.init({
	      appId  : '6d73be7cb8eb044e691415c3abffd495',
	      status : false, // check login status
	      cookie : true, // enable cookies to allow the server to access the session
	      xfbml  : false,  // parse XFBML
	      logging: true
	    });

//    	FB.Event.subscribe('auth.login', function(response) { hotpads.fb.onlogin(response); hotpads.fb.connected(response); });
/*	    
      	FB.Event.subscribe('auth.sessionChange', function(response) {
	    	if (response.session) {
				hotpads.fb.connected(response);				
	    	} else {
				hotpads.fb.notConnected(response);
		    }
		});
*/

		

	    
		
	  };

	  (function() {
	    var e = document.createElement('script');
	    e.src = document.location.protocol + '//' + document.location.host + '/js/raptor/lib/facebook.js';
	    e.async = true;
	    document.getElementById('fb-root').appendChild(e);
	  }());
</script>




















