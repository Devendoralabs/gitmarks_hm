<script type="text/javascript">
//<![CDATA[
 function getElementsByClass(n, c) {
	var cArray = [];
    var z = new RegExp('\\b' + c + '\\b');

    function doTree(n) {
      if (n.className && z.test(n.className)){
        cArray.push(n);
      }
      for (var i=0, len=n.childNodes.length; i<len; i++) {
        doTree(n.childNodes[i]);
      }
    }
    doTree(n);
    return cArray;
  } 
  
  function changeFont() {
  	var newFont = document.getElementById("selFont").value;
  	var els = getElementsByClass(document.getElementsByTagName("body")[0], "html");
	for (var a=0; a<els.length; a++) {
		els[a].style.fontFamily = newFont;
	}
  }
  
  function filter() {
  	filterText = document.getElementById("filter").value;
	els = getElementsByClass(document.getElementsByTagName("body")[0], "e");
	
	if (filterText == "") {
		for (a=0; a<els.length; a++) {
			els[a].style.display = "block";
		}
		} else {
			for (a=0; a<els.length; a++) {
				if (String(els[a].title.toLowerCase()).indexOf(filterText) > -1) {
					els[a].style.display = "block";
					} else {
						els[a].style.display = "none";
				}
			}
	}
  }



  function showByType(type) {
	if (type != 'all') {
		var els = getElementsByClass(document.getElementsByTagName("body")[0], "e");
		for (a=0; a<els.length; a++) {
			els[a].style.display = "none";
		}
		
		els = getElementsByClass(document.getElementsByTagName("body")[0], type);
		for (a=0; a<els.length; a++) {
			els[a].style.display = "block";
		}
			} else {
				var els = getElementsByClass(document.getElementsByTagName("body")[0], "e");
				for (a=0; a<els.length; a++) {
					els[a].style.display = "block";
				}
	}
  }
  
  function showDesc() {
	var els = getElementsByClass(document.getElementsByTagName("body")[0], "e");
	if (document.getElementById("showDesc").innerHTML == "Hide Entity Descriptions") {
		for (a=0; a<els.length; a++) {
			var e = els[a]
			e.style.styleFloat = "left";
			e.style.width = "5em";
			e.style.height = "5.6em";
			e.style.textAlign = "center";
			e.getElementsByTagName("div")[4].style.display = "none";
		}
		document.getElementById("showDesc").innerHTML = "Show Entity Descriptions";
	
		} else {
		
			for (a=0; a<els.length; a++) {
				var e = els[a]
				e.style.styleFloat = "none";
				e.style.width = "70%";
				e.style.height = "7.1em";
				e.style.textAlign = "left";
				e.getElementsByTagName("div")[4].style.display = "block";
			}
			document.getElementById("showDesc").innerHTML = "Hide Entity Descriptions";
	}
  }
  
 window.onload = function () {
	var els = getElementsByClass(document.getElementsByTagName("body")[0], "e");
	for (a=0; a<els.length; a++) {
		els[a].onmouseover = showMore;
		els[a].onmouseout = showLess;
	}
 	function showLess(e) {
		getElementsByClass(this, "html")[0].style.fontSize="24pt";
		getElementsByClass(this, "nr")[0].style.display="none";
		getElementsByClass(this, "un")[0].style.display="none";
	}	
 	function showMore(e) {
		getElementsByClass(this, "html")[0].style.fontSize="14pt";
		getElementsByClass(this, "nr")[0].style.display="block";
		getElementsByClass(this, "un")[0].style.display="block";
	}
 } 
//]]>
</script>
