<div class="e k" title="quotation mark = APL quote"><div class="html">&quot;</div><div class="hr">&amp;quot;</div><div class="nr">&amp;#34;</div><div class="un">u0022</div><div class="desc">quotation mark = APL</div></div>
<div class="e k" title="ampersand"><div class="html">&amp;</div><div class="hr">&amp;amp;</div><div class="nr">&amp;#38;</div><div class="un">u0026</div><div class="desc">ampersand</div></div>
<div class="e k" title="less-than sign"><div class="html">&lt;</div><div class="hr">&amp;lt;</div><div class="nr">&amp;#60;</div><div class="un">u003C</div><div class="desc">less-than</div></div>
<div class="e k" title="greater-than sign"><div class="html">&gt;</div><div class="hr">&amp;gt;</div><div class="nr">&amp;#62;</div><div class="un">u003E</div><div class="desc">greater-than</div></div>

<div class="e a" title="leftwards arrow"><div class="html">&larr;</div><div class="hr">&amp;larr;</div><div class="nr">&amp;#8592;</div><div class="un">u2190</div><div class="desc">leftwards</div></div>
<div class="e a" title="upwards arrow"><div class="html">&uarr;</div><div class="hr">&amp;uarr;</div><div class="nr">&amp;#8593;</div><div class="un">u2191</div><div class="desc">upwards</div></div>
<div class="e a" title="rightwards arrow"><div class="html">&rarr;</div><div class="hr">&amp;rarr;</div><div class="nr">&amp;#8594;</div><div class="un">u2192</div><div class="desc">rightwards</div></div>
<div class="e a" title="downwards arrow"><div class="html">&darr;</div><div class="hr">&amp;darr;</div><div class="nr">&amp;#8595;</div><div class="un">u2193</div><div class="desc">downwards</div></div>
<div class="e a" title="left right arrow"><div class="html">&harr;</div><div class="hr">&amp;harr;</div><div class="nr">&amp;#8596;</div><div class="un">u2194</div><div class="desc">left right</div></div>
<div class="e a" title="downwards arrow with corner leftwards = carriage return"><div class="html">&crarr;</div><div class="hr">&amp;crarr;</div><div class="nr">&amp;#8629;</div><div class="un">u21B5</div><div class="desc">downwards arrow with corner leftwards = carriage return</div></div>
<div class="e a" title="leftwards double arrow"><div class="html">&lArr;</div><div class="hr">&amp;lArr;</div><div class="nr">&amp;#8656;</div><div class="un">u21D0</div><div class="desc">leftwards double</div></div>
<div class="e a" title="upwards double arrow"><div class="html">&uArr;</div><div class="hr">&amp;uArr;</div><div class="nr">&amp;#8657;</div><div class="un">u21D1</div><div class="desc">upwards double</div></div>
<div class="e a" title="rightwards double arrow"><div class="html">&rArr;</div><div class="hr">&amp;rArr;</div><div class="nr">&amp;#8658;</div><div class="un">u21D2</div><div class="desc">rightwards double</div></div>
<div class="e a" title="downwards double arrow"><div class="html">&dArr;</div><div class="hr">&amp;dArr;</div><div class="nr">&amp;#8659;</div><div class="un">u21D3</div><div class="desc">downwards double</div></div>
<div class="e a" title="left right double arrow"><div class="html">&hArr;</div><div class="hr">&amp;hArr;</div><div class="nr">&amp;#8660;</div><div class="un">u21D4</div><div class="desc">left right double</div></div>

<div class="e arrow" title="lozenge"><div class="html">&loz;</div><div class="hr">&amp;loz;</div><div class="nr">&amp;#9674;</div><div class="un">u25CA</div><div class="desc">lozenge</div></div>
<div class="e arrow" title="black spade suit"><div class="html">&spades;</div><div class="hr">&amp;spades;</div><div class="nr">&amp;#9824;</div><div class="un">u2660</div><div class="desc">black spade</div></div>
<div class="e arrow" title="black club suit = shamrock"><div class="html">&clubs;</div><div class="hr">&amp;clubs;</div><div class="nr">&amp;#9827;</div><div class="un">u2663</div><div class="desc">black club suit = shamrock</div></div>
<div class="e arrow" title="black heart suit = valentine"><div class="html">&hearts;</div><div class="hr">&amp;hearts;</div><div class="nr">&amp;#9829;</div><div class="un">u2665</div><div class="desc">black heart suit = valentine</div></div>
<div class="e arrow" title="black diamond suit"><div class="html">&diams;</div><div class="hr">&amp;diams;</div><div class="nr">&amp;#9830;</div><div class="un">u2666</div><div class="desc">black diamond</div></div>

<div class="e m" title="for all"><div class="html">&forall;</div><div class="hr">&amp;forall;</div><div class="nr">&amp;#8704;</div><div class="un">u2200</div><div class="desc">for</div></div>
