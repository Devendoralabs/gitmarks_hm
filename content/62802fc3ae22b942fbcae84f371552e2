var arrTitle = strPageTitle.split( "|" );
var str_data = "Check out this great article on projo.com: \n\n" + arrTitle[ 0 ] + "\n\n" + location.href + "\n";
mail_str = "mailto:?subject= projo.com: " + arrTitle[ 0 ];
mail_str += "&body=" + escape (str_data);
location.href = mail_str;
}
</script></td>
    <td width="33%"><li class="email"><a href="javascript:mailpage()" title="Email this story">E-mail story</a></li></td>
  </tr>
