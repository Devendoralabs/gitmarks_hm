<script type='text/javascript' defer='defer'>
function LoadLNL() {
  // a hack to make LNL document.write insert to the right location
  document.write = function(text) {/*alert(text);*/ $(document.getElementsByTagName('body')[0]).append(text);}
  
  // create the script dynamically; append it to the end of the <body> element
  var script = document.createElement('script');
  script.id = "LiveNetLife";
  script.src = "http://bin.livenetlife.com/?siteid=6c8847e6-d31f-6914-78b2-605053acbf82";
  script.type = 'text/javascript';
  script.siteid="6c8847e6-d31f-6914-78b2-605053acbf82"
  document.getElementsByTagName('body')[0].appendChild(script);

  // since it is not sure that the scripts will be consecutive siblings (we are running asynchronously)
  // create the <script id='__gwt_marker'/> ad-hoc right away
  var script = document.createElement('script');
  script.id = '__gwt_marker_name.jureferlez.lnl.pageplugin.LNLPagePlugin';
  document.getElementsByTagName('body')[0].appendChild(script);
}
$(document).ready(function(){  
  setTimeout(LoadLNL,10); // fire LoadLNL 10ms after the document is ready
  //LoadLNL();
});
