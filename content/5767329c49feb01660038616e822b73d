  var LiveNetLife_MaximizedStart = false;
  var LiveNetLife_MaxInactivityBeforeDisconnectMiliseconds = 120000;
  var LiveNetLife_MaxInactivityBeforeAwayStatusMiliseconds = 60000;
  var LiveNetLife_TimeSpeedMultiplierWhenNoFocus = 3.0;
  var LiveNetLife_ObserveAudio = false;
