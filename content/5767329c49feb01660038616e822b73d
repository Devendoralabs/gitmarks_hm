<script type="text/javascript">
var dd_hash = null; // window.location.hash;
var dd_query = null; // full content query: hash + category/lecture filter
var dd_cntqry = null; // count query: full query without ordering
var dd_xhr_data = null;
var dd_xhr_cnt = null;

//$(document).ready( function() {
  $('.dd_ctl').click(update_browse);
  $('.dd_href').click(dd_decorate_href);
  $('.drilldown_loc a').click(dd_decorate_href);
  var hash = window.location.hash;
  if (hash && hash[0]=='#') hash = hash.substr(1,hash.length);
  var lecid=7229;
  if (lecid && hash || !lecid) {
    dd_set_hash(hash);
    load_browse_form(hash); // load form on load
  }
//});

function jump_to_overview() {
  $('#dd_overview').attr("checked", "checked"); 
  update_browse();
  return false;
}
function dd_filter_query(query,keys,invert) {
  // filter 'order' and page number out of the query
  if (!query || !query.length) return '';
  if (keys) keys = keys.split(',');
  else keys = ['o','p'];
  var sel = [], pairs = query.split("&");
  for (s in pairs) {
    s = pairs[s];
    var found = false;
    key = s.split('=')[0];
    for (var j = 0; j < keys.length; j++) found |= (key == keys[j]);
    if (found==!!invert) sel.push(s);
  }
  return sel.join('&');
}

function dd_decorate_href() {
  
    var query = ''+this
    query = query.split('#',2);
    if (query.length<2) {
      var hash = dd_filter_query(dd_hash,'p');
      window.location = query[0]+'#'+hash
    } else {
      var loc = query[0];
      query = query[1];
      var hash = dd_filter_query(dd_hash,'p,cid');
      if (hash) query = hash + (query ? '&' + query : '');
      var sloc = ''+window.location;
      if (loc && sloc.substr(0,loc.length)!=loc) {
        window.location = loc + '#' + query;
      }
      dd_set_hash(query);
    }
  
  return false;
}
function isDigit(c){return ((c >= "0") && (c <= "9"))}
function skipInitialAmp(s) {return (s.indexOf('&')===0) ? s.slice(1) : s; }
function load_browse_form(query) {
  var f = $('#browse_form');
  var result = {};
  var has_ordering=false;
  if (query) { 
    var pairs = query.split("&");
    var pair, key, val, vals;
    for (pair in pairs) {  
      pair = pairs[pair].split("=");
      key = pair[0]; val=pair[1];
      result[key] = val;  
      if (key=='t' || key=='l' || key=='o' || key=='s') {
        vals = val.split(',');
        for (var j=0; j<vals.length; j++) $('#browse_form [name='+key+']').filter('[value='+vals[j]+']').each( function(n){this.checked=true;} ) ;
        has_ordering |= (key=='o'); 
      } else if (key=='q') {
        $('#browse_form [name='+key+']').val(decodeURIComponent(val));
      } else if (key[0]='y') {
        $('#browse_form [name='+key+']').filter('[value='+val+']').each( function(n){this.checked=this.selected=true;} ) ;
      } else {
        alert(' '+key+' '+val);
        $('#browse_form [name='+key+'] > [value='+val+']').each( function(n){this.checked=true;} ) ;
      }
    }
  }
  
  if (has_ordering) {
    $('.dd_if_browsing').addClass('browsing');
  } else {
    $('.dd_if_browsing').removeClass('browsing');
  }
  
};

function join_cb_vals(name,xor_mode) {
  var items = $('#browse_form [name='+name+']');
  if (xor_mode && items.not(':checked').length==0) return '';
  var a = new Array();
  items.filter(':checked').each( function(i) {val = $(this).val(); if (val) a[a.length] = val;} );
  if (a.length==0) return '';
  return name+'='+a.join(',');
}
function form2filter() {
  f = $('#browse_form');
  var sel = new Array()
  // get checkboxes and extract window.hash
  var order=join_cb_vals('o'); if (order) sel[sel.length]=order;
  var scope=join_cb_vals('s'); if (scope) sel[sel.length]=scope;
  var types=join_cb_vals('t',true); if (types) sel[sel.length]=types;
  var langs=join_cb_vals('l',true); if (langs) sel[sel.length]=langs;
  var cats=join_cb_vals('c'); if (cats) sel[sel.length]=cats;
  var y1=$('#browse_form [name=y1] > :selected').val();
  if (isDigit(y1[0])) sel[sel.length]='y1='+y1;
  var y2=$('#browse_form [name=y2] > :selected').val();
  if (isDigit(y2[0])) sel[sel.length]='y2='+y2;
  var search=$('#browse_form [name=q]').val(); if (search) sel[sel.length]='q='+encodeURIComponent(search);

  //  toggle expandable items
  
  if (order!='') {
    $('.dd_if_browsing').addClass('browsing');
  } else {
    $('.dd_if_browsing').removeClass('browsing');
  }
  

  return sel.join('&')
}
function update_browse() {
  dd_set_hash(form2filter());
  //$("#dd_form_sections").empty();
  return true;
}
$('#browse_form').submit( function() {
  update_browse();
  return false;
});

function dd_set_hash(query) {
  if (query==dd_hash) return;
  
  dd_hash=dd_filter_query(dd_hash,'cid',true);
  if (dd_hash && !dd_filter_query(query,'cid',true)) query += '&'+dd_hash;
  
  dd_hash=query;
  if (window.location.hash.substring(1) != dd_hash) window.location = '#'+dd_hash;
  query+='&id=7229';
  query = skipInitialAmp(query);
  if (query!=dd_query) {
    dd_query = query;
    cntqry = dd_filter_query(query);
    //alert(''+cntqry);
    if (cntqry!=dd_cntqry) {
      dd_cntqry=cntqry;
      if (dd_xhr_cnt) dd_xhr_cnt.abort();
      dd_xhr_cnt = $.ajax({
        url: '/site/ajax/drilldown/counts/?'+dd_cntqry,
        dataType: 'html',
        cache: true,
        success: function(msg) {$('#drilldown_counts').html( msg );},
        complete: function() {dd_xhr_cnt=null;}
      });
    }
    if (dd_xhr_data) dd_xhr_data.abort();
    $('#drilldown_list').html('');
    //width = Math.floor($('#drilldown_list').width()/160);
    width = 5;
    dd_xhr_data = $.ajax({
      url: '/site/ajax/drilldown/?'+ dd_query + '&w='+width,
      cache: true,
      dataType: 'html',
      success: function(msg) {$('#drilldown_list').html( msg );},
      complete: function() {dd_xhr_data=null;}
    });
  }
}
function go_to_page(num) {
  var hash = dd_filter_query(dd_hash,'p');
  if (num>1) {
    if (hash) hash += '&p='+num;
    else hash = 'p='+num;
  }
  dd_set_hash(hash);
}
</script>

</div>




