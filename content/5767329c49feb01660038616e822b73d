<script type="text/javascript" defer="defer">
$(window).load(function() {
//$(document).ready(function() {
  var gaJsHost = (("https:" == document.location.protocol) ? "https://ssl." : "http://www.");
  $.ajax({
      url: gaJsHost+'google-analytics.com/ga.js', type: 'get', dataType: 'script', cache: true,
      success: function() {setTimeout(GA_Track_Pageview, 100);}
  });
});
//$(document).ready(function() {setTimeout(GA_Track_Pageview, 100);});
function GA_Track_Pageview() {
  // get user login info...
  
    var track_user = 'anonymous';
  

  try {
    var pageTracker = _gat._getTracker("UA-353712-6");
    pageTracker._setVar(track_user);
    
    pageTracker._trackPageview('/lecture/eu_supported/pascal/ssll09_canberra/');
  } catch (err) {}

  var alt_analytics_trackers = {
    "mit_ocw": "UA-353712-10",
    "mit_world": "UA-353712-11",
    "yale_oyc": "UA-353712-15",
    "cern": "UA-353712-16",
    "pascal": "UA-353712-17"
  }
  try {
    var tracker_id = alt_analytics_trackers["pascal"];
    //alert('tracking '+"pascal"+'->'+tracker_id);
    var altTracker = _gat._getTracker(tracker_id);
    altTracker._setVar(track_user);
    altTracker._trackPageview('/lecture/eu_supported/pascal/ssll09_canberra/');
  } catch(err) {}
}
</script>

