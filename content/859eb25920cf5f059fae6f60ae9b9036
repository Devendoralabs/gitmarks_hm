<script type="text/javascript">
function rotateSocial() {
  if ($('sl_1').visible()) {
    $('sl_1').hide();
    new Effect.toggle('sl_2', 'appear');
  } else {
    $('sl_2').hide();
    new Effect.toggle('sl_1', 'appear');
  }
}

window.onload = function() {
  setInterval('rotateSocial();', 10000);  
  $$("div.fz_icon a").each(function(el) { new FancyZoom(el); });      
};
</script>


  
