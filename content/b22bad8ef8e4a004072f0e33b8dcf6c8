$(document).ready(function(){
initTopNav();
window.activeTopNavTab = 0;
switch(topNavSelected) {
case "home":
activeTopNavTab = 0;
break;
case "clonewars":
activeTopNavTab = 1;
break;
case "video":
activeTopNavTab = 2;
break;
case "games":
activeTopNavTab = 3;
break;
case "movies":
activeTopNavTab = 4;
break;
case "fans":
activeTopNavTab = 5;
break;
case "kids":
activeTopNavTab = 6;
break;
case "vault":
activeTopNavTab = 7;
break;
case "shop":
activeTopNavTab = 8;
break;
case "default":
activeTopNavTab = 0;
break;
}
if (activeTopNavTab > 0) {
$("#topNavTab" + activeTopNavTab).css({
backgroundPosition: "bottom"
});
}
window.referrer = window.location.pathname;
switch(userMemberStatus) {
case "nonmember":
$("#topNavLogLink").html("Sign in");
$("#topNavLogLink").attr("href","/webapps/registration/sign-in?forward=" + referrer);
$("#topNavLog").addClass("topNavLogNoHs");
break;
case "member":
$("#topNavLogLink").html("Sign out");
$("#topNavLogLink").attr("href","/webapps/registration/sign-out");
$("#topNavSettingsLink").css({display:"block"});
$("#topNavLog").addClass("topNavLogNoHs");
break;
case "hyperspace":
$("#topNavLogLink").html("Sign out");
$("#topNavLogLink").attr("href","/webapps/registration/sign-out");
$("#topNavLog").addClass("topNavLogHs");
$("#topNavSettingsLink").css({display:"block"});
break;
case "hidden":
$("#topNavRightDiv").css({width:"167px"});
$("#topNavSettingsLink").css({display:"none"});
$("#topNavLog").css({display:"none"});
break;
}
});
function resetTopNav() {
$("#topNav").css({height:"68px"});
if (jQuery.browser.msie && (jQuery.browser.version < 7)) {
$("select").css({
visibility: "visible"
});
}
$(".topNavReset").css({display:"none"});
for (i=1; i<topNavCatCount; i++) {
$("#topNavCol" + i).css({display:"none"});
if (i != activeTopNavTab) {
$("#topNavTab" + i).css({
backgroundPosition:"top"
});
}
}
}
function showTopNavColumn() {
$("#topNav").css({height:"413px"});
if (jQuery.browser.msie && (jQuery.browser.version < 7)) {
$("select").css({
visibility: "hidden"
});
}
$(".topNavReset").css({display:"block"});
var colId = $(this).attr("id");
var colNumber = colId.substring(colId.length-1);
for (i=1; i<topNavCatCount; i++) {
if (i==colNumber) {
$("#topNavCol" + i).css({display:"block"});
$("#topNavTab" + i).css({backgroundPosition:"bottom"});
} else {
$("#topNavCol" + i).css({display:"none"});
if (i != activeTopNavTab) {
$("#topNavTab" + i).css({
backgroundPosition: "top"
});
}
}
}
}
function initTopNav() {
if (typeof(topNavSelected) == "undefined") {
topNavSelected = "home";
}
if (typeof(userMemberStatus) == "undefined") {
userMemberStatus = "hidden";
}
$(".topNavColumn a").each(function () {
if(jQuery.browser.msie && (jQuery.browser.version < 7)) {
$(this).parent().width(160);
} else {
// "offSet" must be total of left and right padding for each <a> element
var offSet = 30;
var colWidth = (($(this).parent().width())-offSet) + "px";
$(this).css({width:colWidth});
}
$(this).mouseover(function() {
$(this).css({backgroundImage:"url(/img/global/topnav_row_bg.png)"});
});
$(this).mouseout(function() {
$(this).css({backgroundImage:"none"});
});
});
$(".topNavCategory").each(function () {
$(this).mouseover(showTopNavColumn);
});
window.topNavCatCount = $(".topNavColumn").length + 1;
$(".topNavReset,#topNavRightDiv,#topNavLogo").mouseover(resetTopNav);
$("#topNavLogo").click(function(){
document.location = "/index.html";
});
}
</script>
