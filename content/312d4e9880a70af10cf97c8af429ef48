	<script src="http://www.microsoft.com/click/services/RioTracking2.js" type="text/javascript"></script>
	
	<script type="text/javascript">
		var onclickTrackingFunctionsDictionary;
		var GlobalTrackingObject = function() {
			trackingFilePath = '';
			onclickTrackingFunctionsDictionary = [];
			_nielsenAnalyticsEnabled = false;
			this._mouseEvent = null;
			//atlas queue
			this._atlasQueue = [];
			this._clearScript = null;
			this._documentHead = document.getElementsByTagName("head")[0];

			this.matchPageProtocol = function(url) {
				var pos = url.indexOf('://');
				if (-1 < pos) {
					var protocol = url.substring(0, pos + 1);
					if (protocol != window.location.protocol) {
						var domainAndPath = url.substr(pos + 1);
						url = window.location.protocol + domainAndPath;
					}
				}
				return url;
			};
			this.onmousedown = function(event) {
				if (typeof (event.button) != 'undefined') {
					if (event.button == 4) {
						this._mouseEvent = "middleMouseClicked";
					}
				}
				else if (typeof (event.which) != 'undefined') {
					if (event.which == 1) {
						this._mouseEvent = "middleMouseClicked";
					}
				}
				else {
					this._mouseEvent = null;
				}
			};
			this.onmouseup = function(event, element) {
				if (typeof (event.button) != 'undefined') {
					if (event.button == 4) {
						jQuery(element).triggerHandler('click');
					}
				}
				else if (typeof (event.which) != 'undefined') {
					if (event.which == 1) {
						jQuery(element).triggerHandler('click');
					}
				}
			};
			this.onclicktrack = function(trackid, sender) {
				eval(onclickTrackingFunctionsDictionary[trackid]);
				if (sender != null) {
					if (typeof (sender.href) != 'undefined') {
						if (sender.href.match(/javascript/gi) == null && sender.href != "#") {
							if (sender.href != null && sender.href != "" && sender.target != "new" && !event.shiftKey && !this._mouseEvent == "middleMouseClicked") {
								setTimeout('document.location = "' + sender.href + '"', 100);
							}
						}
					}
				}
			};
			this.trackatlasonclick = function(atlastrackstring) {
				if (!atlastrackstring) {
					return;
				}
				else if (!document.getElementsByTagName || !document.createElement || !document.appendChild) {
					return false;
				}
				else {
					this.processAtlas(atlastrackstring);
				}
			};
			this.processAtlas = function(atlasUrl) {
				atlasUrl = this.matchPageProtocol(atlasUrl);
				var self = this;
				var script = document.createElement("script");
				script.type = "text/javascript";
				script.src = atlasUrl;
				this._atlasQueue.unshift(script);
				self._documentHead.appendChild(script);
				clearTimeout(this._clearScript);
				this._clearScript = setTimeout(function() {
					while (self._atlasQueue.length > 0) {
						self._documentHead.removeChild(self._atlasQueue.pop());
					}
				}, 300);
			};
			this.trackatlasonpageload = function(atlastrackstring) {
				if (!atlastrackstring) {
					return;
				}
				this.processAtlas(atlastrackstring);
			};
			this.OnVideoStart_nielsentracking = function OnVideoStart_nielsentracking(mediaName, totalDuration, position) {
				if (globaltracking._nielsenAnalyticsEnabled) {
					gg1.ggPM(3, mediaName, "content", "<uurl>" + mediaName + "<\/uurl><length>" + totalDuration + "<\/length><title>" + mediaName + "<\/title>");
					gg1.ggPM(5, position);
					gg1.ggPM(49, 30);
				}
			};
			this.OnVideoEnd_nielsentracking = function OnVideoEnd_nielsentracking(position) {
				if (globaltracking._nielsenAnalyticsEnabled) {
					gg1.ggPM(7, position);
				}
			};
			this.OnVideoPause_nielsentracking = function OnVideoPause_nielsentracking(position) {
				if (globaltracking._nielsenAnalyticsEnabled) {
					gg1.ggPM(7, position);
				}
			};
			this.OnVideoResume_nielsentracking = function OnVideoResume_nielsentracking(position) {
				if (globaltracking._nielsenAnalyticsEnabled) {
					gg1.ggPM(5, position);
					gg1.ggPM(49, 30);
				}
			};
		};

		var globaltracking = new GlobalTrackingObject();
	</script>
	
