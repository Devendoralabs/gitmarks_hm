	    function rb_click(obj) {
		    trackingOption = obj.value;
	    }

	    function onDownloadClick() {
		    toggleMediaOff();
		    window.location = trackingOption;
		    var modalchoice;
		    if (trackingOption == "javascript:window.open('',null,'toolbar=0,location=no,directories=0,status=0,scrollbars=yes,resizable=1,width=1,height=1,top=0,left=0');(window.location) ? window.location='http://go.microsoft.com/fwlink/?LinkID=182776' : document.location.href='http://go.microsoft.com/fwlink/?LinkID=182776';")
			    modalchoice = 'IE8';
		    else
			    modalchoice = 'MSNOIE8';
		    omniModTracking(this, 'IE8DownloadHero', 'href=#', '', 1, 'RadioButton Modal ' + modalchoice, 'event3');
		    //if (trackingOption == "javascript:window.open('',null,'toolbar=0,location=no,directories=0,status=0,scrollbars=yes,resizable=1,width=1,height=1,top=0,left=0');(window.location) ? window.location='http://go.microsoft.com/fwlink/?LinkID=182776' : document.location.href='http://go.microsoft.com/fwlink/?LinkID=182776';")
			    //thankYouRedirect();
	    }        
