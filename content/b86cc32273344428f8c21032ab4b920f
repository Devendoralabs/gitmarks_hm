<script>
$(document).ready(function() { 
    var options = { 
        success: showResponse,
        error: commentError,
        beforeSend: function(){ $('#comment-button').attr("disabled", "disabled"); }
    };
    $('#comment-button').click(function(e) {
        
        $('#comments_throbber').show();
    });
    $('#comment_form').ajaxForm(options); 
});
function commentError(request, text, error) {
    
    clearcommentErrors();
    
    if(request.status && request.status > 0) {
        
            Recaptcha.reload();
        

        var error_list = JSON.parse(request.responseText);
        
        for (var name in error_list) {
            $("#id_" + name).addClass('error_highlight');
            for (var i=0; i<error_list[name].length; i++){
                
                var error_message = error_list[name][i];
				if (error_message == 'incorrect-captcha-sol') {
                	$('#error_' + name).html('Incorrect Recaptcha Solution');
                	$("#recaptcha_response_field").addClass('error_highlight');
                }
                else
                	$('#error_' + name).html(error_message);
            }
        }
    }
    $('#comment-button').removeAttr("disabled");
    $('#comments_throbber').hide();
}
function clearcommentErrors() {
    var error_list = new Array('comment', 'display_name', 'captcha', 'email');
    
    for (var i=0; i<error_list.length; i++){
        if (error_list[i] == 'captcha')
            $("#recaptcha_response_field").removeClass('error_highlight');
        else
            $("#id_" + error_list[i]).removeClass('error_highlight');
        $('#error_' + error_list[i]).html('');
    }
}
function showResponse(responseText, statusText)  {
    var data = JSON.parse(responseText);
    clearcommentErrors();
    $(document).trigger('comment_added', data.comments_count);
    $('#new_comments').append(data.html_response);
    $('#comment_div' + data.added_comment_id).fadeIn("slow");
    $('#id_comment').val("");
    $("#comment_added_status").fadeIn(2000, function(){ $("#comment_added_status").fadeOut(2000); });
    $('#comment_form_errors').html("");
    
        Recaptcha.reload();
    
    $('#comment-button').removeAttr("disabled");
    $('#comments_throbber').hide();
} 
</script>
