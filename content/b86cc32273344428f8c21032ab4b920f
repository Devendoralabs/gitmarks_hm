<script>

function clearSearch(){
    if ((document.search_form.q.value=='Search Focus') || (document.search_form.q.value=='')){
        document.search_form.q.value='';
        return false;
    } else {
        return true;
    }
}



$("#header_inner_top").hover(  
    function() {  
        $("#research_products_sub").addClass("off"); 
        $("#research_products_btn").removeClass("research_products_on");
        $("#research_products_btn").addClass("research_products");  
    }
);
$("#research_products_btn").hover(  
    function() {  
        $("#research_products_sub").removeClass("off");
        $("#research_products_btn").removeClass("research_products");
        $("#research_products_btn").addClass("research_products_on"); 
    }

);


$("#research_products_sub").hover(  
    function() { },
    function() {  
        $("#research_products_sub").addClass("off");
        $("#research_products_btn").removeClass("research_products_on");
        $("#research_products_btn").addClass("research_products");  
    }
);


