<script>
$(document).ready(function(e) {
    if(isHighLoad()) {
        var canrate = true;
        var avgrating = 4.000000000000000;
        var currentrating = 0;
        var numratings = 1;
        var hasrated = false;
        var ratingscount = 1;
        $('#ratings_handler1').rating('6019', canrate, hasrated, currentrating, avgrating, ratingscount);
        $('#ratings_handler2').rating('6019', canrate, hasrated, currentrating, avgrating, ratingscount);
        $('#ratings_handler3').rating('6019', canrate, hasrated, currentrating, avgrating, ratingscount);
    } else {
        $.ajax({
            type: "GET",
            url: "/ratings/load_ratings/6019/",
            success: function(responseText){
                var data = JSON.parse(responseText);
                var canrate = (data.can_rate == 1) ? true : false;
                var avgrating = data.average_rating;
                var currentrating = data.rating;
                var numratings = data.ratings_count;
                var hasrated = (data.has_rated == 1) ? true : false;
                var ratingscount = data.ratings_count;
                $('#ratings_handler1').rating('6019', canrate, hasrated, currentrating, avgrating, ratingscount);
                $('#ratings_handler2').rating('6019', canrate, hasrated, currentrating, avgrating, ratingscount);
                $('#ratings_handler3').rating('6019', canrate, hasrated, currentrating, avgrating, ratingscount);
            },
            error: function(response, text){
                if(response.status && response.status > 0) {
                    $('#ratings_handler1').hide();
                    $('#ratings_handler2').hide();
                    $('#ratings_handler3').hide();
                }
            }
        });
    }
});
</script>
    
<!--script src="http://static.focus.com/275/global/js/saved_content.js" type="text/javascript" charset="utf-8"></script-->
<script>
$(document).ready(function() {
    if(isHighLoad()) {
        $('.article-save > a').content_save('6019', false);
    } else {
        $.ajax({
            type: "GET",
            url: '/saved_content/is_saved/6019/',
            success: function(responseText){$('.article-save > a').content_save('6019', true);},
            error: function(request){if(request.status && request.status > 0) {$('.article-save > a').content_save('6019', false);}}
        });
    }
});


$(document).bind('content_saved_6019', function(e) {
    openmembertools("You must be a registered user of Focus to save content.");
});

</script>

	<script type="text/javascript" charset="utf-8">
	    $(document).ready(
	        function() {
	            $("#author_name").bind("mouseover",
	                function(){
	                    var pos = $("#author_name").position();    
	                    $('#profile-flyover').css( { position: 'absolute', left: pos.left, top: pos.top});
	                    setTimeout(function() { $("#profile-flyover").show(); }, 500);
	                }
	            );
	            
	            $("#profile-flyover-close").click(function(){$("#profile-flyover").hide();});
	            
	            $("#profile-flyover").hover(  
	                function() {},
	                function() { $("#profile-flyover").hide(); }
	            );
	        }
	    );
	</script>

