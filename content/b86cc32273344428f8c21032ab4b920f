<script>
    
    function editComment(comment_id) {
        $('#commentslink_'+comment_id+'_throbber').show();
        $.get('/comments/edit/'+comment_id+'/', function(data){$('#commentbody_'+comment_id).html(data);$('#commentslink_'+comment_id+'_throbber').hide();$('#editlink_'+comment_id).hide();});
    }
    function loadEdits(comment_ids) {
        var _comment_ids = new String(comment_ids);
        $.ajax({
            type: "POST",
            url: "/comments/can_edit/",
            data: {comment_ids: comment_ids},
            success: function(responseText){
                comment_ids = comment_ids.split(',');
                var data = JSON.parse(responseText);
                for(var i=0; i<comment_ids.length; i++) {
                    var comment_id = comment_ids[i];
                    var can_edit = (data[comment_id] == 1) ? true : false;
                    
                    if(can_edit) {
                        $('#editlink_'+comment_id).show();
                    }
                }
            }
        });
    }
    $(document).ready(function() {
        $(document).bind('comment_added', function(e, num_comments) {
            var linktext = "";
            if (/nswer/.test($('span.comments_count').html())) {
                linktext = (num_comments > 1) ? 'Answers' : 'Answer';
            } else {
                linktext = (num_comments > 1) ? 'comments' : 'comment';
            }
            $('.comments_count > a').html(num_comments + ' ' + linktext);
            $('.comments_count > span').html(num_comments + ' ' + linktext);
            $('span.comments_count').html(num_comments + ' ' + linktext);
        });
        $(document).bind('comment_removed', function(e, num_comments) {
            var linktext;
            if (/nswer/.test($('span.comments_count').html())) {
                linktext = (num_comments > 1) ? 'Answers' : 'Answer';
            } else {
                linktext = (num_comments > 1) ? 'comments' : 'comment';
            }            $('.comments_count > a').html(num_comments + ' ' + linktext);
            $('.comments_count > span').html(num_comments + ' ' + linktext);
            $('span.comments_count').html(num_comments + ' ' + linktext);
        });
    });
</script>


