<script language="javascript" type="text/javascript">
//<![CDATA[
	
	_s( 'SEARCH FLIGHTS', "Search flights" );_s( 'WAIT', "wait" );_s( 'I WANT TO FLY FROM', "I want to fly from..." );_s( 'I WANT TO FLY TO', "I want to fly to..." );_s( 'FROM PRICE', "from" );_s( 'ANYWHERE', "Anywhere" );_s( 'ARROW UP', "▲" );_s( 'ARROW DOWN', "▼" );_s( 'WITH', "with" );_s( 'CHEAP FLIGHTS FROM AIRPORT', "Cheap Flights from %s" );_s( 'CHEAP FLIGHTS TO AIRPORT', "Cheap Flights to %s" );_s( 'CHEAP FLIGHTS FROM TO', "Cheap flights from %1$s to %2$s" );	
	//Init Search Engine
	document.addEvent( 'domready', function() {
		$GLOBALS['SearchEngine'] = new SearchEngine( {
			lang: "en",
			cmd: '/cmd/?cmd=GetAirportsMatch&format=json',
			proxy: true,
			extLang: true,
			redirectPage: 'Routes',
			forceComplete: false,
			submit: {
				text: _d( 'SEARCH FLIGHTS' ),
				wait: _d( 'WAIT' )
			},
			// Default value
			origin: {
				text: _d( 'I WANT TO FLY FROM' ),
				anywhere: _d( 'ANYWHERE' ),
				loaded: null,
				iata: null			},
			destination: {
				text: _d( 'I WANT TO FLY TO' ),
				anywhere: _d( 'ANYWHERE' ),
				loaded: null,
				iata: null			},
			calendar: {
				active: false
			}
		} );
		
		Redirect.setSite( 'whichbudget' );
		Redirect.setLink( {
			whichbudget: {
				host: "http://www.whichbudget.com"			}
		} );
	} );
	
	// Add Action on Top Searches TAB
	document.addEvent( 'domready', function() {
		$('ts_button').addEvent( 'click', function() {
			$('topSearch').getElement( '.bloc' ).toggleClass( 'close' );
			if( $('topSearch').getElement( '.bloc' ).hasClass( 'close' ) )
				$('ts_arrow').set( 'text', _d( 'ARROW DOWN' ) );
			else
				$('ts_arrow').set( 'text', _d( 'ARROW UP' ) );
		} );
	} );
	
//]]>
</script>	
