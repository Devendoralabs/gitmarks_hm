		// <![CDATA[
		_s( 'ARROW DOWN', "▼" );		
		var LangManager = new Class( {
			
			Implements: Options,
			
			options: {
				bloc: $('languages'),
				list: $('langList'),
				arrow: $('languages').getElement( '.arrow' ),
				open: false
			},
			
			initialize: function() {
				var $this = this;
				this.options.bloc.addEvents( {
					click: function() {
						if( $this.options.open == false ) {
							$this.options.list.setStyle( 'display', 'block' );
							$this.options.arrow.set( 'text', _d( 'ARROW UP' ) );
							$this.options.open = true;
						} else {
							$this.options.list.setStyle( 'display', 'none' );
							$this.options.arrow.set( 'text', _d( 'ARROW DOWN' ) );
							$this.options.open = false;
						}
					}
				} );
			}
			
		} );
		new LangManager;
		// ]]>
		</script>
		
