<script>
var imgW;
var imgH;
var tmpW;
var tmpH;
function abc()
{
    document.getElementsByTagName('body')[0].style.overflow='hidden'; 
    document.getElementsByTagName('body')[0].scroll="no";
    imgW=document.getElementById("img1").width;
    imgH=document.getElementById("img1").height;
    tmpW=imgW*document.body.clientWidth/1264;
    tmpH=imgH*document.documentElement.clientHeight/795;
  //  alert(document.body.clientWidth);
  //  alert(document.documentElement.clientHeight);
  //  alert(imgW);
  //  alert(imgH);
  document.getElementById("table").width=document.body.clientWidth;
  //  document.getElementById("table").style.height=document.documentElement.clientHeight;
  document.getElementById("td1").height=document.documentElement.clientHeight/2-10*document.documentElement.clientHeight/795;
  document.getElementById("td2").height=document.documentElement.clientHeight/2-10*document.documentElement.clientHeight/795;
  document.getElementById("td3").height=document.documentElement.clientHeight/2-10*document.documentElement.clientHeight/795;
  document.getElementById("td4").height=document.documentElement.clientHeight/2-10*document.documentElement.clientHeight/795;
  if(document.body.clientWidth>document.documentElement.clientHeight)
  {
    document.getElementById("img1").height=tmpH;
    document.getElementById("img2").height=tmpH;
    document.getElementById("img3").height=tmpH;
    document.getElementById("img4").height=tmpH;
  }
  else
  {
    document.getElementById("img1").width=tmpW;
    document.getElementById("img2").width=tmpW;
    document.getElementById("img3").width=tmpW;
    document.getElementById("img4").width=tmpW;
  }
    

}
