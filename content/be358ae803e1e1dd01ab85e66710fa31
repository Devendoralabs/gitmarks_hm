<div id="requestAccessContainer" title="Request To Join Project" style="display:none;">
    <div class="SubText" style="padding-bottom:1em;">
        Complete this form to send a request to become a member of this project. Enter any comments that 
        you wish for the project coordinators to see when approving your request, for example 
        what role you would like to have in the project.
    </div>
    <div>
        <div class="CharCounter"><span id="charCounter">250</span> characters left</div>
        <textarea id="requestAccessComment" rows="5"></textarea>
        <div id="requestAccessError" class="ErrorMessage StandardPaddingTop StandardPaddingBottom" style="display:none;"></div>
    </div>
    <div class="Buttons" style="float:right">
        <input type="button" id="requestAccessSave" class="Button" onclick="requestAccess()" value="Send Request" />
        <input type="button" id="requestAccessCancel" class="Button" onclick="cancelRequestAccess()" value="Cancel" />
    </div> 
</div>

<script type="text/javascript">
    function displayRequestAccess() {
        OpenDialog('#requestAccessContainer', true, '36em', 'ClosePanel');
        return false;
    }

    function requestAccess() {
        var error = $('#requestAccessError').hide();
        var comment = $('#requestAccessComment').val();
        if ($normalizeText(comment).length === 0) {
            error.text('* Please provide your comments for the project coordinators.').show();
            return;
        }

        CodePlex.UpdateProgress.showWithCustomText(true, 'Saving...');
        $.post('http://phpexcel.codeplex.com/team/requestAccess',
               { comment: $('#requestAccessComment').val() },
               function(data) {
                   CodePlex.UpdateProgress.show(false);
                   if (data.RedirectUrl && data.RedirectUrl.length) location.href = data.RedirectUrl;
                   else if (data.IsError) {
                       $('#requestAccessError').text(data.Message).show();
                   } else {
                       $('#joinUsContainer').empty()
                                            .addClass('SuccessMessage')
                                            .html(data.Message)
                                            .show().fadeOut(5000, function() {
                                                $(this).empty()
                                                       .removeClass('SuccessMessage')
                                                       .text('Request to join project is pending')
                                                       .show();
                                            });
                       OpenDialog('#requestAccessContainer', false);
                   }
               }, 'json');
    }

    function cancelRequestAccess() {
        OpenDialog('#requestAccessContainer', false);
        $('#requestAccessError').hide();
        $('#requestAccessComment').val('');
        $('#charCounter').text('250');
    }

    $(document).ready(function() {
        $('#requestAccessComment').enhancedTextBox({ limit: 250, showCounter: true });
    
    });
</script>
</div></div>
<script type="text/javascript">
    $(document).ready(function () {
        $('.PageActivity .Link').click(function () {
            var val = $(this).attr('d:value');
            $.post('http://phpexcel.codeplex.com/stats/getActivity',
                  { period: val },
                  function (data) {
                      $('.PageActivity span').hide().parent().removeClass('Selected').addClass('Unselected');
                      $('.PageActivity .Link').show();
                      $('#Page' + val + 'Label').show().parent().removeClass('Unselected').addClass('Selected');
                      $('#Page' + val + 'Link').hide();
                      $('#pageViews').text(data.PageViews);
                      $('#visits').text(data.Visits);
                      $('#downloads').text(data.Downloads);
                      
                  }, 'json');
            return false;
        });
    });
</script>
