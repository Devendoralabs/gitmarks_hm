<div id="contact">
<script language="javascript" type="text/javascript">
function validate(f){
	clearText(f.name);
	clearText(f.email);
	clearText(f.phone);
	var errors = "";
	if(f.name.value == ""){
		errors = "Enter your Name\n";
	}
	if(f.phone.value == ""){
		errors += "Enter your Phone\n";
	}
	if(f.email.value == ""){
		errors += "Enter your Email\n";
	}
	if(errors != ""){
		alert(errors);
		replaceText(f.name);
		replaceText(f.email);
		replaceText(f.phone);
		return false;
	}
	return true;
	
}
</script>
<form action="/do/contact" onSubmit="return validate(this);">

