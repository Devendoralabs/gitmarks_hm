        <script type="text/javascript">
            $(function () {
                var tooltiptimeout;
                var tooltip = $("#fav-tip");
                $("a.fav-toggle").unbind('click').click(function (event) {
                    clearTimeout(tooltiptimeout);
                    var link = $(this);
                    link.after(tooltip);
                    tooltip.find("a").attr("href", "/users/login?returnurl=" + link.attr("href"));
                    tooltip.fadeIn("fast");
                    tooltiptimeout = setTimeout(function () { tooltip.fadeOut(); }, 9000);
                    event.preventDefault();
                    event.stopPropagation();

                    $("body").unbind('click').click(function () {
                        tooltip.fadeOut("fast");
                    });
                    return false;
                });

                $("a#close", tooltip).click(function () {
                    tooltip.fadeOut("fast");
                    return false;
                });
            });
        </script>

        <div id="fav-tip">
            <a id="close">x</a>
            <p>« You can add this job to your Favorites by <a href="/users/login?returnurl=%2Fjobs">logging in to Stack Overflow Careers</a>.</p>
        </div>
    
