	function bizo_callback(bizo_data) {
		var bizo_codes_to_custom_segments = {    
    		"executive":"1080",
    		"mid_management":"1081",
			"non_management":"1082",
			"jd":"1010",
			"high_net_worth":"1040",
			"large":"1002",
			"fortune_500":"1001",
			"small":"1005",
			"medium":"1003",
			"xlarge":"1006",
			"medium_large":"1003",
			"finance":"1053",
			"accounting_and_accounting_services":"1053",
			"consulting":"1053",
			"insurance":"1053",
			"finance_creditcards_transaction_processing":"1053",
			"finance_investing_banking":"1053",
			"finance_portfolio_management_financial_advice":"1053",
			"finance_venture_capital_private_equity":"1053",
			"finance_wealth_management":"1021",
			"finance_accounting":"1021",
			"non_profit":"1060",
			"information_technology":"1027",
			"software":"1063",
			"telecommunications":"1063",
			"manufacturing":"1058",
			"retail":"1062",
			"operations":"1025",
			"healthcare":"1055",
			"medical_health":"1024",
			"medical_doctor":"1024",
			"construction":"1051",
			"construction_residential_building":"1051",
			"media_and_internet":"1059",
			"advertising_and_marketing":"1059",
			"publishing":"1059",
			"commercial_printing":"1059",
			"graphic_design":"1059",
			"sales":"1026",
			"marketing":"1026",
			"law_firms_and_legal_services":"1057",
			"legal":"1023",
			"energy_utilities_and_waste_treatment":"1052",
			"real_estate":"1061",
			"government":"1054",
			"human_resources_and_staffing":"1056",
			"hr":"1022",
			"hospitality_lodging_resorts":"1064",
			"transportation":"1064",
			"transportation_aviation_aerospace":"1064",
			"transportation_marine_shipping":"1064",
			"transportation_trucking_and_logistics":"1064",
			"agriculture_animals_and_livestock":"1050",
			"agriculture_crops":"1050",
			"consumer_veterinary":"1050",
			"metals_and_mining":"1058",
			"hospitality":"1064"
		}

		
		
		var exdate=new Date();
		exdate.setDate(exdate.getDate()+7);
		
		var custbizo = ""; 
		if(bizo_data.bizographics.functional_area) {
			for(var i = 0; i < bizo_data.bizographics.functional_area.length; i++) {
      			var bizo_code = bizo_data.bizographics.functional_area[i].code;
      			var bizo_parent_code = bizo_data.bizographics.functional_area[i].parent_code;
      
      			// ind and fa are both called "finance" :(
      			if ((bizo_code == 'finance') || (bizo_parent_code == 'finance')) {
					custbizo += '1021,';	
				} else if (bizo_codes_to_custom_segments[bizo_code]) {
					if (custbizo.indexOf(bizo_codes_to_custom_segments[bizo_code]) == -1) {
						custbizo += bizo_codes_to_custom_segments[bizo_code] + ',';	
					}
				} else if (bizo_codes_to_custom_segments[bizo_parent_code]) {
					if (custbizo.indexOf(bizo_codes_to_custom_segments[bizo_parent_code]) == -1) {
						custbizo += bizo_codes_to_custom_segments[bizo_parent_code] + ',';	
					}
				
				}
    		}
		}	
   		if(bizo_data.bizographics.industry) {
			for(var i = 0; i < bizo_data.bizographics.industry.length; i++) {
      			var bizo_code = bizo_data.bizographics.industry[i].code;
      			var bizo_parent_code = bizo_data.bizographics.industry[i].parent_code;
      
      			// look up custom segment by code or parent code
      			if (bizo_codes_to_custom_segments[bizo_code]) {
					if (custbizo.indexOf(bizo_codes_to_custom_segments[bizo_code]) == -1) {
						custbizo += bizo_codes_to_custom_segments[bizo_code] + ',';	
					}
				} else if (bizo_codes_to_custom_segments[bizo_parent_code]) {
					if (custbizo.indexOf([bizo_codes_to_custom_segments[bizo_parent_code]]) == -1) {
						custbizo += bizo_codes_to_custom_segments[bizo_parent_code] + ',' ;	
					}
				}
    		}
		}	
		
		
		if(bizo_data.bizographics.seniority) {
			if (bizo_codes_to_custom_segments[bizo_data.bizographics.seniority.code]) {
		    	custbizo += bizo_codes_to_custom_segments[bizo_data.bizographics.seniority.code] + ',';
			}		
  		}
		if(bizo_data.bizographics.group) {
			if (bizo_codes_to_custom_segments[bizo_data.bizographics.group.code]) {
		    	custbizo += bizo_codes_to_custom_segments[bizo_data.bizographics.group.code] + ',' ;
			}		
  		}
		
		if(bizo_data.bizographics.education) {
			if (bizo_codes_to_custom_segments[bizo_data.bizographics.education.code]) {
		    	custbizo += bizo_codes_to_custom_segments[bizo_data.bizographics.education.code] + ',';
			}		
  		}
		
		if(bizo_data.bizographics.company_size) {
			if (bizo_codes_to_custom_segments[bizo_data.bizographics.company_size.code]) {
		    	custbizo += bizo_codes_to_custom_segments[bizo_data.bizographics.company_size.code] + ',';
			}		
  		}		
		
		var custbizoarr = custbizo.split(",");
		var bizostr = "";	
		for(var i=0; i<custbizoarr.length; i++) {
				if (custbizoarr[i].length > 1) {
					bizostr += "biz=" + custbizoarr[i] + ';';
				}	
		}
	
		document.cookie= "bizo4cf=" + escape(bizostr) + ";path=/;expires=" + exdate.toUTCString();
	}
</script>

