			function expandClick(num) {
				if ($("#rest" + num).css("display") != 'inline') {
					$("#rest" + num).css({ display:'inline' });
					$("#expand" + num).css({ display:'none' });
				}
			}
			
			function collapseClick(num) {
				if ($("#expand" + num).css("display") != 'inline') {
					$("#rest" + num).css({ display:'none' });
					$("#expand" + num).css({ display:'inline' });
				}
			}
