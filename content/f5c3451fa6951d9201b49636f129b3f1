<script type="text/javascript">
	function formatItem(row) {
		var strRow="";
		switch(row[2]){
			case 'Stock': case 'ETF': case 'Fund': strRow+='<div class="sm_ac_ticker">'+row[0]+'</div>' + '<div class="sm_ac_name">'+row[1]+'</div>' + '<div class="sm_ac_type">'+row[2]+'</div>'; break;
			case 'Header':
			if(row[1]=='Ticker') strRow+="Choose a symbol to get quote:";
			if(row[1]=='Keyword') strRow+="Choose a Topic to search news:";
			break;
			case 'Topic': strRow+='<div style="float:left;">'+row[0]+'</div>' + '<div class="sm_ac_type">'+row[2]+'</div>'; break;
		}
		return strRow;
	}
	
	$('input#searchString').result(function(event, data, formatted) {
		
			var type = data[2];
		
		switch(type){
			case 'Stock': case 'ETF': case 'Fund': location.href='http://www.smartmoney.com/quote/'+formatted+'/'; break;
			case 'Topic': location.href='http://www.smartmoney.com/search/?type=news&searchterm='+formatted; break;
		}
	});
	
	
	function formatJSONItem(row){
		var strRow="";
		switch(row["COL_3"]){
			case 'Stock': case 'ETF': case 'Fund': strRow+='<div class="sm_ac_ticker">'+row["COL_1"]+'</div>' + '<div class="sm_ac_name">'+row["COL_2"]+'</div>' + '<div class="sm_ac_type">'+row["COL_3"]+'</div>'; break;
			case 'Header':
			if(row["COL_2"]=='Ticker') strRow+="Choose a symbol to get quote:";
			if(row["COL_2"]=='Keyword') strRow+="Choose a Topic to search news:";
			break;
			case 'Topic': strRow+='<div style="float:left;">'+row["COL_1"]+'</div>' + '<div class="sm_ac_type">'+row["COL_3"]+'</div>'; break;
		}
		return strRow;
	}
	
	function parseData(data){
		var parsed = [];
		var results = data["SEARCHRESULTS"];

		for(var i=0; i< results.length; i++){
			row = results[i];
			parsed[parsed.length] = {
					data: row,
					value: row["COL_1"],
					result: row["COL_1"]
				};
		}
		return parsed;
	}
	
	function formatResult(row) { return row[0].replace(/(<.+?>)/gi, ''); }
	
		$("#searchString").autocomplete('/search/suggest2.cfm', {
		width: 305, delay: 200, max: 25, resultsClass: "sm_ac_results", loadingClass: "sm_ac_loading", multiple: false, selectFirst: false, formatItem: formatItem,  matchSubset:0, scrollHeight:500
		});
	
</script>

