<script>
function goHeaderSearch()
{
    var word=document.getElementById("txtSearchWord").value;
    if(word=="" || word=="search for a timeline, event, group") return;
    location.href="http://www.xtimeline.com/search.aspx?q="+escape(word);
}
function goHeaderLogin()
{
    location.href="http://www.xtimeline.com/login.aspx?returnurl="+escape(location.href);
}
</script>

