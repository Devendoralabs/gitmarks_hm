	<script type="text/javascript">
		// nasty wordpress hack
		document.addEventListener('load', function () {
			function getElementsByClass(searchClass,node,tag) {
				var classElements = new Array();
				if ( node == null )
					node = document;
				if ( tag == null )
					tag = '*';
				var els = node.getElementsByTagName(tag);
				var elsLen = els.length;
				var pattern = new RegExp('(^|\\\\s)'+searchClass+'(\\\\s|$)');
				for (i = 0, j = 0; i < elsLen; i++) {
					if ( pattern.test(els[i].className) ) {
						classElements[j] = els[i];
						j++;
					}
				}
				return classElements;
			}

			var empty_ul = getElementsByClass('post-meta', document, 'ul');

			for (var i = 0; i < empty_ul.length; i++) {
				if(empty_ul[i].getAttribute('class') == 'post-meta') {
					if (empty_ul[i].innerHTML.replace(/^\s*|\s*$/mg, '') === '') {
						empty_ul[i].parentNode.removeChild(empty_ul[i]);
					}
				}
			}
		}, true);
	</script>

