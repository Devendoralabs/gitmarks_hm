<script type="text/javascript">

    var domainPrice = 397.00;
    var domainName = 'uglywebsitecontest.com';
    var currencyRates = new Object;
    currencyRates['USD'] = 1.00000;
    currencyRates['GBP'] = 0.63700;
    currencyRates['EUR'] = 0.72630;
    currencyRates['CAD'] = 1.06740;
    currencyRates['AUD'] = 1.14170;

    function setPaymentMethod(m)
    {
        switch (m) {
            case 1: // CC
                $("#divCC").css("display", "block");
                $("#divInvoice").css("display", "none");
                $("#divPayPal").css("display", "none");
                copyFieldValues(0);
                break;
            case 2: // PayPal
                $("#divCC").css("display", "none");
                $("#divInvoice").css("display", "none");
                $("#divPayPal").css("display", "block");
                break;
            case 3: // Invoice
                $("#divCC").css("display", "none");
                $("#divInvoice").css("display", "block");
                $("#divPayPal").css("display", "none");
                copyFieldValues(1);
                break;
        }

    }

    function onInvoiceCurrencyChange()
    {
        var currency = $('#selInvoiceCurrency').val();
        var price = domainPrice * currencyRates[currency];

        $('#spanInvoiceAmount').text(price.toFixed(2) + ' '  + currency);
    }

    function submitForm()
    {
        var error = false;
        var errorFields = '';
        var emailre = /^(([A-Za-z0-9]+_+)|([A-Za-z0-9]+\-+)|([A-Za-z0-9]+\.+)|([A-Za-z0-9]+\++))*[A-Za-z0-9]+@((\w+\-+)|(\w+\.))*\w{1,63}\.[a-zA-Z]{2,6}$/;
        //var emailre = /.*/;

        if ( $('#radCC').attr('checked') )
        {
            // check email
            if ( ! emailre.exec($('#txtEmail').val() ) )
            {
                $('#txtEmail').addClass('gl-text-error');
                $('#txtEmail').focus();
                errorFields += 'Email\n';
                error = true;
            }
            else
            {
                $('#txtEmail').removeClass('gl-text-error');
            }

            // validate CC input
            var fields = ['#txtFirstName', '#txtLastName', '#txtPhone', '#txtAddress1',
                '#txtCity', '#txtZip', '#txtNameOnCard', '#txtCVV'];
            var fieldNames = ['First Name', 'Last Name', 'Phone', 'Address1',
                'City', 'Zip', 'Name On Card', 'CVV'];

            // check field existance
            for (var i=0; i<fields.length; i++)
            {
                if ( $(fields[i]).val() == '' )
                {
                    $(fields[i]).addClass('gl-text-error');
                    errorFields += fieldNames[i] + '\n';
                    if (!error){ $(fields[i]).focus(); }
                }
                else
                {
                    $(fields[i]).removeClass('gl-text-error');
                }
                error = error || ($(fields[i]).val() == '');
            }

            // validate cc number
            var ccre = /\d{16}|\d{15}/;
            if ( ! ccre.exec($('#txtCC').val()) )
            {
                $('#txtCC').addClass('gl-text-error');
                errorFields += 'Card Number\n'
                if (!error){ $('#txtCC').focus(); }
                error = true;
            }
            else
            {
                $('#txtCC').removeClass('gl-text-error');
            }

        }

        if ( $('#radInvoice').attr('checked') )
        {
            // check email
            if ( ! emailre.exec($('#txtInvoiceEmail').val()) )
            {
                $('#txtInvoiceEmail').addClass('gl-text-error');
                $('#txtInvoiceEmail').focus();
                errorFields += 'Email\n';
                error = true;
            }
            else
            {
                $('#txtInvoiceEmail').removeClass('gl-text-error');
            }

            // validate CC input
            var fields = ['#txtInvoiceFirstName', '#txtInvoiceLastName', '#txtInvoiceAddress1',
                '#txtInvoiceCity', '#txtInvoiceZip',
                '#txtInvoicePhone'];
            var fieldNames = ['First Name', 'Last Name', 'Address1',
                'City', 'Zip',
                'Phone'];

            // check field existance
            for (var i=0; i<fields.length; i++)
            {
                if ( $(fields[i]).val() == '' )
                {
                    $(fields[i]).addClass('gl-text-error');
                    errorFields += fieldNames[i] + '\n';
                    if (!error){ $(fields[i]).focus(); }
                }
                else
                {
                    $(fields[i]).removeClass('gl-text-error');
                }
                error = (error || ($(fields[i]).val() == ''));
            }
        }

        if (error)
        {
            alert('Following fields are required:\n' + errorFields);
            return false;
        }

        $("#divSubmitButton").css("display", "none");
        $("#divLoader").css("display", "block");

        document.forms['frmOrderForm'].submit();
        return false;
    }

    function showFaq(num)
    {
        //hide all
        for (var i=1; i<=6; i++)
            $('#divFaq' + i).hide();

        $('#divFaq' + num).show();

        return false;
    }

    function updateDomainPrice()
    {
        $.get(
            url('/store/get-domain-price'),
            {'domain' : domainName},
            function(response){
                if (response.price != domainPrice)
                {
                    $('#spanTotalPrice').text( response.formatted_price );
                    domainPrice = response.price;
                    alert('Domain price has been updated!')
                }
                setTimeout('updateDomainPrice()', 2000);
            },
            'json'
        );
    }

    $(function(){
        //$('img.question-mark').cluetip( {splitTitle: '|'} );
        $("#accordion").css('display', 'block').accordion( { 'header' : 'h3' } );
        setTimeout('updateDomainPrice()', 2000);
    });

    function onApplyDiscountClicked()
    {
        var code = $('#txtDiscountCode').val().toUpperCase();

        $.post(url('/store/apply-discount-code'),
            {'domain' : domainName, 'code' : code},
            function(response)
            {
                if (response.errorCode == 0)
                {
                    $('#spanTotalPrice').text( response.formatted_price );
                    domainPrice = response.price;
                    onInvoiceCurrencyChange();
                    alert(response.message);
                    $('#divDiscount').html("");
                }
                else
                {
                    alert(response.errorDesc);
                }
            },
            'json'
        );
        return false;
    }


    function copyFieldValues(fromCC)
    {
//First Name
//Last Name
//Email Address
//Phone
//Address1
//Address2
//City
//Zip Code
//State
//Country
        if (fromCC)
        {
            if ($('#txtFirstName').val() != "") $('#txtInvoiceFirstName').val($('#txtFirstName').val());
            if ($('#txtLastName').val() != "") $('#txtInvoiceLastName').val($('#txtLastName').val());
            if ($('#txtEmail').val() != "") $('#txtInvoiceEmail').val($('#txtEmail').val());
            if ($('#txtPhone').val() != "") $('#txtInvoicePhone').val($('#txtPhone').val());
            if ($('#txtAddress1').val() != "") $('#txtInvoiceAddress1').val($('#txtAddress1').val());
            if ($('#txtAddress2').val() != "") $('#txtInvoiceAddress2').val($('#txtAddress2').val());
            if ($('#txtCity').val() != "") $('#txtInvoiceCity').val($('#txtCity').val());
            if ($('#txtZip').val() != "") $('#txtInvoiceZip').val($('#txtZip').val());
            $('#selInvoiceState').val($('#selState').val());
            $('#selInvoiceCountry').val($('#selCountry').val());
        } else
        {
            if ($('#txtInvoiceFirstName').val() != "") $('#txtFirstName').val($('#txtInvoiceFirstName').val());
            if ($('#txtInvoiceLastName').val() != "") $('#txtLastName').val($('#txtInvoiceLastName').val());
            if ($('#txtInvoiceEmail').val() != "") $('#txtEmail').val($('#txtInvoiceEmail').val());
            if ($('#txtInvoicePhone').val() != "") $('#txtPhone').val($('#txtInvoicePhone').val());
            if ($('#txtInvoiceAddress1').val() != "") $('#txtAddress1').val($('#txtInvoiceAddress1').val());
            if ($('#txtInvoiceAddress2').val() != "") $('#txtAddress2').val($('#txtInvoiceAddress2').val());
            if ($('#txtInvoiceCity').val() != "") $('#txtCity').val($('#txtInvoiceCity').val());
            if ($('#txtInvoiceZip').val() != "") $('#txtZip').val($('#txtInvoiceZip').val());
            $('#selState').val($('#selInvoiceState').val());
            $('#selCountry').val($('#selInvoiceCountry').val());
        }
    }

</script>

