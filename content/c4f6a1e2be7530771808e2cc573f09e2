<script type="text/javascript" src="/scripts/cookies.js"></script>
<script type="text/javascript" language="JavaScript1.2">
    $(document).ready(function () {
        $(".localeChangeBox").change(function() {
            var origSearch = location.href.split("?")[1];
    
            if(origSearch != null && origSearch.length > 0){
                var search = origSearch.split("&");
                var newSearch = "";
    
                for(i = 0; i < search.length; ++i){
                    if(search[i].substring(0, 7) == "locale="){
                        newSearch += "locale=" + $(this).val();
                        if(i < search.length - 1) 
                        	newSearch += '&';
                    } else if(search[i].length > 0) {
                        newSearch += search[i];
                        if(i < search.length - 1) 
                        	newSearch += '&';
                    }
                }
                if(newSearch.indexOf("locale") == -1) {
                	newSearch += "&locale=" + $(this).val();
                }
                window.location = window.location.pathname + "?" + newSearch;
            } else {
                window.location = window.location.pathname + "?locale=" + $(this).val();
            }
        });
    });
</script>
<div style="padding:1px 0 0;position:relative;text-align:right;width:100%;">
     
        <form name="localeChange">
    
        <select name="locale" class="localeChangeBox" style="width:160px;">
            
                <option value="DE" >Deutsch (German)</option>
            
                <option value="EN" selected="selected">English</option>
            
                <option value="ES" >Español (Spanish)</option>
            
                <option value="FR" >Français (French)</option>
            
        </select>
    
        </form>
    
