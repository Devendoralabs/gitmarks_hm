        function pause(millisecond) {
            var now = new Date();
            var exitTime = now.getTime() + millisecond;
            while(true)
            {
                now = new Date();
                if(now.getTime() > exitTime) return;
            }
        }
    
        function rotateTestimonial() {
            $('#testimonialText').html(testimonials[tIdx]);
            $('#testimonialName').html(names[tIdx]);
            $('#testimonial').fadeIn(250, function() {
                $('#testimonial').animate({
                    leftMargin: "0"
                }, 15000, 'linear', function() {
                    $('#testimonial').fadeOut(250, function(){ rotateTestimonial(); });
                    tIdx++;
                    if(tIdx >= testimonials.length) { tIdx = 0; }
                });
            });
        }
    
