<script type="text/javascript">
    $(document).ready(function() {
        $("#searchQueryField").focus(function() { 
            if($(this).val() == "Search") {
                $(this).attr("value", "");
                $(this).css("color", "#000000");
            }
        });
        
        $("#searchQueryField").blur(function() { 
            if($(this).val() == "") {
                $(this).attr("value", "Search");
                $(this).css("color", "#9b8579");
            }
        });
    
        $("#topSearch").submit(function() {
            var value = $("#searchQueryField").attr("value");
            if(value != "" && value != "Search") {
                submit();
            } else {
                return false;
            }  
        });
        
        $("#searchQueryField").css("color", "#9b8579");
    });
</script>

    
    
        
    

