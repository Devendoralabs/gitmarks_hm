<script language="JavaScript">
// tease out the data for sending to comment app
// taken liberally from drugpicklist.js
var _enames="", _eids="";
var _dnames="", _dids="";
var _first=null, _firstid;
function parseForm(drugField, symptomField)
{
    _enames=_eids="";
    _dnames=_dids="";
    _first=null;
  
    var drugs=document.getElementById(drugField);
    var effects=document.getElementById(symptomField);
    if (drugs!=null)
    {
        drugs = drugs.value.replace("{","").replace("}","");
        var drugStrParser = /,(\d+)=(\w+):(.+?)(?=(?:,\d+=|$))/g;
        var match;
        
        while((match = drugStrParser.exec(drugs)) != null) 
        {
            if (_first==null) {_first=match[3]; _firstid=match[1]} 
            _dids += match[1] + ','
            _dnames += match[3] + '|';
        }
        //alert("drugnames:" + dnames); alert("drugids:" + dids);
        
        if (effects!=null)
        {
            effects = effects.value;
            var list = effects.split(",,");
            for(var i=0; i< list.length; i++)
            {
                var symp = list[i];
                if (symp.indexOf("=") != -1)
                {
                    var synId = symp.substring(0, symp.indexOf("="));
                    var name = symp.substring(symp.indexOf("=")+1);
                    name = name.substring(name.indexOf(":")+1);
                    synId=synId.replace('[','').replace(']', '');
                    _eids += synId + ',';
                    _enames += name + '|';
                }
            }
        }
    }
}

function doReviews(verb, autosearch) {
    parseForm('drugName', 'addedSymptoms');
    if (verb == "cs") {
        window.location = "searchReviews.do";
    } else if (_first != null || autosearch == 0) {
		var url = "DrugDetail.do?sid=" + _firstid + "&view=" + verb + "&dname=" + escape(_first)
						+ "&dids=" + _dids + "&eids=" + _eids
                        + "&autosearch=" + autosearch
                        + "&dnames=" + escape(_dnames)
                        + "&enames=" + escape(_enames);
        window.location = url;
    } else {
        alert("You have not selected any drugs.");
    }
}

</script>
<tr>
    <td width="670px">
        
            &nbsp;<a href="mainReviews.do">User Reviews</a>&nbsp;|
            &nbsp;<a href="javascript:gotoAddReview()">Add Review</a>&nbsp;|
            &nbsp;<a href="javascript:doReviews('cs', 0)">Search Reviews</a>
        
    </td>
  <td>
	  <script language="JavaScript" type="text/javascript">
	  var drugSearch = new drugSearchList('drugSearch');
	  drugSearch.inputId = "searchDrugField";
	  drugSearch.popupLeftOffset = -400;
	  </script>
	  Drug Search <input
                type="text" name="searchDrug"
                size="25" id="searchDrugField" autocomplete="off" 
                style="color:gray;"
				onkeyup="drugSearch.handleKeyUp(event, this.value)"
				onkeydown="drugSearch.handleKeyDown(event)"
				onkeypress="drugSearch.handleKeyPress(event)"
		        onclick="drugSearch.handleKeyUp(event, this.value)">
  </td>
