		function CTIsPlayback() {
		   try { return parent && parent.WebPlayer; }
		   catch(e) { return false; }
		}
		if(!CTIsPlayback() ) {
			if (top != self) {
			    top.location = self.location;
			}
		}

		if (document.addEventListener){
		    document.addEventListener("onmouseup", display, false);
		    document.addEventListener("click", hideTellMeMore, false);
		}
		else if (document.attachEvent){
		    document.attachEvent("onmouseup", display);
		    document.attachEvent("onclick", hideTellMeMore);
		}
        </script>

		

