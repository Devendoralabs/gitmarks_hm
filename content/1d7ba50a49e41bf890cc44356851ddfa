		function rolloverchange(el) {

			if(el.checked) {
				$("preview2").style.display="";
				$("properties2").style.display="";
				$("color2").style.display="";
				btor.set_rollover_mode(true);
				btor.fix_disabled_layers();
				new Ajax.Request('/home/set_rollover', {asynchronous:true, evalScripts:true});
			} else {
				btor.set_rollover_mode(false);
				$("preview2").style.display="none";
				$("properties2").style.display="none";
				$("color2").style.display="none";
				new Ajax.Request('/home/remove_rollover', {asynchronous:true, evalScripts:true});
				btor.fix_disabled_layers();
				window.setTimeout( function() {
					btor.fix_disabled_layers();
				}, 100);
			}
		}	
		
		
		function show_logintop() {
			if($("welcomeguest").style.display=="none") {
				$("welcomeguest").style.display="";
				$("logintop").style.display="none";
				
			} else {
				$("logintop").style.display="";
				$("welcomeguest").style.display="none";
			}
		}
		

		function buttons_click() {
			if($("buttonstab").className =="tabon") {				
				
			} else {
				$("buttonsdisplay").style.display="";
				$("iconsdisplay").style.display="none";
				$("buttonstab").className='tabon';
				$("iconstab").className='taboff';
				if($("favstab")!=null) {
					$("favsdisplay").style.display="none";
					$("favstab").className='taboff';
				}
			}
		}
		
		function icons_click() {
			if($("iconstab").className =="tabon") {				
				
			} else {
				$("iconsdisplay").style.display="";
				$("buttonsdisplay").style.display="none";
				$("iconstab").className='tabon';
				$("buttonstab").className='taboff';
				if($("favstab")!=null) {
					$("favsdisplay").style.display="none";
					$("favstab").className='taboff';
				}
				
			}
		}
		
		function favorites_click() {
			if($("favstab").className =="tabon") {				
				
			} else {
				$("favsdisplay").style.display="";
				$("iconsdisplay").style.display="none";
				$("buttonsdisplay").style.display="none";
				$("favstab").className='tabon';
				$("iconstab").className='taboff';
				$("buttonstab").className='taboff';
				
			}
		}
		
		function update_url(el) {
			if(el.value == "") {
				$('code_area').value = $('code_no_link').value;
			} else {
				$('code_area').value = "<a href=\"" + el.value + "\">" + $('code_no_link').value + "</a>";
			}
		}
