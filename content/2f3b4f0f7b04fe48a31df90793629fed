<script language="javascript" src="/scripts/dojo-release-1.1.0/dojo/dojo.js" djConfig="isDebug: false, parseOnLoad: true"></script>
<script type="text/javascript">
   dojo.require("dojo.string");

   function search() {
      var search = dojo.byId("search");
      var searchQuery = dojo.string.trim(search.value);
      if(searchQuery.length > 0) {
         location.replace("/searchresult.jspc?searchQuery=" + searchQuery + "&type=people");
      }
   }   
</script>

