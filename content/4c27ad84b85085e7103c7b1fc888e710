	<script type="text/javascript">
	jQuery(function() {
		jQuery("#tabs").tabs({event: 'mouseover'}).addClass('ui-tabs-vertical ui-helper-clearfix');
		var columnHeight = jQuery("div#sidebar").height();
		jQuery("div#post-wrap").css({'height':columnHeight});
	});
	</script>
</body>
</html>

