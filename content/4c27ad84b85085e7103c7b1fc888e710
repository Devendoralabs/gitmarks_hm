<script type='text/javascript'><!--// <![CDATA[
function adMover(item, target) {
var target = document.getElementById(target);
var item = document.getElementById(item);
if (target && item)
  target.appendChild(item);
}

adMover("sidebarbanners", "sidebarbannerscontainer");
adMover("premiumsponsors", "premiumsponsorscontainer");
adMover("contentad", "contentadcontainer");
adMover("topad", "topadcontainer");
adMover("commentad", "commentadcontainer");
adMover("logobadge", "logobadgecontainer");
adMover("premiumsponsorbottom", "premiumsponsorbottomcontainer");
adMover("homepagepremiumad", "homepagepremiumadcontainer");

function hidetrackinglinks(){var urls=document.getElementsByTagName("a");var urlcount=(urls.length);for(var i=0;i<urlcount;i++){var url=document.getElementsByTagName("a")[i].href;if(url.substring(0,68)=="http://auslieferung.commindo-media-ressourcen.de/www/delivery/ck.php"){var urlsplit=url.split("oadest=");var newurl=unescape(urlsplit[1]);urls[i].href=newurl;urls[i].setAttribute("onmousedown","this.href='"+url+"'")}}}
hidetrackinglinks();

// ]]> --></script>


