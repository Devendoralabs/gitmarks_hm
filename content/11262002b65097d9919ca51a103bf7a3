  	<script type="text/javascript" charset="utf-8">

  		function cnt(w){
				var y=w;
				var r = 0;
				a=y.replace(/\s/g,' ');
				a=a.replace(/[-_*]/g,' ');
				a=a.split(' ');
				for (z=0; z<a.length; z++) {if (a[z].length > 0) r++;}
				return r;
			}

			function desc_cnt() {
			    var textfield = "textarea#id_job_description";
			    var cntfield = "span#characters_left";
			    var maxlength = 128;
			    var warning_level = 5;

			    try{
			    var t = $(textfield);
			    var c = $(cntfield);
			    var t_v = t.val();
			    var l = t_v.length;

				var l = cnt(t_v);

				}
			    catch(err) {
			        return false;
			    }


				var left = maxlength - l;

			    c.empty();

			    if(left < 0) {
			        over = l - maxlength;
					if(over > 1) {
			        	c.append(over + " words over the limit");
					} else {
						c.append(over + " word over the limit");
					}
			        c.removeClass('warning');
			        c.removeClass('perfect');
			        c.addClass('nomore');
			    } else if (left == 0) {
			        c.removeClass('warning');
			        c.removeClass('nomore');
			        c.addClass('perfect');
			        c.append("You've reached the maximum of "+maxlength+" words");
			    } else if ((left <= warning_level) & (left > 0)) {
			        c.removeClass('nomore');
			        c.removeClass('perfect');
			        c.addClass('warning');
					if(left > 1) {
			        	c.append(left + " words left"); 
					} else {
						c.append(left + " word left");
					}
			    } else if (left > warning_level) {
			        c.removeClass('nomore');
			        c.removeClass('warning');
			        c.removeClass('perfect');
			        c.append(left + " words left");
			    }
			}

			function submit_form() {
			    var the_form = "form#listingform";
			    try{
			        var f = $(the_form)
			    }
			    catch(err) {
			        return false;
			    }
			    f[0].submit();
			    return false;
			}



			$(document).ready(function() {

			/*       $("textarea#id_job_description").keyup(desc_cnt);
			        $("textarea#id_job_description").keydown(desc_cnt);*/
			        $("a#try-it-again").click(submit_form);

			        $("a.choosepaypal").click(function(){
			            $("div#ccstuff").slideUp('fast', function() {
			                $("div#ppstuff").slideDown('fast');
			            });
			        });

			        $("a.chooseauthorizenet").click(function(){
			            $("div#ppstuff").slideUp('fast', function() {
			                $("div#ccstuff").slideDown('fast');
			            });
			        });

			        $("input#source_authorizenet").change(function(){
			           $('div#ccstuff').slideDown('fast', function(){
			               $("div#payment_source").slideUp('fast');
			           }); 
			        });

			        $("input#source_paypal").change(function(){
			            $('div#ppstuff').slideDown('fast', function(){
			                $("div#payment_source").slideUp('fast');
			            });
			        });

			        /*desc_cnt();*/
			    }
			);
  	</script>

