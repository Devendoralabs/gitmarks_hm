<script>
	function SubmitBeta()
		{
		HidePopupDiv("betaFeedbackDiv");

		QBbusy ("Sending feedback...");

		setTimeout ("SubmitBeta2()", 100);
		}
		
	function SubmitBeta2()
		{
		var jax = new jaxreq ("9fwpesnn?a=qbi_addrecord");
		
		jax.AddValue ("_fid_10", GetValue("BetaComments"));
		jax.AddValue ("_fid_6",  "Beta Feedback");
		jax.AddValue ("_fid_76", "unknown@quickbase.com");
		jax.AddValue ("_fid_74", "");
		jax.AddValue ("_fid_75", "");
		
		try	{
			jax.DoSyncCmd ();
			}
		catch(e)
			{
			alert(e);
			QBbusyHide();
			return;
			}
		
		QBbusyHide();

		if (jax.success)
			{
			var x = Math.floor (Math.random() * 3);
			
			if		(x == 0)			ShowConfirmationMsg ("Feedback received. Thanks very much!");
			else if	(x == 1)			ShowConfirmationMsg ("Feedback received. Much appreciated!");
			else						ShowConfirmationMsg ("Feedback received. Thank you!");
			}
		else
			jax.ShowErrAlert();
		
		}
</script>

<div id=betaFeedbackDiv class=PopBox style="width:480px; display:none; visibility:hidden;">

