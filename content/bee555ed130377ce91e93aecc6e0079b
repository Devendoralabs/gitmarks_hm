<script language=JavaScript>

		function fi (ftype, fflags, fname)		// this should be replaced by the generic FINFO data structure build via AddFieldInfo (jrice 8/20/2006)
			{
			this.type			= ftype;
			this.name			= fname;
			this.flags			= fflags;
			}

		fi.prototype.IsMC			= function()	{ return ((this.flags & 0x01) != 0); }
		fi.prototype.IsBI			= function()	{ return ((this.flags & 0x02) != 0); }
		fi.prototype.HasFormula		= function()	{ return ((this.flags & 0x04) != 0); }
		

	var finfo = new Object();







</script>







