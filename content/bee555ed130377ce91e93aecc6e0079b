var GoalCodes = [];
GoalCodes[PageCodes["/Prod/AppCreate/ReturnToMyQB"]] = "appcreate";
GoalCodes[PageCodes["/Prod/AppCreate/GoToApp"]] = "appcreate";

function QBPgCode(trackitem) {
    //look up a code for the trackitem 
    //return 0 if nocode
    result = 0;
    if (PageCodes.hasOwnProperty(trackitem)) {
        result = PageCodes[trackitem];
    }
    return result;
}

function QBlogtrack(lg) {
    if (trackToLog && lg) {
        var trackitem = lg; // the trackitem name

        // get & strip specific detail args if any
        var extra = ""; 
        var details = lg.indexOf("?");
        if (details != -1){
           trackitem = lg.substring(0, details); // strip the details
           extra = lg.substring(details + 1).replace(/&/g, "~~"); //encode details as abc=def~~gji=hij
       }

        // set the viewed custom var of this page to follow the path of this visit
	    GoogleAnalyticsTracker.push(['_setCustomVar',
                        2,                    //slot used for pagepath info
                        "viewed",             //what was viewed, to capture path/funnel
                        trackitem,            //value
                        GoogleAnalyticsVarScope.page //scope
                        ]); 
	    

        // find a code for item being logged and add any detail info
        var code = QBPgCode(trackitem); 

        if (GoalCodes[code] !== undefined)
        {
            // set the created custom var of this page to segment goal reached during the session
	        GoogleAnalyticsTracker.push(['_setCustomVar',
                            3,                    //slot used for goal info
                            "completed",          //goal met capture for this session
                            GoalCodes[code],      //value
                            GoogleAnalyticsVarScope.session //scope
                            ]); 
	    
        }

        // make the isapi param
        var Xtparam = "&Xt=" + extra;
        var isapinfo = "&isapinfo=ga." + code;
        //logMessage("trkitem: " + lg + " isapi: " + isapinfo + "Xt=" + Xtparam);

        $.ajax({
            url: "main?a=QBI_logtrack&lg=" + lg + isapinfo + Xtparam,
            success: function (html) {
                ; // alls well ux was logged
            },
            error: function (err) {
                ; // tracking could not be logged
            }
        });
    }
};


/* track both pageview and event */
function QBTrackPageviewAndEvent(pg, category, action, optLabel, optValue) {
    QBTrackPageView(pg);
    QBTrackEvent(category, action, optLabel, optValue);
}

/*  track pageview to GA given a string for the page
    PageDescription/ActionTarget/NameOfAction?data1=value1&data2=value2
*/
function QBTrackPageView(pg) {
    if (GetAndSetupGATracker()) {
        if (pg && pg[0] != "/") {
            pg = pg.replace(/ /gi, "_");//spaces to underscores
            pg = "/Prod/" + pg; // prefix to distinguish inproduct page activities in GA from marketing tracking
        }
        QBlogtrack(pg);
        GoogleAnalyticsTracker.push(['_trackPageview', pg]);
    }
}

/*  track event to GA given a category, action, and optional labels (string) and values(int)
    PageDescription, SubPanel, NameOfAction, optional int value
*/
function QBTrackEvent(category, action, optLabel, optValue) {
    if (GetAndSetupGATracker()) {
    if (typeof(optLabel) == 'undefined'){
        GoogleAnalyticsTracker.push(['_trackEvent', category, action]);
        }
    else
        {
        GoogleAnalyticsTracker.push(['_trackEvent', category, action, optLabel, optValue]);
        }
    }
}

/* Track event to GA and then call function after timeout
   this is useful when you are going to leave the current page after doing tracking
   if there is no timeout, will fail to track
*/
function setupGATrackAndCallback(jsStatement, pg, category, action, optLabel, optValue) {
    var evt = category || false;
    if (!evt)
        QBTrackPageView(pg);
    else
    QBTrackPageviewAndEvent(pg, category, action, optLabel, optValue);
    setTimeout(jsStatement, 300);
}

function readCookieGoogle(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}

