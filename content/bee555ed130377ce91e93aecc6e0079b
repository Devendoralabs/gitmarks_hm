<script lang=javascript>

var sortURLs = new Array();

sortURLs[""]	= "";


var kNumFrozenCols	= "";
var kWhichScroller	= "";

var gIsStandardViewPage = true;

var kScroller = null;
	

//**************************************

function ScrollProc (ev)
	{
	if (kScroller)		// sometimes gets called before LoadProc (something to do with the back button)
		{
		kScroller.AdjustFrozenCol();
		kScroller.AdjustFrozenRow();
		}
	}

//**************************************

function ResizeProc ()
	{
	if (kScroller)	
		kScroller.AdjustInsets();
	}

//**************************************

function DesignSideBarHook (isOpening, wid)
	{
	if (kScroller)	
		kScroller.AdjustInsets();
	}


//**************************************

</script>



