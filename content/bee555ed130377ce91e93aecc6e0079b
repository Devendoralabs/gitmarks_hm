<script>
	
	function CloseAppTokenReminder(dbid)
		{
		noAppTokenDiv = document.getElementById("appTokenEnable");
		noAppTokenDiv.style.display = "none";
		
		var jax = new jaxreq (gReqAppDBID + "?a=QBI_SetShowDevKeyAppReminder");
			
		jax.AddValue ("showreminder", "0");
		
		jax.DoSyncCmd ();

		if (jax.success)
			RefreshPage();
		else
			jax.ShowErrAlert();
		
		}
		
	function EnabledAppTokenClicked(dbid)
		{
		closeElem ("appTokenEnable");
		
		var jax = new jaxreq (gReqAppDBID + "?a=QBI_EnableRequireDevKeyApp");
			
		openElem ("appTokenSetConfirm");		
				
		jax.DoSyncCmd ();

		if (jax.success) {
			openElem ("appTokenSetConfirm");
			}
		else {
			jax.ShowErrAlert();
			}
		}
		
	function ShowEnableAppTokenHelp()
		{
		openElem ("AppTokenHelp");
		}
		
	function CloseAppTokenConfirm()
		{
		closeElem ("appTokenSetConfirm");
		}
		
		
</script>




