<script type="text/javascript">
var IsGoogleAnalyticsEnabled = "1" == "1" ? true : false;
var isInTrial = "false" == "true" ? true : false;

var trackToLog = true;

var GoogleAnalyticsTracker;

/*all pages will have the google analytics tracking functions, but will only  load 
the ga.js file and actually do tracking if it is enabled
if tracking is not enabled, will not load the file but functions will still be available
(even though they do nothing) so that there are no js errors*/
(function()
{    //asynch install google analytics js code

    if (IsGoogleAnalyticsEnabled)
	{
        if (typeof (_gat) != 'object')// not already loaded?
		{
			var ga = document.createElement('script'); 
			ga.type = 'text/javascript'; 
			ga.async = true; 
			ga.src = 'https://ssl.google-analytics.com/ga.js';
			//ga.src = 'https://ssl.google-analytics.com/u/ga_debug.js'; 
			var s = document.getElementsByTagName('script')[0]; 
			s.parentNode.insertBefore(ga, s); 
		}
    }
})();

var GoogleAnalyticsVarScope = {
    "visitor" : 1, //a set value count for all visits by this computer/browser/life of cookies
    "session" : 2, //a set value counts for whole session (session ends after >30minutes without a pageview)
    "page": 3     //a set value counts for one page trackpageview, 
}
//Use Custom var 
//GoogleAnalyticsTracker.push(['_setCustomVar',
//                    number,  //slot used 1-5 when slots # are same, visit trumps session & session trumps page
//                    string,  //name for custom var, name + value string length < 64
//                    string,  //value for custom var, name + value string length < 64; 
//                    GoogleAnalyticsVarScope.page //scope of variable
//                    ]); 

//call before each tracking for common setup code
//returns false if GA is not enabled
function GetAndSetupGATracker() {
    if (!IsGoogleAnalyticsEnabled)
        return false;
	GoogleAnalyticsTracker = window._gaq || (window._gaq = []);
    GoogleAnalyticsTracker.push(['_setAccount', 'UA-230037-1']); // ga tracking profile
    GoogleAnalyticsTracker.push(['_setDomainName', 'none']);
    GoogleAnalyticsTracker.push(['_setAllowLinker', true]);

    // set the custom var to tell if this session is a trialer
	GoogleAnalyticsTracker.push(['_setCustomVar',
                    4,                    //slot used for session trialer info
                    "trialer",            //user category
                    isInTrial ? "true" : "false",               //value
                    GoogleAnalyticsVarScope.session //scope
                    ]); 
    return true;
}

