		<textarea type=text NAME="BetaComments" cols=60 rows=10></textarea>

		<div style="float:right; padding-top:24px; margin-right:6px">
			<input type=button value="Submit" onclick="SubmitBeta(this.form)">
			<input type=button value="Cancel" onclick="HidePopupDiv(this)">
		</div>
	</form>       

</div>






<script>
	var gwidth =
		{
		gh:550,
		g1:750,
		g2:500,
		g3:550,
		g4:850,
		g5:750,
		
		uh:650,
		u1:550,
		u2:565,
		u3:600,
		u4:700
		
		
		};
		
	var gheight =
		{
		// Persistent
		p1:720, // security
		p2:1500, // pricing
		p3:670, // import
		p4:400, // account admin
		
		// Team Management
		gh:500,
		g1:550, // manage files and 
		g2:280, // grid edit...
		g3:230, // email
		g4:820, // reports for managers
		g5:550, // granting access
		
		// Sales Management
		s1:850, // track status
		s2:650, // collect leads
		s3:720,	// sales reports
		s4:700, // import from ACT
		
		// Task Management
		t1:700, // send reminder emails
		t2:825, // timelines
		t3:820, // track status
		t4:550, // working with Project
		
		// User Management
		uh:650,
		u1:575, // controlling access
		u2:975, // customizing
		u3:730, // dashboards
		u4:635  // display different info
		};
				
	function OpenFeatureWindow(feature)
		{
		var scroll = false;
		var height = gheight[feature];
		if (height > screen.availHeight - 50)
			{
			height = screen.availHeight - 50;
			scroll = true;
			}
		var width  = 600;	// use same width for all windows
		if (gwidth[feature])
			width = gwidth[feature];
			
		var ext = feature != "p2" ? ".html" : ".asp";
		var url =  "/gs/" + feature + ext;
		
		var title = "Feature";
		var left = screen.availWidth/2 - width/2;
		var wfeatures = "resizable=yes,width=" + width + ",height=" + height + ",left=" + left;
		if (scroll)
			wfeatures += ",scrollbars=yes"
		var w = window.open(url, title, wfeatures );
		w.resizeTo(width,height);
		w.focus();
		
		return false;
		}

function HandleTrialMessage (msgCode)
	{
	if (msgCode == "try")
		ShowPopupDiv('tryAnotherBox');
	else
		OpenFeatureWindow (msgCode);
	
	return false;
	}

</script>			


