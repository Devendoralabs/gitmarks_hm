    function firstTimeHotelInit()
    {
        if (rFltSrch != null && rFltSrch.length==1 && "" != "true" && "" == "true") {
            if (parseInt(rFltSrch[0].travelers) < 5) {
                jq(document.hotels.rooms).val('1');
                jq(document.hotels.guests1).val(sadult[0]);
            } else {
                jq(document.hotels.rooms).val('2');
                roomsChanged();
                jq(document.hotels.guests2).val(rFltSrch[0].travelers);
            }
            document.hotels.checkin_date.value = rFltSrch[0].leg[0].leave;
            if (rFltSrch[0].leg.length == 2) {
                document.hotels.checkout_date.value = rFltSrch[0].leg[1].leave;
            }
            if (rFltSrch[0].sdestlocation.match(/ USA$/)) {
                document.hotels.location.value=sdestlocation.substring(0,rFltSrch[0].sdestlocation.length-4);
            } else {
                document.hotels.location.value=rFltSrch[0].sdestlocation;
            }
            var found=false;

            document.hotels.othercity.value=document.hotels.location.value;
            cityfocus();
        }

        document.hotels.datelimit.value = (today.getMonth() - 0 + 1) + "/" + today.getDate() + "/" + today.getFullYear();
    }

    function getGroopleTrack()
    {
                return 'kayak';
    }





        
        
        
            
        
