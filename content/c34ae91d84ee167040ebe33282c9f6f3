jq(function() {
        
    
    jq("#recenthistorycontainer div.arrowSectionHeader").click(function() {
        var e = jq("#" + this.id + "content");
        e[e.is(":visible") ? "hide" : "show"]();
        jq(this).toggleClass("arrowSectionHeaderOpen");
    });
    
    jq("#fdexpert").click(function() {
      var bb = jq('#recenthistorycontainer').find(".fdsectionheaderbottomboot");
      bb[bb.is(":visible") ? "hide" : "show"]();
      jq(this).toggleClass("arrowSectionHeaderBottom");
    });

        jq("#fdimgbutton")
        .mousedown(function(){jq(this).attr("src", "/v358/images/button/redesign/a-search-button-fd-down.gif")})
        .mouseup(function(){jq(this).attr("src", "/v358/images/button/redesign/a-search-button-fd-up.gif")})
        .mouseout(function(){jq(this).attr("src", "/v358/images/button/redesign/a-search-button-fd-up.gif")});
    });

function disableSearchButton()
{
        jq("#fdimgbutton").unbind("mouseup").unbind("mousedown");
    setTimeout(function() {
        jq("#fdimgbutton").attr({src: "http://cdn5.kayak.com/v358/images/button/redesign/a-search-button-fd-disabled.gif", disabled: 'disabled'});
    }, 1);
        document.body.style.cursor = 'wait';
    formAlreadySubmitted = true;
}
