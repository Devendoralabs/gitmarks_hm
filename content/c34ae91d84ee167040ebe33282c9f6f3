<script language='javascript'>

var useSolrSmarty = true;
function clearDates()
{
    // clear values
    document.hotels.checkin_date.value = '';
    document.hotels.checkout_date.value = '';
    // reset the date help fields
    var datehelp = document.getElementById('checkin_date_domlabel');
    if (datehelp) {
        datehelp.innerHTML = "mm/dd/yyyy";
    }
    datehelp = document.getElementById('checkout_date_domlabel');
    if (datehelp) {
        datehelp.innerHTML = "mm/dd/yyyy";
    }
    // clear the date metacookies
    jq.get("/s/sparkle?action=clearMetaCookies&names=SD1,SD2");
}


</script>

