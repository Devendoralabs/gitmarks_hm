		function setDisp(disp)
		{
			var exp = new Date();
			exp.setDate(exp.getDate()+365);
			document.cookie = 
				"HPDISPALL2=" + (disp?"Y":"N") + 
				"; domain=.thedailywtf.com" +
				"; expires=" + exp.toGMTString();
			window.location = '/';
		}
