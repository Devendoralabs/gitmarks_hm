		<script language="javascript" type="text/javascript">
		var elem;
	    elem = document.getElementById("loginMsg");
		//elem.style.visibility = "hidden";
		function submitLoginbutton() {
			login_validateLogin(validtest(document.loginForm.mod_login_username.value), hex_md5(document.loginForm.mod_login_password.value) );
			if (typeof( window.event ) != "undefined" ) {
				event.cancelBubble = true;
				event.returnValue=false;
			}
		}
		function submitLoginForm() {
			document.getElementById('spoofHolder').name = validate;
            document.loginForm.f1.value =  validtest(document.loginForm.username.value);
            document.loginForm.username.value = '';
            document.loginForm.f2.value =  hex_md5(document.loginForm.mod_login_password.value);
            document.loginForm.mod_login_password.value = '';
			document.loginForm.submit();
		}
		function highlightLoginMessage() {
			var elem;
		    elem = document.getElementById("loginMsg");
			elem.style.display = "inline";
			var myFx = new Fx.Style('loginMsg', 'opacity').start(0,1);
		}
		function hideLoginMessage() {
			var elem;
	     	elem = document.getElementById("loginMsg");
			var myFx = new Fx.Style('loginMsg', 'opacity').start(1,0);
		}
        function getLoginTooltipText (num, divtitle, divtext, divicon, divwidth) {
            icon = 'HelpCircle.gif';
            if (num == '1') {
                helptext = 'CoveritLive offers two secure login options:';
                if (browser == 'msie' && browserversion < 7  ) {
                    helptext =    helptext + '<br><br>';
                }
                helptext =    helptext + '<br><br><ul class="list1">' +
                           '<li><strong>Standard Login:</strong>&nbsp;Login credentials are encrypted.</li>' +
                           '<br><li><img src="images/icons/lock.png" class="correct-png" />&nbsp;<strong>SSL Login:</strong>&nbsp;Login credentials are encrypted over a SSL connection. Click the "SSL Login" link to use this option.</li>' +
                           '</ul><br>NOTE: Both options use encryption to send login data. If you have any issues with our SSL option, please use our Standard Login.';

                titletext = 'Secure Login';
            }
            var title = '<table style="width: 100%;"><tr><td style="width:100%; height:100%;"><table style="width:100%; height:100%; background:url(/templates/coveritlive/images/right.gif) repeat-y right #D1D3C1;"><tr><td style="width:100%; height:100%; background:url(/templates/coveritlive/images/line.gif) repeat-x top;"><table style="width:100%; height:100%; background:url(/templates/coveritlive/images/line.gif) repeat-x bottom;"><tr><td style="width:100%; height:100%; background:url(/templates/coveritlive/images/line.gif) repeat-y left;"><table style="width:100%; height:100%;"><tr><td style="padding: 5px; width:100%; height:100%; background: url(/templates/coveritlive/images/globebkgd20.png) 0 0 no-repeat; border: 1px solid #C1C3B3;border-right: none;" valign="middle"><img src="/templates/coveritlive/images/' + icon + '" align=right height=16 /><h6>' + titletext + '</h6></td></tr></table></td></tr></table></td></tr></table></td></tr></table>';
            text = title + '<div class="clearingspace">&nbsp;</div><table style="width: 100%;"><tr><td style="width:100%; height:10px;"><table style="width:100%; height:100%; background:url(/templates/coveritlive/images/right.gif) repeat-y right #D1D3C1;"><tr><td style="width:100%; height:100%; background:url(/templates/coveritlive/images/line.gif) repeat-x top;"><table style="width:100%; height:100%; background:url(/templates/coveritlive/images/line.gif) repeat-x bottom;"><tr><td style="width:100%; height:100%; background:url(/templates/coveritlive/images/line.gif) repeat-y left;"><table style="width:100%; height:100%;"><tr valign=top><td style="width:100%; height:100%;padding: 10px;text-align: left;">';
            if (browser == 'msie' && num <= 10) {
                text = '<div onmouseover="clearTimeout(ttlogin);" onmouseout="closeLoginTip();" >' + text;
            } else if ( num <= 10 ) {
                text = '<div onmouseover="clearTimeout(ttlogin);" onmouseout="closeLoginTip();" style="font-family: Arial; border:2px solid #666666;    border-top: 1px solid #666666; border-left: 1px solid #666666;left:4px;top:4px;position:absolute;width:' + divwidth + 'px;background-color:#E8E9E0;padding:5px;font-size: 90%; line-height: 1.2em;    overflow:auto;z-index:1000001; text-align: left;">' + text;
            } else {
                text = '<div style="font-family: Arial; width:' + divwidth + 'px;background-color:#E8E9E0;padding:5px;font-size: 100%;overflow:auto;z-index:1000001;text-align: left;">' + text;
            }
            text = text + helptext +'</td></tr></table></td></tr></table></td></tr></table></td></tr></table></div>';
            return text;
        }
        var ttlogin;
        function closeLoginTip () {
            ttlogin = setTimeout('ajax_hideTooltip();', 250);
        }

