        function loadNews(link, id, moreTab)
        {
            var newsPanel = $("newsPanel");
            var panel = (id ? $("newsTab" + id + "_content") : $("allNewsTab"));

            var tabs = $$($("newsTabBar"), "div");
            
            for(var i=0; i<tabs.length; i++)
            {
                if(moreTab)
                {
                    if(tabs[i].id != "moreTab" && tabs[i].className == "TabButton TabButton_Active")
                    {
                        tabs[i].className = "TabButton";
                    }
                    else if(tabs[i].id == "moreTab")
                    {
                        tabs[i].className = "TabButton TabButton_Active";
                        tabs[i].style.cursor = "pointer";
                    }
                }
                else
                {
                    if(tabs[i].id != (id ? "newsTab" + id : "allNewsTab") && tabs[i].className == "TabButton TabButton_Active")
                    {
                        tabs[i].className = "TabButton";
                    }
                    else if(tabs[i].id == (id ? "newsTab" + id : "allNewsTab"))
                    {
                        tabs[i].className = "TabButton TabButton_Active";
                    }
                }
            }
            
            if(panel)
            {
                for(var i=0; i<newsPanel.childNodes.length; i++)
                {
                    if(newsPanel.childNodes[i].nodeName.toLowerCase() == "div")
                    {
                        if(newsPanel.childNodes[i].id != (id ? "newsTab" + id + "_content" : "allNews"))
                        {
                            newsPanel.childNodes[i].style.display = "none";
                        }
                        else
                        {
                            newsPanel.childNodes[i].style.display = "";
                        }
                    }
                }
            }
            else
            {
                var callback =
                {
                    success: function(o)
                    {
                        var xmlDoc = o.responseXML;
                        
                        for(var i=0; i<newsPanel.childNodes.length; i++)
                        {
                            if(newsPanel.childNodes[i].nodeName.toLowerCase() == "div")
                            {
                                newsPanel.childNodes[i].style.display = "none";
                            }
                        }
                        
                        panel = document.createElement("div");
                        panel.id = "newsTab" + id + "_content";
                        newsPanel.appendChild(panel);
                        
                        var news = $$(xmlDoc, "news");
                        
                        var html = "<table cellpadding=\"0\" cellspacing=\"0\" style=\"width:100%;\">";
                        
                        for(var i=0; i<news.length; i+=2)
                        {
                            html += "<tr>" +
                                "<td style=\"width:50%;\">" +
                                    "<div style=\"width:100%;height:31px;line-height:30px;overflow:hidden;\">" +
                                        "<div style=\"padding-left:20px;margin-right:10px;height:30px;border-bottom:dotted #ccc 1px;background-image:url(App_Themes/Default/Images/Common/Icons/NewsItem.gif);background-repeat:no-repeat;background-position:left center;\">" +
                                            "<a href=\"" + $$(news[i], "url")[0].firstChild.nodeValue + "\" onclick=\"this.target='_blank';\">" + $$(news[i], "headline")[0].firstChild.nodeValue + "</a>" +
                                            " <span style=\"font-size:95%;color:#999;\">&ndash; " + $$(news[i], "companyName")[0].firstChild.nodeValue + "</span>" +
                                        "</div>" +
                                    "</div>" +
                                "</td>";
                            
                            if(i+1 >= news.length)
                            {
                                html += "<td style=\"width:50%;\">" +
                                            "&nbsp;" +
                                        "</td>" +
                                    "<tr>";
                            }
                            else
                            {
                                html += "<td style=\"width:50%;\">" +
                                            "<div style=\"width:100%;height:31px;line-height:30px;overflow:hidden;\">" +
                                                "<div style=\"padding-left:20px;margin-left:10px;height:30px;border-bottom:dotted #ccc 1px;background-image:url(App_Themes/Default/Images/Common/Icons/NewsItem.gif);background-repeat:no-repeat;background-position:left center;\">" +
                                                    "<a href=\"" + $$(news[i+1], "url")[0].firstChild.nodeValue + "\" onclick=\"this.target='_blank';\">" + $$(news[i+1], "headline")[0].firstChild.nodeValue + "</a>" +
                                                    " <span style=\"font-size:95%;color:#999;\">&ndash; " + $$(news[i+1], "companyName")[0].firstChild.nodeValue + "</span>" +
                                                "</div>" +
                                            "</div>" +
                                        "</td>" +
                                    "<tr>";
                            }
                        }
                        
                        html += "</table>";
                        html += "<div style=\"padding-left:20px;height:30px;line-height:30px;background-image:url(App_Themes/Default/Images/Common/Icons/Rss.png);background-repeat:no-repeat;background-position:left center;\"><a href=\"Rss/Newswire.aspx?id=" + id + "\" style=\"color:#c15b02;\">Subscribe to: <b>" + link.innerHTML + "</b></a></div>";

                        panel.innerHTML = html;
                        Daedalus.Ajax.HideLoadingPanel();
                    }
                };
                
                Daedalus.Ajax.ShowLoadingPanel();
                YAHOO.util.Connect.asyncRequest("GET", "Ajax/GetNews.aspx?id=" + id, callback, "");
            }
        }
        
        function cancelEvent(evt)
        {
            if (!evt) var evt = window.event;

            if (window.event)
                window.event.cancelBubble = true;
            else if(evt.stopPropagation)
                evt.stopPropagation();
        }

        //]]>
    </script>


        </div>

