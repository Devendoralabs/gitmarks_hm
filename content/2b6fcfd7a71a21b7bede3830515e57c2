<script type="text/javascript" charset="utf-8">
  var pathUrl = '/';
	if(!pathUrl) pathUrl = site_root;
  var shown_tab_type = 'for_sale';
  function show_homepage_tab(tab_type)
  {
    if (!tab_type)
      return;

    // Try to populate location field from another tab if empty
    var dest = xb_get_element('searchtext' + (tab_type=='for_sale'?'':'_'+tab_type));
    if (dest && (dest.value=='' || dest.value==zsbdef2))
    {
      var tabs = ['','for_rent','sold'];
      var cont = true;
      for(var i in tabs)
      {
        if ((tabs[i]!='' && tabs[i]!=tab_type) || (tabs[i]=='' && tab_type!='for_sale'))
        {
          var src = xb_get_element('searchtext' + (tabs[i]==''?'':'_'+tabs[i]));
          if (src && src.value!='' && src.value!=zsbdef2)
          {
            dest.value = src.value;
            handle_edit_box(dest,zsbdef2,false);
            cont = false;
          }
        }
        if (!cont)
          break;
      }
    }

    if(tab_type=='for_rent')
      xb_get_element('advanced_search_link_' + tab_type).href = pathUrl + 'advanced/rent/';
    else if (xb_get_element('advanced_search_link_' + tab_type))
      xb_get_element('advanced_search_link_' + tab_type).href = pathUrl + 'advanced/';

    add_class(shown_tab_type + '_search_box_form', 'gone')
    remove_class(shown_tab_type + '_homepage_tab', 'green_tab_selected');
    
    remove_class(tab_type + '_search_box_form', 'gone');
    add_class(tab_type + '_homepage_tab', 'green_tab_selected');
    
    shown_tab_type = tab_type;
  }
  show_homepage_tab('for_sale');
</script>

          
          
          
