  function close_promo()
  {
    var promo=xb_get_element('newsfeed_promo');
    if (promo)
    {
      promo.style.display = 'none';

      // Make sure they don't see it again for 1. setCookie defined in site.js
      setCookie('hppromo','1', build_cookie_exp_date(90));
    }
  }
