<script>
function runBeacon()
{
  try {
    var tempurl = window.location.href;
    var regex = new RegExp("((http|https)://)([a-zA-Z0-9]+\.)(.*)");
    var comurl = tempurl.match(regex);
    var beacon_url = comurl[3]+comurl[4];

    try {
      var id = getCookie('tlftmusr');
    } catch (e) {
      var id = '';
    }

    COMSCORE.beacon({
      c1:2,
      c2:6036483,
      c3:"",
      c4:beacon_url,
      c5:"",
      c6:"",
      c15:id
    });
  } catch (e) {}
};
runBeacon();
</script>
<noscript>
  <img src="http://b.scorecardresearch.com/p?c1=2&c2=6036483&c3=&c4=&c5=&c6=&c15=&cj=1" />
</noscript>
<!-- End comScore Tag -->


  <!-- CLEAN UP l_carousel COOKIE -->
  <script>
  if(getCookie('l_carousel'))
  {
    var ck = getCookie('l_carousel').split(';;')
    var new_ck = '';
    for(i=0;i<ck.length;i++)
    {
      if(ck[i]!='' && new_ck=='')
        new_ck = ';;'+ck[i]+';;'
    }
    var date = new Date();
    date.setTime(date.getTime()+(24*60*60*1000));
    // format: Tue, 14 Jul 2009 00:58:30 GMT
    setCookie('l_carousel',new_ck,date.toGMTString());
  }
  </script>


