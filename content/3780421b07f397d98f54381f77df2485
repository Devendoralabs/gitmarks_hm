    <script type="text/javascript">
       //<![CDATA[
         var gridCSSFile = "/include/style/grid/LayoutWideGrid.css";
       //]]>     
    </script>
    <script type="text/javascript">
    //<![CDATA[
    var reflow = false,
        viewport = jQuery(window),
        head = document.getElementsByTagName("head")[0],
        grid = new adobe.dom.CSSStyleSheet(gridCSSFile),
        theme = new adobe.dom.CSSStyleSheet("/include/style/default-wide.css");

    setRenderMode();

    jQuery(window).bind("resize", setRenderMode);

    function setRenderMode() {
        if(!head || !grid || !theme) { return; }

        var _reflow = (viewport.width() > 1331);

        if(_reflow == reflow) { return; }

        if(_reflow) {
            grid.enable(head);
            theme.enable(head);
        } else {
            grid.disable();
            theme.disable();
        }

        reflow = _reflow;
    }
    //]]>
</script>
