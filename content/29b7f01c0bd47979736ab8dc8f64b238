      };


      /*
       * Locate the span element and attach the ShareThis button.
       */
      var el = document.getElementById("sb_art_sharethis");

      /*
       * Create a sharelet with no button and a myCustomCallback onclick handler.
       */
      var obj = SHARETHIS.addEntry({
        title:'Search Rank Easy to Manipulate',
        type: 'url'},
        {button:false, onclick:myCustomCallback}//, offsetLeft: -el.scrollWidth}
      );
      obj.attachButton(el);
        // following param not used
        /* icon: 'http://wired.com/images/slideshow/magazine/1506/pl_print_f.jpg', */
    //]]>
</script>

        </div>

    </div> <!-- close toolbox_art_bot -->

