<script language="JavaScript" type="text/javascript">
<!-- // inspired by example at javascript-array.com

var timeout	= 499;
var closetimer = null;
var delay = 500;
var opentimer = 0;

var current_item = 0;
var default_item = 0;
var id = 'i1';
var close_ok = true;


default_item = document.getElementById('i8');
current_item = default_item;
current_item.style.display='block';


function open_it(newid)
{	
	id = newid;
	close_ok = true;
	opentimer = window.setTimeout('show_it()', delay);
}

function show_it()
{	
	// cancel close timer
	cancel_close_time();
	// close old layer
	if(current_item) current_item.style.display='none';
	
	current_item = document.getElementById(id);
	current_item.style.display='block';
}

// close showed layer
function close_it()
{
	if(current_item!=default_item && close_ok)
	{
		current_item.style.display='none';
		default_item.style.display='block';
		current_item = default_item;
	}
}

function close_time()
{
	if(opentimer)
	{
		window.clearTimeout(opentimer);
		opentimer = null;
	}
	
	closetimer = window.setTimeout(close_it, timeout);
}

function cancel_close_time()
{
	if(closetimer)
	{
		window.clearTimeout(closetimer);
		closetimer = null;
	}
	close_ok = false;
}

function menuAllOut(event)
{
	var toElement = null;
	if (event.relatedTarget)
		toElement = event.relatedTarget;
	else if (event.toElement)
		toElement = event.toElement;
	while (toElement && toElement.id != "menuAll")
	{
		toElement = toElement.parentNode;
	}
	if (!toElement)
	{
		close_ok = true;
		close_time();
	}
}
// close layer when click-out
document.onclick = close_it; 
-->
</script>

        
