<script type="text/javascript">
var hashstr = location.hash;
var hashstring = hashstr.substring(1);

function packCheck() {
  if (hashstring) {
    var keyValuePairs = new Array();
    for(var n=0; n < hashstring.split("&").length; n++) {
      keyValuePairs[n] = hashstring.split("&")[n];
      if (keyValuePairs[n] == "frompack") {
        document.getElementById('packNote').className = 'show';
      }
    }
  }
}

</script>

