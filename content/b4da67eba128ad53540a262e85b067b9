            $(function() {
                var path = "/";
                $('.nav ul li a').each(function () {
                    var href = $(this).attr('href');
                    if ((path == '/' && href == '/') ||
                        (href != '/' && path.indexOf(href) == 0)) {
                        $(this).parent().addClass('active');
                        return false;
                    }
                });
            });
