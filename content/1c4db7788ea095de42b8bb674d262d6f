<script language="javascript" type="text/javascript">
	function validateSearch()
	{
		var zipcode		= trim(document.getElementById('idsearchByZip').value);
		var state		= trim(document.getElementById('idsearchByState').value);
		if(zipcode == "" && state == "" )
		{
		alert('Please Enter Zip Code Or State');
		return false;
		}
		
	}
	function validate_Search()
	{
		var zipcode		= trim(document.getElementById('idsearchBy_Zip').value);
		var state		= trim(document.getElementById('idsearchBy_State').value);
		if(zipcode == "" && state == "" )
		{
		alert('Please Enter Zip Code Or State');
		return false;
		}
		
	}	
</script>
