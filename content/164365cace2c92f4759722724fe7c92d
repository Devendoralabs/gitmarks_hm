function setMsg(msg){
	alert(msg)
	$j("#msg").html(msg);
	position = (bwidth-$j("#msg").width())/2;
	$j("#msg").css("left",position).show();		
	setTimeout('$j("#msg").hide()',5000);
}
function getRanKey(params){	
	seq = 0;
	avilcount = 0;
	finalA = {};
	$j("#useval").val("");
	$j.ajax({
		dataType: 'jsonp',
		jsonp: 'jsonp_callback',
		url: "http://9.latest.my-reg.appspot.com/checker",
		data: params+"&jpost=1",
		success: function(data){
		if(data){					
			$j("#ranval").val(data.key);					
		}
		}
	});			
	getAllCData();
}
function allTest(){
	getAllCData();
	return "good";
}
var avilcount = 0;
var cdfreq = "";
function getAllCData(){	
		
		
		var pinputs = $j("input[name='package']");
		var psites = 0;
		seq = seq + 1;
		key = $j("#ranval").val();
		var completedF = false;
		if(seq==1)cdfreq = $j("input[name='package']:checked").attr("package");
		if(key != ""){
		var params = "ran="+key+"&jpost=0&seq="+seq;		
		$j.ajax({
		   dataType: 'jsonp',
		   jsonp: 'jsonp_callback',
		   url: "http://9.latest.my-reg.appspot.com/checker",
		   data: params,
		   success: function(data){				
				if(data){					
					$j("#useval").val(data.result);	
					$j.each(data, function(i, val) {	
						if(!finalA[i]){					
							try{
								if(val == "Available"){
									avilcount++;
									$j(".popup-loader label").html(avilcount+"/"+cdfreq);
									if(avilcount >= cdfreq){																			
										completedF = true;
										seq = 22;
										hidePopDiv();
										addWatRegCart();								
										$j("#useval").val("completed");										
									}
								}
							}catch(e){}
							finalA[i] = i;						
						}						
					});	
								
					if(data.result=="completed"){							
						if(!completedF){							
							$j.each(pinputs,function(m,n){
								psites = $j(this).attr("package");
								range = $j(this).attr("range").split("-")[0];
								if(psites>=avilcount && range>=avilcount)$j(this).attr("disabled",true);
							})
							$j("input[name='package']:not(:disabled)").attr("checked",true);
							if(parseInt($j("input[name='package']:not(:disabled)").length))allcdatF = true;
							$j(".popup-contentbody .pop-msg").html("Selected Top "+cdfreq+" sites.. Availabel only "+avilcount+" sites select appropriate package to proceed further...").show();
							$j(".popup-contentbody #cart-but").show();
							$j(".popup-loader").css("background","none");
						}
					   
					}			
				}	
				
			}
		 });		
		
		
			
			}
		
			if($j("#useval").val() != "completed"){
				window.setTimeout('try{allTest()}catch(e){alert(e)}',1000);
			}
			else{					
				$j("#checkIt").bind("click",checkIt);			
				$j("#save-res-lnk a").attr("href",udomain+"dashboard-account.htm#myss").attr("target","_parent");		
			}
		
			
	
}
function removeCartErrors(){
	$j("tr[class='wderror'] span").remove();
	$j("tr[class='wderror']").removeClass('wderror');
}
function updateCart(json){
	allcdatF = true;
	$j.each(json,function(m,n){		
		if(m=="package"){
			$j("input[name='package'][value='"+n+"']").attr("checked",true);
		}
		else{
		 $j("#"+m).val(n);
		}
	})
}
function saveSearch(){	
	var filter = {};	
	var ssids = "";
	var avtxt = "";
	var slurl = "";
	var flag = false;
	var totalC = 0;
	var doubtC = 0;	
	var restrictedC = 0;
	
	availC = topRA[catA]["a"]; 
	takenC = topRA[catA]["t"];
	doubtC = topRA[catA]["d"];
	minC = topRA[catA]["mi"];
	maxC = topRA[catA]["ma"];
	hyphenC = topRA[catA]["h"];
	
	restrictedC = minC + maxC + hyphenC;
	totalC = catA-(availC+takenC+doubtC+minC+maxC+hyphenC);		
	doubtC = (totalC) ? doubtC+totalC : doubtC;		
	slurl = $j("#sUrl").val();
	
	$j.each($j(".siteList:visible").find("div[ssid]"),function(m,n){
		ssids += (flag) ? "," : "";
		ssids += '"'+$j(this).attr("ssid")+'"';
		ssids += ':"'+$j(this).find("img:last").attr("sts")+'"';
		flag = true;
	})	
		10/12/2010
	$j.extend(filter,{"type": "svsearch","action":"add","avail":availC,"unavail":takenC,"restricted":restrictedC,"doubt":doubtC,"skey":searchKey,"cat":advitem,"jsn":ssids,"slurl":slurl,"topRA":topRA});		
	$j.post("actions.php",filter,
			function(data){	
				
	});
	
	
}
