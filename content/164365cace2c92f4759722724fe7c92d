		$j("#searchOptions,#advsearch").bind("click",advSearch);			
		
		$j("body * > #tabs").click(function(){
			var tab = $j(this).attr("tab");
			if(tab){		
				$j("body * > #tabs").removeClass("tactive").addClass("tinactive");
				$j(this).addClass("tactive").removeClass("tinactive");
				$j("body * > #tab-cont").hide();
				$j("body * > #tab-cont[tab='"+tab+"']").show();
			}
		})			
		$j(".siteList table tr:nth-child(even)").css("background-color","#c5e9f3");		
