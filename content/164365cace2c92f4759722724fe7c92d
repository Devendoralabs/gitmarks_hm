function setCatA(){	
	catA = [];	
	if(parseInt($j("#chkGroup input[type='checkbox']:checked").length)){
		$j.each($j("#chkGroup input[type='checkbox']:checked"),function(m,n){			
			catA.push($j(this).val());
		})
	}else{
		catA.push(advitem);
	}	
}
var total = 0;
function setCounts(){
	var avail = 0;
	var unavail = 0;
	var doubt = 0;	
	var percent = 0.03;
	var bal = 0;	
	var ftotal = 0;
	if(fcatA==20){
	$j.each(catA,function(m,n){		
		avail = $j(".siteList[category='"+n+"'] img[alt='Available']").length + avail;
		unavail = $j(".siteList[category='"+n+"'] img[alt='UnAvailable']").length + unavail;
		doubt = $j(".siteList[category='"+n+"'] img[alt='Doubt']").length + doubt;
		//doubt = (doubt>0) ? doubt : "0";		
		$j("#availa").html(avail.toString());
		$j("#unavaila").html(unavail.toString());
		$j("#doubt").html(doubt.toString());
		total = parseInt(avail)+parseInt(unavail)+parseInt(doubt);	
		$j("#total").html(total.toString());	
	})	
	}else{
		$j("#availa").html(topRA[fcatA]["a"].toString());
		$j("#unavaila").html(topRA[fcatA]["t"].toString());
		$j("#restricteda").html(parseInt(topRA[fcatA]["mi"])+parseInt(topRA[fcatA]["ma"])+parseInt(topRA[fcatA]["h"]));
		$j("#doubt").html(topRA[fcatA]["d"].toString());
		//alert(availC+" "+takenC+" "+doubtC)
		total = parseInt(topRA[fcatA]["a"])+parseInt(topRA[fcatA]["t"])+parseInt(topRA[fcatA]["d"])+parseInt(topRA[fcatA]["mi"])+parseInt(topRA[fcatA]["ma"])+parseInt(topRA[fcatA]["h"]);	
		$j("#total").html(total.toString());	
	}
	//bal = Math.ceil(stotal * percent);	
	//usernamecheck//only bal = Math.ceil(50 * percent);	
	bal = 2;
	//usernamecheck//only ftotal = topRA[50]["a"]+topRA[50]["t"]+topRA[50]["d"]+topRA[50]["mi"]+topRA[50]["ma"]+topRA[50]["h"];//new modification for top 500 search for all categories...
	
	//usernamecheck//only
	try{
	ftotal = $j("img[alt='Available']").length+$j("img[alt='UnAvailable']").length+$j("img[alt='Doubt']").length+$j("img[alt='Minimum']").length+$j("img[alt='Maximum']").length+$j("img[alt='Hyphen']").length;
	}catch(e){
		alert(e)
	}
	if((20 - ftotal) <= bal){		
		$j("#useval").val("completed");
		seq = 22;
	}
	update();
}
function doActions(){
	var action = $j(this).attr("action");
	var skey = $j("#search");
	var cartF = true;
	var cartR = "";
	var pack = "";

	if(action == "share"){
		$j("#share_img").trigger("click");
	}
	else if(action == "login"){
		loginPopD();
	}
	/*else if(action == "sea"){			
			addWatRegCart(action);
	}*/
	else{
		skey = (skey.val()=="" || skey.attr("title")==skey.val()) ? "" : skey.val();
		//pack = $j("#ord-pack input[name='package']:checked").attr("package");
		$j("#pack").val("20");
		$j("#svc").val(action);
		$j("#sstab").val("bpack");
		$j("#skey").val(skey);
		$j("#svcfrm").submit();
	}
}
function ssActions(){	
	var sstab = $j(this).attr("sstab");
	if(sstab=="search")window.location = udomain+"/brands.htm";
}
