<script type="text/javascript">
var viewPortH = 0;
var viewportW = 0;
$j(document).ready(function(){
	setDefaultWH();
	setPopMask();
	//showPopDiv();
})
function setDefaultWH(){
	viewPortH = $j(window).height();
	viewportW = $j(window).width();	
}
function setPopMask(){
	var mainH = parseInt(window.parent.$j("#main-bg").height());
	mainH = (mainH) ? mainH : 0;	
	bht = window.parent.$j("body").height();
	bwt = window.parent.$j("body").width();	
	ht = Math.max(mainH, bht, viewPortH);
	//alert(mainH+"  "+bht+"  "+viewPortH+"  "+ht);
	wt = (bwt>=viewportW) ? bwt : viewportW;	
	window.parent.$j("#popup-mask").height(ht+"px").width(wt+"px");	
}
function showAPopDiv(bheight){	
	$j("#apopup-mask").show();
	var windowH =  $j(window).height();	
	try{
		var popW = parseInt($j(".apopup-contentbody iframe").attr("width").replace("px",""))+10+"px";	
	}catch(e){
		var popW = parseInt($j(".apopup-contentbody").css("width").replace("px",""))+"px";		
	}
	
	popPosT = (windowH<=$j('.apopupdiv').height()) ? 20 : ( windowH - $j('.apopupdiv').height() )/2;	
	popPosL = ( viewportW - $j('.apopupdiv').width() )/2;
	$j('.apopupdiv').css({'top':popPosT,'left':popPosL,"width":popW});
	//$j('.popupdiv').css('left',popPosL);
	$j(".apopupdiv").show();
}
function hideAPopDiv(){	
	$j("#apopup-mask").hide();
	$j(".apopupdiv").hide();
}
function showPopDiv(){	
	$j("#popup-mask").show();
	var windowH =  $j(window).height();	
	try{
		var popW = parseInt($j(".popup-contentbody iframe").attr("width").replace("px",""))+10+"px";	
	}catch(e){
		var popW = parseInt($j(".popup-contentbody").css("width").replace("px",""))+"px";		
	}
	
	popPosT = (windowH<=$j('.popupdiv').height()) ? 20 : ( windowH - $j('.popupdiv').height() )/2;	
	popPosL = ( viewportW - $j('.popupdiv').width() )/2;
	$j('.popupdiv').css({'top':popPosT,'left':popPosL,"width":popW});
	//$j('.popupdiv').css('left',popPosL);
	$j(".popupdiv").show();
}
function hidePopDiv(){	
	$j("#popup-mask").hide();
	$j(".popupdiv").hide();
	try{$j("#checkIt").bind("click",checkIt);}catch(e){}
}
function showDIV(divID){
	$j("#blockUI").height($j("body").height());
	var topC = ($j("#blockUI").height()-$j("#"+divID).height())/2;
	var leftC = ($j("#blockUI").width()-$j("#"+divID).width())/2;
	
	$j("#"+divID).css({top:topC,left:leftC});
	$j("#blockUI").show();
	$j("#"+divID).show();
}
function hideDIV(divID){
	$j("#blockUI").hide();
	$j("#"+divID).hide();
}
function refreshPage(){
	window.parent.location.reload();
}
</script>
