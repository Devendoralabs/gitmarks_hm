	var finalA = [];	
	function getCData(){	
		
		seq = seq + 1;
		key = $j("#ranval").val();
		
		if(key != ""){
		var params = "ran="+key+"&jpost=0&seq="+seq;
		var gdata = ""
		
		
		$j.ajax({
		   dataType: 'jsonp',
		   jsonp: 'jsonp_callback',
		   url: "http://9.latest.my-reg.appspot.com/checker",
		   data: params,
		   success: function(data){				
				var time_sec = 0;
				var ilen = 0;
				if(data){								
					$j("#useval").val(data.result);	
					time_sec = data.time
					$j.each(data, function(i, val) {	
						if(!finalA[i]){					
							try{	
								ilen = parseInt($j("#"+i+" input[type='hidden']").length);	
								//alert(ilen+"   "+i)
								if(val == "Available"){									
									availC++;
									setTotalC("a",i);
									if(ilen){
										var url = $j("#"+i+" input[id='signup_url']" ).val();									
										$j("#"+i+" .inner").html('<a href='+url+' target="_blank"> <img class="imagePaddingRight" alt="Available" title="Click to reserve your Userbrand yourself" src="/images/Available1.gif" sts="a"/></a>');
										$j("#"+i+" a").attr("href",url);
										////alert("Available"+$j("#"+i+" .inner").html)
									}
								}else{
									if(val == "UnAvailable"){
										
										takenC++;
										setTotalC("t",i);
										if(ilen){
										    //alert(i+"  "+$j("div[id="+i+"] .inner").length)
											//$j("#"+i+" .inner").html("ra");
											var prfurl = $j("#"+fcatA+" #"+i+" input[id='prf_url']" ).val().replace("uname",searchKey);							
																			
											$j("#"+i+" .inner").html('<a href='+prfurl+' target="_blank"><img class="imagePaddingRight"  alt="UnAvailable" title="Click to see the existing profile" src="/images/UnAvailable1.gif" sts="t" /></a>');
											$j("#"+i+" a").attr("href",prfurl);
											//$j("div[id=" + i +"] .inner").append(" "+val);
											////alert("UnAvailable"+$j("#"+i+" .inner").html)
										}										
									}
									else if(val == "Doubt"){										
										doubtC++;
										setTotalC("d",i);
										if(ilen){
											$j("#"+i+" .inner").html('<img class="imagePaddingRight" width="12" height="13" alt="Doubt" title="unknown" src="/images/Doubt.gif" sts="d" />');
											//$j("div[id=" + i +"] .inner").append(" "+val);
											////alert("Doubt"+$j("#"+i+" .inner").html)
										}										
									}
									else if(val == "min"){										
										minC++;
										setTotalC("mi",i);
										if(ilen){
											$j("#"+i+" .inner").html('<img class="imagePaddingRight" alt="Minimum" title="Minimum length" src="/images/restricted.gif" sts="mi" />');											
										}										
									}
									else if(val == "max"){										
										maxC++;
										setTotalC("ma",i);
										if(ilen){
											$j("#"+i+" .inner").html('<img class="imagePaddingRight"  alt="Maximum" title="Maximum length" src="/images/restricted.gif" sts="ma" />');											
										}										
									}
									else if(val == "hyphen"){										
										hyphenC++;
										setTotalC("h",i);
										if(ilen){
											$j("#"+i+" .inner").html('<img class="imagePaddingRight" alt="Hyphen" title="Hyphen not allowed" src="/images/restricted.gif" sts="h" />');											
										}										
									}
								}
								
							}catch(e){}
						finalA[i] = i;
						
						}
					});	
								
					if(data["result"]=="completed"){						
					   searchAct("aft");
					}			
				}				
				setCounts();
				
				
			}
		 });		
		
		
			
			}			
			try{
				if($j("#useval").val() != "completed"){
					window.setTimeout('try{test()}catch(e){alert(e)}',1000);
				}
				else{
					$j("#checkIt").bind("click",checkIt);
					$j("#save-res-lnk a").attr("href",udomain+"/dashboard-mySavedSearch.htm").attr("target","_parent");			
				}
			}catch(e){
				alert(e)
			}
			
		}
	
