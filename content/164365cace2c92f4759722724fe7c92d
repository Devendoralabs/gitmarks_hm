			$j("#wmsg").html(werror).show();
			setTimeout('$j("#wmsg").hide()',5000);
		}
		checkLogin();
		
		$j("body").click(function(e){
			var lic = $j(e.target).parent().find("li").length;				
			if(!lic){
				$j("#user-prof ul").hide();
			}
		})
		
