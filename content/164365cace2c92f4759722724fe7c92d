function loginPopD(){	
	var searchthis = $j("#search");
	searchtit = searchthis.attr("title");
	searchKey = searchthis.val();
	searchKey = (searchtit==searchKey) ? "" : searchKey;
	$j(".popup-contentbody").html("<iframe id='ordsigfrm' src='"+udomain+"/ordersignuplogin.php?svc=search&opt=login&skey="+searchKey+"&scat="+advitem+"' frameborder='0' scrolling='no' width='730px' height='265px' ></iframe>").css({"padding":"10px"});
	showPopDiv();
}

function setTotalC(status,site){
	var rank = tmJSN[site];
	if(rank<=20){
		topRA[20][status]  = topRA[20][status]+1; 
		topRA[50][status]  = topRA[50][status]+1; 
		topRA[100][status] = topRA[100][status]+1;
		topRA[250][status] = topRA[250][status]+1;
		topRA[500][status] = topRA[500][status]+1;
	}else if(rank<=50){
		topRA[50][status]  = topRA[50][status]+1; 
		topRA[100][status] = topRA[100][status]+1;
		topRA[250][status] = topRA[250][status]+1;
		topRA[500][status] = topRA[500][status]+1;
	}else if(rank<=100){
		topRA[100][status] = topRA[100][status]+1; 
		topRA[250][status] = topRA[250][status]+1;
		topRA[500][status] = topRA[500][status]+1;
	}else if(rank<=250){
		topRA[250][status] = topRA[250][status]+1; 
		topRA[500][status] = topRA[500][status]+1;
	}else if(rank<=500){
		topRA[500][status] = topRA[500][status]+1; 
	}
}

function setDiffTopRA(){
	var difCatA = new  Array("20","50","100","250","500");
	var diff = 0;
	$j.each(difCatA,function(m,cat){
		diff = topRA[cat]["a"]+topRA[cat]["t"]+topRA[cat]["d"]+topRA[cat]["mi"]+topRA[cat]["ma"]+topRA[cat]["h"];
		diff = cat-diff;
		topRA[cat]["d"] = topRA[cat]["d"]+diff;
	})
}
function addWatRegCart(action){
	var filter = {};	
	var package = $j("#ord-pack input[type='radio']:checked").val();	
	var skey = $j("#oskey").val();
	//var action = $j(this).attr("action");
	var freq = 7;
	var email = "";
	var svc = action;
	var reppdf = "";
	var cktype = "";
	var maxCrd = $j("#ord-pack input[type='radio']:not(:disabled):last").attr("package");
	if(svc=="reg"){
		freq = $j("input[name='package']:checked").attr("package");
	}
	
	
	$j.extend(filter,{"type": "cart","action":"wat","pack":package,"skey":skey,"freq":freq,"email":email,"svc":svc,"max":maxCrd});
			
	if($j("input[type='checkbox']").is(":checked")){
		//alert($j("input[type='checkbox']:checked").attr("name"));
		$j.each($j("input[type='checkbox']:checked"),function(m,n){
			//reppdf += ","+$j(this).attr("name")+":1"; 
			cktype = $j(this).attr("name");
			if(cktype=="rprice")
				$j.extend(filter,{"rprice": 1});
			else if(cktype=="pprice")
				$j.extend(filter,{"pprice": 1});
		})
		
	}	
	$j.post("actions.php",filter,
			function(data){					
				$j("#sstab").val("summary");
				$j("#svcfrm").submit();								
	},"json");
}
function unCheckBox(){
	var checktr = $j(this).parent().parent();	
	$j("#ord-pack input[type='checkbox']").attr("checked",false);
	checktr.find("input[type='checkbox']").attr("checked",true);
}
function ckCheckBox(){	
	if($j(this).attr("checked")){
		var checktr = $j(this).parent().parent().parent();	
		$j("#ord-pack input[type='radio']").not(checktr.find("input[type='radio']")).attr("checked",false);
		$j("#ord-pack input[type='checkbox']").not(checktr.find("input[type='checkbox']")).attr("checked",false);
		checktr.find("input[type='radio']").attr("checked",true);
	}
}

$j.extend({
  getUrlVars: function(){
    var vars = [], hash;
    var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
    for(var i = 0; i < hashes.length; i++)
    {
      hash = hashes[i].split('=');
      vars.push(hash[0]);
      vars[hash[0]] = hash[1];
    }
    return vars;
  },
  getUrlVar: function(name){
    return $j.getUrlVars()[name];
  }
});
jQuery.fn.togglenClass = function(a, b) {
    return this.each(function() {
        jQuery(this).attr("class",(jQuery(this).attr("class") == a ? b : a));
    });
};
</script>
